<script type="text/javascript">

function checkAddCommentForm()
{
	var form = document.getElementById('form_add_comment');
	if (!form) return false;

	var err = document.getElementById('errors_add_comment');
	if (!err) return false;
	err.innerHTML = "";

	var errors = new Array;
	form.comment_username.style.backgroundColor = '#ffffff';
	if (form.comment_password) form.comment_password.style.backgroundColor = '#ffffff';
	form.comment_text.style.backgroundColor = '#ffffff';
	
	if (form.comment_username.value == '' || form.comment_username.value == 'имя')
	{
		errors.push("Не введено имя");
		form.comment_username.style.backgroundColor = '#ffff99';
	}
	
	if (form.comment_password && (form.comment_password.value == '' || form.comment_password.value == 'пароль'))
	{
		errors.push("Не введен пароль");
		form.comment_password.style.backgroundColor = '#ffff99';
	}
	
	if (form.comment_text.value == '')
	{
		errors.push("Не введен текст комментария");
		form.comment_text.style.backgroundColor = '#ffff99';
	}
//	if (closed == '1')
//	{
//		errors.push("Новость закрыта для дальнейшего добавления комментариев");
//		form.comment_text.style.backgroundColor = '#ffff99';
//	}
	if (errors.length > 0)
	{
		var err_inner = "";
		for (i = 0; i < errors.length; i++)
		{
			err_inner = err_inner + "<b>(!)</b>&nbsp;" + errors[i] + "<br>\n";
		}
		err.innerHTML = err_inner;
		return false;
    }

    form.submit();
}

function showCommentsDislaimer()
{
	var more = document.getElementById('comments_disclaimer_more');
	var bottom = document.getElementById('comments_disclaimer_bottom');

	more.style.display = 'none';
	bottom.style.display = 'block';
}

</script>

