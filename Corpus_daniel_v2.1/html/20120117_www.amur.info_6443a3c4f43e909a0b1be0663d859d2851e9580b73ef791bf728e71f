<script type="text/javascript">

function showDisclaimer()
{
	var info = document.getElementById('bottom_info');
	var disc = document.getElementById('bottom_disclaimer');
	if (!info) return;
	if (!disc) return;

	disc.interval = setInterval("slideDisclaimer()", 10);
}

function slideDisclaimer()
{
    var info = document.getElementById('bottom_info');
    var disc = document.getElementById('bottom_disclaimer');
    if (!info) return;
    if (!disc) return;

	var h_info = info.clientHeight;
	if (h_info == 0) h_info = info.offsetHeight; // IE workaround
	if (h_info > 1) h_info = h_info - 1;

	var h_disc = disc.clientHeight;
	if (h_disc < disc.scrollHeight) h_disc = h_disc + 1;

	if ((h_info == 1) && (h_disc == disc.offsetHeight))
	{
		clearInterval(disc.interval);
	}
	else
	{
		info.style.height = h_info + "px";
		disc.style.height = h_disc + "px";
	}
}

function showInfo()
{
	var info = document.getElementById('bottom_info');
	var disc = document.getElementById('bottom_disclaimer');
	if (!info) return;
	if (!disc) return;

	info.interval = setInterval("slideInfo()", 10);
}

function slideInfo()
{
	var info = document.getElementById('bottom_info');
	var disc = document.getElementById('bottom_disclaimer');
	if (!info) return;
	if (!disc) return;

	var h_info = info.clientHeight;
	h_info = h_info + 1;

	var h_disc = disc.clientHeight;
	h_disc = h_disc - 1;

	if (h_disc < 1)
	{
		clearInterval(info.interval);
	}
	else
	{
		info.style.height = h_info + "px";
		disc.style.height = h_disc + "px";
	}
}

</script>

