<script type="text/javascript">

function showPrevPhoto(index)
{
	index = index - 1;
	makeRequest('/include/photo-preview.inc?id=58133&type=news&offset=' + index + '&rand=' + Math.random(), loadPhotoPreview);
}

function showNextPhoto(index)
{
	index = index + 1;
	makeRequest('/include/photo-preview.inc?id=58133&type=news&offset=' + index + '&rand=' + Math.random(), loadPhotoPreview);
}

function loadPhotoPreview(req)
{
	if (req.readyState == 4)
	{
		if (req.status == 200)
		{
			var photo_preview = document.getElementById('photo_preview');
			if (!photo_preview) return false;
			photo_preview.innerHTML = req.responseText;
		}
	}
}

</script>
