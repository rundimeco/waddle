<script type="text/javascript">

var set1 = [1,2];
var set2 = [3,4];
var weather_online_timeout;

function selectButton(set)
{
    var butt1 = document.getElementById('butt' + set[0]);
    if (!butt1) return false;
    var butt2 = document.getElementById('butt' + set[1]);
    if (!butt2) return false;
    var arr1 = document.getElementById('arr' + set[0]);
    if (!arr1) return false;
    var arr2 = document.getElementById('arr' + set[1]);
    if (!arr2) return false;

    butt1.style.backgroundColor='#66CCFF'; 
    butt2.style.backgroundColor='#EDECE5'; 
    arr1.src='/images/uparr.gif'; 
    arr2.src='/images/dot.gif'
}

function showWeatherOnline()
{
    set1 = [1,2];
    document.getElementById('weather-forecast').style.display = 'none';
    document.getElementById('weather-online').style.display = '';
    //makeRequest('/include/weather.inc?type=0&rand=' + Math.random(), loadWeather);
    clearTimeout(weather_online_timeout);
    weather_online_timeout = setTimeout("showWeatherOnline()", 60000);
}

function showWeatherForecast()
{
    set1 = [2,1];
    document.getElementById('weather-online').style.display = 'none';
    document.getElementById('weather-forecast').style.display = '';
    //makeRequest('/include/weather.inc?type=1&rand=' + Math.random(), loadWeather);
}

function loadWeather(req)
{
    if (req.readyState == 4)
    {
        if (req.status == 200)
        {
            var weather = document.getElementById('weather');
            if (!weather) return false;
            weather.innerHTML = req.responseText;
        }
    }
}

function showCurrencyLocal()
{
    set2 = [3,4];
    document.getElementById('currency-cb').style.display = 'none';
    document.getElementById('currency-local').style.display = '';
    //makeRequest('/include/currency.inc?type=0&rand=' + Math.random(), loadCurrency);
}

function showCurrencyCB() 
{
    set2 = [4,3]; 
    document.getElementById('currency-local').style.display = 'none';
    document.getElementById('currency-cb').style.display = '';
    //makeRequest('/include/currency.inc?type=1&rand=' + Math.random(), loadCurrency);
}

function loadCurrency(req)
{
    if (req.readyState == 4)
    {
        if (req.status == 200)
        {
            var currency = document.getElementById('currency');
            if (!currency) return false;
            currency.innerHTML = req.responseText;
        }
    }
}

weather_online_timeout = setTimeout("showWeatherOnline()", 60000);

</script>
