<script language="JavaScript" type="text/javascript">
var selectmenu=document.getElementById("news_select")
var n_item = selectmenu.length;
var i;
var this_url = location.href;
this_url = this_url.replace(new RegExp(/^http:\/\//i),"");
this_url = this_url.replace(new RegExp(/^[^\/]*\//i),"\/");
var matched=0;
for (i=0;i<n_item;i++){
if (selectmenu.options[i].value == this_url){
selectmenu.options[i].selected=true;
matched=1;
break; 
}
}
if (matched==0){
  var this_url_news_cat = get_news_id_cat(this_url);
  for (i=0;i<n_item;i++){
  	var curr_url_news_cat = get_news_id_cat(selectmenu.options[i].value);
  	
    if (this_url_news_cat[0] == curr_url_news_cat[0] &&
        this_url_news_cat[1] == curr_url_news_cat[1] ){
      selectmenu.options[i].selected=true;
      matched=1;
      break;
    }
  }	
}

// 


function get_news_id_cat(the_url){
  var result = the_url.match(/\/cgi-bin\/nw\/show\.cgi\/(\d+)\/(\d+)\/(\d+)\/(\d+)/);    
  if(result == null){
  	return [0,0];
  }
  return [result[4], result[1]];
}
</script>

