function CheckKeyWord(){
	var sf = document.searchform;
	with(sf){
  if (query.value == "ÇëÊäÈë¹ØŒüŽÊ") {
		query.value = "";
		setTimeout("query.value='ÇëÊäÈë¹ØŒüŽÊ'", 1000);
	}
 }
	return true;
}

// žÄ±äÌáœ»µÄ²éÑ¯¶¯×÷
function CheckFormAction(v){
