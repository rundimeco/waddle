	with(sf){
  if (v=='web') {
		action = "http://www.sogou.com/web";
    p.value = "02240100";				
	} else {
		p.value = "02240101";
		action = "http://news.sogou.com/news";
	}
 }
	return true;
}

//žÄ±äÊäÈë¿òÑÕÉ«
function CheckColor(){
