for(var p in sogou_is_one)

 

{

 

              sogou_is_one_url += (one_cnt++ ? "&" : "?") + q(p) + "=" + q(sogou_is_one[p]);

 

}

 

var one_js = document.createElement('SCRIPT');

 

one_js.src=sogou_is_one_url;

 

document.getElementsByTagName('head')[0].appendChild(one_js);}
else{
