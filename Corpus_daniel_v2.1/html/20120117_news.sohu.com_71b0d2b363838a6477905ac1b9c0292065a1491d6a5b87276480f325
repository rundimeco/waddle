function q(s) {return s.replace(/%/g,"%25").replace(/&/g,"%26").replace(/#/g,"%23");}

 

var sogou_is_one_url = "http://sy.brand.sogou.com/brand_ad_new";

 

var one_cnt = 0;

 

for(var p in sogou_is_one)

 

{

 

              sogou_is_one_url += (one_cnt++ ? "&" : "?") + q(p) + "=" + q(sogou_is_one[p]);

 

}

 

