		var tcO = document.getElementById("shareIn").getElementsByTagName("a");
		function shareIn(o){
			for(var i=0; i<o.length; i++){
				o[i].onmouseover = function (){
					this.parentNode.getElementsByTagName("em")[0].style.display = "block";
