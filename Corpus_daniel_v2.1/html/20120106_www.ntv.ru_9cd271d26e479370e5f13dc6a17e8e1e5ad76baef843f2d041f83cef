    if(banners.length > 0) {
      var ids = "";
      for(i=0; i< 4 && i<banners.length; i++) {
        el = banners[i];
        if(ids != "") {
          ids = ids + ".";
        }
        ids = ids + $(el).attr("key");
        $("#tn_list").append(el);
      }
      $.post("/services/banners/show.jsp","ids=" + ids);
    }
