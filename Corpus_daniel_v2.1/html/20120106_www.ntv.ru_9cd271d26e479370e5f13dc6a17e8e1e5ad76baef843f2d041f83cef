 var rating_fb =  data[0].total_count;//data[0].like_count+data[0].share_count;
  if(rating_fb && rating_fb >0 ){
        $("#fbcount").html(rating_fb);
    }
  });
  
 $.getJSON('http://urls.api.twitter.com/1/urls/count.json?url=' + pageuri + '&callback=?', function(data) {
    if(data.count && data.count>0){
          $("#twcount").html (data.count);
   }
});
