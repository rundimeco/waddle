var mailcount ={};
function mailcallback(data){
   for(var key in data) {
       mailcount =  data[key];
       if(mailcount.shares>0)
         $("#mailcount").html (mailcount.shares);
      }
}
       
    $.ajax({
        url: 'http://connect.mail.ru/share_count?callback=1&url_list='+ pageuri+'&func=mailcallback',
        dataType: 'jsonp'}); 
    
  //http://api.pinterest.com/v1/urls/count.json?callback=YOUR_CALLBACK&url=YOUR_URL
  //YOUR_CALLBACK({count: 123,url: "YOUR_URL"});
  function pinterestcallback(data){
          if(data.count>0)
              $("#pincount").html(data.count);
  }
       $.ajax({
        url: 'http://api.pinterest.com/v1/urls/count.json?&url='+pageuri+'&callback=pinterestcallback',
