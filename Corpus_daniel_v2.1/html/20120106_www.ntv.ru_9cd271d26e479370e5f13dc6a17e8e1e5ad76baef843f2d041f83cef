                   // <![CDATA[
                   var vPage = 1;
                   var  newId = 257472;
                   $("#morebtn").click(function() {
                       
                       var url  = "/rest/novosti/news.jsp";
                       container = $("#vlcontainer");
                      
                       $.get(url, {"page" : vPage + 1,"newsId":newId}, function(data) {
                           vPage ++;
                           container.append(data);
                           val = jQuery.trim(data);
                           if(!(val && val.length > 0) || val.indexOf("<!-- END OF LIST -->") >= 0) $("#morebtn").hide();
                       }); 
                   });   
