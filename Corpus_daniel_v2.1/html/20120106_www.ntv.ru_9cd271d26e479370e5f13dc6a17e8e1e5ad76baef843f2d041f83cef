 <script src="/js/jquery.tinysort.min.js" type="text/javascript"></script>  
<script type="text/javascript">
  var CHECK_TIME = 60000;
  var SCRIPT_URL = "/esi/live_upd.jsp";
  var TOTAL_NEWS = 10;
 
   
   function getMyWall(){
   var el = $("#event_container");
   if(el.length==0) return;
  
   var prev = $("#event_container > li").eq(0); // берем самый первый 
   var delcount = 0;
   
   if(prev.length > 0){ 
   $.post(SCRIPT_URL, {"last" : 1}, function(data) {
      try {
          
      $(data).each(function() {
        
       //взять key  и если его нет то вставить  pos 
         var key =  $(this).attr("key");
         if(key!= undefined){
            var pos = $("#event_container > li[key='"+key+"']").length;
            if(pos == 0) {
                $(prev).before(this);
                delcount++;
            }
        }
      });
   //TODO анимация по удалению последнего 
   if(delcount>0){
        el.children().tsort("",{attr:'key',order:'desc'});
        var num = TOTAL_NEWS-1;
        $("#event_container > li:eq("+num+")").nextAll().remove();
    //показать новое 
    el.children().fadeIn(1000);
   }
      } catch(err) {}
    });
    
   }

   }
   
 jQuery(document).ready(function() {
   $.post(SCRIPT_URL, function(data) {
      $("#event_container").html(data);
   });
   var sportTimer = setInterval("getMyWall()",CHECK_TIME);
 });
</script>


