objectWeatherJQ.find(".weather-next").find("a").attr("href", "http://pogoda.interia.pl/miasta?id=" + cityId);
objectWeatherJQ.find(".weather-more").attr("href", "http://pogoda.interia.pl/miasta?id=" + cityId);
jQuery(".weather-selected-city").removeClass("weather-selected-city");
objectWeatherJQ.removeClass("weather-cities-active");
}
}
});
};

if (cookieCityId && defaultCityId !== cookieCityId){
cityName = objectWeatherJQ.find("#city-"+cookieCityId).text();
changeCity(cookieCityId,cityName);
}

jQuery("body").click(function (e) {
if (jQuery(e.target).hasClass("weather-city")) {
if (objectWeatherJQ.hasClass("weather-cities-active")) {
objectWeatherJQ.removeClass("weather-cities-active");
} else {
objectWeatherJQ.addClass("weather-cities-active");
}
} else {
objectWeatherJQ.removeClass("weather-cities-active");
}

});

