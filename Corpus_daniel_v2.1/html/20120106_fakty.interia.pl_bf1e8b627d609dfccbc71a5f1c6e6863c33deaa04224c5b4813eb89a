var weatherDay1JQ, weatherDay2JQ,cityId,cityName,cityItemJQ,
objectWeatherJQ = jQuery('#weather-box'),
defaultCityId = 11763,
cookieCityId = jQuery.cookie("cityId"),

changeCity = function (cityId,cityName){
jQuery.ajax({
type: 'GET',
url: '/ajax/widget/weather',
data: {
cityId: cityId,
for3days: 1
},
success: function (data) {
if (data && data.cityId && data.list) {
if (data.list.day0) {
objectWeatherJQ.find(".weather-state").css("background-image", '/i/weather-icons/' + data.list.day0.iconId + '.png');
objectWeatherJQ.find(".wheater-deg").text(data.list.day0.tempAvg);
}
if (data.list.day1) {
weatherDay1JQ = objectWeatherJQ.find(".weather-day1");
weatherDay1JQ.css("background-image", '/i/weather-icons/small' + data.list.day1.iconId + '.png');
weatherDay1JQ.find("span").html(data.list.day1.tempMin + '/' + data.list.day1.tempMax + '&deg;C');
}
if (data.list.day2) {
weatherDay1JQ = objectWeatherJQ.find(".weather-day2");
