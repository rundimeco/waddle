function mapHover(){
var mapJQ = jQuery("#boxPolishMap1"),
mapAreaJQ = mapJQ.find("area"),
deafultClass =  'map-province',
mapProvinceJQ = jQuery("<div/>").addClass(deafultClass).appendTo(mapJQ),
mapTip = jQuery("<div/>").addClass("map-tip").html("<span class='map-text'></span>").appendTo(mapProvinceJQ),
mapTipTxt = mapTip.find(".map-text"),
mapClass;

mapAreaJQ
.mouseover( function()
{
mapClass = jQuery(this).attr('class');
mapProvinceJQ.addClass(mapClass + ' map-active');
mapTipTxt.html(jQuery(this).attr('alt'));
mapTip.show();
})
.mouseout( function() {
mapProvinceJQ.removeClass().addClass(deafultClass)
mapTipTxt.html('');
mapTip.hide();
});
}
mapHover();
