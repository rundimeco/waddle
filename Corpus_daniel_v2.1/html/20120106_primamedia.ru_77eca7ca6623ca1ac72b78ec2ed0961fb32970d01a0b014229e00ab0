// SeViR Simple Horizontal Accordion @2007
// http://letmehaveblog.blogspot.com

$(document).ready(function(){
		$(window).resize(function(){
		if (document.body.clientWidth <= 1250)
		{
		  $("#right-baner").hide();
		  $("#left-baner").hide();
		}
		else
		{
			$("#right-baner").show();
			$("#left-baner").show();
		}
	});



	$('input.but').live('mouseover', function(){ $(this).addClass('but_hover'); })
	$('input.but').live('mouseout', function(){ $(this).removeClass('but_hover'); })
	
	// показ скртых банеров при загрузке стр
	$('.banner-hidden').show();  
});
</script>
