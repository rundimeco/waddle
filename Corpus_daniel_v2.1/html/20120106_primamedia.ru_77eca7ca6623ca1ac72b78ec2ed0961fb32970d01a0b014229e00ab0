<div class="noprint"><div class="user-bar"><div class="regionalcell"><div class="regionchooser"><script type="text/javascript">
    $(document).ready(function() {
        $('.show-popup').click(function(){

        $('.popup').hide();
        var cont = $(this).attr('rel');
        var pos = $(this).offset();

        $(cont).css({
            'left': 535,
            'top': pos.top - 45
        })
        .fadeIn('normal');

        return false;
    });

    $(document).click(function(e) {
        var $clicked = $(e.target);
        if (!$clicked.parents().hasClass('popup') ) {
        $('.popup').hide();
    }
});

$('.hide-popup').click(function(){
  $('.popup').hide();
  return false;
});
});
</script>

