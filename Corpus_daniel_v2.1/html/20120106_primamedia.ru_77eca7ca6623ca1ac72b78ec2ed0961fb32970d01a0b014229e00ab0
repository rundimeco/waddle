	var selectedVariants = 0;

	function updateVote(el) {
	  f = document.form_vote;

	  if (f.elements[el].checked==true) selectedVariants++;
	  else selectedVariants--;

	  if (selectedVariants>2) {
		f.elements[el].checked=false;
		alert("Превышено допустимое количество одновременно выбранных вариантов!");
		selectedVariants--;
	  }
	}

	function updateVoteSingle(el) {
	  f = document.form_vote;
	  if (f.f_answer[el].checked==true) selectedVariants=1;
	}

	function checkVote(theForm) {
	  f = document.form_vote;
	  if (selectedVariants<1 && f.f_answer_own.value == "") {
		alert("Необходимо выбрать вариант ответа!");
		return (false);
	  }
	  return (true);
	}
