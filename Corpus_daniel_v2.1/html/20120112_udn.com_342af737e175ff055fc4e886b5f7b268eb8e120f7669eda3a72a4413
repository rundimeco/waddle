<!-- chgFontSize javascript -->
<script>

jQuery("#fs1").bind("mouseenter",function() {
  this.src='/2010/images/fontsize_1o.gif'
});
jQuery("#fs2").bind("mouseenter",function() {
  this.src='/2010/images/fontsize_2o.gif'
});
jQuery("#fs3").bind("mouseenter",function() {
  this.src='/2010/images/fontsize_3o.gif'
});
jQuery("#fs4").bind("mouseenter",function() {
  this.src='/2010/images/fontsize_4o.gif'
});

function GetCkValue( name ) {
  var dc=document.cookie;
  var prefix=name+"=";
  var begin=dc.indexOf("; "+prefix);

  if (begin==-1) begin=dc.indexOf(prefix);
  else begin+=2;
  if (begin==-1) return "";
  var end=document.cookie.indexOf(";",begin);
  if(end==-1) end=dc.length;
  return dc.substring(begin+prefix.length,end);
}

function setCookie(name,value,expiresOrNot,path,domain,secure,flag){
  var url=document.URL;
  var howLong=new Date();
  howLong.setFullYear(howLong.getFullYear()+1);
  var curCookie=name+"="+escape(value) +
  ((expiresOrNot)?";expires="+howLong.toGMTString():"") +
  ((path)?";path="+path:"") +
  ((domain)?";domain="+domain:"") +
  ((secure)?";secure":"");
  document.cookie=curCookie;
  if(flag!="null")history.go(0);
}

if (GetCkValue("FontSize")!="") {
  var fs = GetCkValue("FontSize");

  chgFontSize(fs);
}
else {
  var fs = 2;

  chgFontSize(fs);
}

function chgFontImg(size) {

  jQuery("#fs4").attr("src","/2010/images/fontsize_4.gif");
  jQuery("#fs3").attr("src","/2010/images/fontsize_3.gif");
  jQuery("#fs2").attr("src","/2010/images/fontsize_2.gif");
  jQuery("#fs1").attr("src","/2010/images/fontsize_1.gif");

  jQuery("#fs4").bind("mouseleave",function() {
    this.src='/2010/images/fontsize_4.gif'
  });
  jQuery("#fs3").bind("mouseleave",function() {
    this.src='/2010/images/fontsize_3.gif'
  });
  jQuery("#fs2").bind("mouseleave",function() {
    this.src='/2010/images/fontsize_2.gif'
  });
  jQuery("#fs1").bind("mouseleave",function() {
    this.src='/2010/images/fontsize_1.gif'
  });

  if (size == "4") {
    jQuery("#fs4").attr("src","/2010/images/fontsize_4o.gif");

    jQuery("#fs4").bind("mouseleave",function() {
      this.src='/2010/images/fontsize_4o.gif'
    });
  }
  if (size == "3") {
    jQuery("#fs3").attr("src","/2010/images/fontsize_3o.gif");

    jQuery("#fs3").bind("mouseleave",function() {
      this.src='/2010/images/fontsize_3o.gif'
    });
  }
  if (size == "2") {
    jQuery("#fs2").attr("src","/2010/images/fontsize_2o.gif");

    jQuery("#fs2").bind("mouseleave",function() {
      this.src='/2010/images/fontsize_2o.gif'
    });
  }
  if (size == "1") {
    jQuery("#fs1").attr("src","/2010/images/fontsize_1o.gif");

    jQuery("#fs1").bind("mouseleave",function() {
      this.src='/2010/images/fontsize_1o.gif'
    });
  }

}

function chgFontSize(size) {
  if (size == "4") {
    jQuery("div#story").css("font-size","20px");
    jQuery("div#story").css("line-height","190%");
   // jQuery("div.menu_block > span").each(function () { jQuery(this).css("font-size","12pt")});
    chgFontImg("4");
    setCookie("FontSize","4",false,"/","udn.com","","null");
  }
  else if (size == "3") {
    jQuery("div#story").css("font-size","18px");
    jQuery("div#story").css("line-height","190%");
    chgFontImg("3");
    setCookie("FontSize","3",false,"/","udn.com","","null");
  }
  else if (size == "2") {
    jQuery("div#story").css("font-size","15px");
    jQuery("div#story").css("line-height","190%");
    chgFontImg("2");
    setCookie("FontSize","2",false,"/","udn.com","","null");
  }
  else {
    jQuery("div#story").css("font-size","13px");
    jQuery("div#story").css("line-height","190%");
    chgFontImg("1");
    setCookie("FontSize","1",false,"/","udn.com","","null");
  }
}

</script>

<!-- dignews forum -->
<script>
//jQuery(document).ready(function() {
//  jQuery(".content_hot").append("<div id='dignews'></div>");
//});

function rId() {
  var url=window.location.toString();
  var ary=url.split("/")
  var page=ary[ary.length-1];
  if (page.indexOf("_") != -1) {
    _ary=page.split("_");
    var DigId=_ary[0];
  }
  else {
    var ary2=page.split(".");
    var DigId=ary2[0];
  }

  return DigId;
}

function InitDig(page) {
  DigId = rId();
  jQuery(".content_hot").append("<div id='dignews'></div>");
  jQuery("#dignews").html("<center><img src='/images/forumimg/images/loading2.gif?1x'><br /><br /><font style='font-weight:bolder;font-size:16px'>討論區塊載入中</font></center>");

  if (page == "") page = 1;
  setTimeout("ShowDigContent("+DigId+",'b',"+page+")",100);
  ShowDigCount();
}

function ShowDig(page) {
  DigId = rId();

  jQuery("#dignews").html("<center><img src='/images/forumimg/images/loading2.gif?xx'><br /><br /><font style='font-weight:bolder;font-size:16px'>討論區塊載入中</font></center>");

  if (page == "") page = 1;
  setTimeout("ShowDigContent("+DigId+",'a',"+page+")",100);
}

function ShowDigCount() {
  DigId = rId();

  var htmlContent = jQuery.ajax({
    url: "/forum/postn.jsp?news_id="+DigId,
    async: true,
    success: function(html){
     jQuery("#postn").html(html);
    },
    error: function() {}
  });

}

function ef() {
  DigId = rId();
  window.location.href="http://forum.udn.com/forum/letter.jsp?newsid="+DigId;
}
function ShowDigContent(DigId,TUrl,page) {
  //var htmlContent = 
  jQuery.ajax({
    url: "/forum/post"+TUrl+".jsp?news_id="+DigId+"&currentPage="+page,
    async: true,
    success: function(html){
      jQuery("#dignews").hide();
      jQuery("#dignews").html(html);
      jQuery("#dignews").toggle('slow');
    },
    error: function() {}
  });
  // 要判斷  success 才SHOW or IE6 will hang
  //.responseText;
}

jQuery(document).ready(function() {
  ShowDigCount();
 if (  $jq.browser.msie && $jq.browser.version == '6.0' )  {
   // InitDig(1); 
 }else {InitDig(1); }
});
</script>

