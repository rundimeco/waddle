function show_comments2(lang_id,news_id,page) {
	var rnd = Math.round(Math.random() * 1000000000);
	$('#comment_div').html('loading...');
	$('#show_all_comments').val('loading...');
	$.ajax({
		type: "POST",
		url: "/news/comments/ajax_list_comments.php",
		data: "&id="+news_id+"&lang="+lang_id+'&page='+page+"&rnd="+rnd,
		success: function(msg){
			//$.each(msg, function(key, value){
				//$('#comment_div').html(value);
				$('#comment_div').html(msg);
			 // });
		}
	});
};
</script>
