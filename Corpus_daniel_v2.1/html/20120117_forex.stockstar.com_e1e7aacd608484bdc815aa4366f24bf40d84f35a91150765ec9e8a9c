    <script>
        function checkValue(count) {
            if (document.getElementById("stockCode_" + count)) {
                var stockcode = document.getElementById("stockCode_" + count).value;
                if (!isStock(stockcode)) {
                    alert("请您填写正确的股票代码以便及时获得投资情报");
                    //document.getElementById("stockCode").focus();
                    return false;
                }
            }
            //var description = document.getElementById("description").value;
            if (document.getElementById("phone1_" + count)) {
                var phone1 = document.getElementById("phone1_" + count).value;
                if (!isMobel(phone1)) {
                    alert("请您填写真实手机号以便及时获得投资情报");
                    //document.getElementById("phone1").focus();
                    return false;
                }
            }


        }


        function isStock(value) {
            if (value === null || value.length === 0)
                return false;
            var regex = RegExp("^\\d{6}$");
            if (!value.match(regex)) {
                return false;
            }
            else {
                return true;
            }
        }

        function isMobel(value) {
            if (/^13\d{9}$/g.test(value) || (/^15[0-35-9]\d{8}$/g.test(value)) || (/^18[05-9]\d{8}$/g.test(value))) {
                return true;
            } else {
                return false;
            }
        }

        function showMsg(msg) {
            alert(msg);
        }
    </script>
    
