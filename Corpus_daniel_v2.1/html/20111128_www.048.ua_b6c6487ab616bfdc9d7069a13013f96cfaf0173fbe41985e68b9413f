    //<![CDATA[
var client;
$(document).ready(function(){
	client = jQuery.Zend.jsonrpc({url: '/api/v1/jsonrpc'});
	$('#subscribe').click(function(){
		subscribenews('subscribe', $('#email').val());
	});

	$('#unsubscribe').click(function(){
		subscribenews('unsubscribe', $('#email').val());
	});
});
function subscribenews(action, email){
	var data = client.subscribenews(action, email);
	$.each(data, function(index, value) {
		$("#error_block").html(value);
	});
}
    //]]>
</script>
