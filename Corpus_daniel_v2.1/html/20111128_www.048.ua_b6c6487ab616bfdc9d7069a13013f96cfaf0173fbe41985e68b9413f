function authInfo(session) {
	$.post('/comment/savejsauth', {
		authtype: 'vkontakte'
	}, function(ret) {
		if (ret.status && typeof(authEvent) != 'undefined') {
			authEvent();
		}
	},
	'json');
}" border="0" /></span>		<span style="cursor:pointer;"><img title="Facebook" src="/design/img/comments/btn_facebook.jpg" onClick="javascript:if (typeof(FB) != 'undefined') {
	session = FB.getSession();
	if (session) {
		authInfo(session);
	} else {
		FB.login(function (responce) {
			if (responce.session) {
				authInfo(responce.session);
			}
		});//, {perms:'publish_stream'});
	}
}

function authInfo(session) {
	$.post('/comment/savejsauth', {
		authtype: 'facebook'
	}, function(ret) {
		if (ret.status && typeof(authEvent) != 'undefined') {
			authEvent();
		}
	},
	'json');
}" border="0" /></span>		<span style="cursor:pointer;"><span onClick="javascript:var sH=2*screen.height/3;
