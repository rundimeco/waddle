<script type="text/javascript">
var mounts=new Array('января','февраля','марта','апреля','мая','июня','июля','августа','сентября','октября','ноября','декабря');
$(document).ready(function(){
	var dateObj = new Date();
	dateObj.setHours(dateObj.getHours()+2);
	var new_date_str = dateObj.getUTCDate() + ' ' + mounts[dateObj.getUTCMonth()] +
	' | ' + dateObj.getUTCHours() + ':' + addnul(dateObj.getUTCMinutes());
	$('.header_date').html(new_date_str);
});
function addnul(n)
{
	if (n < 10) {
		return '0' + n;
	}
	return n;
}
</script>
