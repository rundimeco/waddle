<script type="text/javascript">
$(document).ready(function(){

	$("#pogoda_blok").click(
		function(){
			var p = $("#pogoda_blok");
			var offset = p.offset();
	        offsetleft = offset.left;
			offsettop = offset.top + 30;
            $("#pogoda_small").css({'top' : offsettop, 'left' : offsetleft});
			$("#pogoda_small").slideToggle("fast");
			return false;
		},
		function(){
			$("#pogoda_small").slideUp("fast");
		}
	);
});
</script>
