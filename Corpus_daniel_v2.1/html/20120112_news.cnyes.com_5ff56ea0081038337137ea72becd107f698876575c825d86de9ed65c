<script type="text/javascript">
function switchTab(obj, num, len, cn)
{
	var objcss = cn
	for(var i = 1 ; i<=len; i++)
	{
		var otab = obj + '_tab_' + i;
		var ocnt = obj + '_cnt_' + i;
		if (i == num){
		  try{document.getElementById(otab).className=objcss}catch(e){};
		  try{document.getElementById(ocnt).style.display="block"}catch(e){};
		}else{
		  try{document.getElementById(otab).className=""}catch(e){};
		  try{document.getElementById(ocnt).style.display="none"}catch(e){};
		}
	}
}

function switchTab2(obj, num, len, cn)
{
	var objcss = cn
	for(var i = 0 ; i<len; i++)
	{
		var otab = obj + '_tab_' + i;
		var ocnt = obj + '_cnt_' + i;
		if (i == num){
		  try{document.getElementById(otab).className=objcss}catch(e){};
		  try{document.getElementById(ocnt).style.display="block"}catch(e){};
		}else{
		  try{document.getElementById(otab).className=""}catch(e){};
		  try{document.getElementById(ocnt).style.display="none"}catch(e){};
		}
	}
}
</script>
