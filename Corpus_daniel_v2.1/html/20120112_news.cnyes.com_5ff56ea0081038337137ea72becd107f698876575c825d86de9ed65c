<script type="text/javascript">

function goOpenSearch(keyword)
{
    var tempU = '' ; 
    
    var q = ""
    
    if (keyword == '')
    {
        q = document.getElementById('search').value
    }
    else
    {
        q = keyword
    }
    if (q == '請輸入新聞關鍵字' || q == '')
    {
        q = '頭條'
    }
    tempU = '/search.aspx?q=' + encodeURI( q ) + '&D=7&P=1&1=1'
    
    openFullPage(tempU)
}

function openFullPage(myURL)
{   
    MM_openBrWindow(myURL,'','width=1000,height=710,toolbar=yes,location=yes,directories=yes,scrollbars=yes,menubar=yes,status=yes,resizable=yes')
}
function MM_openBrWindow(theURL,winName,features) { //v2.0
  window.open(theURL,winName,features);
}

</script>
