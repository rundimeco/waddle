<script type="text/javascript">
function initEcho()
{
$('.articleSidebar a.comments span').each(function(){
if($(this).html()==0){
$(this).parent().attr('style','display:none;');
}
});
$(".js-kit-comments-count").each(
function(index,el)
{
if($(this).text()==0)
{
var art_id=$(this).attr("id");
$(this).parent().hide();
}
});
}
window.onload=initEcho;
</script>
