							$('#shorttext').hide();
							$('#fulltext').show();
							$('#link_full').text('Скрыть полный текст');
						}
						else {
							$('#fulltext').hide();
								$('#shorttext').show();
								$('#link_full').text('Читать полностью');
							}									
						});
				</script>
					
						
				
									
						
			
	
