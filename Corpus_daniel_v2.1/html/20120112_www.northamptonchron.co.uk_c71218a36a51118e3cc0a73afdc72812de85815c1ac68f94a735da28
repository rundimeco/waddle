        function onBlur(field) {
            if (field.value == '') field.value = field.defaultValue;
        }
        function onFocus(field) {
            if (field.defaultValue == field.value) field.value = '';
        }
		function postComment(field) {
		var body = $('#body');
		var val = body.val();
		var def = body.attr ('defaultValue');
		if (val==def || val=='') {
			alert ("Please enter a comment");
			return false;
		}
		var btn = document.getElementById("postCommentBtn");
		btn.disabled=true;
		btn.textContent='Sending...';
		
		var myJSONObject = {"body": val, "action": "postcomment", "submitted": "true", "ajax": "true", "contentid": "ContentId(1.3402290)"};
		var postMessage = "<br /><b>Your comment has been posted.</b>";
		
		jQuery.post("http://www.northamptonchron.co.uk/news/features/the_chron_looks_at_the_impact_the_pip_implant_debate_is_having_on_professionals_and_patients_1_3402290", myJSONObject, function(result){

			var htmlResult = $(result).find ('#commentsSection');
			$('#commentsSection').html(htmlResult);
			$('#ajaxPost').html(postMessage);
			$('.loggedout').empty();
			$('#loggedinas').text( loginStatusResponse.loginStatus.screenname );
			$('.reportlink').show();

		});

		return false;
		
		}
    </script>
	
