jQuery(window).load(function () {
    if(window.location.href=='http://www.money.pl/ubezpieczenia/wiadomosci/artykul/szejnfeld;podatek;dochodowy;dla;rolnikow;blokuje;reforme;krus,6,0,1253638.html') {
