<script type="text/javascript">

if(typeof(jQuery.cookie("pit_dis")) == "undefined")
{
var show_flag=false;
jQuery(window).scroll(function() {
	if (jQuery(this).scrollTop()<750) {
		jQuery('#dymek').fadeOut();
	} else {
		jQuery('#dymek').fadeIn();
        if (show_flag==false) {
			show_flag = true;	
		}
	}
});	
}
function dymClose()
{
	jQuery('#dymek').remove();
	jQuery.cookie("pit_dis", 1, { expires : 7, domain  : '.money.pl', path: '/' });	
}

jQuery.cookie("pit_dis", 1, { expires : 7, domain  : '.money.pl', path: '/' });	
</script>

