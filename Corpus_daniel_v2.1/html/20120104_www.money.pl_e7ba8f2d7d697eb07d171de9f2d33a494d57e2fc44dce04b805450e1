var footerEnabled='foot_money';
var prevLi=0;
function footOn(idNew, li)
{
    //alert(li)
    var toSet=document.getElementById(idNew);
    var idOld=document.getElementById(footerEnabled);
    idOld.style.display='none';
    toSet.style.display='block';
    if(prevLi)
    {
	prevLi.setAttribute("class", "");
    }
    prevLi=li;
    li.setAttribute("class", "sel");
    footerEnabled=idNew;
}
</script>
