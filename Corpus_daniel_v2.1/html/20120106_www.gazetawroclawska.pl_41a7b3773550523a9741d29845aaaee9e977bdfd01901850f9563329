<script type="text/javascript">
GR("zaladujPlikJS", "http://s.polskatimes.pl/p/js/akcja.js", "plik");

var oG = {"ostatnieID":0, "aktywny":""};
var oLi = GR("$", "menu-regiony").getElementsByTagName("li");
var dlugoscOli = oLi.length;
for (var i = 0; i < dlugoscOli; i++) {
    var kat = oLi[i].id.split("-").shift();
    if (kat == "kategoria") {
        if (i == 0 && !GR("$", oG["ostatnieID"])) {
            oG["ostatnieID"] = 0;
            oG["aktywny"] = oLi[i].id;
        }
        oLi[i].onmouseover = function() { zmien(this.id, true); return false }
        oLi[i].onmouseout = function() { zmien(this.id, false); return false }
    }
}

function zmien(id, focus) {
    if (oG["ostatnieID"] == 0) {
        var tbEl = ['pod'+id];
    }
    else {
        var tbEl = ['pod'+id];
        GR('usunCssClass',GR('$',oG["ostatnieID"]), 'aktywny');
    }
    GR('pokazUkryj',tbEl);
    if (focus) {
        GR('dodajCssClass',GR('$',id), 'aktywny');
    }
    oG["ostatnieID"] = id;
}
(function(tr){
    var nazwa;
    GR('$$', 'menu-pierwszy-poziom', $('menu-regiony'), '', function(e) {
        nazwa = e.innerHTML;
        tr.push({elements: [e], href: '/menu/poziom_1:' + nazwa.trim().replace(/<\/?b>/ig, '')});
    });
    GR('$$', 'menu-drugi-poziom', $('menu-regiony'), '', function(e) {
        nazwa = e.innerHTML;
        tr.push({elements: [e], href: '/menu/poziom_2:' + nazwa.trim().replace(/<\/?b>/ig, '')});
    });
    tr.push({elements: [$('menu-drugi-poziom_logo')], href: '/menu/poziom_2:logo_gratka'});
})(oG_OSA.tracking);

</script>       </div>

