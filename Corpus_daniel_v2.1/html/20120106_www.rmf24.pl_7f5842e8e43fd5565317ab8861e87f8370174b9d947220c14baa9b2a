<script type="text/javascript" src="http://w.iplsc.com/external/jquery.floatbox/1.0.6/jquery.floatbox.js"></script>
<script type="text/javascript" src="http://w.iplsc.com/internal/jquery.setFloatbox/1.0.6/jquery.setFloatbox.js"></script>
<script type="text/javascript">
    //<![CDATA[

jQuery(document).ready(function(){
jQuery('a[rel="floatbox[login]"], a[rel="floatbox[login_referer]"], a[rel="floatbox[login_href]"]')
.each(function () {
var ajaxUrl = '/ajax/logowanie';

switch(true) {
case jQuery(this).attr('rel') === 'floatbox[login_referer]':
ajaxUrl = '/ajax/logowanie?referer=%2Ffakty%2Fpolska%2Fnews-kolejny-bubel-w-zwiazku-z-refundacja-lekow-tym-razem-chodzi-%2CnId%2C425207&crc=786870168';
break;

case jQuery(this).attr('rel') === 'floatbox[login_href]':
ajaxUrl = function (aJQ) {
var href = aJQ.attr('href');
var index = href.indexOf('?');
return '/ajax/logowanie' + (index === -1 ? '' : (href.substring(index)));
};
break;
}

jQuery(this).setFloatbox({
ajaxUrl: ajaxUrl,
wrapperClass: 'floatbox-loginForm',
closeButtonHtml: '<div class="close-floatbox-button close-floatbox"><span>zamknij</span></div>',
after: function (htmlJQ) {
setTimeout(function() {
jQuery('#login').focus();
},
1000);

jQuery('#floatbox-box')
.find('a, input, textarea').not('[type="hidden"]')
.each(function(i) {
jQuery(this).attr('tabindex', i + 1);
});
}});
});
});    //]]>
</script>
<script type="text/javascript" src="/j/cookiesPolicy.js"></script>
<script type="text/javascript" src="http://w.iplsc.com/internal/inpl.box.list/1.0.1/inpl.box.list.js"></script>
<script type="text/javascript">
    //<![CDATA[

jQuery(document).ready(function(){
jQuery("#articleObjectListCategory1").boxList({
params: {sort: ['dates', 'views', 'rates', 'forum']},
listAjaxUrl: '/ajax/article-category/rank?categoryId=555',
get: function (liJQ, params) {
var icon = {};
liJQ.find('.title a span.icon').each(function(index, elem){
var iconElem = jQuery(elem).attr('class');
iconElem.substring(5)
icon[iconElem] = 1;
});
return {
title: liJQ.find('.title a').text(),
date: liJQ.find('.date').text(),
type: icon,
link: liJQ.find('.title a').attr('href')};

},
set: function (liJQ, data, params) {
liJQ.find('.title a span:not([class])').text(data.title);
liJQ.find('.date').text(data.date);
liJQ.find('.title a').attr('href', data.link);
if( data.type ) {
jQuery.each(data.type, function(iconType, value){
jQuery('<span/>')
.html(' ')
.addClass('icon '+iconType)
.appendTo(jQuery(liJQ.find('.title a')))
})
}
},
after: function (params) {
if( params.sort == 'dates' )
{
jQuery("#articleObjectListCategory1").find('.seeMore').show();
}
else
{
jQuery("#articleObjectListCategory1").find('.seeMore').hide();
}
var listId = 'articleObjectListCategory1_list_'+params.sort;
jQuery("#articleObjectListCategory1").find('.list').attr('id',listId);
},
fadeSpeed: 200,
cookieEnabled: false});
});
jQuery(document).ready(function(){
if(Inpl.Ad.isAd('box300x250')) {
jQuery('#box300x2501').show();
}
else {
jQuery('#box300x2501').hide();
}
});
