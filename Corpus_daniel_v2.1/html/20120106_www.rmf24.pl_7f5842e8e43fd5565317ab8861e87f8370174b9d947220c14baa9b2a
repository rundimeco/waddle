.attr({type: 'hidden', name: 'senderSess', id: 'senderSess', value: uid_cookie})
.appendTo(senderLi);

jQuery('<input />')
.attr({type: 'hidden', name: 'senderEmail', id: 'senderEmail', value: uemail_cookie})
.appendTo(senderLi);
}
})();


