<script>
jQuery(document).ready(function(){
	jQuery('.productFormAgreeCheck').live('click', function(){
		if(jQuery(this).is('.CheckIn')){
			jQuery(this).removeClass('CheckIn').addClass('CheckOut');
			jQuery(this).find('input').val('nie')
			jQuery(this).parent().find('.error_msg').show()
		}else if(jQuery(this).is('.CheckOut')){
			jQuery(this).removeClass('CheckOut').addClass('CheckIn')
			jQuery(this).find('input').val('tak')
			jQuery(this).parent().find('.error_msg').hide()	
		}
	})
	function closeModalBox(){
		jQuery('#dialogBox').hide().empty();
		jQuery('#glassBox').fadeOut(400, function(){
			jQuery("#story").find('p').show();
		});
		setCookie('closeId', '786691_1313247');
		
	}
	function showModalFormBox(){
		
		jQuery('#dialogBox').empty();
		jQuery('#glassBox').fadeTo(400, 0.5);
		
		jQuery('.subscriptFormBox').clone().appendTo('#dialogBox').show();
		jQuery('#dialogBox').show();
		
		
	}
	
	function closeAndShowModalBox(){
		
		jQuery('html, body').animate({scrollTop: '0px'}, 300);
		if(jQuery('#glassBox').is(":hidden")){
			jQuery('#dialogBox').css({
				'position' : 'absolute',
				'top' : '50px',
				'bottom' : 'auto'
			});
			jQuery('#dialogBox').empty();
			jQuery('.subscriptFormBox').clone().appendTo('#dialogBox').show("fast", function(){
				jQuery('#glassBox').fadeTo(400, 0.5, function(){
					jQuery('#dialogBox').addClass('noAccessLoginBoxDialog').css('left', (document.body.clientWidth - jQuery('#dialogBox').outerWidth()) / 2);
					jQuery('#dialogBox').fadeIn(300, function(){
						jQuery('#dialogBox').find('.subscriptForm').attr('id', 'subscriptFormSender')
						jQuery('#dialogBox').find('.subscriptForm').find('.dateto').attr('id', 'dateto')
						jQuery('#dialogBox').find('.subscriptForm').find('.datefrom').attr('id', 'datefrom')
						
						jQuery('#subscriptFormSender').validate({
							submitHandler : function(form) {
								jQuery(form).ajaxSubmit({
									url : '/call-action',
									type : 'POST',
									success: showResponse,
									error: showError
								});
							},
							groups: {
								postal: "postal1 postal2",
								date: "datefrom dateto"
							},
	  						errorPlacement: function(error, element) {
	     						if (element.attr("name") == "postal1" || element.attr("name") == "postal2" )
	   								error.insertAfter("#subscriptFormSender #postal2");
	    						else if (element.attr("name") == "datefrom" || element.attr("name") == "dateto" )
	   								error.insertAfter("#subscriptFormSender #dateto");
	       						else
	       							error.insertAfter(element);
	   							}
						})
						
					});
				});	
			});
			
			
		}else{
			jQuery('#dialogBox').css({
				'position' : 'absolute',
				'top' : '50px',
				'bottom' : 'auto'
			});		
			jQuery('#dialogBox').fadeOut(300, function(){
				jQuery(this).empty();
				jQuery('.subscriptFormBox').clone().appendTo('#dialogBox').show();
				jQuery(this).fadeIn(300, function(){
					jQuery('#dialogBox').find('.subscriptForm').attr('id', 'subscriptFormSender')
					jQuery('#dialogBox').find('.subscriptForm').find('.dateto').attr('id', 'dateto')
					jQuery('#dialogBox').find('.subscriptForm').find('.datefrom').attr('id', 'datefrom')
					jQuery('#subscriptFormSender').validate({
						submitHandler : function(form) {
							jQuery(form).ajaxSubmit({
								url : '/call-action',
								type : 'POST',
								success: showResponse,
								error: showError
							});
							
						},
						groups: {
							postal: "postal1 postal2",
							date: "datefrom dateto"
						},
  						errorPlacement: function(error, element) {
     						if (element.attr("name") == "postal1" || element.attr("name") == "postal2" )
   								error.insertAfter("#subscriptFormSender #postal2");
    						else if (element.attr("name") == "datefrom" || element.attr("name") == "dateto" )
   								error.insertAfter("#subscriptFormSender #dateto");
       						else
       							error.insertAfter(element);
   							}
   							
   							
					})
				})
			});
		}
		
	}
	
	function showResponse(responseText, statusText, xhr, $form){
		jQuery('html, body').animate({scrollTop: '0px'}, 300);
		responseText = jQuery.trim(responseText);
		if(responseText == 'duplicatePhone'){
			jQuery('.duplicatePhone').show();
		}else if(responseText == 'wrongDate'){
			jQuery('.wrongDate').show();
		}else{
			jQuery('#dialogBox').find('.subscriptFormBox_responseBox').html(responseText);	
		}
		jQuery('#dialogBox').undelegate('.confirmFormBtn','click', function(){
			return false
		})
	}
	function showError(){
		jQuery('html, body').animate({scrollTop: '0px'}, 300);
		jQuery('#dialogBox').find('.subscriptFormBox_formBody').slideUp(300,function(){
			jQuery('#dialogBox').find('.subscriptFormBox_Info').html('<strong class="errorLabel">Wysłanie formularza nie powiodło się</strong><br />prosimy spróbować ponownie później');	
		});		
	}
	jQuery(function(a) {a.datepicker.regional.pl = {
			closeText : "Zamknij",
			prevText : "&#x3c;Poprzedni",
			nextText : "Następny&#x3e;",
			currentText : "Dziś",
			monthNames : ["Styczeń", "Luty", "Marzec", "Kwiecień", "Maj", "Czerwiec", "Lipiec", "Sierpień", "Wrzesień", "Październik", "Listopad", "Grudzień"],
			monthNamesShort : ["Sty", "Lu", "Mar", "Kw", "Maj", "Cze", "Lip", "Sie", "Wrz", "Pa", "Lis", "Gru"],
			dayNames : ["Niedziela", "Poniedziałek", "Wtorek", "Środa", "Czwartek", "Piątek", "Sobota"],
			dayNamesShort : ["Nie", "Pn", "Wt", "Śr", "Czw", "Pt", "So"],
			dayNamesMin : ["N", "Pn", "Wt", "Śr", "Cz", "Pt", "So"],
			weekHeader : "Tydz",
			dateFormat : "yy-mm-dd",
			firstDay : 1,
			isRTL : !1,
			showMonthAfterYear : !1,
			yearSuffix : "",
			changeMonth: true,
			changeYear: true
		}, a.datepicker.setDefaults(a.datepicker.regional.pl)
	})
	
	jQuery('#datefrom').live('focus', function(event) {
		jQuery(this).datepicker();
	});
	jQuery('#dateto').live('focus', function(event) {
		jQuery(this).datepicker();
	});
	jQuery('.subscriptionBtn').bind('click', function(){
		closeAndShowModalBox();
	})
	jQuery('.cancelFormBtn').live('click', function(){
		closeModalBox()
	})
	
	jQuery('#dialogBox').delegate('.confirmFormBtn','click', function(){
		var sender = false;
		jQuery('.valid_checkAgree').each(function(index) {
			  if(jQuery(this).val() != 'tak'){
			  	jQuery(this).parent().parent().find('.error_msg').show();
			  	sender = false;
			  }else{
				sender = true
			  }
		});
		if(sender = true){
			jQuery('#subscriptFormSender').submit();	
		}else{
			return false;
		}
		
	})
	
	jQuery('.subscriptFormBox_closeIcon').live('click', function(){
		closeModalBox()
	})
	
})
</script>


