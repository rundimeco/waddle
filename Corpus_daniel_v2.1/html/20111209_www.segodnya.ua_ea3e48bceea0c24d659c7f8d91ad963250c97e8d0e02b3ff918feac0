      $('#password').keyup(function(event){
            if (event.keyCode == 13) {
                $('#login_form_submit').click();
            }
       });
        login_api = $("a.has_popup").overlay({
                expose: '#1F1618',
                effect: 'default',
                fixed:true,
                top:"20%",
                api: true,
                onLoad: function(event) {
                    $('#login').focus();
	                $('#code_img').attr('src','/exec/code.php?'+(new Date()).getTime()).show();
	                $('#reg_code_captcha').attr('src','/exec/code.php?'+(new Date()).getTime()).show();
                },
                onBeforeLoad:function(){
                	$("iframe").hide()
                },
                onBeforeClose:function(){
                    $("iframe").show();
					sunsite.anchorset('');
                },
                close:$('.layer_box_close')
       });
    });
    
// via mail register
    
    $('#register_via_email').click(function(){
        var reg_emailre = /^[\w]{1}[\w\.\-_]*@[\w]{1}[\w\-_\.]*\.[\w]{2,9}$/i;
        if ($('#reg_nick').val() =='')
        {
            show_error('#register_email_error','Вы не указали имя на сайте');
            $('#reg_nick').focus();
            return false;
        }
        if (!$('#reg_email').val().match(reg_emailre))
        {
            show_error('#register_email_error','Введите пожалуйста свой E-mail');
            $('#reg_email').focus();
            return false;
        }
        
        var reg_phone = /^[0-9]{10}$/i;
        if (!$('#reg_phone').val().match(reg_phone))
        {
            show_error('#register_email_error','Укажите корректный номер телефона');
            $('#reg_phone').focus();
            return false;
        }
        
        if ($('#reg_passwd').val() == '')
        {
            show_error('#register_email_error','Введите пожалуйста пароль');
            $('#reg_passwd').focus();
            return false;
        }
        if ($('#reg_code').val()=='')
        {
            show_error('#register_email_error','Введите пожалуйста защитный код');
            $('#reg_code').focus();
            return false;  
        }
        $('#register_form_email').hide();
        $('#loading').show();
        $.getJSON('/auth/email/register.html',$('#register_form_email').serialize(),function(data){
            
            if (data.error !='' && data.error != undefined)
            {
                $('#loading').hide();        
                $('#register_form_email').show();
                show_error('#register_email_error',data.error);
                
                $('#reg_code').val('');
                $('#reg_code_captcha').attr('src','/exec/code.php?'+(new Date()).getTime());
                
            }
            else
            {
                $('#loading').hide();
                //$('#register_form_email_result').show();
                $('#main_header').hide();
                $('#main_body').hide();
                
                //$('#check_email_header').show();
                $('#check_email_body').show();
                
                show_error('#register_email_error',data.msg);
            }
        })
        
        return false;
    })
    
    
    // email login
    $('#login_form_submit').click(function(){
        /*
        var reg_emailre = /^[\w]{1}[\w\.\-_]*@[\w]{1}[\w\-_\.]*\.[\w]{2,9}$/i;
        if (!$('#email').val().match(reg_emailre) && ($('#email').val()!='root'))
        {
            show_error('#login_email_error','Введите пожалуйста свой E-mail');
            $('#email').focus();
            return false;
        }
        */
        if ($('#password').val() == '')
        {
            show_error('#login_email_error','Введите пожалуйста пароль');
            $('#pass_field_fake').focus();
            return false;
        }
        hide_error('#login_email_error');
        alax_preload(this,1);
        $.post('/passport/login.html',{
                'email':$('#email').val(),
                'passwd':$('#password').val(),
                mode:'ajax',
                captcha: $('#f_code').val()
        }, function(data){
            eval(data);
            if (typeof(data) !='' && data.success == true) {
                $('#login_form_submit span').text('Вошли');
                document.location.href = document.location.href+(sunsite.anchorhref()?'#'+sunsite.anchorhref():'');
                document.location.reload();
            } else if (data.error != null) {
                show_error('#login_email_error',data.error);
                var isrc = $('#code_img').attr('src');
                $('#code_img').attr('src',isrc+'?'+(new Date()).getTime())
                if(data.login_attempt >= 2) $('#code_holder').show();
                
            }
            
            alax_preload('#login_form_submit',0);
                        
        }) 
        
        return false;
    });
    $('#button_facebook a, #button_twitter a, #button_google a, #button_vkontakte a').click(function(){
        location.href = $(this).attr('href')+"?redirect="+encodeURIComponent(document.location.pathname)+"&anchor="+sunsite.anchorhref();
        $('#soc_net').html('Подключаемся...');
		return false;
    })
    function show_error(selector, text) {
        $(selector).show().text(text);
    }
    function hide_error(selector) {
        $(selector).hide();
    }
    function alax_preload(selector, mode) {
        if (mode == 1) {
            if (!$(selector).prev().hasClass("ajax_preload")) {
                $(selector).before("<div class=\"ajax_preload\">Подождите несколько секунд <img width=\"16\" height=\"16\" alt=\"\" src=\"/user/img/loader.gif\" /></div>");
            }
        } else {
            $(selector).prev(".ajax_preload").remove();
        }
    }
    $('#reminder_show').click(function(){
        $('#email_login').hide();
        $('#email_reminder').show();
        return false;
    })
    $('#decline_email_reminder').click(function(){
        $('#email_login').show();
        $('#email_reminder').hide();
        return false;
    })
    // email reminder
    $('#login_form_reminder').click(function(){
        var reg_emailre = /^[\w]{1}[\w\.\-_]*@[\w]{1}[\w\-_\.]*\.[\w]{2,9}$/i;
        if (!$('#login_reminder').val().match(reg_emailre))
        {
            show_error('#login_email_reminder_error','Введите пожалуйста свой E-mail');    
            $('#login_reminder').focus();
            return false;
        }
        
        hide_error('#login_email_reminder_error');
        alax_preload(this,1);
        $.getJSON('/auth/email/reset.html',{'f[email]':$('#login_reminder').val()},function(data)    {
            //eval(data);
            if (data.msg != null && data.msg != 'undefined' )
            {
                show_error('#login_email_error',data.msg);
                $('#decline_email_reminder').click();
                $('#email').val($('#login_reminder').val());
                $('#password').focus()
            }
            else if (data.error != null)
                show_error('#login_email_reminder_error',data.error);
            alax_preload('#login_form_reminder',0);
        }) 
        
        return false;
    })
    
    $('#confirm_sms_btn').click(function(){
        if($('#sms_code').val() == '') {
            show_error('#login_email_error','Введите код из смс');
            $('#sms_code').focus();
            return false;
        }
        $.getJSON('/auth/confirm_sms.html',{ 'phone_num': $('#reg_phone').val(), 'sms_code': $('#sms_code').val() },function(data)    {
            if (data.error != null) {
                show_error('#login_email_reminder_error','Вы ввели неверный код. Код отправлен повторно');
            } else {
                document.location.href = document.location.pathname+(sunsite.anchorhref()?'#'+sunsite.anchorhref():false);
            }
        });
        return false;
    });
    $('#repeat_sms_code').click(function(){
        $.getJSON('/auth/confirm_sms.html',{ 'phone_num': $('#reg_phone').val(), 'repeat': true },function(data)    {
            if (data.error != null) {
                alert(data.error);
            } else {
                alert('Код отправлен повторно');
            }
        });
        return false;
    });
</script>



</div>


