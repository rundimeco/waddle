 <script type="text/javascript" language="javascript">
  
function disab( FormName,ElemName )
{
 var   chkOk=true;

 document.forms[FormName].elements[ElemName].disabled=true;
 document.forms[FormName].submit();

return chkOk;

}

function disabnosubmit( FormName,ElemName )
{
 var   chkOk=true;

 document.forms[FormName].elements[ElemName].disabled=true;
 

return chkOk;

}

     function invoke(form, event, container) {
              var params = Form.serialize(form);
              if (event != null) params = event + '&' + params;
               form.summ.value="";
            var myAjax =   new Ajax.Updater(container , form.action, {method:'post', postBody:params});  
			
          } 
          function invokecomm(form, event, container) {
          if( $F("summ").length<1 ){
      	    alert("輸入錯誤。");
       		   return false;
          }
              var params = Form.serialize(form);
              if (event != null) params = event + '&' + params;
             params=params+"&code=b5";
             var myAjax =  new Ajax.Updater(container , '/xtr/b5/postcomm', {method:'post', postBody:params}); 
              //$("postco").reset; 
			   form.summ.value="";
                form.name.value="";
                form.nfrom.value="";
                form.mail.value="";
			form.elements['add'].disabled=true; 
		//	getcommHTML(1);
		//	 disabnosubmit(form.name,'add');
          }      
    function getcommHTML(cpage)
    { 
     
        var url = '/xtr/b5/viewcomm';
         
          var pars ='id=644768&code=b5&page='+cpage;

         var myAjax = new Ajax.Updater('viecomm', url, {method: 'get', parameters: pars,evalScripts:true });

    }
	 function getcommFullHTML()
    {
        var url  = '/xtr/b5/viewcomm';
        var pars = 'id=644768&f=f&code=b5';

         var myAjax = new Ajax.Updater('viecomm', url, {method: 'get', parameters: pars,evalScripts:true });

    }
</script>
  
 
 
