   // jwplayer(). play();
   
  jwplayer("flashplayerbox2").onComplete(function() {
   played=played+1;
 
   if(played>=admp4){
 
     if(document.theForm.autonext.checked == true  ){
      
           document.location = "/xtr/b5/2012/01/11/a644767.html.-大陸客續增-香港迪士尼盈利升逾倍.html#video";
 
            
            
