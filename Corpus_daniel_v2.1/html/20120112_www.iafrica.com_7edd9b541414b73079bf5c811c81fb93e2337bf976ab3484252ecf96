    vc_arr[1] = "MR";
    vc_arr[2] = "ME";
    vc_arr[3] = "MC";
    vc_arr[4] = "F";
    vc_arr[5] = "B";
    vc_arr[6] = "C";
    
    vimg_arr[1] = "url(/i/pics/pics_rev/tab_item_sel_bg_tile.gif) repeat-x";
	vimg_arr[2] = "url(/i/pics/pics_rev/tab_item_sel_bg_lft.gif) no-repeat";
