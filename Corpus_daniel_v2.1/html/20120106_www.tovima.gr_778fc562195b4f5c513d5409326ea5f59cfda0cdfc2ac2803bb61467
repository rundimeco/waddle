var newsroom;
$(function () {
   newsroom = new NewsRoomBox ( 'newsroom', true, 12, 60000 );
   newsroom.UpDateLinks();
   var $newsroomselector = $('#newsroomselector');
   $newsroomselector.val(newsroom.Category());
   $newsroomselector.change(function () {
       newsroom.SetCategory($newsroomselector.val());
       newsroom.Refresh(true);
       $newsroomselector.val(newsroom.Category());
       newsroom.UpDateLinks();
   });
});
