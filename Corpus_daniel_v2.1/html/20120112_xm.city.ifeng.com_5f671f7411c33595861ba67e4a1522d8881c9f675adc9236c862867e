var docid="141504"; // 文章ID
var docname="%E5%9C%A3%E5%85%83%E5%A5%B6%E7%B2%89%E8%A2%AB%E7%96%91%E8%87%B4%E6%AD%BB%20%E6%88%96%E5%8A%A0%E5%89%A7%E5%9B%BD%E4%BA%A7%E5%A5%B6%E7%B2%89%E4%BF%A1%E4%BB%BB%E5%8D%B1%E6%9C%BA"; // 文章标题
