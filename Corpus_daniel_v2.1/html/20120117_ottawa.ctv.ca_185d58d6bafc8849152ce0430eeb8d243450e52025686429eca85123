function controlVideo(action)

{

previousTime = 0;

if (action == 1)

{

document.getElementById("VIDEO").controls.stop();

}

else if (action == 2)

{

document.getElementById("VIDEO").controls.play();

}

else if (action == 3)

{

if (x == -1)

{

startVideo();

}

else

{

if (stateI == "pause")

{

document.getElementById("pausePlay").setAttribute('src', 'http://www.ctv.ca/mar/images/local990/broadbandpause.jpg');

document.getElementById("VIDEO").controls.play();

stateI = "play";

}

else

{

stateI = "pause";

document.getElementById("pausePlay").setAttribute('src', 'http://www.ctv.ca/mar/images/local990/broadbandplay.gif');

document.getElementById("VIDEO").controls.pause();

}

}

}

else if (action == 4)

{

currentPos++;

if (currentPos == maxPos )

{

currentPos = 1;

}

functionName = document.getElementById("videoSelection" + currentPos + cT).getElementsByTagName('a')[0].getAttribute('href');

parseCurrentVideo(functionName);





//alert(functionName);

}

else if (action == 5)

{

currentPos--;

if (currentPos == 0)

{

currentPos = maxPos - 1;

}

functionName = document.getElementById("videoSelection" + currentPos + cT).getElementsByTagName('a')[0].getAttribute('href');

parseCurrentVideo(functionName);





//document.getElementById("VIDEO").controls.stop();

}

else if (action == 6)

{

alert(document.getElementById("VIDEO").fullScreen);

document.getElementById("VIDEO").fullScreen = true;

}

else if (action == 7)

{

alert(event);

}

}



function parseCurrentVideo(functionName)

{

functionName = functionName.replace("javascript:", "");

functionName = functionName.replace("changeVideo(", "");

functionName = functionName.replace(/'/g,"");



changeVideo(functionName.split(",")[0],functionName.split(",")[1],functionName.split(",")[2],functionName.split(",")[3],functionName.split(",")[4],functionName.split(",")[5].replace(")",""));





}



var mediaString;

function switchVideoT()

{

document.getElementById("defaultBroad").style.display="none";

newString = '<OBJECT name="VIDEO" id="VIDEO" width="300" height="225" CLASSID="CLSID:6BF52A52-394A-11D3-B153-00C04F79FAA6" type="application/x-oleobject">' + mediaString + '</OBJECT>';

document.getElementById("mainVideo").innerHTML = newString;

}





function switchVideo(inString)

{

inString = inString + "&ads=" + playAd + "";

if (playAd == "true")

{

simpleAd();

}

//mediaString = '<OBJECT name="VIDEO" id="VIDEO" width="300" height="225" CLASSID="CLSID:6BF52A52-394A-11D3-B153-00C04F79FAA6" type="application/x-oleobject">';

mediaString = "";

mediaString = mediaString + '<PARAM NAME="SendPlayStateChangeEvents" VALUE="True">';

mediaString = mediaString + '<PARAM NAME="TransparentAtStart" VALUE="True">';

mediaString = mediaString + '<PARAM NAME="AutoStart" VALUE="1">';

mediaString = mediaString + '<PARAM name="uiMode" value="none">';

mediaString = mediaString + '<PARAM name="fullScreen" value="false">';

mediaString = mediaString + '<PARAM name="PlayCount" value="1">';

mediaString = mediaString + '<PARAM NAME="CurrentPosition" VALUE="-1">';

mediaString = mediaString + '<PARAM NAME="Volume" VALUE="' + (currentVolume * 4) + '">';

mediaString = mediaString + '<PARAM NAME="URL" VALUE="' + inString + '">';

//mediaString = mediaString + '</OBJECT>';

if (x == -1)

{

z = setTimeout("switchVideoT()",1000);

}

else

{

//switchVideoT();

//	document.getElementById("VIDEO").innerHTML = mediaString;

document.getElementById("VIDEO").URL = inString;



}



}





function playVideo()

{

document.getElementById("VIDEO").controls.play();

}

</script>

