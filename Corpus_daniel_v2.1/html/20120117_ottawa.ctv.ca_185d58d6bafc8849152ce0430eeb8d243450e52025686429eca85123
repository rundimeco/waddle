<script type="text/javascript">
function selectNewVideo(Video)
{
    currentPosition = -1;
    for (o = 1; o < VideoPlaying.length; o++)
	{
	    if (VideoPlaying[o] == Video)
	    {
	        currentPosition = o;
	    }	        
	}
    if (currentPosition == -1)
    {
       return;
    }

	if (cT == "T")
    {
    clearVideoSelection(-1, "M");
    clearVideoSelection(currentPosition, "T");
    }
    else
    {
    clearVideoSelection(currentPosition, "M");
    clearVideoSelection(-1, "T");
    }
}

Interface.SetUpNext = function( pVideo )
{
	//selectNewVideo(pVideo);
}


Interface.GetInstance().PlayerViewer = document.getElementById("Viewer");

Interface.SetNowPlaying = function( Video )
{
	if( Playlist.GetInstance && Playlist.GetInstance() && Playlist.GetInstance().Current )
	{
		var isAd = Playlist.GetInstance().Current.IsAd;
	}
	selectNewVideo(Video);


//totalTime = parseInt(clip_end.substring(0, 2))*3600 + parseInt(clip_end.substring(3,5))* 60 + parseInt(clip_end.substring(6,8))

//clip_end = "-1";



	
	//document.getElementById( "Video1" ).style.fontWeight = (Video == Video1 ? "bold" : "");
	//document.getElementById( "Video4" ).style.fontWeight = (Video == Video4 ? "bold" : "");
	//document.getElementById( "Video2" ).style.fontWeight = (Video == Video2 ? "bold" : "");
	
	document.getElementById("AdArea").innerHTML = ( isAd ? "<em>Video will play after advertisement</em>" : "" );
}













 



 

 

 

 

 








