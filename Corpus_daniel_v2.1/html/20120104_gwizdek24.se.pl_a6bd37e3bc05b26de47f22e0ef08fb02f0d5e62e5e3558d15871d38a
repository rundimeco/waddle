            onResume: function() {
                if($f("FlowPlayer_1488").getClip().type == 'video')
                    $f().getPlugin("gatracker").trackEvent('Wznowiony');
            },
            onStop: function() {
                if($f("FlowPlayer_1488").getClip().type == 'video')
                    $f().getPlugin("gatracker").trackEvent("Czas odtwarzania: " + $f("FlowPlayer_1488").getTime());
            },
            onFinish: function() {
                if($f("FlowPlayer_1488").getClip().type == 'video')
                    $f().getPlugin("gatracker").trackEvent("Czas odtwarzania: " + $f("FlowPlayer_1488").getTime());
            }
        });
    }
</script>

        
        
        
        
        
        <a href="http://se24.se.pl/sport/,/" title="">Podsumowanie RSMP 2012 okiem kamery MSZ Racing Team</a>
        
    </div>
