<script type="text/javascript">

    

    var h = $("[id=header] div.row2").css({ height: "auto" }).height();
    $("[id=header] div.row2").css({ height: "34px" });
    if (h > 40) {
        $("div.foldmenu").show();
    }
    $(document).ready(function () {
        $("div.foldmenu").click(function () {
            if ($(this).attr("closed") == "true") {
                $("[id=header] div.row2").animate({ height: h });
                $(this).attr("closed", "false")
                $(this).find("img").attr("src", "/themes/1/default/media/images/header_menu_multiple_opened.png");
            } else {
                $("[id=header] div.row2").animate({ height: 34 });
                $(this).attr("closed", "true");
                $(this).find("img").attr("src", "/themes/1/default/media/images/header_menu_multiple_closed.png");
            }
        });
    });


</script>

