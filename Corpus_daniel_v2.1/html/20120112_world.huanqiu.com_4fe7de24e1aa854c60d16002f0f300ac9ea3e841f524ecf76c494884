function getCookie(cookie)
{
    if (document.cookie.length > 0) 
    {
        cookie_begin = document.cookie.indexOf(cookie + "=")
        if (cookie_begin != -1) 
        {
            cookie_begin = cookie_begin + cookie.length + 1
            cookie_over = document.cookie.indexOf(";", cookie_begin)
            if (cookie_over == -1) 
                cookie_over = document.cookie.length
            return decodeURI(document.cookie.substring(cookie_begin, cookie_over))
        }
    }
    return ""
}
