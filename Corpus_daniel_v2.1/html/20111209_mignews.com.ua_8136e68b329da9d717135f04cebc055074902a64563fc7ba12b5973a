  function userlogin(){
    var login=$('#userlogin').val();
    var pass=$('#userpass').val();
    if(login=='' || pass==''){
      alert('Введите логин и пароль!');
    }else{
      $.post("/userlogin/", {'login':login,'password':pass},
  	function(data){
  	    if(data!='0'){
  	       //self.location.href='/ru/articles/95460.html';
