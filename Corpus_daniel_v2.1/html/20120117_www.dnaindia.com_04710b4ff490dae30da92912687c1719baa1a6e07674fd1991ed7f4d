    function frame_auto_resize(id,extra){
      var nh;
      //var nw;
      document.getElementById(id).height= "100px";
      if(document.getElementById){
        nh=document.getElementById(id).contentWindow.document .body.scrollHeight+extra;
        //nw=document.getElementById(id).contentWindow.document .body.scrollWidth;
      }
      document.getElementById(id).height= (nh) + "px";
      //document.getElementById(id).width= (nw) + "px";
    }

    function show_comment(){
    cb = document.getElementById("comment_box");
    cbf = document.getElementById("comment_box_frame");
    if(cb.style.visibility=='visible')	{
      cb.style.visibility='hidden';
      cb.style.display='none';
    }
    else	{
      //cbf.src="/comments_post_frame710.php?newsid=1638572&commentid=0";
      cb.style.visibility='visible';
      cb.style.display='block';
     }
    }
