 <script language="javascript">funEqual("N_left","N_right")</script>
</div>
<script type="text/javascript">
//By longbill.cn
function img_resize(img,mw,mh)
{
var w = parseInt(img.width);
var h = parseInt(img.height);
var pw = mw/w;
var ph = mh/h;
if (pw>=1 && ph>=1) return;
var p = (ph>pw)?pw:ph;
img.width = parseInt(w*p-1);
img.height = parseInt(h*p-1);
}
window.onload = function()
{
var imgs = document.getElementsByTagName("img");
for(var i=0;i<imgs.length;i++) img_resize(imgs[i],600,800);//600為圖片的最大寬度，800為最大高度
}
</script>
