    <script type="text/javascript" >
    //<[CDATA[
        function replaceAll(txt, replace, with_this) {
          return txt.replace(new RegExp(replace, 'g'),with_this);
        }
        $(document).ready(function() {
            $.getJSON("http://findnsave.sacbee.com/api/aacoupons.json?callback=?", function(json) {AACoupons(json)});
            });    
        function AACoupons(data) {
        //console.log(data);
        num_coups = data.result.coupon.length;
        counter = Math.ceil(Math.random() * num_coups);
    	template = '<table><tr><td valign="top">'
                        + '<a href="__url__#navlink=fns_combo"><img class="standard_image" src="__img__"></a>'
                        + '</td><td valign="top">'
                        + '<h5 class="coupon_business"><a href="__url__#navlink=fns_combo">__advname__ </a></h5>'
                        + '<p class="coupon_offer">__title__</p>'
                        + '</td></tr></table>'
                        + '<p><a href="__url__#navlink=fns_combo" class="view_deal">&nbsp;</a></p>';
        mylist = jQuery('ul.analog_coupons');
        for (x = counter; x < counter+3; x++) 
        {
            ci = x%num_coups;
            cc = data.result.coupon[ci];
            ct = template.replace('__img__',cc.couponPhotoUrl).replace(new RegExp('__url__', 'g'),cc.couponUrl);
            ct = ct.replace('__advname__',cc.advertiserName).replace('__title__',cc.title);
            //couponPhotoUrl
            //couponUrl
            //alert(ct);
            mylist.append('<li>'+ct+'</li>');
        }
    
        };
     //]]>
    </script>
