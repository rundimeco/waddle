<script type="text/javascript">
// OAS Ad Serving code

//var OAS_url = 'http://ads.newsquest.co.uk/RealMedia/ads/';
var OAS_url = 'http://oas.newsquestdigital.co.uk/RealMedia/ads/';

var OAS_sitepage = 'www.eveningtimes.co.uk' + window.location.pathname;


if(OAS_sitepage.charAt(OAS_sitepage.length - 1) == '/')
OAS_sitepage += 'index.php';

//var OAS_listpos = 'Top,Right,Frame2,x01,x02,x03,x04,x05,x06';
var OAS_listpos = 'Top,Right,Frame2,x01,x02';


var OAS_version = 10;

var OAS_rn = new String (Math.random());

var OAS_rns = OAS_rn.substring (2, 11);




OAS_query = '';
if(quantSegs.length > 0)

{ OAS_query += '&qcSegements='+quantSegs; }



    if(audienceScienceSegments.length > 0)
    {
        OAS_query += '&asSegements='+audienceScienceSegments;
    }


function OAS_NORMAL(pos) {
    document.write('<a href="' + OAS_url + 'click_nx.ads/' + OAS_sitepage + '/1' + OAS_rns + '@' +
                   OAS_listpos + '!' + pos +'?' + OAS_query + '">'); document.write('<img src="' +
                   OAS_url + 'adstream_nx.ads/' + OAS_sitepage + '/1' + OAS_rns + '@' + OAS_listpos +
                   '!' + pos +'?' + OAS_query + '" alt="" /><'+'/a>');
}

OAS_version = 11;
if (navigator.userAgent.indexOf('Mozilla/3') != -1 || navigator.userAgent.indexOf('Mozilla/4.0 WebTV') != -1)
    OAS_version = 10;

if (OAS_version >= 11) 
    document.write('<scr' + 'ipt type="text/javascript" src="' + OAS_url + 'adstream_mjx.ads/' +
                    OAS_sitepage + '/1' + OAS_rns + '@' + OAS_listpos +'?' + OAS_query + '"><\/scr' + 'ipt>');



function OAS_AD(pos) {
if (OAS_version >= 11)
OAS_RICH(pos);

else
OAS_NORMAL(pos);

}

function OAS_moveAds() {
// An implementation of 'fast ads'
var ls = document.getElementById("leaderboard-source");

var lt = document.getElementById("leaderboard");

if (ls && lt) lt.appendChild(ls);

var ms = document.getElementById("mpu-source");

var mt = document.getElementById("mpu");

if (ms && mt) mt.appendChild(ms);

var ss = document.getElementById("skyscraper-source");

var st = document.getElementById("skyscraper");

if (ss && st) st.appendChild(ss);

var bs = document.getElementById("buttonAds-source");

var bt = document.getElementById("buttonAds");

if (bs && bt) bt.appendChild(bs);















}
YAHOO.util.Event.onDOMReady(OAS_moveAds);


</script>


