	function hideFB(){
		//無法連上FB時，隱藏以下區塊
		$("#fbLikeButton, #fbShareButton, .to-fbLike, .to-fbShare, #fbComments, #side_fbFans, .other_info iframe").empty().hide();
		$("img#fbpic").onerror=null; //控制不要一直跳
	}
</script>
