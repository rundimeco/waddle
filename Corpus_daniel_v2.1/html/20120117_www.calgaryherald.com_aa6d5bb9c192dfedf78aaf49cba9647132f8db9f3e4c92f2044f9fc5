var _publishflow_config = {
    client_id: 'calgaryherald.com'
};
(function() {
    function execPublishflowLogger() {
        var _element = document.createElement('script');
        _element.setAttribute('language', 'javascript');
        _element.setAttribute('type', 'text/javascript');
        _element.setAttribute('src', 'http://s3.amazonaws.com/publishflow/javascript/media.js');
        document.body.appendChild(_element);
    }
    var _onLoadMethod = window.onload;
    window.onload = (typeof window.onload != 'function') ? execPublishflowLogger : function() {
        _onLoadMethod();
        execPublishflowLogger();
    };
})();
</script>
