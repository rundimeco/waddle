                        document.write('<script language="JavaScript1.1" src="http://ad.ca.doubleclick.net/N3081/adj/montrealgazette.com/news/story;loc=top;sz=960x42;kw=ron;kw=news;nk=print;pr=mg;ck=news;page=story;kw=mg;tile='+dartad_tile+';'+adcookieTag+surroundTag+'ord=43603204?"><\/script>');
                        </script>
                    
                        <script type="text/javascript">
                        if(adid != "") // check if the ad should start a surround session only if a surround session has not yet started
                        {
                            surroundTag = "srnd="+adid+";"; // setup the key-value for a surround session
                            setSurroundCookie(adid); // store the surround session id in a cookie.
                        }
