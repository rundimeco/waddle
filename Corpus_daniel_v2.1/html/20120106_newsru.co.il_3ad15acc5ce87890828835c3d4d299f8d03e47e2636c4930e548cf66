                        }                        

                        function UrlToMobile(currentUrl){
                        if ( currentUrl.match("//newsru.co.il") )
                            {	
                                currentUrl = currentUrl.replace("//newsru.co.il", "//m.newsru.co.il");
                         }
            
                        else if 	( currentUrl.match("//www.newsru.co.il/") )
                             {
                            currentUrl = currentUrl.replace("//www.newsru.co.il", "//m.newsru.co.il");
                         }
                         return currentUrl;
                        }

                    if(bowser.mobile){

                        var currentUrl  = document.URL;
                        console.log("location.href=" + location.href);
                        if (location.href.match('dropmobile=1')) {
                            //document.cookie = "isMobile=; expires=Wed, 31 Dec 2098 10:00:00 UTC;";
                            deleteCookie("isMobile");
                            //document.getElementById("mobile_banner").style.display="table";
                            console.log("?dropmobile=1");
                            var lhref = currentUrl;
                            lhref = lhref.replace('?dropmobile=1','');
                            console.log("lhref.replace=" + lhref);
                            window.location.href  = lhref;
                        } 

                        
                        currentUrl = UrlToMobile(currentUrl);
                        
                        document.getElementById("mobile_link1").href = currentUrl;
                        document.getElementById("mobile_link2").href = currentUrl;
                        document.getElementById("mobile_link21").href = currentUrl;
                        document.getElementById("mobile_link22").href = currentUrl;
                        if((document.cookie.indexOf("isMobile=true") !== -1) && (document.cookie.indexOf("isMobile=true") !== 0)) {
                            console.log("isMobiletrue=" + document.cookie.indexOf("isMobile=true"));
                            console.log("currentUrl=" + currentUrl);
