                        function deleteCookie(name) {
                                console.log('deleteCookie');
                                document.cookie = "isMobile=false; path=/; expires=Wed, 31 Dec 2098 10:00:00 UTC;";
                        
                        }    
                        
                        function followwMobile(){
                            console.log('followwMobile');
                            var currentUrl  = document.URL;
                            currentUrl = UrlToMobile(currentUrl);
                            console.log('UrlToMobile=' + currentUrl);
                            document.cookie = "isMobile=true; path=/; expires=Wed, 31 Dec 2098 10:00:00 UTC;";
