				<script xmlns:rbc_news="http://www.rbc.ru/" xmlns:metadata="http://rss.rbc.ru/metadata" type="text/javascript">
			var arrayWithItems = [
				
			];
			
			
				var REDIRECT_URI = 'http://redir.rbc.ru/cgi-bin/redirect.cgi?';
				var ITEMS_TO_SHOW = 1;

				var Cookie = {
					/**
					 * Установка значения куки
					 * 1) если на задавать параметр expires, то кука установится на год
					 * 2) чтобы удалить куку нужно задать отрицательное значение параметра expires
					 * 3) если задать параметр expires как "session", то кука станет сессионной
					 * @param {String} name
					 * @param {String} value
					 * @param {Number} expires
					 */
					set: function( name, value, expires, domain ){
						// дата истечения срока действия куки
						var expiresValue = '';
						if( expires !== 'session' ){
							expiresValue = 'expires=' +
								( new Date( new Date().getTime() + ( expires || 365 ) * 86400000 ) ).toGMTString() + '; '
						}
						// запись куки
						document.cookie = name + '=' + value + '; ' + expiresValue + 'path=/' + ((domain) ? '; domain=' + domain : '');

					},
					/**
					 * Получение значения куки
					 * @param {String} name
					 * @return значение куки с именем name
					 * @type String
					 */
					get: function( name ){
						var re = new RegExp( '\\s*' + name + '=' );
						var cookies = document.cookie.split(';');
						// проверка всех кук страницы
						for( var i = cookies.length; i--; ){
							var c = cookies[i];
							if ( re.test( c ) ){ return c.replace( re, '' ); }
						}
						return '';
					}
				};

				
				window.onload = show_informers();
				
				function show_informers() {
					var countInf = 0;
					return function() {
						if ( countInf++ > 0 ) return;
						//Cookie.set('someCounter','3');
	
						//загружаем массив из куки
						var firstElementNumber = Cookie.get('firstElementNumber');
						if (firstElementNumber) {
							var newArray = [];
							var y = firstElementNumber;
							for (var i = 0; i < arrayWithItems.length; i++) {
								newArray.push(arrayWithItems[y]);
								y++;
								if (y == arrayWithItems.length) {
									y = 0;
								}
							}
							arrayWithItems = newArray;
						}
						firstElementNumber++;
						if (firstElementNumber >= arrayWithItems.length) {
							firstElementNumber = 0;
						}
						//записываем новый порядок к куку
						Cookie.set('firstElementNumber', firstElementNumber);
	
						var informerContainer = document.getElementById('informer');
						var oldContent = informerContainer.innerHTML;
	
						informerContainer.innerHTML = '';
						var htmlCache = printInformer( arrayWithItems, ITEMS_TO_SHOW );
	
						for ( var i = 0; i < htmlCache.length; i++ ) {
							informerContainer.appendChild( htmlCache[i] );
						}
	
						informerContainer.innerHTML += oldContent;
					}

				}
				

                function create_element_first( itemToShow ) {
                    var div = document.createElement('div');
                    div.align = 'center';

                    var href = document.createElement('a');
                    href.href = REDIRECT_URI + itemToShow.url;
                    href.title = itemToShow.header;

                    var img = document.createElement('img');
                    img.src = itemToShow.imgSrc;
                    img.alt = itemToShow.header;
                    img.width = 145;
                    img.height = 96;

                    href.appendChild( img );
                    div.appendChild( href );

                    img = null; href = null;

                    return div;
                }

                function create_element_seconds( itemToShow ) {
                    var li = document.createElement('li');

                    var href = document.createElement('a');
                    href.href = REDIRECT_URI + itemToShow.url;
                    href.title = itemToShow.header;

                    var text = document.createTextNode( itemToShow.header );
                    href.appendChild( text );

                    li.appendChild( href );

                    href = null; text = null;

                    return li;
                }

				function printInformer(array, numberOfItemsToShow) {
					htmlCache = [];
					var itemToShow = get_informer( array[0] );
					if ( itemToShow ) {
						htmlCache.push( create_element_first( itemToShow ) );
						htmlCache.push( create_element_seconds( itemToShow ) );
					}
					for ( var i = 1; i < numberOfItemsToShow; i++ ) {
						itemToShow = get_informer( array[i] );
						if ( itemToShow ) {
							htmlCache.push( create_element_seconds( itemToShow ) );
						}
					}

					return htmlCache;
				}

				function get_informer( infpl ) {
					if ( ! infpl )			return false;
					if ( ! infpl.length )	return false;
					var num = Math.floor( Math.random() * infpl.length );
					return infpl[ num ];
				}
			
		</script>		<div xmlns:rbc_news="http://www.rbc.ru/" xmlns:metadata="http://rss.rbc.ru/metadata" class="block">
