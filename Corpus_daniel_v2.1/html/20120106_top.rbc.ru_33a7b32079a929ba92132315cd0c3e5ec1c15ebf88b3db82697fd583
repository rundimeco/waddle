<!-- social buttons init START -->
<script>
	$(document).ready(function() {
        initFB();
		tryVK();
	});
	
	function tryVK() {
		requirejs.config({
		   baseUrl: 'http://userapi.com/js/api/',
		   urlArgs: ''
		});
		
		require(['openapi'], function () {
			initVK();
			tryTwitter();
		}, function (err) {
			tryTwitter();
		});
	};
	
	function tryTwitter() {
		requirejs.config({
		   baseUrl: 'http://platform.twitter.com/'
		});
		
		require(['widgets'], function () {
			initTwitter();
			tryOdkl();
		}, function (err) {
			tryOdkl();
		});
	};
	
	function tryOdkl() {
		requirejs.config({
		   baseUrl: 'http://stg.odnoklassniki.ru/share/'
		});
		
		require(['odkl_share'], function () {
			initOdkl();
		}, function (err) {});
	};
	
	function initFB() {
		if ( window.FB ) {
			$('.facebookButtonBig').css({visibility: 'visible'});
			_ga.trackFacebook();
		};
	};
	
	function initVK() {
		if ( window.VK ) {
			$('.VKbuttonBig').css({visibility: 'visible'});
			try { VK.init({apiId: 2947582, onlyWidgets: true}) } catch(err) {};

			var title_654651 = $('TITLE').text();
			try {
				VK.Widgets.Like("vk_like", {width: 130, pageTitle: title_654651, type: "button"});
				_ga.trackVkontakte();
			} catch(err) { $('.VKbuttonBig').hide() }
		};
	};
	
	function initTwitter() {
		if ( window.__twttrlr ) {
			$('.twitterButtonBig').css({visibility: 'visible'});
			_ga.trackTwitter();
		}
	};
	
	function initOdkl() {
		if ( window.ODKL ) {
			$(window).load(function() {
				try {
					$('BODY').append('<link href="http://stg.odnoklassniki.ru/share/odkl_share.css" rel="stylesheet" />');
					$('.odnoklassnikiButtonBig').css({visibility: 'visible'});
					ODKL.init();
				} catch(err) {}
			});
		};
	};
</script>
<!-- social buttons init END -->

