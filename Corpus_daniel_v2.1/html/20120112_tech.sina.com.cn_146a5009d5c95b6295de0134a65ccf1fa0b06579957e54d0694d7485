					function submitelec(obj){
						var value="";
						if(obj.name=="form_s4"){
							value=obj.sn_p_0.value;
							if(value!=""){
								window.open("http://tech.sina.com.cn/elec/search/search_lcd.shtml?sn_name="+value);
