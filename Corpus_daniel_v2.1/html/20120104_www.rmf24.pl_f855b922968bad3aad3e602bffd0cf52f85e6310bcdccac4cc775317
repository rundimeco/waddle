<script type="text/javascript">
    //<![CDATA[

jQuery(document).ready(function(){
jQuery("#articleObjectListCategory1").boxList({
params: {sort: ['dates', 'views', 'rates', 'forum']},
listAjaxUrl: '/ajax/article-category/rank?categoryId=555',
get: function (liJQ, params) {
var icon = {};
liJQ.find('.title a span.icon').each(function(index, elem){
var iconElem = jQuery(elem).attr('class');
iconElem.substring(5)
icon[iconElem] = 1;
});
return {
title: liJQ.find('.title a').text(),
date: liJQ.find('.date').text(),
type: icon,
link: liJQ.find('.title a').attr('href')};

},
set: function (liJQ, data, params) {
liJQ.find('.title a span:not([class])').text(data.title);
liJQ.find('.date').text(data.date);
liJQ.find('.title a').attr('href', data.link);
if( data.type ) {
jQuery.each(data.type, function(iconType, value){
jQuery('<span/>')
.html(' ')
.addClass('icon '+iconType)
.appendTo(jQuery(liJQ.find('.title a')))
})
}
},
after: function (params) {
if( params.sort == 'dates' )
{
jQuery("#articleObjectListCategory1").find('.seeMore').show();
}
else
{
jQuery("#articleObjectListCategory1").find('.seeMore').hide();
}
var listId = 'articleObjectListCategory1_list_'+params.sort;
jQuery("#articleObjectListCategory1").find('.list').attr('id',listId);
},
fadeSpeed: 200,
cookieEnabled: false});
});
jQuery(document).ready(function(){
OpenPlayer = function (s) {
if( s.substr(0,7) != "http://" ) s = "http://www.rmfon.pl/play,"+s;
window.open(s, "RMFplayerWindow", 'width=820,height=710,menubar=no,toolbar=no,location=no,scrollbars=no,resizable=no,status=no');
};

getJson = function ()
{
time = new Date();
// get JSON data
jQuery.getJSON("http://d.iplsc.com/rmf/rmfonPlayer_5.json.txt?"+ time.getTime() +"&jsoncallback=?",
function (data) {

});
};

putHtml = function ()
{
// put html template
jQuery('#serviceRmfCityofmusicObjectPlayer1 .player').html('<div class="now"><div class="description"><b>Teraz gramy</b><h3><a href="">&nbsp;</a></h3><span class="title">&nbsp;</span></div><div class="play"><a target="_blank" onclick=\'OpenPlayer("5");return false;\' href="#">Posłuchaj</a></div></div><div class="next"><div class="description"><b>Następny</b><h3><a href="">&nbsp;</a></h3><span class="title">&nbsp;</span></div></div>');
getJson();
};

player = function (data)
{
var item_now;
var item_next;
jQuery.each(data, function(index, value) {
if(value.order == 0 ) {
item_now = value;
}
if(value.order == 1 ) {
item_next = value;
}
});
if (data && item_now && item_next) {
/* change player data */
if( item_now.bio != '' || item_now.title != '' ) {
jQuery('#serviceRmfCityofmusicObjectPlayer1 .player .now .description').show();
jQuery('#serviceRmfCityofmusicObjectPlayer1 .player .now .description h3 a').attr('href', 'http://www.rmfon.pl/artysci/'+ item_now.bio+'.html' );
jQuery('#serviceRmfCityofmusicObjectPlayer1 .player .now .description h3 a').html(item_now.author);
jQuery('#serviceRmfCityofmusicObjectPlayer1 .player .now .description span.title').html(item_now.title);
} else {
jQuery('#serviceRmfCityofmusicObjectPlayer1 .player .now .description').hide();
}

if( item_next.bio != '' || item_next.title != '' ) {
jQuery('#serviceRmfCityofmusicObjectPlayer1 .player .next .description').show();
jQuery('#serviceRmfCityofmusicObjectPlayer1 .player .next .description h3 a').attr('href', 'http://www.rmfon.pl/artysci/'+ item_next.bio+'.html' );
jQuery('#serviceRmfCityofmusicObjectPlayer1 .player .next .description h3 a').html(item_next.author);
jQuery('#serviceRmfCityofmusicObjectPlayer1 .player .next .description span.title').html(item_next.title);
} else {
jQuery('#serviceRmfCityofmusicObjectPlayer1 .player .next .description').hide();
}
}
};

var refreshTime = "60";
setInterval(
function(){
getJson();
},
refreshTime * 1000 );

/* if don't get data from API, try to get JSON */
if( jQuery('#serviceRmfCityofmusicObjectPlayer1 .player .now .description h3 a').length === 0 )
{
putHtml();
}
});    //]]>
</script>
