<script type="text/javascript">

 var headerHeight;
 var orgHeight;
 var headline_count;
 var headline_interval;
 var old_headline = 0;
 var current_headline = 0;

$(document).ready(function(){
	headerHeight = $("#header").css('height');
	orgHeight = headerHeight;
	headerHeight = parseInt(headerHeight);
	headerHeight = headerHeight - 75;
	headerHeight = headerHeight / 2;
	
	$("#scrollup").css('height',orgHeight);

   headline_count = $("div.scrollheadline").size();
   $("div.scrollheadline:eq("+current_headline+")").css('top',headerHeight + 'px');
 
   headline_interval = setInterval(headline_rotate,8000); //time in milliseconds
   });
 
 function headline_rotate() {
   current_headline = (old_headline + 1) % headline_count; 
   $("div.scrollheadline:eq(" + old_headline + ")").animate({top: -205},2000, function() {
     $(this).css('top','210px');
   });
   $("div.scrollheadline:eq(" + current_headline + ")").show().animate({top: headerHeight},2000);  
   old_headline = current_headline;
 }

</script>

