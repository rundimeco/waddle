<script type="text/javascript">
/* PRN News Detail*/

function popupBlogIt(blogName, thisObj){
	var s=s_gi(s_account);
	s.linkTrackVars='prop5,eVar3,prop16,prop2,events'; 
	s.linkTrackEvents='event12';		
	s.prop5='blog it release'; 
	s.eVar3=s.prop5; 	
	s.prop16=blogName;		
	s.prop5+='_'+s.prop16;
	s.prop2=s.pageName;
	s.events='event12';
	s.tl(thisObj,'o',blogName);
} // End of function popupBlogIt
		
		
function popupShareIt(shareName, thisObj){
	var s=s_gi(s_account);
	s.linkTrackVars='prop5,eVar3,prop15,prop16,prop2,events';
	s.linkTrackEvents='event12';
	s.prop5='share it release';
	s.eVar3=s.prop5;
	s.prop15='137114418';
	s.prop16=shareName;
	s.prop5+='_'+s.prop16;
	s.prop2=s.pageName;
	s.events='event12';
	s.tl(thisObj,'o',shareName);
} // End of function popupShareIt

function popupPrintAndEmail(prop5Var,prop16Var, thisObj){
	var s=s_gi(s_account);
	s.linkTrackVars='prop5,eVar3,prop15,prop2,prop16,events';
	s.linkTrackEvents='event12';
	s.prop5=prop5Var; 
	s.eVar3=s.prop5; 
	s.prop15='137114418'; 		
	s.prop2=s.pageName;
	s.prop16=prop16Var;		
	s.events='event12';
	s.tl(thisObj,'o',prop16Var);
} // End of function printAndEmail

function linkOnClick(obj){
	var s=s_gi(s_account);
	s.linkTrackVars='prop5,eVar3,prop15';
	s.prop5='External Link';
	s.eVar3=s.prop5;
	s.prop15='137114418';
	s.tl(obj,'o','ExternalLink');
} // End of function linkOnClick

</script>

