		<script type="text/javascript">
	function get_check_value()
	{
		var s=s_gi(s_account);
		s.linkTrackVars='prop16';
		s.linkTrackEvents='None';
		s.prop16= "MyNews_Follow";
		s.tl(this,'o', 'MyNews_Follow');

		var tickersize=document.checkboxform.tickerlist.length;
		var t_value = "";
		if(tickersize)
		{
			for (var i=0; i < tickersize; i++)
			{
				if (document.checkboxform.tickerlist[i].checked && (!document.checkboxform.tickerlist[i].disabled))
				{
					t_value = t_value + document.checkboxform.tickerlist[i].value +",";
					TrackAddTicker(document.checkboxform.tickerlist[i].value);
				}
			}
		} else{
			if (document.checkboxform.tickerlist.checked)
			{
				t_value = document.checkboxform.tickerlist.value;
			}
		}
		if(t_value==""){
			document.getElementById('alertmsg').style.display='block';
			return false;
		} 
		window.location = 'http://www.prnewswire.com/templates/PRN Update User?ticker='+t_value;
	}
	function TrackAddTicker(prop46){
		var s=s_gi(s_account);
		s.linkTrackVars='prop46';
		s.linkTrackEvents='None';
		s.prop46= "MyNews_Add_"+prop46;
		s.tl(this,'o', 'MyNews_Add_'+prop46);
	}
	function displaynewsinv()
	{
		document.getElementById('mynewsinvbox').style.display="block";
	}
	function hidenewsinv()
	{
		document.getElementById('mynewsinvbox').style.display="none";
	}
	</script>
