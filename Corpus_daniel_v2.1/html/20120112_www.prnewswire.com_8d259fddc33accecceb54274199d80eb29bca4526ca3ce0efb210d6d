<script type="text/javascript">
	function BlogSearch(blogSearchText){
		var s=s_gi(s_account);
		var loc = location.href;
		if(loc.indexOf("prnewswire.com")>0){
			s.linkTrackVars='prop5,eVar3,prop15,prop16,prop2,events';
			s.prop5='blog search release';
		} else{
			s.linkTrackVars='eVar3,prop15,prop16,prop2,events';
		}
		s.prop15='137114418';
		s.prop16=''+blogSearchText;
		if(loc.indexOf("prnewswire.com")>0){
			s.eVar3=s.prop5;
			s.prop5+='_'+s.prop16;
		} else{
			s.eVar3=s.prop16;
		}
		s.prop2=s.pageName;	
		s.events='event12';
	} // End of function BlogSearch
</script>

			</div>
		</li>
</ul>
