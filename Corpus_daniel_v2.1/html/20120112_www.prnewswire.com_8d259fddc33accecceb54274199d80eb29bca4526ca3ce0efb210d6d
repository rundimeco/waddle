<script type="text/javascript">
function translationdrop(b1)
{
var e=document.getElementById("translangcomp1");
if ( e.style.display == 'block' ) {
  		e.style.display = 'none';
		b1.style.color="#5F95C3";
		$('.translang').addClass('background1');
	 }
	else 
	  {
	   $('.translang').removeClass('background1');
	   b1.style.color="#ffffff";
           b1.style.backgroundImage="url(http://content.prnewswire.com/designimages/translationheadline_activebg1.gif)";
	   b1.style.height="18px";
	   e.style.display = 'block';
	 }
}
</script>			   
