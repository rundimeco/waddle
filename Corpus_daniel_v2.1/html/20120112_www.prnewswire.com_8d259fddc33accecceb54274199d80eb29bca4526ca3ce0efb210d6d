				<script type="text/javascript">
			var cookieTicker = readCookie('MyTickers');
/******* Added code for displying the tickers when searched from my news page starts ***********/			
		
			function showAll(){		
			document.getElementById('displayten').innerHTML = tickerString+ '&nbsp;|&nbsp;' + remainingTickerString+'<a href="javascript:showTen()"> Hide tickers </a>';
			}

			function showTen(){		
			document.getElementById('displayten').innerHTML = tickerString+'<a href="javascript:showAll()"> View more tickers </a>';
			}	
			
			function page(which){
				location.href=tenTickerhref[which];
			}
			function remPage(which){
					location.href=remainingtickerHef[which];
			}
		  
			
/******* Added code for displying the tickers when searched from my news page ends ***********/
									var stockStringArray="[com.clickability.cms.translator.UtilContext$userObject@1b7ef6a0]";
			var stockArray = stockStringArray.split(",");
			if(cookieTicker && cookieTicker!=""){
				if(document.getElementById('mynews'))
				document.getElementById('mynews').style.display='block';
							}
						if(cookieTicker && cookieTicker!=""){
			    var tickerArray = cookieTicker.split(",");
				if(tickerArray.length<=50){
				var addedTickers =new Array(); 
				var invTickers="";
				for(var i=0;i<tickerArray.length;i++){


				if(document.getElementById(tickerArray[i]+'_span') || document.getElementById(tickerArray[i]+'_chkbox')){


					var ticker = tickerArray[i];
					var index= ticker.indexOf(":");
					var length = ticker.length;
					var tickerdisp = ticker.substring((index+1),length);						
					addedTickers.push(tickerdisp);

					if(document.getElementById(tickerArray[i]+'_span')){
						document.getElementById(tickerArray[i]+'_span').style.display ="none";
					}
					if(document.getElementById(tickerArray[i]+'_chkbox')){
						document.getElementById(tickerArray[i]+'_chkbox').disabled =true;
						document.getElementById(tickerArray[i]+'_chkbox').checked=true;
						document.getElementById(tickerArray[i]+'_id').innerHTML = document.getElementById('tickermsg').innerHTML + "<b>"+tickerdisp +"</b>"+ "&nbsp; in <a href='http://www.prnewswire.com/my-news'><b>My News</b></a>";
                          invTickers++;
					}
					if(stockArray.length==invTickers){
					  document.getElementById('investorimg').style.display='none';
					 }
    				}
				}						
				if(addedTickers.length>0){
				   var tickerString="";
				   for(var j=0;j<addedTickers.length;j++){
					   if(j==0){
							tickerString = addedTickers[j]+tickerString
					   }else{
							tickerString = addedTickers[j]+' | '+tickerString;
					   }
				  }
					document.getElementById('tickermsg').innerHTML=document.getElementById('tickermsg').innerHTML + tickerString ;
					if(document.getElementById(tickerArray[i]+'_span')){
						document.getElementById('tickermsg').style.display="block";
					}
				}
				if(stockArray.length==addedTickers.length){
					document.getElementById('tickerheader').style.display="none";
				}
                }else {
                       document.getElementById('tickerrow').innerHTML='You can add only upto 50 tickers';
                }                                                                                              
		} 
		</script>



