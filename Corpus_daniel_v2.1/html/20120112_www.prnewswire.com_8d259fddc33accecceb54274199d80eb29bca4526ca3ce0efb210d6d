			function showIframe(){
			requestCrossDomain("http://www.ford.com", function(results) {
			if(results != 'false')
			{
				var HTML = "<iframe id='previewFrame' frameborder='0' allowTransparency='true' src='http://www.ford.com'  style='width:96%;padding:2%;background:#EFEFEF;'></iframe>";
				document.getElementById('frameContainer').innerHTML = HTML;
			}
			else
			{
				$("div.previewlinks").css("display", "none");
				$("div#sitePreview").css("display", "none");
			}
			});
			}
			
			
