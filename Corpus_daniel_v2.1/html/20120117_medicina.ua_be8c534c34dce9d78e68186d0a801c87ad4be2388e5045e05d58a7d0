<script>
	var where_search = 'all';
	function setWhere(where) {
		if(where == 'drugs') {
			document.frmSearch.action='/search_drug/zakaz/';
		}
		else {
			document.frmSearch.action='/search/';
		}
		$('#search_tp').remove();
		$('#search_id').remove();
		if(where=="clinics") {
			$('#frmSearch').append('<input type="hidden" name="tp" id="search_tp" value="ct">');
			$('#frmSearch').append('<input type="hidden" name="id" id="search_id" value="1">');
		}
		else {
			if(where=="diseases") {
				$('#frmSearch').append('<input type="hidden" name="tp" id="search_tp" value="ct">');
				$('#frmSearch').append('<input type="hidden" name="id" id="search_id" value="3">');
			}
			else {
				$('#search_tp').remove();
				$('#search_id').remove();
			}
		}

		$('#where_search_'+where_search).removeClass('act');
		$('#where_search_'+where).addClass('act');

		where_search = where;
	}
</script>
		<div class="clr">&nbsp;</div>
				
<script type="text/javascript">
	$(document).ready(function(){
		$('.m1 .ti').hover(
		function(){
			$(this).find('.t').addClass('hov');
			$(this).find('.child').css('display','block');	
		},
		function(){
			$(this).find('.t').delay('500').removeClass('hov');
			$(this).find('.child').delay('500').css('display','none');	
		});
		
		$('.m1 .tim').hover(
		function(){
			$(this).find('.t').addClass('hove');

		},
		function(){
			$(this).find('.t').removeClass('hove');
		});
			
	});
</script>

