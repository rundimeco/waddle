			<script>
	var err_login = 0;
		

	//-----------------------------------
	function show(){
		$('#log_div').show();
	}
	
	
	//-----------------------------------
	function valid_form_login(){
		var err = true;
		if($('#login').val() == ''){
		    err = false;
		    $('#err_login').show();
		}
		else{
		    $('#err_login').hide();
		}
			
		if($('#pass').val() == ''){
		    err = false;
		    $('#err_pass').show();
		}
		else{
			$('#err_pass').hide();
		}
			
		if(err)
        	document.form_login.submit();
			
	}

var auth;
var sensor;
	
$(document).ready(function(){
	auth = $('#authDiv');
	sensor = $('#sensorShadow');
	closeAuth = $('#close');
	
	sensor.click(function(){
		auth.hide();
		sensor.hide();
	})

	closeAuth.click(function(){
		auth.hide();
		sensor.hide();
	})
	
})

function showAuth(id) {
	id.blur();

	auth.css({
		left: "50%",
		margin: "0 0 0 -160px",
		top: "305px",
		display: "block"
	}); 
	sensor.css({
		display: "block",
		height: $(document).height() + "px",
		opacity: 0.5
	});
}


var qst;
var sensor;
	
$(document).ready(function(){
	qst = $('#qstDiv');
	sensor = $('#sensorShadow');
	closeQst = $('#close2');
	
	sensor.click(function(){
		qst.hide();
		sensor.hide();
	})

	closeQst.click(function(){
		qst.hide();
		sensor.hide();
	})
})

function showQst(id) {
	id.blur();

	qst.css({
		left: "50%",
		margin: "0 0 0 -160px",
		top: "305px",
		display: "block"
	}); 
	sensor.css({
		display: "block",
		height: $(document).height() + "px",
		opacity: 0.5
	});
}

		$(document).ready(function(){
			$(".popup a").hover(function() {
			$(this).next(".pop").stop(true, true).animate({opacity: "show"}, "fast");
			$(this).css('color','#666969');
			$(this).css('text-decoration','underline');
			}, function() {
			$(this).next(".pop").animate({opacity: "hide"}, "fast");
			$(this).css('color','#3e4141')
			$(this).css('text-decoration','none')
			});
		});
	</script>





