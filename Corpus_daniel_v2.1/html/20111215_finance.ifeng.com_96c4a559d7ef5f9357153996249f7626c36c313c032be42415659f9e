if("function"!=typeof set_sort_by)
set_sort_by = function(all_hq_arr,a_order,sort_by){
	if("asc"==a_order){
		all_hq_arr.sort(function(b,a){return a[sort_by]-b[sort_by]});
	}else{
		all_hq_arr.sort(function(a,b){return a[sort_by]-b[sort_by]});
