 <script language="JavaScript">
  function refreshnewsfeed(a) {
   if(a!=currFeedType){
     currFeedType = a;
     $jQ(".lentaselector").removeClass("lentared");
     $jQ(".feed"+a).addClass("lentared");
     $jQ("#newsfeedblock").fadeIn("slow");
     JsHttpRequest.query(
      'http://nbnews.com.ua/exec/lenta_exec.php?page=0&type='+a, 
      {"countnews":30},
      function(result, a) {
        $jQ('#page_lenta').html(a);
        $jQ("#newsfeedblock").fadeOut("slow");
      },true
    );
   }
  }
 </script>
