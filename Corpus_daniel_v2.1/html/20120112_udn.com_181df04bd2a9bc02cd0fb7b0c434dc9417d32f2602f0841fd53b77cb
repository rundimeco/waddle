     $jq(this).hide();
     $jq(this).attr("id","embs2") ;
});
*/
$jq('table.border tr td table tr:nth-child(5) td:nth-child(1) table ').each(function() {
   if ( ! $jq.browser.webkit )    $jq(this).hide();
     $jq(this).attr("id","embs2") ;
});
