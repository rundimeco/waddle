		var mfp_user_cookie_name = "userid";
		function mfp_getCookie(c_name) {
			var start = document.cookie.indexOf(c_name+"=");
			if (start ==-1) {return "";}
			start = start+c_name.length+1;
			var end = document.cookie.indexOf(";",start);
			if (end==-1) {end = document.cookie.length;}
