		<script type="text/javascript">
var ctv_mobile_device = new Array("iphone", "ipod", "blackberry");
var agent = navigator.userAgent.toLowerCase();	
for(var i=0; i<ctv_mobile_device.length;i++){		
	if (ctv_mobile_device[i] != "" && agent.indexOf(ctv_mobile_device[i]) > -1) {			
		window.location = getMobileURL();
	} 
}
function getMobileURL(){
	var mobileURL = "http://m.ctv.ca";	
	var hub = "Entertainment";
	var hub = hub.toLowerCase();
	if(hub == "topstoriesv2")
		hub = "topstories";
	if(hub == "entertainmentv2")
		hub = "entertainment";
	if(hub == "nationalnews")
		hub = "thedesk";
	if(hub == "scitech")
		hub = "tech";
	var slug = "alternative-cancer-treatment-robin-gibb-120111";
	var date = "20120111";
	mobileURL = mobileURL + "/" + hub + "/" + date + "/" + slug + ".html";
	return mobileURL;
}
</script>
		


        
