				jQuery("#st div.float-right")
						.before(
								'<div class="float-left"><a href="http://www.straitstimes.com"><img src="http://www.straitstimes.com/STI/STIMEDIA/sp/2010/images/st.gif"></a></div><div class="pageSection" id="pageSectiondiv"></div>');

				jQuery("#pageSectiondiv").html(pageinfo.hidden_section_name);
			}
		}
	}

	//for the rest
	bnnavlink = homeurl + "/BreakingNews/to_be_rep" + "_" + navdate + ".html";

	var navbar = {
		"BN" : [ {
			"Name" : "Breaking News",
			"Link" : bnnavlink.replace(/to_be_rep/g, "Breaking+News"),
