    #storyNav .prev a img   {margin: -5px 6px -5px 0; float:left;}
#storyNav .next {width:100%;float:right; text-align:right;padding:12px 3px; color:#777; font-weight:normal; font-size:11px;}
    #storyNav .next a {font-weight:bold;}
    #storyNav .next a img   {margin: -5px 0 -5px 6px; float:right;}
    #storyNav .next .sub    {font-weight:normal; color:#888;text-align:right; float:right;}
    .pageSectionRed {color:#b83422; background-image:url(http://www.straitstimes.com/STI/STIMEDIA/sp/2010/images/slash.png); background-repeat:no-repeat; background-position:0 6px; padding:0 0 0 21px; margin:0 0 0 6px;}	
	.pageSectionRed a:link, .pageSectionRed a:visited {color:#b83422;}
	.pageSectionRed a:hover, .pageSectionRed a:active {color:#999;}
	
