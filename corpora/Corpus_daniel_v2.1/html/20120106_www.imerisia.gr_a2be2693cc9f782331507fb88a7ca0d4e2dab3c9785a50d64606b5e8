function setFontSize(nAmmount)
{
	if ((nFontSize>9 || nAmmount>0) && (nFontSize<15 || nAmmount<0)) {
	nFontSize=nFontSize + nAmmount;
	document.all.arttext.style.fontSize=nFontSize;
	}
}
