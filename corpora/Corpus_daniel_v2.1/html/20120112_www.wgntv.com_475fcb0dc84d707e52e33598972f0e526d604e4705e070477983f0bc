<div class="module breakingnews blurb">
                    
                	<script type="text/javascript"><!--
var date = new Date();

// 0=Sun 1=Mon 2=Tue 3=Wed 4=Thu 5=Fri 6=Sat
var dow = date.getDay();
var d  = date.getDate();
var day = (d < 10) ? '0' + d : d;
var m = date.getMonth() + 1;
var month = (m < 10) ? '0' + m : m;
var yy = date.getYear();

var year = (yy < 1000) ? yy + 1900 : yy;
var hours = date.getHours();
var minutes = date.getMinutes();

zDate = day + "/" + month + "/" + year;
zTime = hours + ":" + minutes;

//SAT & Sun
if(dow == 6 || dow == 0){
//MORNING
if(hours == 6){document.write('<div style=\"width:647px; height:auto; margin-left:-25px; margin-bottom:-25px;\"><a href=/news/livestreaming/><img src=http://tv.trb.com/includes/tvdata/wgntv/livestreaming/2012/weekend_news.png></a></div>');}

//NINE
else if(hours == 21){document.write('<div style=\"width:647px; height:auto; margin-left:-25px; margin-bottom:-25px;\"><a href=/news/livestreaming/><img src=http://tv.trb.com/includes/tvdata/wgntv/livestreaming/2012/weekend_news.png></a></div>');}
	
//ANYTIME
else {document.write('<div style=\"width:647px; height:auto; margin-left:-25px; margin-bottom:-25px;\"><a href=/news/livestreaming/><img src=http://tv.trb.com/includes/tvdata/wgntv/livestreaming/2012/online_anytime.png></a></div>');}
}

//MON - Friday
else if(dow > 0 && dow < 6){

//MORNING
if(hours > 3 && hours < 9){document.write('<div style=\"width:647px; height:auto; margin-left:-25px; margin-bottom:-25px;\"><a href=/news/livestreaming/><img src=http://tv.trb.com/includes/tvdata/wgntv/livestreaming/2012/morning_news.png></a></div>');}

//MIDDAY
else if(hours > 10 && hours < 13){document.write('<div style=\"width:647px; height:auto; margin-left:-25px; margin-bottom:-25px;\"><a href=/news/livestreaming/><img src=http://tv.trb.com/includes/tvdata/wgntv/livestreaming/2012/midday_news.png></a></div>');}

//FIVE
else if(hours == 17){document.write('<div style=\"width:647px; height:auto; margin-left:-25px; margin-bottom:-25px;\"><a href=/news/livestreaming/><img src=http://tv.trb.com/includes/tvdata/wgntv/livestreaming/2012/news_at_five.png></a></div>');}

//POLITICS TONIGHT
else if(hours == 18 && minutes < 30){document.write('<div style=\"width:647px; height:auto; margin-left:-25px; margin-bottom:-25px;\"><a href=/news/livestreaming/><img src=http://tv.trb.com/includes/tvdata/wgntv/livestreaming/2012/politics_tonight.png></a></div>');}

//NINE
else if(hours == 21){document.write('<div style=\"width:647px; height:auto; margin-left:-25px; margin-bottom:-25px;\"><a href=/news/livestreaming/><img src=http://tv.trb.com/includes/tvdata/wgntv/livestreaming/2012/news_at_nine.png></a></div>');}

//ANYTIME
else {document.write('<div style=\"width:647px; height:auto; margin-left:-25px; margin-bottom:-25px;\"><a href=/news/livestreaming/><img src=http://tv.trb.com/includes/tvdata/wgntv/livestreaming/2012/online_anytime.png></a></div>');}
}

 //--></script>
                </div>



    

	
			



































