<script type="text/javascript">
function actionBlockToggle()
{
 var current = jQuery('#action_block').css('display'), flag;
 flag = (current == 'none') ? 'block' : 'none';
 jQuery('#action_block').css('display', flag);
 jQuery(".reglaments").load("/terms-of-action/");
 return false;
}
</script>



