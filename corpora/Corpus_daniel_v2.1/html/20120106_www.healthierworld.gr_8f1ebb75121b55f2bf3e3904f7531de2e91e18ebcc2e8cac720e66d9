					<div id="menu"><style>
	@import url("http://www.healthierworld.gr/resources/styles/accordion2level.css");
</style>


<script type="text/javascript">
function initMenu(cvalue) {
	var i = 1;
	$('#Menu2 ul').each(function() {
		$(this).attr("id", 'smenu'+i);
		i = i+1;
	});
	
	function initClasses(){
		$('#Menu2 li a').each(function() {
			var checkElement2 = $(this).next();
			if (checkElement2.is('ul')) {
				$(this).removeClass('collapse');
				$(this).addClass('expand');
			}
			if (checkElement2.is(':visible')) {
				
								
			}
		});
	}
	
	$('#Menu2 ul').hide();
	$("#Menu2 ul[id="+cvalue+"]").show();
	initClasses();	
	$('#Menu2 li a').click(function() {
		initClasses();		
		var checkElement = $(this).next();
		if((checkElement.is('ul')) && (checkElement.is(':visible'))) {
			checkElement.slideUp('slow');
			
						
			return false;
		}
		if((checkElement.is('ul')) && (!checkElement.is(':visible'))) {
			$(this).addClass('collapse');
			$('#Menu2 ul:visible').slideUp('slow');
			checkElement.slideDown('slow');
			
						
			return false;
		}
	});
}

$(document).ready(function() {
	
		initMenu();
	
	$('#Menu2').show();
});
</script>


