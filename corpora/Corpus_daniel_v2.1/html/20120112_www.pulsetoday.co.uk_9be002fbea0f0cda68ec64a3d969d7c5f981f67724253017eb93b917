	function _48_INSTANCE_6xSY_maximizeIframe(iframe) {
		var winHeight = 0;

		if (typeof(window.innerWidth) == 'number') {

			// Non-IE

			winHeight = window.innerHeight;
		}
		else if ((document.documentElement) &&
				 (document.documentElement.clientWidth || document.documentElement.clientHeight)) {

			// IE 6+

			winHeight = document.documentElement.clientHeight;
		}
		else if ((document.body) &&
				 (document.body.clientWidth || document.body.clientHeight)) {

			// IE 4 compatible

			winHeight = document.body.clientHeight;
		}

		// The value 139 here is derived (tab_height * num_tab_levels) +
		// height_of_banner + bottom_spacer. 139 just happend to work in
		// this instance in IE and Firefox at the time.

		iframe.height = (winHeight - 139);
	}

	function _48_INSTANCE_6xSY_monitorIframe() {
		var url = null;

		try {
			var iframe = document.getElementById('_48_INSTANCE_6xSY_iframe');

			url = iframe.contentWindow.document.location.href;
		}
		catch (e) {
			return true;
		}

		var baseSrc = 'http://www.facebook.com/plugins/';
		var iframeSrc = 'http://www.facebook.com/plugins/likebox.php?href=http%3A%2F%2Fwww.facebook.com%2FPulseToday.co.uk&amp;width=300&amp;colorscheme=light&amp;show_faces=true&amp;stream=false&amp;header=true&amp;height=285" scrolling="no" frameborder="0" style="border:none; overflow:hidden; width:300px; height:300px;" allowTransparency="true"&';

		if ((url == iframeSrc) || (url == iframeSrc + '/')) {
		}
		else if (Liferay.Util.startsWith(url, baseSrc)) {
			url = url.substring(baseSrc.length);

			_48_INSTANCE_6xSY_updateHash(url);
		}
		else {
			_48_INSTANCE_6xSY_updateHash(url);
		}

		return true;
	}

	function _48_INSTANCE_6xSY_resizeIframe() {
		var iframe = document.getElementById('_48_INSTANCE_6xSY_iframe');

		var height = null;

		try {
			height = iframe.contentWindow.document.body.scrollHeight;
		}
		catch (e) {
			if (themeDisplay.isStateMaximized()) {
				_48_INSTANCE_6xSY_maximizeIframe(iframe);
			}
			else {
				iframe.height = 300;
			}

			return true;
		}

		iframe.height = height + 50;

		return true;
	}

	Liferay.provide(
		window,
		'_48_INSTANCE_6xSY_init',
		function() {
			var A = AUI();

			var hash = document.location.hash;

			if ((hash != '#') && (hash != '')) {
				var src = '';

				var path = hash.substring(1);

				if (path.indexOf('http://') != 0) {
					src = 'http://www.facebook.com/plugins/';
				}

				src += path;

				var iframe = A.one('#_48_INSTANCE_6xSY_iframe');

				if (iframe) {
					iframe.attr('src', src);
				}
			}
		},
		['aui-base']
	);

	Liferay.provide(
		window,
		'_48_INSTANCE_6xSY_updateHash',
		function(url) {
			var A = AUI();

			document.location.hash = url;

			var maximize = A.one('#p_p_id_48_INSTANCE_6xSY_ .portlet-maximize-icon a');

			if (maximize) {
				var href = maximize.attr('href');

				if (href.indexOf('#') != -1) {
					href = href.substring(0, href.indexOf('#'));
				}

				maximize.attr('href', href + '#' + url);
			}

			var restore = A.one('#p_p_id_48_INSTANCE_6xSY_ a.portlet-icon-back');

			if (restore) {
				var href = restore.attr('href');

				if (href.indexOf('#') != -1) {
					href = href.substring(0, href.indexOf('#'));
				}

				restore.attr('href', href + '#' + url);
			}
		},
		['aui-base']
	);

	_48_INSTANCE_6xSY_init();

