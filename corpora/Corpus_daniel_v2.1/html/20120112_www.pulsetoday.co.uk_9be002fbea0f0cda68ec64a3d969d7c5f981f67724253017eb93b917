		function hmhp_deleteMessage(i) {
			eval("var messageId = document._article_display_list_INSTANCE_0fO4_fm13284241._article_display_list_INSTANCE_0fO4_messageId" + i + ".value;");

			document._article_display_list_INSTANCE_0fO4_fm13284241._article_display_list_INSTANCE_0fO4_cmd.value = "delete";
			document._article_display_list_INSTANCE_0fO4_fm13284241._article_display_list_INSTANCE_0fO4_messageId.value = messageId;
			submitForm(document._article_display_list_INSTANCE_0fO4_fm13284241);
		}

		function hmhp_postReply(i) {
			eval("var parentMessageId = document._article_display_list_INSTANCE_0fO4_fm13284241._article_display_list_INSTANCE_0fO4_parentMessageId" + i + ".value;");
			eval("var body = document._article_display_list_INSTANCE_0fO4_fm13284241._article_display_list_INSTANCE_0fO4_postReplyBody" + i + ".value;");			
			eval("var postAsAnonymousUser = document._article_display_list_INSTANCE_0fO4_fm13284241._article_display_list_INSTANCE_0fO4_postAsAnonymousUser.value;");
			
			document._article_display_list_INSTANCE_0fO4_fm13284241.postAsAnonymousUser.value = postAsAnonymousUser;								
			document._article_display_list_INSTANCE_0fO4_fm13284241._article_display_list_INSTANCE_0fO4_cmd.value = "add";
			document._article_display_list_INSTANCE_0fO4_fm13284241._article_display_list_INSTANCE_0fO4_parentMessageId.value = parentMessageId;
			document._article_display_list_INSTANCE_0fO4_fm13284241._article_display_list_INSTANCE_0fO4_body.value = body;			
			
			submitForm(document._article_display_list_INSTANCE_0fO4_fm13284241);
		}

