	function ctem_3_search() {
		var keywords = document.ctem_3_fm._3_keywords.value;
		var flag="true";
		keywords = keywords.replace(/^\s+|\s+$/, '');
		
		if(jQuery('#community').val()=='pulse'){
				flag=jQuery('#searchpulse').attr('checked');
			}
		
		if(keywords != '' && flag) {
			document.ctem_3_fm.submit();
			}
		else{
			
			document.forms["searchMedicaForm"].q.value=keywords;
			document.forms["searchMedicaForm"].submit();
			//search in medica//////////////////
		}	
	}
	function changeradio()
	{
		jQuery('#searchpulse').attr('checked','checked');
	}
	function changeradio1()
	{
		jQuery('#searchpulse').removeAttr('checked');
	}
	
	

