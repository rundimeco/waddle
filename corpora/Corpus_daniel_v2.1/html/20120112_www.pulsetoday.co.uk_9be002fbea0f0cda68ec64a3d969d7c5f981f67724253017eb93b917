function _signupbox_WAR_signupboxportlet_INSTANCE_9Evv_forGuestUserSubmit(){
	var inputGuestValue = jQuery('#_signupbox_WAR_signupboxportlet_INSTANCE_9Evv_isGuestUser').val();
	if(inputGuestValue=="true"){		
			var referrer = document._signupbox_WAR_signupboxportlet_INSTANCE_9Evv_fm1.referrer.value;
			var email = document._signupbox_WAR_signupboxportlet_INSTANCE_9Evv_fm1._signupbox_WAR_signupboxportlet_INSTANCE_9Evv_guestEmailAddress.value;
			document._signupbox_WAR_signupboxportlet_INSTANCE_9Evv_fm1.action = "https://member.ubmmedica.co.uk/register.php";			
			document._signupbox_WAR_signupboxportlet_INSTANCE_9Evv_fm1.elements["username"].value = email;
			document._signupbox_WAR_signupboxportlet_INSTANCE_9Evv_fm1.elements["referrer"].value = referrer + '?groupId=' + "4585159";

			var divUS = "";
			jQuery("input:checkbox:checked").each(function() {
				divUS  += "<input type='hidden' name='asset[]' value='"+this.name+"'> ";
			});
			var myDiv =	document.getElementById("userSelectionDiv");
			myDiv.innerHTML = divUS;		
	}
