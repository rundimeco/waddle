    $("#mos-subscribeNewsletter [name=email]").focusin(function () {
        if ($(this).val() == "Your email address") {
            $(this).val("");
        }
    })
    $("#mos-subscribeNewsletter [name=email]").focusout(function () {
        if ($(this).val() == "") {
            $(this).val("Your email address");
        }
    })
</script>
