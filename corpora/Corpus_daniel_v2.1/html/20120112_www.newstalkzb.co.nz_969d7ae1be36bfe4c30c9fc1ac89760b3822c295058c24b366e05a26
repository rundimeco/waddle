                        <div id="Results_chart" style="display:none;"></div>
                         <!-- module:poll - content bin solid -->
                        <div id="mos-poll02" class="mos-contentbin-solid colspan1 mos-poll" style="display:none">
                            <div class="mos-contentbin-header clearfix">
                                <h2> The Labour leadership Results</h2>	
                            </div>
                            <div class="mos-contentbin-body clearfix">
                                <div class="mos-contentbin-bodylayout-vert clearfix">
                                    <!-- content bin entry -->
                                    <div class="mos-binbody-entry clearfix first">
                                        <div class="mos-binentry-body">
                                            <h3>Are you happy with who's been chosen to lead the Labour Party?</h3>
                                            <p class="mos-entry-summary"></p>

                                            <ul id="pollresaultsChart" class="mos-results-chart clearfix">
                                                <li><span class="mos-poll-answer">Yes</span> <span class="mos-poll-bar"><span class="mos-poll-barfill">
                                                    <img width="60%" height="15" src="images/poll-bar-fill.png"></span></span> <span
                                                        class="mos-poll-percent">60%</span></li>
                                            </ul>
                                            
                                            <p style="display:none;" class="mos-poll-resultTotal"><label>Total:</label> <span id="polltotalvotes" class="mos-poll-number">0</span> votes</p>
                                        </div>
                                    </div>
                                    <!-- content bin entry closed -->
                                </div>
                            </div>
                        </div>
                        <!-- module:poll - content bin solid ends -->
                   





                    
                



                <script>



                    $("#pollsubmit").click(function () {
                        if (funValidatePollMod()) {
                            var datastrforpost = 'operator=select&choice=' + $("#survey [name=choice]:checked").val() + '&pollsubmit=Vote';
                            setCookie("poll", "voted");
                            PollResult(datastrforpost);

                        }
                        return false;
                    });


                    $("#justresault").click(function () {

                        var datastrforpost = "";
                        PollResult(datastrforpost);

                        return false;
                    });

                    function funValidatePollMod() {
                        var isOK = false;
                        if ($('#survey input[name=choice]:checked').val()) {
                            isOK = true;
                        }
                        if (!isOK) {
                            alert("Please select an answer.");
                        }
                        return isOK;
                    }

                    function PollResult(datastr) {
                        var dataString = datastr;
                        $.ajax({
                            type: "POST",
                            url: "/polls/results/MOSType-MultiplePoll-labour-leadership-13dec2011",
                            data: dataString,
                            success: function (data) {

                                $("#Results_chart").html(data);
                                var votechartHtml = " ";
                                var voteTitles = new Array();
                                var voteCount = new Array();
                                var totalvotes = 0;
                                $("#resaultbasevotedata li").each(function (index, domEle) {
                                    var resaultsplite = $(domEle).text().split(":");
                                    voteTitles[index] = resaultsplite[1];
                                    voteCount[index] = resaultsplite[0];
                                });

                                for (var i = 0; i < voteCount.length; i++) {
                                    totalvotes += parseInt(voteCount[i]);
                                }

                                for (var j = 0; j < voteCount.length; j++) {
                                    num = parseInt(voteCount[j]) / totalvotes * 100;
                                    tempVote = Math.round(num * Math.pow(10, 2)) / Math.pow(10, 2);
                                    votechartHtml += "<li><span class='mos-poll-answer'>" + voteTitles[j] + "</span> <span class='mos-poll-bar'><span class='mos-poll-barfill'>";
                                    votechartHtml += "<img src='/images/poll-bar-fill.png' width='" + Math.round(tempVote) + "%' height='15' /></span></span> <span class='mos-poll-percent'>" + Math.round(tempVote) + " %</span></li>";
                                }
                                $("#pollresaultsChart").html(votechartHtml);
                                $("#polltotalvotes").html(totalvotes);
                                $("#mos-poll01").hide();
                                $("#mos-poll02").show();
                            }
                        });
                    }


                    function getCookie(c_name) {
                        var i, x, y, ARRcookies = document.cookie.split(";");
                        for (i = 0; i < ARRcookies.length; i++) {
                            x = ARRcookies[i].substr(0, ARRcookies[i].indexOf("="));
                            y = ARRcookies[i].substr(ARRcookies[i].indexOf("=") + 1);
                            x = x.replace(/^\s+|\s+$/g, "");
                            if (x == c_name) {
                                return unescape(y);
                            }
                        }
                    }

                    function setCookie(c_name, value, exdays) {
                        var exdate = new Date();
                        exdate.setDate(exdate.getDate() + exdays);
                        var c_value = escape(value) + ((exdays == null) ? "" : "; expires=" + exdate.toUTCString());
                        document.cookie = c_name + "=" + c_value;
                    }

                    function checkPollCookie() {
                        var username = getCookie("poll");
                        if (username != null && username != "") {
                            var datastrforpost = "";
                            PollResult(datastrforpost);
                        }

                    }

                    checkPollCookie();

                </script>
                 




