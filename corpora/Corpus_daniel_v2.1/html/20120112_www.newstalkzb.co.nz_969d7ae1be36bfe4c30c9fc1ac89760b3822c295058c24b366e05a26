				    <script type="text/javascript" language="Javascript">
        $('#searchsybmitbtn').click(function () {
            if ($("#mos-siteSearch [name=keyword]").val() == "Keyword" || $("#mos-siteSearch [name=keyword]").val() == "") {
                alert("Please enter a keyword!");
            } else {
                $('#searchForm').submit();
            }
        });
        $("#mos-siteSearch [name=keyword]").focusin(function () {
            if ($(this).val() == "Keyword") {
                $(this).val("");
            }
        });
        $("#mos-siteSearch [name=keyword]").focusout(function () {
            if ($(this).val() == "") {
                $(this).val("Keyword");
            }
        });
    </script>             

