			var leftRail = new Object;

            currLeftRailId = false;
            leftRailVideoOpen = false;

            leftRail.showStoryPhoto = function(id){
                //player is there and showing for leftrail
                if(leftRailVideoOpen){
					closeLeftRailVideo(currLeftRailId);
					leftRail.openMedia(id);
					currLeftRailId = id;
                }else{
                    leftRail.openMedia(id);
                    currLeftRailId = id;
                }
				$('#'+currLeftRailId+' > .rel_active > .active_media > .active_image_closer').css('visibility','visible');
            }
			
			leftRail.openMedia = function(id,videoId) {
		
				//hide any existing active containers
				$('div.rel_active').removeClass('rel_active').addClass('rel_inactive');
				$('div.rel_inactive').hide();
				
				//reset outer container width and margin
				$('div.rel_container').css('width', '200px');
				$('div.rel_container').css('margin-right', '50px');
				
				//show inactive thumb
				$('div.rel_content').show();
				
				//hide active thumb 
				$('#'+id+' > .rel_content').hide();
				
				//remove inactive class, set active class on media container
				$('#'+id+' > .rel_inactive').removeClass('rel_inactive').addClass('rel_active');
				
				//set min-height of active box to avoid jump behavior
				var relHeight = $('#'+id+' > .rel_content').height();
				$('#'+id+' > .rel_active').css('min-height', relHeight+'px');
				
				//adjust width and margin on outer container
				$('#'+id).css('width', '640px');
                if(id != 'media'){
                   $('#'+id).css('margin-bottom', '20px');
		}
				$('#'+id).css('margin-right', '0px');

                //open and animate new active media container
				$('#'+id+' > .rel_active').toggle(100, function() {
					// Animation complete
				});
				// reset video title for main media
				videoTitle = titleList[0];
				return false;
			}
			
			leftRail.closeMedia = function(id) {
				if(id != 'media'){
                    $('#'+id).css('width', '200px');
                    $('#'+id).css('height', 'auto');
                    $('#'+id).css('margin-right', '50px');
                    $('#'+id).css('margin-bottom', '0px');
                    $('#'+id+' > .rel_active').toggle(100, function() {
                        $('#'+id+' > .rel_content').show();
                        $('#'+id+' > .rel_active').removeClass('rel_active').addClass('rel_inactive');
                        $('#'+id+' > .rel_inactive').hide();
                    });
                    return false;
                }
			}
			
			leftRail.openCaption = function() {
				$('#cap_full').show();
				$('#cap_short').hide();
                if(leftRailVideoOpen){
                    setTimeout(positionPlayer(leftRailContainerObj),500);
                }
				return false;
			}
			
			leftRail.closeCaption = function() {
				$('#cap_short').show();
				$('#cap_full').hide();
                if(leftRailVideoOpen){
                    setTimeout(positionPlayer(leftRailContainerObj),500);
                }
				return false;
			}
			
			var mainMedia = new Object;
			mainMedia.openCaption = function() {
				$('#main_cap_full').show();
				$('#main_cap_short').hide();
				mainMedia.sponsoredByAdPosition();
				if(leftRailVideoOpen){
                    setTimeout(positionPlayer(leftRailContainerObj),500);
                }
				return false;
			}
			
			mainMedia.closeCaption = function() {
				$('#main_cap_short').show();
				$('#main_cap_full').hide();
				mainMedia.sponsoredByAdPosition();
				if(leftRailVideoOpen){
                    setTimeout(positionPlayer(leftRailContainerObj),500);
                }
				return false;
			}
			
			mainMedia.sponsoredByAdPosition = function() {
				$("#sponsoredByAd").css({'top': $('#date_partner').position().top, 'left':430});
			}
			
			var byline = $('#hidden').html();
			if(byline!=null) {
				$('#date_partner').html(byline);
				$('#hidden').remove();
			}
			
			//adjust author/date div if partner logo exists
			var authorDivWidth = $('.byline_date_inner').width();
			var logoDivWidth = $('#sponsoredByAd').width();
			var setAuthorDivWidth = (authorDivWidth - logoDivWidth)-5;
			$('.byline_date_inner').css('width', setAuthorDivWidth+'px');
			
			//position sponsored by ad
			$("#sponsoredByAd").offset({top: $('#date_partner').position().top, left:430});
			$("#sponsoredByAd").show();		
			
			//adjust the container height based on contents						
			var byline_height = $('.byline_date').height();	
			$('#date_partner').css('height', byline_height+'px');	
		
