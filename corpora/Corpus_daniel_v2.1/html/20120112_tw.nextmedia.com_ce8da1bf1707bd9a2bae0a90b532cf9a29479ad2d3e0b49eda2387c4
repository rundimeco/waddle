<script>
function changePage(SeoSec,IssueID,art_id,page){
    $.ajax({
      url: '/realtimenews/ajaxarticle/'+SeoSec+'/'+IssueID+'/'+art_id+'/'+page+'/'+Math.random(),
      dataType: 'html',
      type:"GET",
      error: function(xhr) {
      },
      success: function(response) {
          $('#page_side').html(response);
      }
    });
  }
</script>
