<script type="text/javascript">
function offsetsrv() {
    var msec = document.getElementById("localtime").innerHTML;
    var msecserv = document.getElementById("servtime").innerHTML;
    var offsetserv = msec - msecserv;
    if (msecserv==undefined) {
        offsetserv  =undefined;
    }
    return     offsetserv;
}




function time() { 
    var offsetserv = offsetsrv(); // offset onload
//    console.log('offsetserv=' + offsetserv);
    //var offset = offsetserv - 3600000;
    var offset = offsetserv;
    var d = new Date();
    var localTime = d.getTime();
    /* 
    if  ( (d.getMonth()>2 ) && (d.getMonth() < 9) )  {
    offset = offsetserv;  
    } 
    else if (d.getMonth()==2) {
        if ( d.getDate() > 29 ) {
            offset = offsetserv; 
            }
        if ( d.getDate() == 29 ) {
            if ( d.getHours() > 1 ) 
            {
                offset = offsetserv; 
            } 
        }
    }
    else if (d.getMonth()==9) {
    if ( d.getDate() < 28 ) {
        offset = offsetserv; 
        }
        else if ( d.getDate() == 28 )  {
        if ( d.getHours() < 2 ) 
        {
            offset = offsetserv; 
        } 
    }      
    }  
    */    
    //d.setHours(d.getHours() + offset, d.getMinutes() + d.getTimezoneOffset());
    //alert(d.getMonth() + ' ' + d.getDay() + ' ' + d.getHours()  );
    
 //   console.log('localTime=' + localTime);
 //   console.log('offset=' + offset);
    var eurotime =  localTime - offset;  
    var dd = new Date(eurotime);
    var dH=''+dd.getHours();dH=dH.length<2?'0'+dH:dH;
    var dM=''+dd.getMinutes();dM=dM.length<2?'0'+dM:dM;
    var dS=''+dd.getSeconds();dS=dS.length<2?'0'+dS:dS;
    //var tmp=dH+':'+dM;
    var tmp=dH+':'+dM+':'+dS;
 
    if (offsetserv == undefined) {
        tmp = undefined;
    }
        //console.log('offsetserv=' + offsetserv);
    if (offsetserv>=0) {
        let timme = [tmp, offsetserv];
        return timme;
    } 
}

function showTime() {  
    var timme = time();
    var tme = timme[0];
    var offsetsrv = timme[1];
    var tm = tme.split(':');
    tmp = tm[0] + ':' + tm[1]; 
    tmps = tm[0] + ':' + tm[1] + ':' + tm[2];
    if (offsetsrv<1000000000) {
        document.getElementById('tm').innerHTML=tmp;
        var elem = document.getElementById('clock');
        
        elem.style.cssText = 'background-position: -'
        + (127 * parseInt(tm[2] % 10)) + 'px -'
        + ( 79 * parseInt(tm[2] / 10)) + 'px; ';
    }
    setTimeout('showTime()',1000); /*рекурсивный вызов каждую сек. для корректировки времени*/
  }

showTime(); /*выполняем ф-ю вывода времени*/


    
</script>















