        <!--szablon: /home/www/wprost3/system/szablony/news/2/show_v4.php-->
    <h2 class="art-title">Tatry: turyści zabłądzili więc świętowali z&nbsp;ratownikami</h2>  
            <div class="art-date">
        <a href="/newsroom/2012-01-01/kalendarium.html" title="czytaj więcej wiadomości WPROST z dnia 2012-01-01">2012-01-01 13:26</a>
        </div>
        
    <script type="text/javascript">
    <!-- <![CDATA[ 
    var sc_btns = '<'+'div class="art-soc-buttons art-right-box"> \n ' +
                  ''+'<div class="addthis_toolbox addthis_default_style addthis_32x32_style" addthis:url="http://www.wprost.pl/ar/286068/Tatry-turysci-zabladzili-wiec-swietowali-z-ratownikami/" addthis:title="Tatry:%20turyści%20zabłądzili%20więc%20świętowali%20z%20ratownikami">  \n ' +                            
                  '<'+'a class="addthis_button_facebook"'+'><'+'/a> \n ' +
                  '<'+'a class="addthis_button_google_plusone" g:plusone:size="medium" g:plusone:count="false"> \n ' +
                  '<'+'span id="overGoogle"'+'>&nbsp;<'+'/span> \n ' +
                  '<'+'g:plusone lang="" size="medium" count="false" href="http://www.wprost.pl/ar/286068/Tatry-turysci-zabladzili-wiec-swietowali-z-ratownikami/" callback="_at_plusonecallback"></g:plusone> \n ' +
