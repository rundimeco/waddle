					if(fid != superid1) { var preid = $("#"+superid1).prev()[0].id; document.getElementById(preid).style.background = 'none'; }


					if(superid1 != "httpibnliveincom") { $("#"+superid1+"-l").show();}


								var r = $("#"+superid1).html();
				//alert(r);

				r= r.replace('<a href="http://ibnlive.in.com/elections2012/"><font color="#fff000">Elections 2012</font></a>','<a href="http://ibnlive.in.com/elections2012/">Elections 2012</a>');
				r= r.replace("<a href='http://ibnlive.in.com/elections2012/'><font color='#fff000'>Elections 2012</font></a>","<a href='http://ibnlive.in.com/elections2012/'>Elections 2012</a>");
				$("#"+superid1).html(r);


					$("#httpibnliveincom").removeClass("act");
					$("#httpibnliveincom-s").hide();
					$("#httpibnliveincom-s-sparent").hide();
					
//alert(pid1);
					$("#"+superid1).addClass("act");
					if($("#"+superid1+"-m").length) {	$("#"+superid1+"-m").show(); }
					$("#"+pid1).show();
					$("#"+pid1+"-sparent").show();
					$("#"+superid1+"-ss").addClass("act");
					//alert(superid1+"-ss");
//alert(("#"+superid+"-sss"))
					if($("#"+superid+"-sss").length)
						{
						//alert("#"+superid+"-sssf");
						//alert("#"+superid+"-sss");

						$("#"+superid+"-sssf").show();
						$("#"+str+"-sssj").addClass("link");
						$("#"+superid+"-sss").show();
						//alert("#"+superid+"-sssf");
						
						$("#"+superid+"-ss").removeClass().addClass("act");
						}

					/****************/

					
					/****************/
				}
			/****************/
		}
		else if($("#"+str+"-ss").length)
		{
