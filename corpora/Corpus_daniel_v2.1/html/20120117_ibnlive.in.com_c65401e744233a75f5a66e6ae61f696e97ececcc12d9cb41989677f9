				$("#"+superid).html(r);


				$("#"+superid).addClass("act");
				if($("#"+superid+"-m").length) { $("#"+superid+"-m").show(); }
				$("#"+pid).show();
				//alert(pid)
				$("#"+pid+"-sparent").show();
				$("#"+str+"-ss").addClass("act");
				//alert($("#"+str+"-sss").length)

				if($("#"+str+"-sss").length  > 0)
				{
