					var superid1 = tpid1[0];

					//alert(superid1);

					var sfid = $('ul#'+pid1+' li:first').attr('id'); 
					//alert(superid+"-ss")
					document.getElementById(superid+"-ss").style.background = ''
					if(sfid != superid+"-ss")
					{
					var ppreid = $("#"+superid+"-ss").prev()[0].id; document.getElementById(ppreid).style.background = 'none';

					}

