				var lid = $('ul#flevel li:last').attr('id'); //alert(lid)
				if(lid == superid) { document.getElementById(superid).style.background = ''; }





				if(superid != "httpibnliveincom") { $("#"+superid+"-l").show();}
				else if(superid == "httpibnliveincom") { $("#"+str+"-l").show();}
