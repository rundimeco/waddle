function controlVideo(action)
{
previousTime = 0;
if (action == 1)
{
document.getElementById("VIDEO").controls.stop();
}
else if (action == 2)
{
document.getElementById("VIDEO").controls.play();
}
else if (action == 3)
{
if (x == -1)
{
startVideo();
}
else
{
if (stateI == "pause")
{
document.getElementById("pausePlay").setAttribute('src', 'http://www.ctv.ca/mar/images/local990/broadbandpause.jpg');
document.getElementById("VIDEO").controls.play();
stateI = "play";
}
else
{
stateI = "pause";
document.getElementById("pausePlay").setAttribute('src', 'http://www.ctv.ca/mar/images/local990/broadbandplay.gif');
document.getElementById("VIDEO").controls.pause();
}
}
}
else if (action == 4)
{
currentPos++;
if (currentPos == maxPos )
{
currentPos = 1;
}
functionName = document.getElementById("videoSelection" + currentPos + cT).getElementsByTagName('a')[0].getAttribute('href');
parseCurrentVideo(functionName);
//alert(functionName);
}
else if (action == 5)
{
currentPos--;
if (currentPos == 0)
{
currentPos = maxPos - 1;
}
functionName = document.getElementById("videoSelection" + currentPos + cT).getElementsByTagName('a')[0].getAttribute('href');
parseCurrentVideo(functionName);
//document.getElementById("VIDEO").controls.stop();
}
else if (action == 6)
{
alert(document.getElementById("VIDEO").fullScreen);
document.getElementById("VIDEO").fullScreen = true;
}
else if (action == 7)
{
alert(event);
}
}
function parseCurrentVideo(functionName)
{
functionName = functionName.replace("javascript:", "");
functionName = functionName.replace("changeVideo(", "");
functionName = functionName.replace(/'/g,"");
changeVideo(functionName.split(",")[0],functionName.split(",")[1],functionName.split(",")[2],functionName.split(",")[3],functionName.split(",")[4],functionName.split(",")[5].replace(")",""));
}
