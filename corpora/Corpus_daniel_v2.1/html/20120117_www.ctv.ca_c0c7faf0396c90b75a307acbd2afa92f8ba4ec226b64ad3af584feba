Interface.PermalinkClicked = function( pUrl )
{
 if( pUrl.indexOf ( "ad.doubleclick.net" ) > -1 ) //should probably use a regular expression
    {
        document.location = pUrl; //might want to use a new window?
    }
else
{
openW = window.open(pUrl,'bctvVideo','width=1010,height=850,status=no,scrollbars=1,resizable=1,toolbar=0');
if (openW)
{
FlashController.GetInstance().Wait();
}
}
}



