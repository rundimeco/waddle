if (typeof(window["hbx"]) == "undefined")
{       
	if (locBar.indexOf("page-not-found-101122") != -1){
		//do nothing
	}
    else if (locBar.indexOf("servlet/ArticleNews/story/CTVNews") != -1 || locBar.indexOf("www.ctv.ca/CTVNews") != -1 || locBar.indexOf("autos.ctv.ca/CTVNews") != -1)
