if (document.getElementsByTagName("title")[0].innerHTML == (""))
{
if (document.getElementsByTagName("title")[1] != null)
{
document.title = document.getElementsByTagName("title")[1].innerHTML;
}
else if (document.getElementsByTagName("META")[4] != null)
{
document.title = document.getElementsByTagName("META")[4].content
}
}
</script>
<script>
if (document.getElementById("content") != null)
{
var atags = document.body.getElementsByTagName("a");
var tempLink;
for (i = 0; i < atags.length; i++)
{
tempLink = atags[i].getAttribute('href');
if (tempLink != null && tempLink.indexOf("twitter.com") == -1 && tempLink.indexOf("mailto:") == -1)
{
if (atags[i].getAttribute('href').indexOf("mini") == -1 && atags[i].getAttribute('href').indexOf("storyv2") == -1 && atags[i].getAttribute('href').indexOf("/servlet/HTMLTemplate/!ctvPop/News/") == -1)
{
}
atags[i].setAttribute("href", atags[i].getAttribute('href').replace("businessBN/ctv-business", "businessBN/ctv-business?ctvBeta=yes"));

if (atags[i].getAttribute('href').charAt(0) == "/" && atags[i].getAttribute('href').indexOf("mini") == -1)
{
}
}
}
}
