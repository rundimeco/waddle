		galName = galName.replace("/gallery_.html", "")
        var s2 = "toronto";//galName;
        var s3 = "photogallery";
        var totalString = "ctvnews : " + chec(s1) + chec(s2) + chec(s3) + pnn.toLowerCase();
        Tracking.SetPageName(totalString.toLowerCase());
        Tracking.SetSubSections(cap(s1), cap(s2), cap(s3));
        Tracking.SetContentId(galName);
        Tracking.Track();    
    }
	else if (locBar.indexOf("atlantictest.ctv.ca/gallery/html") != -1)
    {
        Tracking.SetSiteSection( "ctvnews" );
        var s1 = "affiliate";
		var galName = locBar.replace("http://atlantictest.ctv.ca/gallery/html/", "");
        var pnn = "photo_0";
		galName = galName.replace("/gallery_.html", "")
        var s2 = "atlantic";//galName;
        var s3 = "photogallery";
        var totalString = "ctvnews : " + chec(s1) + chec(s2) + chec(s3) + pnn.toLowerCase();
        Tracking.SetPageName(totalString.toLowerCase());
        Tracking.SetSubSections(cap(s1), cap(s2), cap(s3));
        Tracking.SetContentId(galName);
        Tracking.Track();    
    }
    else if (locBar.indexOf("www.ctv.ca/gallery/html") != -1)
    {
        Tracking.SetSiteSection( "ctvnews" );
        var s1 = "photogallery";
        var galName = locBar.replace("http://www.ctv.ca/gallery/html/", "");
        var pnn = "photo_0";
        galName = galName.replace("/photo_0.html", "")
        galName = galName.replace("/index_.html", "")
        var s2 = galName;
        var s3 = null;
        var totalString = "ctvnews : " + chec(s1) + chec(s2) + chec(s3) + pnn.toLowerCase();
        Tracking.SetPageName(totalString.toLowerCase());
        Tracking.SetSubSections(cap(s1), cap(s2), cap(s3));
        Tracking.SetContentId(galName);
        Tracking.Track();    
    }
    else if (locBar.indexOf("weatherCity") != -1)
    {
        Tracking.SetSiteSection( "ctvnews" );
        var s1 = "weather";
        var s2 = "hub";
        var s3 = null;
        var pnn = "";
        var totalString = "ctvnews : " + chec(s1) + chec(s2) + chec(s3) + pnn.toLowerCase();
        Tracking.SetPageName(totalString.toLowerCase());
        Tracking.SetSubSections(cap(s1), cap(s2), cap(s3));
        Tracking.Track();    
    }
    else if (locBar.indexOf("/servlet/HTMLTemplate/!ctv/Search") != -1)
    {
        Tracking.SetSiteSection( "ctvnews" );
        var s1 = "search";
        var s2 = "hub";
        var s3 = null;
        var pnn = "searchnews";//document.getElementById("searchText").innerHTML;
        var totalString = "ctvnews : " + chec(s1) + chec(s2) + chec(s3) + pnn.toLowerCase();
        Tracking.SetPageName(totalString.toLowerCase());
        Tracking.SetSubSections(cap(s1), cap(s2), cap(s3));
        Tracking.TrackSearch(document.getElementById("searchText").innerHTML, 0);
        Tracking.Track();    
    }
	else if (locBar.indexOf("/search") != -1)
    {
        Tracking.SetSiteSection( "ctvhome" );
        var s1 = "searchctv_ca";
        var s2 = "hub";
        var s3 = null;
        var pnn = "searchctv_ca";//document.getElementById("searchText").innerHTML;
        var totalString = "ctvhome : " + chec(s1) + chec(s2) + chec(s3) + pnn.toLowerCase();
        Tracking.SetPageName(totalString.toLowerCase());
        Tracking.SetSubSections(cap(s1), cap(s2), cap(s3));
        Tracking.TrackSearch(document.getElementById("searchText").innerHTML, 0);
        Tracking.Track();    
    }
    else if (locBar.indexOf("www.tsnn.ca/ctvnews") != -1 || locBar.indexOf("www.tsn.ca/ctvnews") != -1)
    {

    }    
    else
    {

    }
}
	
	
	
	
</script>
