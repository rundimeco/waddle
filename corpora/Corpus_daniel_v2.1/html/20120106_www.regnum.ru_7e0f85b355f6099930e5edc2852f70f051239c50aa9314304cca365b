    $(function () {
        $("#fppoll>form").submit(function (e) {
            e.preventDefault();
            $("#pollerror").hide();
            $('#fppoll>form input[name="ajax"]').val('1');

            $.post("/poll/calc.php", $("#fppoll>form").serialize(),
				function (data) {
					//alert(data.state);
					if (data.state == 'ok') {
						$("#fppoll").html(data.text);
					} else {
						if (data.state == 'error') {
							$("#pollerror").html(data.error);
							$("#pollerror").show();
						}
					}
				}, 'json');

        });

