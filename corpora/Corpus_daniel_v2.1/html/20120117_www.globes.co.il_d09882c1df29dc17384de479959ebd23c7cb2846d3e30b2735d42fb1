if (x.readyState == 4 && x.status == 200) {
try {	var t = document.getElementById("responses_table");
var responses = x.responseText.split("\n");
for(var i in responses) {
var ri = responses[i].split(",");
if (ri.length < 4) {continue}
var row = t.insertRow(t.rows.length);
row.setAttribute("valign","top");
row.insertCell(0).innerHTML = "<span style='font-family:arial;font-size:11px; '>"+(parseInt(i)+1).toString()+".&nbsp;</span>"; 
row.insertCell(1).innerHTML = '<a style="font-size:11px;" href="javascript:readResponse(1000716239, ' + ri[0] + ')">' + (ri[3] ? ri[3] : "no subject") + '</a>' // subject
row.insertCell(2).innerHTML = "&nbsp;<span style='font-family:arial;font-size:11px;color:#cc0000; '>" + (ri[2] ? ri[2] : "no name")+"</span>" // username
row.insertCell(3).innerHTML = "&nbsp;<span style='font-family:arial;font-size:11px;color:#6d6d6d; '>" + ri[1].substring(0,(ri[1].length-6)).replace(/ /g,"&nbsp;") // datetime
}
} catch(ex) {}
}
}
x.send(null)
} show_responses()
</script>
</div>
