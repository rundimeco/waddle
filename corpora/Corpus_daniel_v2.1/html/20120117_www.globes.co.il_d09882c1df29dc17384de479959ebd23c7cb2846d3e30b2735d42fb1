	<h3>MOST POPULAR</h3>
	<script type="text/javascript" src="/data/js/SpokenArticles.en.js" ></script>
	<script type="text/javascript">
	function Fill_SpokenArticles(divId,a) {
		var sb = []
		for(var i=0; i < a.length ;i++) {
			var ai = a[i]
			sb.push('<a href=/serveen/globes/docview.asp?did=' + ai[0] + '>' + ai[1] + '</a><div class=\"G_divTopSpace5\">&nbsp;</div>')
		}
		var div=document.getElementById(divId)
		div.innerHTML = sb.join('')
	}
	function SwapTabs(tabName)
	{
		var tabs=['Today','Week','Month'];
		for(var i=0;i<3;i++)
		{
			if(tabs[i]==tabName)
			{
				document.getElementById('tab'+tabs[i]).className='tab_on';
				document.getElementById('div'+tabs[i]).style.display='';
			}
			else
			{
				document.getElementById('tab'+tabs[i]).className='tab';
				document.getElementById('div'+tabs[i]).style.display='none';
			}
		}
	}
	</script>
	<div class="sub_title">
		<div class="tab_on" id="tabToday" onclick="javascript:SwapTabs('Today')">
			<div class="left_Border_Menu"></div>
			<div class="InnerText_Menu"><span>Today</span></div>
			<div class="right_Border_Menu"></div>
		</div>
		<div class="tab" id="tabWeek" onclick="javascript:SwapTabs('Week')">
			<div class="left_Border_Menu"></div>
			<div class="InnerText_Menu"><span>Week</span></div>
			<div class="right_Border_Menu"></div>
		</div>
		<div class="tab" id="tabMonth" onclick="javascript:SwapTabs('Month')">
			<div class="left_Border_Menu"></div>
			<div class="InnerText_Menu"><span>Month</span></div>
			<div class="right_Border_Menu"></div>
		</div>
	</div>
	<div class="right_box_content" id="divToday">
		<script type="text/javascript">
		Fill_SpokenArticles('divToday',Daily)
		</script>
	</div>
