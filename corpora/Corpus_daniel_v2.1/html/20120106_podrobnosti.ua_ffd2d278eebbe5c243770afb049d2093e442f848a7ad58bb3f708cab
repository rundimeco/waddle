                        <script type="text/javascript">
                        var comments_ajax_url = '/comments/news/813153/';

                        function commentReply(pid) {
                          jQuery('#comments_pid').val(parseInt(pid, 10));
                          jQuery('#comments_form').show();
                          jQuery('#comment_text').focus();
                        }

                        function validateCommentsForm()
                        {
                          var name = jQuery('#comments_name').val();
                          var text = jQuery('#comments_text').val();
                          var code = jQuery('#comments_code').length ? jQuery('#comments_code').val() : '1';
                          var agree = jQuery('#comments_agree').length ? jQuery('#comments_agree').attr('checked') : true;
                          var error = '';
                          if(name.length == 0) {
                            error = 'Вы должны указать имя';
                            jQuery('#comments_name').focus();
                          }
                          else if(name.length < 3) {
                            error = 'Минимальная длина имени 3 символа';
                            jQuery('#comments_name').focus();
                          }
                          else if(text.length == 0) {
                            error = 'Вы должны ввести свой комментарий';
                            jQuery('#comments_text').focus();
                          }
                          else if(text.length > 500) {
                            error = 'Максимальная длина комментария 500 символов';
                            jQuery('#comments_text').focus();
                          }
                          else if(!agree) {
                            error = 'Вы должны принять условия Пользовательского соглашения';
                          }
                          else if(code.length == 0) {
                            error = 'Вы должны ввести код подтверждения';
                            jQuery('#comments_code').focus();
                          }
                          if(error) {
                            jQuery('#comments_error').html(error).show();
                            return false;
                          }

                          jQuery('input:submit').attr('disabled', 'disabled');

                          return true;
                        }
                        </script>

