                selected.appendChild(txt);
                select_main.style.display = "none";
                var title = this.getAttribute("title");
                if (title == "新闻") {
                  Y_T("ttimesort").value="10";
                  Y_T("tsearchsite").value="163.com";
                } else {
                  Y_T("ttimesort").value="0";
                  Y_T("tsearchsite").value="";
                }
            }
            function at_mouseover () {
                this.className = "active";
            }
            function at_mouseout () {
                this.className = "";
            }
            Y_T("tSelectKey").innerHTML = '<div id="select_area_yt" class="select_box"><span id="selected_yt" class="search_site">新闻</span><div id="select_main_yt" class="select_list"><a title="新闻">新闻</a><a title="网页">网页</a></div></div>';
            var select_area = Y_T("select_area_yt"), 
            selected = Y_T("selected_yt"), 
            select_main = Y_T("select_main_yt"), 
            select_list = select_main.getElementsByTagName("a"), 
            len = select_list.length;
            select_area.onclick = select_main.onmouseover = function(){
                select_main.style.display = "block";
            }
            select_main.onmouseout = function(){
                select_main.style.display = "none";
            }
            
            for (var i = 0; i < len; i++) {
                select_list[i].onclick = clicktselect;
                select_list[i].onmouseover = at_mouseover;
                select_list[i].onmouseout = at_mouseout;
            }
        })();
//]]>
</script>
</div>
	</div>
    <span class="colEnd"></span>
	<div class="bg_endPage_blue clearfix">
