var as_random = as_get_cookie('SINA_GRAN');
if (as_random > 20){ // to remove the SINA_GRAN cookie assigned to channel. to be remove after on 2009/11/1
	as_delete_global_cookie('SINA_GRAN');
	as_delete_cookie('SINA_GRAN');
	as_random = as_get_cookie('SINA_GRAN');
}

if (as_random == null) {
  as_random = Math.floor(Math.random()*7);
} else {
	as_random++;
}
as_set_global_cookie('SINA_GRAN', as_random);

function as_get_random_seq(count){
  return as_random%count;
}

	
var as_section='ros'; //all page are ros except match the following
var as_curr_url = document.location.href;
    as_curr_url= as_curr_url.replace(RegExp("^http://"+location.hostname), '');

var as_exact_match = new Array(
"/",
"/index.html",
"/nw/main/dc.html",
"/nw/main/dv.html",
"/nw/main/mplayer.html",
"/nw/main/apple.html",
"/nw/main/tv.html",
"/nw/main/notebook.html"

);



var as_pattern = new Array(
//"/test/regex.html$"
);
                   
for ( var i = 0; i < as_exact_match.length; i++) {
  if (as_exact_match[i] == as_curr_url) {
  	as_section='index'; break;
  }
}

if (as_section == "ros"){
  for ( var i = 0; i < as_pattern.length; i++) {
    if (as_curr_url.match(as_pattern[i])) {
  	  as_section='index'; break;
    }
  }
}

// check if mobile section pages
var mobile = 0;
if (as_curr_url.match("/mobile/"))	{
	mobile = 1;
}

</script>

