<script type="text/javascript">
function jsPhotoslideObj_initCallback (carousel) {
jQuery('#jsPhotoslideNextBtn').bind('click', function() {
carousel.next();
return false;
});
jQuery('#jsPhotoslidePrevBtn').bind('click', function() {
carousel.prev();
return false;
});
};
jQuery('#jsPhotoslideObj').jcarousel({
initCallback: jsPhotoslideObj_initCallback,
wrap: 'circular',
scroll:1
});
</script>
