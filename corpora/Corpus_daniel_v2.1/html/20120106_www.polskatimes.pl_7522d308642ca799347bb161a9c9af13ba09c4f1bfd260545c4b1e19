<script type="text/javascript">


  GR("o_formularz_inicjalizuj", "form-komentarz", {multi: "fK", "plik": sUrl + "/p/js/komentarze_formularz.js"});
  
  jQuery(document).ready(function(){
    var max = 4000;
    
    function przelicz_znaki() {
      var self = jQuery('#komentarz-tresc');
      if(jQuery(self).val().length > max){
        jQuery(self).val(jQuery(self).val().substr(0, max));
      }

      ilosc_znakow = max - jQuery(self).val().length;
      ilosc_enterow = jQuery(self).val().split(/\\n/).length - 1;
      ilosc_znakow = ilosc_znakow + ilosc_enterow - ilosc_enterow*100;
      jQuery('#komentarz-tresc-limit > b').html(''+(ilosc_znakow)+'');
    }
  
    przelicz_znaki();
    jQuery('#komentarz-tresc').keyup(function() {
      przelicz_znaki();
    });
  });
  podepnij_kciuki = function(idx, self) {
    var tmp = jQuery(self).attr('id').split('-'), id_post = tmp[1], ocena = tmp[0] == 'plus' ? 1 : -1, 
      id_watek = tmp[2], strona = tmp[3], sortowanie = tmp[4];
    jQuery(self).click(function(event) {
      event.preventDefault();
      ocen(id_post, ocena, id_watek, strona, sortowanie, jQuery(this));
    });
  };
  
  jQuery('.kciukGora, .kciukDol').each(podepnij_kciuki);
  
  
  (function(){
    var dane_kontroler = {
      katalog : "/artykul/",
      wartosc : "490508,semir-stilic-w-galatasaray-stambul-to-plotka,id,t",
      rozszerzenie : "html",
      strona : 1,
      async: 1
    };
    id_modul = 41;
    liczba_stron = 1;
    
    jQuery.getScript("http://s.polskatimes.pl/p/js/komentarze.js");
    

    var wiecej = jQuery(document.createElement('input')).attr('id', 'komentarze-wiecej').attr('type', 'button')
      .val('pokaż więcej komentarzy').addClass('gradientJasny').click(function() {
        dane_kontroler.strona++;

        jQuery.get('/ajax/laduj_asynchronicznie/'+id_modul+'.html', dane_kontroler, function(data) {
          jQuery('#art-komentarze').append(data).find('.kciukGora, .kciukDol').each(podepnij_kciuki);
        });

        if (liczba_stron <= dane_kontroler.strona) {
          jQuery('#komentarze-wiecej').remove();
        }
      });
    jQuery('#art-komentarze .stronicowanie').remove();

    if (liczba_stron > dane_kontroler.strona) {
      jQuery('#art-komentarze').after(wiecej);
      tr = [];
      
    }
    
    tr = [];
    tr.push({elements: GR('$$', 'regulamin-link', $('dodaj-komentarz')), href: '/komentarze/dodaj-komentarz-regulamin-link'});
    tr.push({elements: GR('$$', 'regulamin-link', $('dodaj-odpowiedz')), href: '/komentarze/dodaj-odpowiedz-regulamin-link'});
    tr.push({elements: GR('$$', 'przyciskWyslij', $('dodaj-komentarz')), href: '/komentarze/dodaj-komentarz-wyslij'});
    tr.push({elements: GR('$$', 'przyciskWyslij', $('dodaj-odpowiedz')), href: '/komentarze/dodaj-odpowiedz-wyslij'});
    tr.push({elements: [$('link-zwin-rozwin-wszystkie')], href: '/komentarze/zwin-wszystkie'});
    tr.push({elements: GR('$$', 'kciukGora', $('art-komentarze')), href: '/komentarze/kciuk-gora'});
    tr.push({elements: GR('$$', 'kciukDol', $('art-komentarze')), href: '/komentarze/kciuk-dol'});
    tr.push({elements: GR('$$', 'komentarzOdpowiedz', $('art-komentarze')), href: '/komentarze/odpowiedz'});
    GR('$$', 'ikonaNarzedzia', $('art-komentarze'), '', function(e) {
      var nazwa = e.innerHTML.replace(" ", "-");
      tr.push({elements: [e], href: '/komentarze/' + nazwa.trim()});
    });
    tr.push({elements: [$('komentarze-wiecej')], href: '/komentarze/pokaz-wiecej'});
    GR('oTrackEvents', tr);
    
  })();

</script>
