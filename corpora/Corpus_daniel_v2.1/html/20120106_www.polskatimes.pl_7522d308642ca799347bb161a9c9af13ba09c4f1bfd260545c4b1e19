    });
    GR('$$', 'stopkaLogo', $('stopka-dzienniki'), '', function(e) {
        nazwa = e.innerHTML.replace(' ', '-').toLowerCase();
        tr.push({elements: [e], href: '/stopka/' + nazwa.trim()});
    });
})(oG_OSA.tracking);
</script>
