<script type="text/javascript">
function checksearch(form) {
	if(form.sword.value.match(/^\s*$/) || form.sword.value=='поиск') {
		alert('Не заполненно поле для поиска');
		form.sword.focus();
    	return false;
	} else if(form.sword.value.length < 3) {
		echo = '\"' + form.sword.value + '\" слишком маленькое для поиска!';
		alert(echo);
		form.sword.focus();
	    return false;
	}
}
</script>

<script type="text/javascript" >
function doClear(theText) { if (theText.value == theText.defaultValue) { theText.value = "" } }
function doDefault(theText) { if (theText.value == "") { theText.value = theText.defaultValue } }

</script>
