    _gaq.push(['_setCustomVar', 2, 'Sub-Section', 'Science', 3]);
    _gaq.push(['_setCustomVar', 3, 'Byline', 'Nick Collins, Kate Hagan', 3]);
    _gaq.push(['_setCustomVar', 4, 'Page Type', 'Article', 3]);
    // Check whether the user is logged in.
    var loggedIn = false;
    if (window["FD"]) {
        loggedIn = FD.isLoggedIn();
    }
    if (loggedIn) {
        _gaq.push(['_setCustomVar', 5, 'Login Status', 'Logged In', 1]);
    } else {
        _gaq.push(['_setCustomVar', 5, 'Login Status', ' ', 1]);
    }
    _gaq.push(['_trackPageview']);
    (function() {
        var ga = document.createElement('script');
        ga.type = 'text/javascript';
        ga.async = true;
        ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
        var s = document.getElementsByTagName('script')[0];
        s.parentNode.insertBefore(ga, s);
    })();
</script>
