    function loadLogin(json) {
    	if (json.loginStatus.loggedIn) {
    		$('#featurebar_login').empty().append(
    			'<a href="/logout?referrer=http://www.cambridgefirst.co.uk:80/news/cancer_detecting_dye_could_save_many_lives_1_1179674?action=logout" rel="nofollow">Logout</a>');
    		$('#register').remove();	
