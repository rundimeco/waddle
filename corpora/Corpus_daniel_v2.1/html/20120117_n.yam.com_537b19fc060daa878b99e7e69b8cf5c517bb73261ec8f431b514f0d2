				}
			});
		
			$("#sbtn").bind('click', function(){
				$("#content").removeClass().addClass("LeftPhotoInfo_S");
				btn_init();
				$("#sbtn").attr({ src: "/i/content/wsize01_2.gif" });
			});
			$("#mbtn").bind('click', function(){
				$("#content").removeClass().addClass("LeftPhotoInfo_M");
				btn_init();
				$("#mbtn").attr({ src: "/i/content/wsize02_2.gif" });
			});
			$("#lbtn").bind('click', function(){
				$("#content").removeClass().addClass("LeftPhotoInfo_L");
				btn_init();
				$("#lbtn").attr({ src: "/i/content/wsize03_2.gif" });
			});
			$("#xlbtn").bind('click', function(){
				$("#content").removeClass().addClass("LeftPhotoInfo_XL");
				btn_init();
				$("#xlbtn").attr({ src: "/i/content/wsize04_2.gif" });
			});
		});
		function btn_init()
		{
			$("#sbtn").attr({ src: "/i/content/wsize01.gif" });
			$("#mbtn").attr({ src: "/i/content/wsize02.gif" });
			$("#lbtn").attr({ src: "/i/content/wsize03.gif" });
			$("#xlbtn").attr({ src: "/i/content/wsize04.gif" });
		}
		</script>
  
