		var mfp_base = "http://mfp.deliver.ifeng.com/mfp/mfpMultipleDelivery.do?t=html&ADUNITID=169&CHANNEL=finance";
		var mfp_user_cookie_name = "userid";
		function mfp_getCookie(c_name) {
			var start = document.cookie.indexOf(c_name+"=");
			if (start ==-1) {return "";}
			start = start+c_name.length+1;
			var end = document.cookie.indexOf(";",start);
			if (end==-1) {end = document.cookie.length;}
			return decodeURIComponent(document.cookie.substring(start,end));
		};
		function mfp_getCustom() {
			var cus_str= (aaa=location.host.match(/([^\.]+)\.ifeng\.com/i))?aaa[1]:""; 
			if(cus_str==""){
				return "";
			}
			else{
				return encodeURIComponent("channel="+cus_str);
			}
		};
		
		var mfp_url=mfp_base+"&PUBLISHERSESSIONID="+mfp_getCookie(mfp_user_cookie_name)+"&CUSTOM="+mfp_getCustom()+"&"+(Math.random()+"").substr(3);
		document.write('<scr'+'ipt src="'+mfp_url+'"></scr'+'ipt>');
		
	})();
</script>

