	<script type="text/javascript">
		jQuery(document).ready(function() {
			empikBox_76904_tabsInit();		
		});

		function getAjaxProductParams(componentId, categoryId)
		{
			try
			{
				var url = '/ComponentAjax?componentId='+componentId+'&categoryId='+categoryId;
				jQuery.ajax({
					type: "GET",
					url: url,
					success: function(data) {
						jQuery('#empikBox_'+componentId+'_productsBox').html(data);
					}
				});
			}
			catch(err)
			{
			}
		}
		
		function empikBox_76904_tabsInit() {
				var sComponentId = jQuery('#empikBox_76904').attr('id').replace('empikBox_', '');
				try
				{
					getAjaxProductParams(sComponentId, jQuery('#empikBox_76904').find('.empikBox_tabs').children('li:nth-child(1)').attr('id').replace('empikBox_76904_tab_',''));
					jQuery('#empikBox_76904').find('.empikBox_tabs').children('li:nth-child(1)').children('span').addClass('empikBox_tabActive');
					empikBox_76904_fakePics();
				}
				catch(err)
				{
				}
			
			jQuery('#empikBox_76904').find('.empikBox_tabs').children('li').bind('click', function(){
				var sComponentId = jQuery('#empikBox_76904').attr('id').replace('empikBox_', '');
				try
				{
					getAjaxProductParams(sComponentId, jQuery(this).attr('id').replace('empikBox_76904_tab_',''));
					empikBox_tabsEngine(jQuery(this));
					empikBox_76904_fakePics();
				}
				catch(err)
				{
				}				
			});
		}
	
		function empikBox_tabsEngine(oActiveElement) {
			oActiveElement.parent('.empikBox_tabs').find('span').removeClass('empikBox_tabActive');
			oActiveElement.find('span').addClass('empikBox_tabActive');
		}
		
		function empikBox_76904_fakePics() {
			jQuery('#empikBox_76904_productsBox').find('.empikBox_productImg').find('img').bind('error', function(){
				jQuery(this).attr('src', 'http://www.rp.pl/static/img/empikbox_img.jpg');
			});
		}
	</script></div> <div class="clr"></div>

