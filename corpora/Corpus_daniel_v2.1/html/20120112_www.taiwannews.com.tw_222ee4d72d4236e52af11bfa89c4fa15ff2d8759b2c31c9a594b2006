<!--<script type="text/javascript">
    
	function chk_search(search_form) {
	    
		if(search_form.q_item.value == '') {
			alert("Please input query item!");
			return false;
		}else{
		  var obj = document.getElementsByName('query_cate');
		  var chk_value = '';
		  for(i=0;i<obj.length;i++){
		    if(obj[i].checked){
			  chk_value = obj[i].value;
			  break;
		    }
	      }
		  switch(chk_value){
		    case 'PHOTO':
			  search_form.action = '/gallery.php';

			  search_form.query.value = search_form.q_item.value;
			  break;
			case 'VIDEO':
			  search_form.action = 'http://search.cnn.com/search?query='+search_form.q_item.value+'&type=video&sortBy=date&intl=true';
			  break;	
			default:
			  search_form.action = 'do_query.php';	  
		  }
		  return true;
		}
	}

</script>-->

