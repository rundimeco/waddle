function setfontsize(o_icon){	
	var o_fullstory = getElementsByClassName("fullstory");
	var o_title = getElementsByClassName("en_content_title");
	var o_author = getElementsByClassName("author");
	var o_date = getElementsByClassName("artcbe_title");
	var o_source = getElementsByClassName("portal");
	var i = 0 ;
	if(document.form_mail.font_size.value=="small"){		
		for(i=0;i<o_fullstory.length;i++){
			o_fullstory[i].style.fontSize = "16px";
			o_fullstory[i].style.lineHeight = "24px";
		}
		for(i=0;i<o_title.length;i++){
			o_title[i].style.fontSize = "30px";
			o_title[i].style.lineHeight = "34px";
		}
		for(i=0;i<o_author.length;i++){
			o_author[i].style.fontSize = "14px";
			o_author[i].style.lineHeight = "18px";
		}
		for(i=0;i<o_date.length;i++){
			o_date[i].style.fontSize = "12px";
			o_date[i].style.lineHeight = "16px";
		}
		for(i=0;i<o_source.length;i++){
			o_source[i].style.fontSize = "13px";
			o_source[i].style.lineHeight = "16px";
		}
		document.form_mail.font_size.value="big";
		o_icon.src = "images/icon_a-.jpg";
	}
	else{
		for(i=0;i<o_fullstory.length;i++){
			o_fullstory[i].style.fontSize = "14px";
			o_fullstory[i].style.lineHeight = "20px";
		}
		for(i=0;i<o_title.length;i++){
			o_title[i].style.fontSize = "28px";
			o_title[i].style.lineHeight = "30px";
		}
		for(i=0;i<o_author.length;i++){
			o_author[i].style.fontSize = "12px";
			o_author[i].style.lineHeight = "16px";
		}
		for(i=0;i<o_date.length;i++){
			o_date[i].style.fontSize = "10px";
			o_date[i].style.lineHeight = "12px";
		}
		for(i=0;i<o_source.length;i++){
			o_source[i].style.fontSize = "10px";
			o_source[i].style.lineHeight = "14px";
		}
		document.form_mail.font_size.value="small";
		o_icon.src = "images/icon_a+.jpg";
	}	
}
function getElementsByClassName(clsName) 
{ 
	var arr = new Array(); 
	var elems = document.getElementsByTagName("*");
	for ( var cls, i = 0; ( elem = elems[i] ); i++ )
	{
		if ( elem.className == clsName )
		{
			arr[arr.length] = elem;
		}
	}
	return arr;
}

function send_mail(){
var obj_icon = document.getElementById('func_icon');
	var str = obj_icon.innerHTML;
	obj_icon.innerHTML = '';
	var obj = document.getElementById('fullstory');	
	document.form_mail.news_content.value = obj.innerHTML;	
	var obj_title = document.getElementById('mail_title1');	
	document.form_mail.news_title.value = obj_title.innerHTML;
	var obj_source = document.getElementById('mail_source');
	document.form_mail.news_source.value = obj_source.innerHTML;
	document.form_mail.action = "mailto.php";	
	document.form_mail.submit();
	obj_icon.innerHTML = str;
}


function print_article(){
	var obj_icon = document.getElementById('func_icon');
	var str = obj_icon.innerHTML;
	obj_icon.innerHTML = '';
	var obj = document.getElementById('fullstory');	
	document.form_mail.news_content.value = obj.innerHTML;	
	var obj_title = document.getElementById('mail_title1');	
	document.form_mail.news_title.value = obj_title.innerHTML;
	var obj_source = document.getElementById('mail_source');
	document.form_mail.news_source.value = obj_source.innerHTML;	
	document.form_mail.action = "print.php";	
	document.form_mail.submit();
	obj_icon.innerHTML = str;
}
function open_download(url) {
	window.open(url, "dlwin", "left=0,screenX=0,top=0,screenY=0,height=1,innerHeight=1,width=1,innerWidth=1,toolbar=no,status=no,location=no");
}

