<script language="javascript">
        $(function(){


        $('#slideleft').click(
			function(e){		
			  var test=parseInt($('#test_box').attr('title'));
			  var move;
			  if(test > 0 ){
              $("#slider").animate({"left": "+=440px"}, 1500);
			  move = test - 950;
            }
			 $('#test_box').attr('title',move);
		});
		
		$('#slideright').click(
           function(e){
			var test= parseInt($('#test_box').attr('title'));
			var move;
            if(test < 950){
			$("#slider").animate({"left": "-=440px"}, 1500);
			move = test + 950;
			}
            $('#test_box').attr('title',move);
		});
  });
  
</script>

