<script type="text/javascript">

(function(tr, url) {
    var zapiszSie = $('zapisz-sie');
    GR('$$', 'link', $('lista-wydarzenia-dnia'), 'a', function(e) {
        if (e.getAttribute('href').split('#')[0] === url) {
            GR('dodajCssClass', e.parentNode, 'wyrozniony');
        }
    });
    Zdarzenia.zaczep(zapiszSie, 'blur', function() {
        if ('' === zapiszSie.value) {
            zapiszSie.value = 'jan.kowalski@domena.pl';
            GR('dodajCssClass', zapiszSie, 'puste');
        }
    });
    Zdarzenia.zaczep(zapiszSie, 'focus', function() {
        if ('jan.kowalski@domena.pl' === zapiszSie.value) {
            zapiszSie.value = '';
            GR('usunCssClass', zapiszSie, 'puste');
        }
    });
    tr.push({elements: GR('$$', 'zdjecie', $('lista-wydarzenia-dnia')), href: '/boks_wydarzenia_dnia/zdjecie_', licznik: true});
    tr.push({elements: GR('$$', 'link', $('lista-wydarzenia-dnia')), href: '/boks_wydarzenia_dnia/link_do_materialu_', licznik: true});
    tr.push({elements: [$('zapisz-sie-button')], href: '/pasek/newsletter'});
})(oG_OSA.tracking, window.location.pathname);

</script><iframe src="http://www.facebook.com/plugins/fan.php?api_key=&connections=10&height=600&id=284118934367&locale=pl_PL&logobar=false&stream=false&width=318" scrolling="no" frameborder="0" style="border:none; overflow:hidden; width:318px; height:250px;" allowTransparency="true"></iframe><div id="boks-dodaj-artykul">
