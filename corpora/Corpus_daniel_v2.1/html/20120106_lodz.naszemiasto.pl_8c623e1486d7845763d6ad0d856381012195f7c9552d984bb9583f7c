<script type="text/javascript">

(function() {
    var rozwiniete = {}, i,
        li = GR('$$', 'wiecej', $('menu-nowe'), 'li'),
        zwin = function(el) {
            if (!rozwiniete[el.id]) {
                GR('usunCssClass', el, 'rozwin');
            }
        },
        rozwin = function(el) {
            if (rozwiniete[el.id]) {
                GR('dodajCssClass', el, 'rozwin');
            }
        };
    for (i in li) {
        Zdarzenia.zaczep(li[i], 'mouseover', function() {
            var menu = Zdarzenia.pobierz().target;
	        while (!GR('czyCssClass', menu, 'elListyPozPierwszy')) {
		        menu = menu.parentNode;
	        }
            rozwiniete[menu.id] = true;
            setTimeout(function() {rozwin(menu);}, 250);
        });
        Zdarzenia.zaczep(li[i], 'mouseout', function() {
            var menu = Zdarzenia.pobierz().target;
	        while (!GR('czyCssClass', menu, 'elListyPozPierwszy')) {
		        menu = menu.parentNode;
	        }
            rozwiniete[menu.id] = false;
            setTimeout(function() {zwin(menu);}, 250);
        });
    }
    GR('p_tracking_menuMiejskie', 'lodz');
})();
(function(tr){
    var zamienPolskieZnaki = function(str) {
        str = str.replace("ó", "o");
        str = str.replace("ł", "l");
        str = str.replace("ń", "n");
        str = str.replace("ż", "z");
        str = str.replace("ź", "z");
        str = str.replace("ć", "c");
        str = str.replace("ą", "a");
        str = str.replace("ę", "e");
        str = str.replace("ś", "s");
        str = str.replace("\ ", "_");
        return str;
    }
    GR('$$', 'elListyPozPierwszyLink', $('menu-nowe'), '', function(el) {
        GR('$$', 'nazwa', el, 'span', function(e) {
            var nazwa = e.innerHTML;
            tr.push({elements: [el], href: '/menu_miejskie/poziom_1:' + zamienPolskieZnaki(nazwa.trim().toLowerCase())});
        });
    });
    GR('$$', 'menuPodkategoria', $('menu-nowe'), '', function(el) {
        var nazwa = el.innerHTML;
        tr.push({elements: [el], href: '/menu_miejskie/poziom_2:' + zamienPolskieZnaki(nazwa.toLowerCase())});
    });
})(oG_OSA.tracking);

</script>

<script type="text/javascript">
function wstaw_napis() {
    oEl = GR("$", "czego_szukasz");
    if (oEl.value == '') {
        oEl.value = 'Czego szukasz?';
    }
}
function usun_napis() {
    oEl = GR("$", "czego_szukasz");
    if (oEl.value == 'Czego szukasz?') {
        oEl.value = '';
    }
}
Zdarzenia.zaczep(GR("$", "czego_szukasz"), "focus", usun_napis);
Zdarzenia.zaczep(GR("$", "czego_szukasz"), "blur", wstaw_napis);
</script>
