				var google_adnum = 0;

			
				function google_ad_request_done(google_ads) {
			
					if (google_ads.length == 0) {
						return;
					}

					function anchorLink(str,url,visible_url,cssclass) {
						 return '<a href="'+ url + '" '
							+ ((typeof(cssclass)=='undefined')?(' '):(' class="'+cssclass+'" '))
							+ '\'" onmouseover="window.status=\''  + encodeURI(visible_url)
							+ '\'" onmouseout="window.status=\'\'">' + str + '</a>';
					}

					google_adnum += google_ads.length;

					if (typeof(google_info)=="undefined"){
						var feedback_url = "http://services.google.com/feedback/online_hws_feedback";
					} else {
						var feedback_url = google_info.feedback_url;
					}

					if('pub-4010713000150964'=='pub-4010713000150964'){
						var s =	'<div class="'+ wrapperClass +'"><h4 class="list-head-color1 m4-adsbygoogle-header replace"><a class="google-adlink" href="'+feedback_url+'">Ads by Google<span></span></a></h4>';
					} else {
						var s =	'<div class="'+ wrapperClass +'"><h4 class="list-head-color1 m4-adsbygoogle-header replace">Ads by Google<span></span></h4>' +
								'<div class="sponsored-link"><p><a class="google-adlink" href="'
								+feedback_url+'">Ads by Google</a></p></div>';
					}

					for (var i = 0; i < google_ads.length; ++i) {
						if (google_ads[i].type == "image") {
							s +='<a href="' + google_ads[i].url +
								'" target="_top" title="go to ' + google_ads[i].visible_url +
								'"><img style="border:0" src="' + google_ads[i].image_url +
								'"width="' + google_ads[i].image_width +
								'"height="' + google_ads[i].image_height + '"/></a>';
						} else {
							s += '<div class="sponsored-link">' +
								'<h5>' + anchorLink(google_ads[i].line1,google_ads[i].url,
										google_ads[i].visible_url) + '</h5>'
								+ '<p>' + google_ads[i].line2 + '&nbsp;'
								+ ((typeof(google_ads[i].line3)=='undefined')?(' '):( google_ads[i].line3 ))
								+ '<br />'
								+ '<span>' + anchorLink(google_ads[i].visible_url,google_ads[i].url,
										google_ads[i].visible_url,"google-adlink") + '</span>'
								+ '</p></div>';
						}
					}

					s += '</div>' + "\r\n";
					wrapperClass = null;

					document.write(s);
					return;
				}
