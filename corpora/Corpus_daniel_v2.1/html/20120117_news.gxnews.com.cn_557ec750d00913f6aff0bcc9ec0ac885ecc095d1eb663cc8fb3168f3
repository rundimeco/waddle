//论坛热帖、精彩博文切换
function changeTag_bbs_blog(currentID)
{

	if (currentID == 1)
	{
		document.getElementById("right_5_1").className = "bbs_nav1";
		document.getElementById("right_5_2").className = "bbs_nav2";
		document.getElementById("right_2_1").style.display = "";
		document.getElementById("right_2_2").style.display = "none";
	}
	else
	{
		document.getElementById("right_5_1").className = "bbs_nav2";
		document.getElementById("right_5_2").className = "bbs_nav1";
		document.getElementById("right_2_1").style.display = "none";
		document.getElementById("right_2_2").style.display = "";
	}
}
</script>		
