<script language="javascript">

if (typeof(String.prototype.trim)==="undefined") {
	String.prototype.trim = function () { return this.replace(/(^\s*)|(\s*$)/g, ""); }
}

function submitForm(form){
	if ((form.searchType[1] != null && form.searchType[1].checked) || form.searchType.value == 'web') {
		parent.location.href = 'http://search.sky.com/web?channel=news&term=' + form.searchTextBox.value;
	} else {
        parent.location.href = 'http://search.sky.com/news?term=' + form.searchTextBox.value.trim().replace(/\s+/g,"+");
        //Former Sky News Search implementation
		//form.action = '/search';
		//form.submit();
	}
}

function processKey(e,form) {
	if (e.keyCode == 13){
		submitForm(form);
    return false;
  } else {
    return true;
  }
}

</script>




