		<script language="javascript">
			function footerSearch() {
				var q = document.getElementById("f").value.replace(/ /g,"+");
				if (q.length == 0) {
					alert("Please enter your search criteria");
					return;
				}
				document.location.href="/search/search.html?q=" + escape(q);
			}
		</script>
