(function ($) {
	"use strict";
		var storyurl = $('link[rel="canonical"]').attr('href'),
			storyheading = $('div#story h1.heading').text(),
			sharelinks = $('div#story-footer-share-basic');
		sharelinks.find('li.facebook a').live('click', function (ev) {
			window.open('http://www.facebook.com/sharer.php?u=' + storyurl, '_blank', 'width=650, height=400');
			ev.preventDefault();
			return false;
		});
		sharelinks.find('li.twitter a').live('click', function (ev) {
			window.open('https://twitter.com/intent/tweet?text=' + storyheading + '&url=' + storyurl, '_blank', 'width=650, height=400');
			ev.preventDefault();
			return false;
		});
}(jQuery));
</script>
