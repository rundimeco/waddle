function changeTasteRecipe(index)
{
	document.getElementById('taste-recipe-content').innerHTML = tasteRecipesArray[index];
}

function prevTasteRecipe()
{
	if(currentRecipe > 1)
		currentRecipe--;
	else
		currentRecipe = 7;
	changeTasteRecipe(currentRecipe);
}

function nextTasteRecipe()
{
	if(currentRecipe < 7)
		currentRecipe++;
	else
		currentRecipe = 1;
	changeTasteRecipe(currentRecipe);
}

