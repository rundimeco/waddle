<script type="text/javascript">
// resize font size
var resizeFontBtn = 'normal';
function resizeContentFont (size, buttonObj)	{
	
	switch (size)	{
		case 'large'	:	$('#news_text p').css('font-size', '18px');
										$('#news_text p').css('line-height', '27px');		
										$('#news_text h3').css('font-size', '20px');
										break;
		case 'small'	:	$('#news_text p').css('font-size', '12px');
										$('#news_text p').css('line-height', '20px');	
										$('#news_text h3').css('font-size', '12px');
										break;		
		case 'normal'	:	$('#news_text p').css('font-size', '15px');
										$('#news_text p').css('letter-spacing', '1px');	
										$('#news_text p').css('line-height', '22px');		
										$('#news_text h3').css('font-size', '15px');
										break;																		
		default				: break;
	}

	$('#resizeFontBtn'+resizeFontBtn).removeClass('buttonOn');
	$(buttonObj).addClass('buttonOn');
	resizeFontBtn = size;

}
//
</script>

