function showCount()
{
	var phprpc = new PHPRPC_Client('http://comment.xinmin.cn/index.php?app=common&controller=Content.Count.Service&action=phprpc', ['count']);
	phprpc['count']
	(
		'12988780',
		function(text)
		{
		}
	)
}
function loadCommont(indexId,indexUrl)
{
	var phprpc = new PHPRPC_Client('http://comment.xinmin.cn/index.php?app=common&controller=Comment.Service', ['listAction','countAction']);
	phprpc['listAction']
	(
		'comment', '12988780', 10, 1,
		function(text)
		{	
			$('commentCountB').innerHTML  = text.page.totalCount;
			var html = iname = '';
			try
			{
				for(var i = 0; i < text.record.length; i++)
				{
					if(text.record[i].nickname)
					{
						iname = '<a href="http://t.xinmin.cn/jump.php?var='+text.record[i].username+'&type=username" target="_blank">'+text.record[i].nickname+'</a>';
					}
					else
					{
						if(text.record[i].username)
						{
							iname = '<a href="http://t.xinmin.cn/jump.php?var='+text.record[i].username+'&type=username" target="_blank">'+text.record[i].username+'</a>';
						}
						else
						{
							var ip = text.record[i].ip;
							ip = ip.replace(/[\d\s]+$/,"*");
							iname = 'IP：'+ ip;
						}
					}
					if(text.record[i].area != "")
					{
						area = '新民网'+ text.record[i].area +'网友';
					}else{
						area = '新民网网友';							
					}
					html += '<p><font style="color:#c20001">'+ area +'</font>&nbsp;[&nbsp;'+iname+'&nbsp;]</p>';
					html += '<p>'+ text.record[i].content +'&nbsp;&nbsp;[&nbsp;'+ text.record[i].create_datetime +'&nbsp;]</p>';
					if(i != text.record.length-1)
					{
						html += '<p class="dian2"><img src="http://image.xinmin.cn/template/www.xinmin.cn/200907/images/dian.jpg"></p>';	
					}
				}
				$('commentLi').innerHTML = html;
				$('dian').style.display = "";
				$('comment').style.display = "";
			}
			catch (e)
			{
			}
		}
	)
}
function viewComment(id,title,to)
{	//&tpl=xinmin201009
	var url = 'http://comment.xinmin.cn/index.php?app=common&controller=Comment.List&indexId='+id+'&title='+encodeURI(title)+'&url='+ encodeURIComponent(to);
	window.open(url);
}
//选择评分
function rateHover(value) {
	$('xspace-rates-star').className = 'xspace-rates'+value;
}
function rateOut() {
	$('xspace-rates-star').className = 'xspace-rates1';
}
//显示评分
function loadCommont2(scored)
{
	var phprpc = new PHPRPC_Client('http://comment.xinmin.cn/index.php?app=common&controller=Content.Score.Service', ['scoreAction','viewAction']);
	phprpc['viewAction']('12988780', function(text){
		$('commentLi3').innerHTML  = text.scoreCount;
		//已经投票
		if(scored == 0)
		{
			$('commentLi2').innerHTML  = '当前平均分:<font color="red">&nbsp;<b>'+ Math.round(text.averageScore*10)/10 +'</b>&nbsp;</font>分';
		}else{
			$('commentLi2').innerHTML  = '&nbsp;&nbsp;当前平均分:<font color="red">&nbsp;<b>'+ Math.round(text.averageScore*10)/10 +'</b>&nbsp;</font>分';
			$('commentLi2_2').innerHTML = '<font color="red">感谢您的评价!</font>'
		}
	});
}
//提交评分
function score(score,indexId)
{
	var phprpc = new PHPRPC_Client('http://comment.xinmin.cn/index.php?app=common&controller=Content.Score.Service', ['scoreAction','viewAction']);
	phprpc['scoreAction'](score, indexId, function(){
		$('xspace-rates-a').style.display = "none";//不能再次评选
		$('xspace-rates-star').className = 'xspace-rates'+score;//显示用户的评选
		loadCommont2('1');//重载评选结果
	});
}

/* 改变字体大小 */

function doZoom(size) 
{ 
	var des=document.all?document.all['des']:document.getElementById('des');
	var title=document.all?document.all['title']:document.getElementById('title');
	var about=document.all?document.all['about']:document.getElementById('about');
	var article_info=document.all?document.all['article_info']:document.getElementById('article_info');
	if(size == 1){
		if(des)
		{
			des.style.fontSize='18px';
		}
		about.style.fontSize='14px';
		title.style.fontSize='24px';
		article_info.style.fontSize='16px';
	}else if(size == 2){
		if(des)
		{
			des.style.fontSize='14px';
		}
		about.style.fontSize='12px';
		title.style.fontSize='22px';
		article_info.style.fontSize='14px';		
	}else{
		if(des)
		{
			des.style.fontSize='12px';
		}
		about.style.fontSize='10px';
		title.style.fontSize='18px';
		article_info.style.fontSize='12px';
	}
}
</script>
<script src="http://image.xinmin.cn/template/www.xinmin.cn/200912/js/content.js"></script>
<script>
function LoadFullViewJsCallBack(){
  if(typeof forSFullView == "object" && forSFullView.FullView){
    forSFullView.FullView();
  }
}
function LoadFullViewJs(){
  var jsFile = "http://image.xinmin.cn/template/www.xinmin.cn/200912/js/content.js";  //全文浏览主js文件url
  jsFile += "?t="+ (new Date()).getTime();
  var js = document.createElement("script");
  js.setAttribute("src",jsFile); 
  js.setAttribute("type","text\/javascript");
  js.setAttribute( "id", "sinaFullViewJsUrl");
  //for ie
  js.onreadystatechange = function(){
  if(js.readyState=="loaded"){
     LoadFullViewJsCallBack();
  }
};
//for ff
js.onload = LoadFullViewJsCallBack;
  document.body.insertBefore(js,null); // null for ff
}
</script>
<script>
//复制此贴
document.getElementById("url").value = '“兵马俑”坐镇 巧克力梦公园(图)_新民网'+document.URL;
function copyCode(v)
{	 
	if(navigator.userAgent.indexOf("MSIE")>0){ 
		document.getElementById(v).select();
		var js=document.getElementById(v).createTextRange();
		js.execCommand("Copy");
		alert("接Ctrl+V,粘贴到MSN/QQ上发给您的好友们吧!");
	}else{
		alert("对不起，您的浏览器不支持此功能，请换浏览器！")
	}
}
</script>
<script>
function checkLogin()
{
	/*----------------------------------------------------登录判断--------------------------------------------------*/
	XMPS.loginCheck
	({
		success:function(data)
		{
			var fbpl_t = $('fbpl').getPosition().y;
			var fbpl_l = $('fbpl').getPosition().x;
			
			var uid  = Cookie.read('Ynr_Tuid');
			var u_name = Cookie.read('XMPS_USERNAME');
			var nickname = Cookie.read("XMPS_NICKNAME");
			
			if((uid==null) || (u_name==null)){
				alert('您尚未登录，请先登录');
				$('login_no').setStyles({
					position: 'absolute',
					left: fbpl_l,
					top: fbpl_t,
					display: 'block'
				});
			}else{
				if(($('content').value == '新民网提示您：有话不说，憋着难受！') || ($('content').value == ''))
				{
					alert('请填写评论');
					$('content').focus();
					return false;
				}else{
					if($('limittxt').getStyle('color') == 'red'){
						alert('请勿超过140字');
						return false;
					}else{
						
						if((navigator.userAgent.indexOf("MSIE")>0) || (isFirefox=navigator.userAgent.indexOf("Firefox")>0)){
							/*
							var parmabc = {
								'index_id' : '12988780',
								'index_title' : '“兵马俑”坐镇 巧克力梦公园(图)',
								'index_url' : document.URL,
								'username' : Cookie.read("XMPS_USERNAME"),
								'nickname' : Cookie.read("XMPS_NICKNAME"),
								'content' : $('content').innerHTML
							}
							*/
							//alert(parmabc.index_id +'<br/>'+parmabc.index_title +'<br/>'+parmabc.index_url +'<br/>'+parmabc.nickname +'<br/>'+parmabc.username +'<br/>'+'<br/>'+parmabc.content +'<br/>' );
							if(navigator.userAgent.indexOf("MSIE")>0){
								var rpc = new PHPRPC_Client('http://comment.xinmin.cn/index.php?app=common&controller=Comment', ['addCommentForRpcAction']);
								rpc.addCommentForRpcAction('12988780','“兵马俑”坐镇 巧克力梦公园(图)',document.URL,Cookie.read("XMPS_USERNAME"),Cookie.read("XMPS_NICKNAME"),$('content').innerHTML,function (result) {
									//alert('ok')
								})
							}else{
								var rpc = new PHPRPC_Client('http://comment.xinmin.cn/index.php?app=common&controller=Comment', ['addCommentForRpcAction']);
								rpc.addCommentForRpcAction('12988780','“兵马俑”坐镇 巧克力梦公园(图)',document.URL,Cookie.read("XMPS_USERNAME"),Cookie.read("XMPS_NICKNAME"),$('content').get('value'),function (result) {
									//alert('ok')
								})
							}
							$('pl_close').addEvent('click', function(){
								$('pl_success').setStyle('display','none');
								//$('postCommentForm').submit();
								window.location.reload();
							});
							$('pl_close2').addEvent('click', function(){
								$('pl_success').setStyle('display','none');
								//$('postCommentForm').submit();
								viewComment(12988780,"“兵马俑”坐镇 巧克力梦公园(图)", document.URL);
								window.location.reload();
							});
						}else{
							$('postCommentForm').submit();
							$('pl_close').addEvent('click', function(){
								$('pl_success').setStyle('display','none');
								window.location.reload();
							});
							$('pl_close2').addEvent('click', function(){
								$('pl_success').setStyle('display','none');
								window.location.reload();
							});
						}
						
						/*
						if(isFirefox=navigator.userAgent.indexOf("Firefox")>0){ 
							var content = $('content').get('value') + ' / 【原文】：' + zf_title + zf_url + ' ' + '';
						}
						*/
						if(navigator.userAgent.indexOf("MSIE")>0){
							var content = $('content').innerHTML + ' / 【原文】：' + zf_title + zf_url + ' ' + '';
						}else{
							var content = $('content').get('value') + ' / 【原文】：' + zf_title + zf_url + ' ' + '';
						}
						
						//同步勾选
						if($('withwb').checked){
							var rpc = new PHPRPC_Client('http://t.xinmin.cn/api/PKService.php?action=phprpc&random='+Math.random(), ['postBlog']);
							rpc.postBlog({'content':content,'uid':uid,'nickname':nickname},function (result) {
								
								if(result != 'blog_post_success'){
									alert('您的评论过长，将无法发到上海滩微博，请适当缩减');
									return false
								}else{											
									$('pl_success').setStyles({
										position: 'absolute',
										left: fbpl_l,
										top: fbpl_t,
										display: 'block'
									});
								}
								
							});
						}else{
							$('pl_success').setStyles({
								position: 'absolute',
								left: fbpl_l,
								top: fbpl_t,
								display: 'block'
							});
						}
						
					}
				}
			}
		},
		failure:function(error)
		{
			var fbpl_t = $('fbpl').getPosition().y;
			var fbpl_l = $('fbpl').getPosition().x;
			alert('您尚未登录，请先登录');
			$('login_no').setStyles({
				position: 'absolute',
				left: fbpl_l,
				top: fbpl_t,
				display: 'block'
			});
		}
	});	
	
	/*----------------------------------------------------登录判断--------------------------------------------------*/
}


//评分
function input_hint(obj,str,n) 
{
	if (n == 0 && obj.value == str)
	{
		
		obj.value="";$("content").style.color='#000000';
	}
	if (n == 1)
	{
		if(obj.value == str)
		{
			obj.value="";$("content").style.color='#000000';
		}
	}
	if (n == 2 && obj.value == "")
	{
		obj.value = str;$("content").style.color='#9A9A9A';
	}
	else
	{
		$("content").style.color='black';
	}
}
function textCounter(field, countfield, maxlimit) {
	if (field.value.length > maxlimit){
		$('limittxt').setStyle('color','red');
		$('limittxt').setStyle('font-weight','bold');
		field.value = field.value.substring(0, maxlimit);
		}
	else{
		$('limittxt').setStyle('color','black');
		$('limittxt').setStyle('font-weight','normal');
		countfield.value = maxlimit - field.value.length;
		}
}
</script>
<script>
function baidu_hare(title,content){
	var type = 'text';
	var link = document.URL;//页面url

	var itieba_share = 'http://tieba.baidu.com/i/sys/share?link=' + encodeURIComponent(link)
	+'&type=' + encodeURIComponent(type)
	+'&title=' + encodeURIComponent(title)
	+'&content=' + encodeURIComponent(content);
	if (!window.open(itieba_share, 'itieba', 'toolbar=0,resizable=1,scrollbars=yes,status=1,width=626,height=436')) 
	{
		location.href = itieba_share;
	}
}
</script>
