return false;
});

jQuery('#objectCitySingle1').boxList({
listAjaxUrl: '/ajax/widget/weather',
params: {cityId: [11651,11666,11696,11741,11746,11763,11788,11797,11844,11848,11875,11905,11942,11982,11998,12013]},
listRebuild: false,
get: function (liJQ, params) {
return {
iconId: liJQ.find('input[name="iconId"]').attr('value'),
phaseName: liJQ.find('.phase').html(),
cityName: liJQ.find('.cityName').html(),
tempFeelThi: liJQ.find('.temperature .value').html(),
pressure: liJQ.find('.pressure. value').html()};
},
set: function (liJQ, data, params) {
liJQ.css('display', '');
liJQ.find('.thumbnail img')
//.attr('src', '/i/weather/' + (liJQ.hasClass('i1') ? '' : 'm_') + data.iconId + '.gif')
.attr('src', '/i/weather/m_' + data.iconId + '.gif')
.attr('alt', data.phaseName);
liJQ.find('input[name="iconId"]').attr('value', data.iconId);
liJQ.find('.phase').html(data.phaseName);
liJQ.find('.cityName').html(data.cityName);
liJQ.find('.temperature .value').html(data.tempFeelThi);
liJQ.find('.pressure. value').html(data.pressure);

jQuery('#objectCitySingle1').show();

jQuery.cookie('cityId', data.cityId, {expires: 7, path: '/'});
},
cookiePrefix: 'weather_'});

jQuery('#objectCitySingle1 .navigation .change .listFunctions').absolution({
relativeJQ: jQuery('#objectCitySingle1').parents('.scrollStripe').find('.boxBody').eq(0),
nestClass: 'weatherCityChange'});
});    //]]>
</script></body>
</html>
