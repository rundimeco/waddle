            function getSibling(el){//FF
		var prev =  el.previousSibling;
		while(prev.nodeType != 1){
			prev = prev.previousSibling;
		}
		return prev;
	};
	function _replyBycid(data){
		var tempHTML = "";
		if(typeof data !== "undefined"){
			
			var l = data.hot_list.length;
			l = l > 5 ? 5 : l;
			for(var i=0; i < l; i++){
				if(i==l-1){
				tempHTML+="<li style='border-bottom:0px;'><span class='member'>腾讯"+data.hot_list[i].province_city+"网友 <a href='http://"+TencentArticl.cmtBase+"."+TencentArticl.site+".qq.com/comment_user2.htm?uin="+data.hot_list[i].uin+"' target='_blank'>"+data.hot_list[i].nick+"</a></span>";
				if(data.hot_list[i].subflag==1){
						tempHTML+="<p>"+data.hot_list[i].reply_content+"<a href=http://"+TencentArticl.cmtBase+"."+TencentArticl.site+".qq.com/comm/comment?site="+TencentArticl.site+"&id="+TencentArticl.cmtId+" target='_blank'>[详细]</a></p>";
					}else{
						tempHTML+="<p>"+data.hot_list[i].reply_content+"</p>";
					}
				}else{
								tempHTML+="<li><span class='member'>腾讯"+data.hot_list[i].province_city+"网友 <a href='http://"+TencentArticl.cmtBase+"."+TencentArticl.site+".qq.com/comment_user2.htm?uin="+data.hot_list[i].uin+"' target='_blank'>"+data.hot_list[i].nick+"</a></span>";
				if(data.hot_list[i].subflag==1){
						tempHTML+="<p>"+data.hot_list[i].reply_content+"<a href=http://"+TencentArticl.cmtBase+"."+TencentArticl.site+".qq.com/comm/comment?site="+TencentArticl.site+"&id="+TencentArticl.cmtId+" target='_blank'>[详细]</a></p>";
					}else{
						tempHTML+="<p>"+data.hot_list[i].reply_content+"</p>";
					}
				}
			}
		}
	TencentArticl.$(TencentArticl.comtList).innerHTML = tempHTML;
	}
/*列贴结束*/
</script><!--[if !IE]>|xGv00|a79abf1b509fbee9b28ec509c88fab62<![endif]-->
 
