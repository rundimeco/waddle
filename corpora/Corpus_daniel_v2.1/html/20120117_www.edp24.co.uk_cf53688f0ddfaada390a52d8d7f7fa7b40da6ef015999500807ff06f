<script type="text/javascript">

	function loadCommentsDynamic(json) {		
		if( json.loginStatus.email ) {
			$('#frm-email-input').val( json.loginStatus.email );
		}
		if (json.loginStatus.loggedIn) {
			$('.loggedout').empty();
			$('#username').text( json.loginStatus.userName );
		} else {
			$('.loggedin').empty();
		}
	}
	
	$(document).ready(function() {
		$.getJSON('/ajax?action=loginStatus', loadCommentsDynamic);
	});

</script>

