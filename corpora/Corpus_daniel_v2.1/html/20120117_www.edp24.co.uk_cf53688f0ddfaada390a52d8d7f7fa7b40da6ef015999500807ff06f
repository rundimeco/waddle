			/* On loading the page: get the post code of the current user, if any */
			$(document).ready(function() {			
				$.getJSON('/ajax?action=loginStatus', loadPostCode);
			});
			
			/* Callback for loading postcode - if member profile postcode is available, replace 
			   the HTML of the weather div with the personalised weather teaser, using that postcode */	
			function loadPostCode(json) {
				if( json.loginStatus.postCode != null ) {
					var url = "http://www.edp24.co.uk:80/home/weather_7_1157?ot=archant.PopupPageLayout.ot&contentonly=true&ajax=true&postcode=" + json.loginStatus.postCode;	
					$.get( url , replaceWeatherHtml );		
				}		
			}
			
			/* Callback for loading personalised weather teaser HTML */		
			function replaceWeatherHtml(html) {
				$('.weather-feeds').before( html ).remove();
			}
		</script>
	      
      
      
  
