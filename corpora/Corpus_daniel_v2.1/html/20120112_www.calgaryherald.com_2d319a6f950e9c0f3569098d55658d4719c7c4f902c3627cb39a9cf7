			function getPluckContent(ArtKey) {
			    var commentsPageRequest = new PluckSDK.CommentsPageRequest();
			    commentsPageRequest.CommentedOnKey = new PluckSDK.ExternalResourceKey({ Key: ArtKey });

			    PluckSDK.SendRequests([commentsPageRequest], responsePluckContentCallback);
			}

			function responsePluckContentCallback(responses) {
				//safely dump the response to Firebug
				var totalItems = isNaN(responses[0].TotalItems) ? 0 : responses[0].TotalItems;
				jQuery("#pluckCommentCount").text(totalItems);
			}

			//pluckContentID set in main.xsl
			getPluckContent(pluckContentID);
		</script>
