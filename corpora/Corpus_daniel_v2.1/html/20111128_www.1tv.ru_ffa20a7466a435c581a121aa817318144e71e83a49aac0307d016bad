                VK.Widgets.NewRecommended = function(objId, options) {
                    VK.Widgets.Recommended(objId, options);
                    return this.count;
                }
 
                // call creating widget
                var widget_id = VK.Widgets.NewRecommended("vk_recommend", {
                    period: 'week',
                    limit: 6,
                    max: 6,
                    verb: 1,
                    width: "280",
                    height: "300"
                });
 
