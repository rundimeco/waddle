    <div class=""><div class="g-cl_m"></div><a href="http://ivona.bigmir.net/showbiz/viva"><img src="http://bm.img.com.ua/widget/fac517bda34412b03e616c7a4e7399e1.jpg" alt="http://ivona.bigmir.net/showbiz/viva"></a></div>
            
    
    
    
    
    
    
	
    

    <div class="g-cl_m"></div> 
	



<script type="text/javascript">

/**
* inspired by Christopher Manciero
* http://www.chrismanciero.com/
*
*
*
*/

var timerlen = 5;
var slideAniLen = 300;
var pluginDivsHeight = {};

// plugin v2 class
function PluginV2 (id, num) {
	this.id = id;
	this.num = num;
	this.parent_id = 'parent_' + id;
}

PluginV2.prototype = {
	GA_account: 'UA-3119939-34',
	num: 0, // real number of the plugin
	id: '',	// id of the div
	parent_id: '',	// id of the parent div
	moving: false,	// true if it's already moving
	dir: null,	// direction of moving
	endLength: 0,	// length of opened div
	startTime: 0,	// start time
	timerID: 0, // timer id
	obj: null, // div to slide
	initialized: false, 	// whether plugins were initialized

	trackLink: function( domain, pluginName ){
		
		var pageTracker=_gat._getTracker(this.GA_account);    
		pageTracker._trackEvent('баян', domain, pluginName );
		return true;
	},
	
	// openes plugin
	open: function() {
		if (this.moving)
			return;

		if (document.getElementById(this.id).style.display != "none")
			return;

		this.addClass(this.parent_id, 'gb-informers_project_open');

		this.moving = true;
		this.dir = 'down';
		this.startSlide();
	},

	// closes plugin
	close: function() {
		if (this.moving)
			return;

		if (document.getElementById(this.id).style.display == "none")
			return;

		this.removeClass(this.parent_id, 'gb-informers_project_open');
		document.getElementById(this.id).style.overflow = 'hidden';

		this.moving = true;
		this.dir = 'up';
		this.startSlide();
	},

	// toggles plugin
	toggle: function() {
		if (!this.initialized) {
			return;
		}

		if (document.getElementById(this.id).style.display == "none") {
			// div is hidden, so let's open it
			this.open();
		}
		else {
			// div is visible, so close it
			this.close();
		}
	},

	isOpened: function() {
		return document.getElementById(this.id).style.display != "none";
	},

	// starts plugin slide
	startSlide: function() {
		this.obj = document.getElementById(this.id);
		this.endLength = pluginDivsHeight[this.obj.id];
		this.startTime = (new Date()).getTime();

		if (this.dir == "down")
			this.obj.style.height = "1px";

		this.obj.style.display = "block";

		var self = this;	// closure

		this.timerID = setInterval(function(){self.slideTick()}, timerlen);
	},

	/**
	*  	checks if it's time to end the slide or to keep sliding
	*/
	slideTick: function() {
		var elapsed = (new Date()).getTime() - this.startTime;

		if (elapsed > slideAniLen)
			this.endSlide();
		else {
			var d = Math.round(elapsed / slideAniLen * this.endLength);
			if(this.dir == "up")
				d = this.endLength - d;

			obj = this.obj;
			obj.style.height = d + "px";
		}

		return;
	},

	/*
   	*	ends sliding
   	*/
	endSlide: function () {
		clearInterval(this.timerID);

		if (this.dir == "up")
			this.obj.style.display = "none";

		this.obj.style.height = this.endLength + "px";

		delete(this.moving);
		delete(this.timerID);
		delete(this.startTime);
		delete(this.endLength);
		delete(this.obj);
		delete(this.dir);

		return;
	},

	// some utils

	hasClass: function(id, className) {
		var obj = document.getElementById(id);
		return obj.className.indexOf(className) != -1;
	},

	addClass: function(id, className) {
		var obj = document.getElementById(id);
		if (!this.hasClass(id, className)) obj.className += " " + className;
	},

	removeClass: function(id, className) {
		var obj = document.getElementById(id);
		if (this.hasClass(id, className)) {
			var st = obj.className;
    		obj.className = st.replace(' gb-informers_project_open', '');
		}
	},

	getElementCurrentStyle: function(element, CssStyleProperty) {
		if (!element) return false;
		if (element.currentStyle) {
			return element.currentStyle[this.cssToJavascriptStyle(CssStyleProperty)];
		}
		else if (window.getComputedStyle) {
			var compStyle = window.getComputedStyle(element, '');
			return compStyle.getPropertyValue(CssStyleProperty);
		}
		else {
			return '';
		}
	},

	cssToJavascriptStyle: function(CssProperty) {
		var stringArray = CssProperty.toLowerCase().split('-');
		if (stringArray.length == 1) {
			return stringArray[0];
		}
		var ret = (CssProperty.indexOf("-") == 0) ? stringArray[0].charAt(0).toUpperCase() + stringArray[0].substring(1) : stringArray[0];
		for (var i = 1; i < stringArray.length; i++) {
			var s = stringArray[i];
			ret += s.charAt(0).toUpperCase() + s.substring(1);
		}
		return ret;
	},

	addGoogleAnalytics: function () {

//		var pluginDiv = document.getElementById(this.parent_id);
//		var children = pluginDiv.getElementsByTagName('a');
//		var parameters = pluginDiv.getAttribute('rel');
//
//		for (var i = 0; i < children.length; i++) {
//			var tagName = children[i].tagName.toLowerCase();
//			children[i].href = children[i].href + parameters;
//		}
		
	},

	init: function() {
    
		var checked = false;
		if (pluginTicksCounter++ > 15) {
			checked = true;
		}

		var rootDiv = document.getElementById('gb_informers');
		var pluginDivs = rootDiv.getElementsByTagName("div");
		var photoChecked = true;

		if (document.getElementById('gb-informers_photo_box') != null) {
			photoChecked = false;
		}

		for (var i = 0; i < pluginDivs.length; i++) {

			// check whether the style is applied already
			if (!checked && pluginDivs[i].className == 'gb-informers_project_content') {

				if (!photoChecked) {
					//gel all child divs
					var childDivs = pluginDivs[i].getElementsByTagName("div");
					for (var j = 0; j < childDivs.length; j++) {
						if (typeof(childDivs[j].className) != 'undefined') {
							if (childDivs[j].className == 'gb-informers_photo') {

								if (pluginPhotoLoadCounter > 3) {
									photoChecked = true;
								}
								else {
									return;
								}
							}
						}
					}

					if (!photoChecked) {
						continue;
					}
				}

				if (((this.getElementCurrentStyle(pluginDivs[i], 'width') == '300px') && photoChecked)) {
					checked = true;
				}
				else {
					return;
				}
			}
		}
        
		if (checked) {
			for (var i = 0; i < pluginDivs.length; i++) {
				if (pluginDivs[i].className == 'gb-informers_project_content_box') {

					if (pluginDivs[i].style.display == 'none') {
						pluginDivs[i].style.display = 'block';
						pluginDivsHeight[pluginDivs[i].id] = pluginDivs[i].offsetHeight;
						pluginDivs[i].style.display = 'none';
					}
					else {
						pluginDivsHeight[pluginDivs[i].id] = pluginDivs[i].offsetHeight;
					}

					var id = pluginDivs[i].id;
					var key = 'plugin_v2_';

					var num = id.substring(id.indexOf(key) + key.length)
					PluginsV2[num] = new PluginV2(id, num);
					PluginsV2[num].addGoogleAnalytics();
				}

				this.initialized = true;
                togglePluginFirst();
				clearInterval(pluginTimerID);
			}
		}
       
	},

	incPhotoLoadCounter: function() {
		pluginPhotoLoadCounter++;
	}
}

var PluginsV2 = new Array();
var pluginTicksCounter = 0;
var pluginPhotoLoadCounter = 0;
var pluginTimerID = setInterval('PluginV2.prototype.init()', 100);

function togglePluginFirst() {
    var firstBlock = 1;
    
    
                                                firstBlock = 3; // re-init firstBlock
                        
    
    for (i in PluginsV2) {
		if (typeof PluginsV2[i] == 'object' && typeof PluginsV2[i].num != 'undefined') {
            if (i!=firstBlock) {
                PluginsV2[i].close();
            }
		}
	}
}

function togglePlugin(id) {
    for (i in PluginsV2) {
		if (typeof PluginsV2[i] == 'object' && typeof PluginsV2[i].num != 'undefined' && PluginsV2[i].isOpened()) {
			PluginsV2[i].close();
		}
	}
	var Plugin = PluginsV2[id];
	Plugin.toggle();
	return false;
}

//Analytics 
var _gaq = _gaq || [];
_gaq.push(['_setAccount', PluginV2.prototype.GA_account]);
_gaq.push(['_trackPageview']);

(function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();
</script>



<!-- gb-informers -->
<div class="gb-informers " id="gb_informers">

	    
													<div id="parent_plugin_v2_1" class="_mport gb-informers_project gb_inf_pr_first" >
        	<a href="http://mport.bigmir.net" class="gb-informers_project_name " onclick="togglePlugin('1'); return false">
