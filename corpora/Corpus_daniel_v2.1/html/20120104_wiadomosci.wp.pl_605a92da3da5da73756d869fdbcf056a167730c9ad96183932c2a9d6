		</script>
				<script>
			window.WPsocial.config('GPLUS', {
				parseTags : 'onload',
				gplusLang : 'pl'
			});
		</script>
				<script>
			window.WPsocial.settings.set({version : '1.0.0',baseLink : '//a.wpimg.pl/a/i/wpsocialadmin/' });
			var i, c;
			c = _wpSocialQueue.p.length;for ( i =0; i < c; i++) { 
				if (window._wpSocialQueue.p[i].useBaseLink) {
					window._wpSocialQueue.p[i].scriptUrl =  WPsocial.settings.getBaseLink(true) + window._wpSocialQueue.p[i].scriptUrl;    
				}    
				window.WPsocial.addPlugin(window._wpSocialQueue.p[i]);
			}
			c = _wpSocialQueue.a.length;
			for (i = 0; i < c; i++) {
				window.WPsocial.loadApplication(window._wpSocialQueue.a[i]);
			}
		</script>
		
</body>
