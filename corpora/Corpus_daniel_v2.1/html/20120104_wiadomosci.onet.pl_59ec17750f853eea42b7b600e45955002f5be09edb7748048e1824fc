function google_ad_request_done(google_ads) {
    
    if (google_ads.length > 0 && typeof google_info !== "undefined" && typeof google_info.feedback_url !== "undefined") {
        var header = document.createElement("div");
        header.className = "bxad_title1";
        var a1 = document.createElement('a');
        a1.href = google_info.feedback_url;
        a1.title = "Reklamy Google"
        a1.innerHTML = a1.title;
        header.appendChild(a1);
        
        var bxadBox = document.getElementById("googleBox");
        bxadBox.appendChild(header);
        
        var adsList = document.createElement("ul");
        adsList.id = "bxad_ads_script_bxad_slot_bxad_slot_0";
        
        var i;
        for (i=0; i<google_ads.length; i++) {
            var ad = google_ads[i];
            var ad_line = document.createElement("li");
            if (i==0) {ad_line.className="li_left";}
            var ad_title = document.createElement("h6");
            var title_link = document.createElement("a");
            title_link.href = ad.url.replace(/amp;/g,"");
            title_link.innerHTML = ad.line1;
            title_link.target = "_blank";
            ad_title.appendChild(title_link);
            ad_line.appendChild(ad_title);
            var ad_body = document.createElement("p");
            ad_body.innerHTML = ad.line2 + " " + ad.line3 + " ";
            var main_ad_link = document.createElement("a");
            main_ad_link.href=ad.url.replace(/amp;/g,"");;
            main_ad_link.target = "_blank";
            main_ad_link.innerHTML = ad.visible_url;
            ad_body.appendChild(main_ad_link);
            ad_line.appendChild(ad_body);
            
            adsList.appendChild(ad_line);
        }
        bxadBox.appendChild(adsList);
        
        
    }

}


function urlencode(str){var hexStr=function(dec){return'%'+dec.toString(16).toUpperCase()};var ret='',unreserved=/[\w.]/;str=(str+'').toString();for(var i=0,dl=str.length;i<dl;i++){var ch=str.charAt(i);if(unreserved.test(ch)){ret+=ch}else{var code=str.charCodeAt(i);if(code===32){ret+='+'}else if(code===45){ret+='%2D'}else if(code<128){ret+=hexStr(code)}else if(code>=128&&code<2048){ret+=hexStr((code>>6)|0xC0);ret+=hexStr((code&0x3F)|0x80)}else if(code>=2048&&code<65536){ret+=hexStr((code>>12)|0xE0);ret+=hexStr(((code>>6)&0x3F)|0x80);ret+=hexStr((code&0x3F)|0x80)}else if(code>=65536){ret+=hexStr((code>>18)|0xF0);ret+=hexStr(((code>>12)&0x3F)|0x80);ret+=hexStr(((code>>6)&0x3F)|0x80);ret+=hexStr((code&0x3F)|0x80)}}}return ret}

</script>

