	<script type="text/javascript">
	storyPlayer = new Object();
	storyPlayer.doPlay = false; //player has already played
	storyPlayer.pos = 'media';
	
	storyPlayer.ready = function() {
		//console.log ('****************** do play set to ' + storyPlayer.doPlay + ' **************************');
		if (!storyPlayer.doPlay) { //initial state (autostart true/false)
			if (videoAutoStart == 'true') {
				$('#rel_image_feature').show();
				$('#main_media_click').show();			
				playStoryVideo('media',15297688,true);
				//console.log ('****************** 1 **************************');
			} else {
				kdp.sendNotification('doStop');
				 $('#main_media_click').show();
				 //console.log ('****************** 2 **************************');
			}
		} else { //already engaged (media & leftrail) 
			kdp.sendNotification('doPlay');
			storyPlayer.doPlay = true;
			//console.log ('****************** 3 **************************');
		}
	}
    
    getObjPos = function(obj){
        var topVal= 0,leftVal= 0;
        while(obj){
            leftVal += obj.offsetLeft;
            topVal += obj.offsetTop;
            obj = obj.offsetParent;
        }
        return {"left":leftVal,"top":topVal};
    }

    positionPlayer = function(obj){

        objPos = $(obj).offset();
        storyCoreObjPos = $('#story_core').offset();
        mediaplayerObjPos = $('#mediaplayerContainer').offset();

        $('#mediaplayerContainer').css('position', 'absolute');
        $('#mediaplayerContainer').css('z-index', '1003');
        $('#mediaplayerContainer').offset({top: objPos.top, left: objPos.left});

        mediaplayerObjPosNew = $('#mediaplayerContainer').offset();
    }
	
    sendPlaylistToPlayer = function(){
    	if (!storyPlayer.doPlay && storyPlayer.pos == 'media') {  //video already loaded
				kdp.sendNotification('doPlay');
				$('#rel_image_feature').show();
				$('#main_media_click').show();
    	} else { // need to change media
        	pc.playVideoByCID({id:currentVideoId});
        }
		storyPlayer.doPlay = true;  
    }
	
	playerLockedState = true;
    playerLocked = function(){
	    return playerLockedState;
    }

	
    playStoryVideo = function(id,videoId,repos){
    	storyPlayer.pos = id;
    	if (id == 'media' && videoAutoStart == 'true') {
    		kdp.setKDPAttribute("omniture", "videoViewEvent", "event1,event16");
    	} else {
    		kdp.setKDPAttribute("omniture", "videoViewEvent", "event1");
    	}
		pc.fwUpdateKeyValues("show",window.playlist["adContext-"+videoId]);
		currentVideoId = videoId;
		//playDelay = (repos)?500:6000;
		//setTimeout("sendPlaylistToPlayer()",playDelay);
		//kdp.sendNotification('doStop');
		sendPlaylistToPlayer();
		if(repos){
			leftRailVideoOpen = (id == 'media')?false:true;
			if(id == 'media'){
				leftRailVideoOpen = false;
				$('#caption_video').html(videoTitle);
			}else{
				$('#caption_video').html(videoPhotoCaption);
			}
			kdp.sendNotification("doStop");
			if(currLeftRailId){
				leftRail.closeMedia(currLeftRailId);
			}
			leftRail.openMedia(id,currentVideoId);

			leftRailContainerObj = document.getElementById(id);
			positionPlayer(leftRailContainerObj);
        	$('#'+id+' > .rel_active > .active_image_closer').css("visibility","visible");
			mainMedia.sponsoredByAdPosition();
			currLeftRailId = id;
		}

		if (leftRailVideoOpen == false) {
			$('#caption_video').html(videoTitle);
		}

    }


    closeLeftRailVideo = function(id){
		leftRail.closeMedia(id);
		leftRailVideoOpen = false;
		videoTitle = titleList[0];
		kdp.sendNotification("doStop");
		$('#mediaplayerContainer').css('top', '-1000px');
		resetAd();
    }

	playNextVideo = function(){
	    playlistStr = playlistIds;
	    if(playlistStr != ''){
	        if(playlistStr.indexOf('~') != -1){
	            playlistArr = playlistStr.split('~');
	            for(i=0;i<playlistArr.length;i++){
	                if(playlistArr[i] == currentVideoId){
	                    if((i+1) < playlistArr.length){
                            nextVideoId = playlistArr[i+1];
                            videoTitle = titleList[i+1];
	                    }else{
	                        nextVideoId = playlistArr[0];
	                        videoTitle = titleList[0];
	                    }
	                    if (leftRailVideoOpen) {
	                    	 playStoryVideo('rel_'+(i+1),nextVideoId,false);
	                    } else {
	                    	 playStoryVideo('media',nextVideoId,false);
	                    }
	                   
	                    break;
	                }else{
	                    //close the player
	                }
	            }
	        }
	    }
	}
   
   adCheck = "";
   
    positionAd = function() {

		//check itremble to display placeholder
		adCheck = setInterval("restrictAd()",500);
    	
    }
    
    restrictAd = function() {
    	var results = $('#_fw_container_300x250slot').html();
    	//console.log(' ------------- ' + results + ' ------------- ');
    	if (results != undefined && results != "") {
    		clearInterval(adCheck);
    		if (results.indexOf('reid=336259') != -1) {
    			//check position of videoplayer to determine placeholder

				if (storyPlayer.pos == 'media') {
					placeholderDiv = '#adCompanionBanner';
				} else {
					placeholderDiv = '#adCompanionBannerLower';
				}
				
				$(placeholderDiv).show();
				adPlaceholder = $(placeholderDiv).offset();
				$('#itremble').offset({top:adPlaceholder.top,left:adPlaceholder.left});
				//console.log('************* show ad ********************');
				
    		} 
    		
    	}
    	//console.log('************* restrict ad ********************');
    	
    }
    
    resetAd = function() {
    	$('#itremble').offset({top:-1000, left:0});
    	$('.adPlaceHolders').hide();
    	$('#_fw_container_300x250slot').html('');
    	clearInterval(adCheck);
    }
    
	$(function(){
		$('#adCompanionBanner').addClass('adPlaceHolders');
		$('#adCompanionBannerLower').addClass('adPlaceHolders');
		resetAd();
	});

	</script>


		
