<script type="text/javascript">
if(jQuery)
{
    $('input.stars').rating({callback: function(value, link){
	var self = this;
	$(self.form).ajaxSubmit({success: function(){
	    $('input.stars').rating('select', value);
	    $('input.stars').rating('readOnly', true);
	}});
    }});
}
else
{
    $(document).ready(function(){
	$('input.stars').rating({callback: function(value, link){
	    var self = this;
	    $(self.form).ajaxSubmit({success: function(){
		$('input.stars').rating('select', value);
		$('input.stars').rating('readOnly', true);
	    }});
	}});
    });
}
</script>
