<script language="javascript">
 
 
 
 
function google_ad_request_done(google_ads)
{
 
    var s = '';
    var i;
 
    /*
    * Verify that there are actually ads to display.
    */
    if (google_ads.length == 0) {
    return;
    }
 
    s+='<table align="left" width="661px" bgcolor="#ffffff" style="border:0px solid #D3D3D3;7;width:100%;text-align:left; " cellspacing=0;  cellpadding="2px"><tr><td>';
 
    /*
    * If an image or flash ad is returned, display that ad.
    * Otherwise, build a string containing all of the ads and
    * then use a document.write() command to print that string.
    */
    if (google_ads[0].type == "flash")
    {
    }
    else if (google_ads[0].type == "image")
    {
    }
     else if (google_ads[0].type == "html")
     {
     }
     else
    {
        if (google_ads.length == 1) {
        /*
        * Partners should adjust text sizes
        * so ads occupy the majority of ad space.
        */
        /*
        s += '<a target="_blank" href=\"' +
        google_info.feedback_url + '\" style="color:#000;">Διαφημίσεις Google</a><a style="text-decoration:none" href="' +
        google_ads[0].url + '" onmouseout="window.status=\'\'" onmouseover="window.status=\'go to ' +
        google_ads[0].visible_url + '\';return true"> <span style="text-decoration:none;font-size:12pt"> <b>' +
        google_ads[0].line1 + '</b><br/></span></a> <span style="color:#000;font-size:12pt">' +
        google_ads[0].line2 + '&nbsp;' +
        google_ads[0].line3 + '<br/></span> <a style="color:#000;font-size:12pt;text-decoration:none" <a target="_blank" href="' +
        google_ads[0].url + '" onmouseout="window.status=\'\'" onmouseover="window.status=\'go to ' +
        google_ads[0].visible_url + '\';return true"><span>' +
        google_ads[0].visible_url + '</span></a>';
        */
        }
        else if (google_ads.length > 1)
        {
 
 
              /*
              * For text ads, append each ad to the string.
              */
 
            for(i = 0; i < google_ads.length; ++i)
            {

                if (i==google_ads.length-1){
                  
                s+= "<div class='hover1'  style='border-bottom:1px solid #ccc;;padding-top:10px;padding-bottom:8px; padding-left:5px; padding-right:5px;'>";
                s+='<a   target="_blank" style="text-decoration:none;font-size:11pt" href="' +
                google_ads[i].url + '" onmouseout="window.status=\'\'" onmouseover="window.status=\'go to ' +
                google_ads[i].visible_url + '\';return true"> <div style="text-decoration:none;font:Georgia, Times New Roman, Times, serif;color:#004A80"> <b>' +
                google_ads[i].line1 + '</b></div></a> <a style="color:#004A80;font:Georgia, Times New Roman, Times, serif;text-decoration:none;font-size:11pt" <a target="_blank" href="' +
                google_ads[i].url + '" onmouseout="window.status=\'\'" onmouseover="window.status=\'go to ' +
                google_ads[i].visible_url + '\';return true"><div style="font-size:10pt;color:#115688;">' +
                google_ads[i].visible_url + '</div></a><div style="color:#404040;font-size:10pt;font:Georgia, Times New Roman, Times, serif;">' +
                google_ads[i].line2 + '&nbsp;' +
                google_ads[i].line3 + '</div></div>';


}else{


                s+= "<div class='hover1' style='border-bottom:1px solid #ccc;padding:10px 0; padding-left:5px !important; padding-right:5px; font:Georgia, Times New Roman, Times, serif;'>";
                s+='<a target="_blank" style="text-decoration:none;font-size:11pt; font:Georgia, Times New Roman, Times, serif;" href="' +
                google_ads[i].url + '" onmouseout="window.status=\'\'" onmouseover="window.status=\'go to ' +
                google_ads[i].visible_url + '\';return true"> <div style="text-decoration:none;color:#004A80;font:Georgia, Times New Roman, Times, serif;"> <b>' +
                google_ads[i].line1 + '</b></div></a> <a style="color:#004A80;text-decoration:none;font-size:11pt" <a target="_blank" href="' +
                google_ads[i].url + '" onmouseout="window.status=\'\'" onmouseover="window.status=\'go to ' +
                google_ads[i].visible_url + '\';return true"><div style="font-size:10pt;color:#115688;">' +
                google_ads[i].visible_url + '</div></a><div style="color:#404040;font-size:10pt;font:Georgia, Times New Roman, Times, serif;">' +
                google_ads[i].line2 + '&nbsp;' +
                google_ads[i].line3 + '</div></div>';
