<script>
(function(_d) {
  var s = location.hash, reg, ctn, tmp, v;
  //example:
  //s = 'highlight=沪深300';
  s = s.match(/highlight=([\u2E80-\uFE4Fa-zA-Z0-9]+)/);
  if (s && s[1]) {
    reg = new RegExp(s[1], 'g');v = '\u200b\u200b'+ s[1] +'\u200b\u200b';ctn = document.getElementById('Cnt-Main-Article-QQ');tmp = ctn.firstChild;while (tmp) {if (tmp.nodeType == 3) {tmp.nodeValue = tmp.nodeValue.replace(reg, v);}if (tmp.firstChild) {tmp = tmp.firstChild;} else if (tmp.nextSibling) {tmp = tmp.nextSibling;} else {do {tmp = tmp.parentNode;} while (tmp !== ctn && !tmp.nextSibling);if (tmp === ctn) {break;} else {tmp = tmp.nextSibling;}}}reg = new RegExp(v, 'g');s = '<span style="background:yellow;">'+ s[1] +'</span>';ctn.innerHTML = ctn.innerHTML.replace(reg, s);
  //reset the doc title, possible conflict with ad swf 
  tmp = document.title.match(/^([^#]+)#highlight=/); if (tmp) {document.title = tmp[1];}} 
}(this));
</script>
