var puffLinks = $('*[type=puff] a');
//Add click event
puffLinks.click(function(){
	//get object id
	var parentEl = $(this);
	var objectId = "";
	var containerName = "";
	
	//We want to find the content object on the page.
	do {
		parentEl = parentEl.parent();
	} while (parentEl && 
			!parentEl.attr('tagName').toLowerCase().match(/(tm:)?contentobject/));

	if (!parentEl) {
		tm.logger.error(['Puff Tracking:', 'No parent tm:contentobject tag'], OMNI_DEBUG_LEVEL);
		return !tm.logger.enabled; //if debug mode don't follow link
	} else {
		objectId = parentEl.attr('objectid');
	}
	
	// So we now have the content object we are looking for - we continue on up the tree
	// to get the name of the container holding it.
	do {
		parentEl = parentEl.parent();
	} while (parentEl && 
			!parentEl.attr('tagName').toLowerCase().match(/(tm:)?container/) );

	if (!parentEl) {
		tm.logger.error(['Puff Tracking:', 'no parent tm:container tag'], OMNI_DEBUG_LEVEL);
		return !tm.logger.enabled; //if debug mode don't follow link
	} else {
		containerName = parentEl.attr('name');
	}

	tm.logger.debug(['Puff Tracking:', 'objectId:', objectId, 
			'containerName:', containerName, parentEl ], OMNI_DEBUG_LEVEL);

	s.eVar13 = s.pageName + '-' + containerName + '-' + objectId;
	s.events = 'event17';
	s.t();
	
	tm.logger.info(['Puff Tracking:', 'Omniture event sent' ], OMNI_DEBUG_LEVEL);

	return tm.logger.debugLevel != OMNI_DEBUG_LEVEL; //if debug mode don't follow link
});
//Log to console if required.
tm.logger.debug( [ 'Puff Tracking: Links', puffLinks ], OMNI_DEBUG_LEVEL );




//--></script>
