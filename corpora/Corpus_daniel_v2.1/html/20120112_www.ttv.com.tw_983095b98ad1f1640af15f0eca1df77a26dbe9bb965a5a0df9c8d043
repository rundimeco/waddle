function fblike() {
	$.ajax({
		type: "GET",
	    url: "/news/fblikecount.asp",
		data: {type:"news", i:"10101119061712I", newsid:"{0AFE1FC7-E2BC-4470-8EE8-6334BB7736F3}"},
	    cache: false,
		error: function(xhr) {
//			alert('FB Like Request Error!!');
		},
		success: function(response) {
//			alert('FB Like Request Success!!');
		}
	});
	
//    var ttvnewspage = document.getElementById("ttvnewspage");
//    ttvnewspage.style.display = "block";
}

