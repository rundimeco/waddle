			<script language="JavaScript" type="text/javascript">

userPref = new Object();
userPref.getAutoStart = function(){
    asCookie = readCookie("upautostart");
    asCookieVal = (asCookie)?asCookie:'false';
    asCookieVal = (asCookieVal.charAt(0) == 't')?'true':'false';
    return asCookieVal;
}

userPref.setAutoStart = function(state){
    stateStr = state+'';
    createCookie("upautostart", stateStr,1000);
    userPref.renderAutoStartControls();
}

userPref.renderAutoStartControls = function(){
    ascObj = document.getElementById('autostartcontrols');
    if(ascObj){
        ascntrlStr = (userPref.getAutoStart() == 'false')?'Auto Start: <a href="#" onclick="userPref.setAutoStart(true);return false;">On</a> <span> | </span> Off':'Auto Start: On <span> | </span> <a href="#" onclick="userPref.setAutoStart(false);return false;">Off</a>';
        ascObj.innerHTML = ascntrlStr;
    }
}

