spamReasonInvalidInfo: 'Podaj powód',
spamTextInvalidInfo: 'Podaj powód',
userIsLogged:Inpl.User.isLoggedIn()||true           });

jQuery('.list .functions .spam a').not('[rel="no-js"]').click(function() {
if(jQuery.find('.postSpamForm').length == 0){
jQuery('.list .functions .spam a').not('[rel="no-js"]').unbind('click');
var el = jQuery(this);
jQuery.get('/getSpamForm', function(data){
jQuery("#_mw_postSpamForm").html(data);
var imageCodeImg= new Inpl.Captcha.Image("imageCodeImg-id","imageCodeImgRefresh","imageCodeImgImg","imageCodeImg-input");
var boxForm = jQuery.fn.boxForum();
boxForm.render();
el.trigger('click');

});
}
return false;
});
});
});
