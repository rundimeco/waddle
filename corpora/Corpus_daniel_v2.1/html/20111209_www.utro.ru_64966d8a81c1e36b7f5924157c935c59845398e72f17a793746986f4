<script type="text/javascript">
<!--
/**
 * @description
 */
SocialInvite = (function() {
    var _beginPopupDate = '01 Mar 2013 00:00'   // С какого числа начинать отображение приглашения
        , _countPopupDays = 1000                // сколько дней показывать приглашение
        , _inviteFB = false
        , _inviteVK = false
        , _loadFBSDK = false

        , options = {
            DAYS_WHEN_SHOW   : [ 0, 1, 2, 3, 4, 5, 6 ]    // Saturdays, Sundays
          , SHOW_FROM_HOUR   : 3           // including
          , SHOW_TO_HOUR     : 2           // TOMORROW, not including this hour
          , ARTICLES_TO_READ : 0
          , todayDate        : new Date()  // updateToday() for testing purposes
        };


    function init( passedOptions ) {
        loadOptions( passedOptions );
        updateArticlesCounter();
        tryFB();
    };

    function loadOptions( passedOptions ) {
        if ( passedOptions ) {
            options = $.extend( true, options, passedOptions );
        }
    }


    function tryFB(){
        requirejs.config({
            baseUrl: 'http://connect.facebook.net/ru_RU/',
            waitSeconds: 15
        });
        require(['all'], function ($) {
            tryVK( true );
            loadFacebookSdk();
            setTimeout( tryloadFBSDK, 1000 );
        }, function (err) {
            if ( err.requireType == 'nodefine'){
                loadFacebookSdk();
                setTimeout( tryloadFBSDK, 1000 );
                tryVK( true );
            } else{
                tryVK( false );
            }
        });
    }

    function tryVK( loadFBflag ){
        requirejs.config({
            baseUrl: 'http://vk.com/js/api/',
            waitSeconds: 15
        });

        require(['openapi'], function ($) {
            loadVkontakteOpenApi();
            loadPopupWindow( loadFBflag, true );
        }, function ( err) {
            if ( err.requireType == 'nodefine'){
                loadVkontakteOpenApi();
                loadPopupWindow( loadFBflag, true );
            } else{
                loadPopupWindow( loadFBflag, false );
            }
        });
    }

    function tryloadFBSDK(){
        var counter = 0;        
        if ( (_loadFBSDK == false) && ( counter < 3) ){
            counter = counter + 1;
           /* FB.init({
              appId      : '157031727688210', // App ID
              status     : true, // check login status
              cookie     : true, // enable cookies to allow the server to access the session
              xfbml      : true // parse XFBML
            });*/

            _loadFBSDK = true;
            $('.fb-like-title').show();
            $('.fb-like-box').show();
            $('.win_subscribe_soc-block_fb .soc-block_hint').show();
           
            setTimeout( tryloadFBSDK, 1000 );
        };        
    }

    function loadFacebookSdk() {
        window.fbAsyncInit = function() {
            FB.init({
              appId      : '157031727688210', // App ID
              status     : true, // check login status
              cookie     : true, // enable cookies to allow the server to access the session
              xfbml      : true // parse XFBML
            });

            _loadFBSDK = true;
            $('.fb-like-title').show();
            $('.fb-like-box').show();
            $('.win_subscribe_soc-block_fb .soc-block_hint').show();
        };
    };

    function loadVkontakteOpenApi( ) {
    	var mode = 1;
    	var $body = $('body');

    	if($body.hasClass('main_page')||$body.hasClass('article_list_page')||$body.hasClass('article_item_page')){
    		mode = 0;
    	}
    	window.vkAsyncInit = function() {
    		VK.init({
    			apiId: 27131839
              , onlyWidgets: true
    		});
    		$('#vk_widget').show();

            VK.Widgets.Group("vk_like_mine", {mode: 1, width: "245", height: "110"}, 27131839);
            $('#vk_like_mine').show();
            $('.win_subscribe_soc-block_vk .soc-block_hint').show();

    	};
		window.vkAsyncInit();
    }

    function loadPopupWindow( loadFBflag, loadVKflag ) {
//            document.write('0');
        if ( shouldShowWindow() ) {
//            document.write('1');
            $("#popup_box").css("height", $(document).height());
            $("#popup_inner").css("height", $(window).height());

            if (( loadFBflag === true ) && ( loadVKflag === true )) {
                $("#popup_box").fadeIn();
            } else if ( (( loadFBflag !== true ) && ( loadVKflag === true )) || (( loadFBflag === true ) && ( loadVKflag !== true )) ) {
                $('.win_subscribe').addClass('win_one');
                $("#popup_box").fadeIn();
            }

            $(".win_subscribe_close").click( function(){
                $("#popup_box").fadeOut();
                $.cookie("socialInvite", "true", { path: '/', expires: 31 });
                return false;
            });

            $(".win_subscribe_footer").click( function(){
                $("#popup_box").fadeOut();
                 $.cookie("socialInvite", "true", { path: '/', expires: 31 });
                return false;
            });

            $(window).bind("resize", function(){
                $("#popup_box").css("height", $(document).height());
                $("#popup_inner").css("height", $(window).height());
            });
//            document.write('2');
        }
    }

    /**
     * @return {Boolean}
     */
    function shouldShowWindow() {
        var shouldShowWindow = false,
            socialCookie = $.cookie( 'socialInvite' );

        if (
            socialCookie !== 'true' &&
            (
                isRightDay() || ( isRightTime() && didReadALot() )
            )
        ) {
            shouldShowWindow = true;
        }
        
//        shouldShowWindow = true; //показывать всегда
        
        return shouldShowWindow;
    }


    function updateArticlesCounter() {
        // work with the counter only when the time is right
        // reset the counter otherwise (rise up and shine, a new day has come!)
        if ( isRightTime() ) {
            var articlesRead = $.cookie( 'articlesRead' ) / 1;

            // check for valid data
            if ( typeof articlesRead !== 'number' || isNaN( articlesRead ) ) {
                articlesRead = 0;
            }

            $.cookie( 'articlesRead', articlesRead+1, { path: '/', expires: 31 } );
        } else {
            $.cookie( 'articlesRead', 0, { path: '/', expires: 31 } );
        }
    }


    function isRightDay() {
        var currentDay = options.todayDate.getDay()
          , rightDay = false;

        if ( $.inArray( currentDay, options.DAYS_WHEN_SHOW ) !== -1 ) {
            rightDay = true;
        }

        return rightDay;
    }

    function isRightTime() {
        var fromDate = getDate( 'fromDate' ),
            toDate   = getDate( 'toDate' );   // TOMORROW

	// is current time between 'from' and 'to'?
        return ( fromDate <= options.todayDate && options.todayDate < toDate );
    }

    function getShift() {
        var hour  = options.todayDate.getHours();

        if ( hour >= 0 && hour < options.SHOW_TO_HOUR ) {
           return 'yesterday';
        } else {
           return 'today;'
        }
    }

    function getDate( type ) {
        var year  = options.todayDate.getFullYear(),
            month = options.todayDate.getMonth(),
            shift = getShift();

        switch( type ) {
            case 'fromDate':
                var day = ( shift === 'yesterday' ? options.todayDate.getDate() - 1 : options.todayDate.getDate() );
	        return new Date( year, month, day, options.SHOW_FROM_HOUR );
                break;
            case 'toDate':
                var day = ( shift === 'yesterday' ? options.todayDate.getDate() : options.todayDate.getDate() + 1 );
                return new Date( year, month, day, options.SHOW_TO_HOUR ); // TOMORROW
                break;
        }
    }

    function didReadALot() {
        var articlesRead = $.cookie( 'articlesRead' ) / 1;

        return articlesRead > options.ARTICLES_TO_READ;
    }


    return {
        init: init
    };
})();

$(document).ready(function() {
  if (navigator.userAgent.match(/(Opera.Mini|android|iphone)/i)==null) SocialInvite.init();
});

//-->
</script>

