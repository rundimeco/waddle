  <div class="article_text"><script type="text/javascript">
                    $(document).ready(function(){ 
    var midiaGallery = new ImageMediaGallery('div#main-article-gallery[jKey=onarticleImages]',{onArticle : true , visibleItems : 4, onArticleMediaImages: true , articleId :432509 });
