function CaptchaSendComment3134() {
    document.getElementById('animationDiv23134').style.display = 'block';
    document.getElementById('buttonSendDiv23134').style.display = 'none';
    var caTxt = document.getElementById('txtCA2_3134').value;
    var caCd = document.getElementById('hidCA2_3134').value;

    var caSubject = document.getElementById('ctl00_ctl00_cpAB_desktopRR_ctl08_ctl03_txtSubject').value;
    var caEmail = document.getElementById('ctl00_ctl00_cpAB_desktopRR_ctl08_ctl03_txtEmlFrom').value;
    var caEmailToCntrl=document.getElementById('ctl00_ctl00_cpAB_desktopRR_ctl08_ctl03_ddlEmlTo');
    
    var caEmailTo='';
    if(caEmailToCntrl==null)
        caEmailTo='chinese@voanews.com';
    else
        caEmailTo=caEmailToCntrl.value;

    var caComment = document.getElementById('ctl00_ctl00_cpAB_desktopRR_ctl08_ctl03_txtBody').value;
    if(typeof www != 'undefined' && typeof www.rferl != 'undefined' && typeof www.rferl.org != 'undefined' && typeof www.rferl.org.Services != 'undefined' && typeof www.rferl.org.Services.CaptchaService != 'undefined' && typeof www.rferl.org.Services.CaptchaService.CaptchaContactUsSendComment != 'undefined')//For CMS
        www.rferl.org.Services.CaptchaService.CaptchaContactUsSendComment('3134',caSubject, caEmail, caEmailTo, caComment,caTxt,caCd,CaptchaContactUsSendCallback3134);
}
function GetCaptchaContactUsBody3134() {
    if (true)
    {
        var panel = document.getElementById('captchaDiv23134');
        panel.innerHTML = '<input type="hidden" value="disabled" id="txtCA2_3134"/><input type="hidden" value="disabled" id="hidCA2_3134" />';
    }
    else if(typeof www != 'undefined' && typeof www.rferl != 'undefined' && typeof www.rferl.org != 'undefined' && typeof www.rferl.org.Services != 'undefined' && typeof www.rferl.org.Services.CaptchaService != 'undefined' && typeof www.rferl.org.Services.CaptchaService.GetCaptchaBody != 'undefined')//For CMS
        www.rferl.org.Services.CaptchaService.GetCaptchaBody(2,3134,CaptchaContactUsCallback3134,ErrorCallbackContact);
} 
function CaptchaContactUsCallback3134(result){
    if(result && result!='')
    {
        var panel = document.getElementById('captchaDiv23134');
        panel.innerHTML =result;
    }
}
function CaptchaContactUsSendCallback3134(result){
    document.getElementById('animationDiv23134').style.display = 'none';
    document.getElementById('buttonSendDiv23134').style.display = 'block';
    
    var sendButtonOffs = $('#buttonSendDiv23134').offset();
    var positionPanel = function (oBox) {
        oBox.css("top", sendButtonOffs.top).css("left", sendButtonOffs.left - oBox.outerWidth(true));
    }

    if(result && result!='')
        positionPanel(smallMessageBox({html : result}));
    else
    {
        positionPanel(smallMessageBox({html : '您的邮件发送成功！'}));
        document.getElementById('ctl00_ctl00_cpAB_desktopRR_ctl08_ctl03_txtBody').value='';
    }
    GetCaptchaContactUsBody3134();
}
function ErrorCallbackContact(error)
{
}
