      <script>
        (function () {
          var resizeTimeout;
          function pokazUkryjModul() {
            var pokazOd = 480,
                idModul = 182,
                szerokoscOkna = jQuery(window).width(),
                modul = jQuery('#modul_asynchroniczny_' + idModul);
            if (szerokoscOkna >= pokazOd && !modul.is(":visible")) {
              modul.show();
            } else if (szerokoscOkna < pokazOd && modul.is(":visible")) {
              modul.hide();
            }
          }
          pokazUkryjModul();
          jQuery(window).resize(function () {
            clearTimeout(resizeTimeout);
            resizeTimeout = setTimeout(function () {
              pokazUkryjModul();
            }, 200);
          });
        })();
      </script>
    
