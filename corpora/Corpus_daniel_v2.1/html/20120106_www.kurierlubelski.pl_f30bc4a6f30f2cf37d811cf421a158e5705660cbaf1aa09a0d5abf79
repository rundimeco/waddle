      (function() {
        var strona = 2,
        ilosc_sond = 2,
        poprzednia = jQuery('#sonda-poprzednia'),
        nastepna = jQuery('#sonda-nastepna'),
        dane_kontroler = {
          katalog : "/nauka/",
          wartosc : "",
          rozszerzenie : "html",
        },
        id_modul = 28;
        
      
      dane_kontroler.strona = (strona - 1 > 0) ? strona - 1 : ilosc_sond;
      poprzednia.click(function() {
        jQuery.get('/ajax/laduj_asynchronicznie/'+id_modul+'.html', dane_kontroler, function(data) {
            jQuery('#sonda-modul').replaceWith(data);
        });
        return false;
      });
      dane_kontroler.strona = (strona + 1 > ilosc_sond) ? 1 : strona + 1;
      nastepna.click(function() {
        jQuery.get('/ajax/laduj_asynchronicznie/'+id_modul+'.html', dane_kontroler, function(data) {
            jQuery('#sonda-modul').replaceWith(data);
        });
        return false;
      });
      })();
      
    </script>
