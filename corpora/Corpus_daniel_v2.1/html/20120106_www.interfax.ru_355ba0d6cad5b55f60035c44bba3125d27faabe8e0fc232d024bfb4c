var vk_init_ok = false;function ShowSNBlock(iBlockId) {
	var o;
	var o2;
	for (var i = 0; i < 3; i++) {
		o = document.getElementById("sn_" + i);
		o2 = document.getElementById("btn_sn_" + i);
		if (o) {
			if (i == iBlockId) {
				if (o2) o2.className = "socnet_act";
				o.style.display = "block";
				if (i == 2 && (!vk_init_ok)) { vk_ifx(); vk_init_ok = true; }
				} else { 
				if (o2) o2.className = "socnet_nonact";
				o.style.display = "none";
				}
			}
		}
	return false;
}
ShowSNBlock(1);
