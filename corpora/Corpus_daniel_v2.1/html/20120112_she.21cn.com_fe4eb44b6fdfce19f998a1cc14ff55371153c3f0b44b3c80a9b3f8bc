//RenderPicList
function RenderPicList(dataId){
    var count = 15;
    var span = 5;
    var url = 'http://picture.21cn.com/script/' + dataId + ',0,' + count + ',2.js?func=jQuery&jsoncallback=?';
    var tmpHtml = '';
    $.getJSON(url, function(data){
        var ds = data.resultList.list;
        tmpHtml = '<div>';
        
        $.each(ds, function(i){
            if (i !== 0 && i % span === 0) {
                tmpHtml = tmpHtml + '</div><div>';
            }
			var ds_url = 'http://picture.et.21cn.com/slide/'+ds[i].channelId+','+ds[i].folderId+'.shtml'
            tmpHtml = tmpHtml + '<a href="'+ds_url+'" target="_blank" ><img src="' + ds[i].coverUrl + '" alt="" width="100" height="75" /><em>' + ds[i].folderName + '</em></a>';
        });
        tmpHtml = tmpHtml + '</div>';
        $('#picList').html(tmpHtml);
        $(".scrollable2").scrollable({
            circular: true
        }).autoscroll({
            interval: 4000
        }).navigator();
    });    
}
	$(function(){					  
            RenderPicList(106);
			});
            </script>
					</div>
				</div>
</div>
