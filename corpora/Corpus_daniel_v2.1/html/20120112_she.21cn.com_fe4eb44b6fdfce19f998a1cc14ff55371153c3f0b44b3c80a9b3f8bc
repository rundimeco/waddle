<script type="text/javascript">
    $(function(){
    //tooltip
    $("#weibo_list .J_weibo").tooltip({predelay:500,offset: [-8, 0],
        onBeforeShow: function(){
            var tip = this.getTip();
            if (!tip.hasClass('init')) {
                tip.addClass('init');
                var iframe = document.createElement("iframe");
                iframe.src = tip.children().attr('src');
                iframe.width = '250';
                iframe.height = '80';
                iframe.frameBorder = '0';
                iframe.scrolling = 'no';
                iframe.className = 'share_self';
                if (iframe.attachEvent) {
                    iframe.attachEvent("onload", function(){
						$(".loading",tip).hide();
                    });
                }
                else {
                    iframe.onload = function(){
						$(".loading",tip).hide();
                    };
                }                
                tip.prepend(iframe);
            }
        }
    });
	});
    </script> 
