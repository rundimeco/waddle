var selectedTab = window.location.toString().split("#")[1]; // handle preset profile tabs
if (!selectedTab) {
selectedTab = "small lead photo";
}
s.prop37 = selectedTab; // Content Type (active article tab)
}
s.prop15 = document.location.href;
s.prop35 = ""; // wildcard
s.prop36 = "editorial"; // section type
s.prop42 = "news"; // article type or blog section
s.prop43 = "news"; // Sub-section 1
s.prop44 = "news:national"; // Sub-section 2
s.prop45 = "news:national"; // Sub-section 3
