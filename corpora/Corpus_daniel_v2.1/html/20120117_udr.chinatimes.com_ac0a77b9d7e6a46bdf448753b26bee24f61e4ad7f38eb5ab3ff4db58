function replace_kw(){
	var tmp_html = $("body").html();
	tmp_html = tmp_html.replace(/療效/g," ");
	tmp_html = tmp_html.replace(/好康/g," ");
	tmp_html = tmp_html.replace(/優惠/g," ");
	tmp_html = tmp_html.replace(/保證/g," ");
	$("body").html(tmp_html);
};
