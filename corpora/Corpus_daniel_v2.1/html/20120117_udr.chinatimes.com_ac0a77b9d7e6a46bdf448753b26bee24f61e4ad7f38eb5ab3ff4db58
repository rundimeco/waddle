<script type="text/javascript"> 
$(function(){
	$("#question_post_submit").click(function(){
		$.post("aJax/question_action.asp",{"index":"1","id":"1405","cont":$("#question_post_cont").val()},function(data){
			if(data == "0"){
				//更新列表
				$("#question_post_form").hide("slow");
				$("#question_post_cont").val("");
				question_post_list(1);
			};
		});
	});
	question_post_list(1);
});

function question_post_new(){
	
	alert("請先登入會員，方可進行回應！");
	
};
function question_post_list(i){
	var sourceHtml = "" , tempHtml = "" , xHtml = "" , page = 1 ;
	sourceHtml += '<div class="news2_view_response_area">';
	sourceHtml += '	<div class="news2_view_response_bg"><!--content--></div>';
	sourceHtml += '	<div class="news2_view_response_title">留言：</div>';
	sourceHtml += '	<div class="news2_view_response_content"><!--account-->│<!--post_time--></div>';
	sourceHtml += '</div>';
	$.getJSON("aJax/question_action.asp",{"index":"2","id":"1405","p":i},function(data){
		if (data.rsc > 0){
			$("#question_post_count").html(data.rsc+" 篇回響");
			if(data.total_page > data.page){
				page = i + 1;
				$("#questoin_post_page").html('<a href="javascript:question_post_list('+page+');">下一頁</a>');
			}else{
				$("#questoin_post_page").html('&nbsp;');
			};
			$.each(data.item_list, function(ii,items){
				tempHtml = sourceHtml ;
				tempHtml = tempHtml.replace(/<!--content-->/g,items.content) ;
				tempHtml = tempHtml.replace(/<!--account-->/g,items.account) ;
				tempHtml = tempHtml.replace(/<!--post_time-->/g,items.post_time) ;
				xHtml += tempHtml ;
			});
			if(i == 1){
				$("#question_post_list").html(xHtml);
			}else{
				$("#question_post_list:last").append(xHtml);
			};
		};
	});
};
</script>
