function clickDay(ID)
{
for (i = 0; i< 5; i++)
{
document.getElementById("day" + i).style.backgroundColor = "#dcdcdc";

}
document.getElementById("day" + ID).style.backgroundColor = "#a9a9a9";
document.getElementById("currentData").innerHTML = "<table>" + document.getElementById("whatson" + "CJOH" + ID).innerHTML + "</table>";

currentID = ID;

document.getElementById("pictureSpot").innerHTML = document.getElementById("mappingData" + ID).innerHTML;
}
</script>
