<SCRIPT type="text/javascript" language="Javascript">
<!--
//
// class metoxes_moy
//
function Metoxes_moy(name,parent){
  this.name = name;
  this.parent = parent;
  this.inner = make_name(this.name)+"_inner";
  this.inner2 = make_name(this.name)+"_inner2";

  this.inner_popup4 = make_name(this.name)+"_inner_popup4";
  this.inner_popup3 = make_name(this.name)+"_inner_popup3";
  this.inner_popup2 = make_name(this.name)+"_inner_popup2";
  this.inner_popup = make_name(this.name)+"_inner_popup";
  this.inner_help2 = make_name(this.name)+"_inner_help2";
  this.inner_help = make_name(this.name)+"_inner_help";
  this.listenerAttached = false;
  
  this.getting = false;
  this.is_shown = false;
  this.is_opened = false;
//methods
  this.init = metoxes_moy_init;
  this.load = metoxes_moy_load;
  this.cretera_tosend = metoxes_moy_cretera_tosend;
  this.after_get = metoxes_moy_after_get;
  this.get = metoxes_moy_get;
  
  this.show = metoxes_moy_show;
  this.draw = metoxes_moy_draw;
  this.draw_inside = metoxes_moy_draw_inside;
  this.draw_changes = metoxes_moy_draw_changes;
  
  this.show_prosthesi_metoxis = metoxes_moy_show_prosthesi_metoxis;
  this.draw_prosthesi_metoxis = metoxes_moy_draw_prosthesi_metoxis;
  this.hide_prosthesi_metoxis = metoxes_moy_hide_prosthesi_metoxis; 
  this.show_help_prosthesi_metoxis = metoxes_moy_show_help_prosthesi_metoxis;
  this.draw_help_prosthesi_metoxis = metoxes_moy_draw_help_prosthesi_metoxis;
  this.hide_help_prosthesi_metoxis = metoxes_moy_hide_help_prosthesi_metoxis;  
  this.show_afairesi_metoxis = metoxes_moy_show_afairesi_metoxis;
  this.draw_afairesi_metoxis = metoxes_moy_draw_afairesi_metoxis;
  this.hide_afairesi_metoxis = metoxes_moy_hide_afairesi_metoxis;
  this.show_help_afairesi_metoxis = metoxes_moy_show_help_afairesi_metoxis;
  this.draw_help_afairesi_metoxis = metoxes_moy_draw_help_afairesi_metoxis;
  this.hide_help_afairesi_metoxis = metoxes_moy_hide_help_afairesi_metoxis;
  this.rem = metoxes_moy_rem;
//
  this.init();
}

function metoxes_moy_init(){
  this.set = new Array();
  this.count = 0;
  this.set_just_changed = new Array();
  this.last_modified = 0;
  this.last_transaction_id = -1;
  
  if (!this.listenerAttached) {
    this.listenerAttached = true;
    my_objects[my_objects.length] = this;
  }	
  	
}

function metoxes_moy_load(what){
  this.set_just_changed = new Array();
  var length = "";var length2 = "";var metavoli=0.0;
  for(var i in what){ 
    //var length = this.set.length;
    length = what[i]["secsym"];
    metavoli = (typeof(this.set[length])=="object" && this.set[length].price>0.0)?(parseFloat(what[i]["price"])-this.set[length].price):0.0; // se sxesi me proigoymeni timi     
    if(typeof(this.set[length])!="object"){
      this.set[length] = new Object();
    }
    this.set[length].secsym = what[i]["secsym"];
    this.set[length].price = parseFloat(what[i]["price"]);
    this.set[length].diff = parseFloat(what[i]["diff"]);
    this.set[length].price_diff = parseFloat(what[i]["price_diff"]);
    this.set[length].apodosi_etous = parseFloat(what[i]["apodosi_etous"]);
    this.set[length].metavoli = metavoli;
   
    length2 = this.set_just_changed.length;
    this.set_just_changed[length2] = length;
  }  
  delete length; delete length2;
}

function metoxes_moy_cretera_tosend(check_cookie){
  var toret = new Array();
  toret["last_modified"] = this.last_modified;
  toret["last_transaction_id"] = this.last_transaction_id;
  var secsyms_to_send = new Array();
  this.count = 0;
  for(var i in this.set){
    secsyms_to_send[secsyms_to_send.length] = "'"+this.set[i].secsym+"'";
    this.count++;	
  }
 // alert(d(readCookie("my_secsyms")))
  if(this.count>0){
    toret["secsyms"] = e(secsyms_to_send.join(","));
  }
  else if(check_cookie) {
  	this.count++;
  	toret["secsyms"] = e(d(readCookie("my_secsyms")));
  }
  var date = new Date();
  date.setTime(date.getTime()+(10*365*24*60*60*1000));
//  document.cookie="my_secsyms="+e(secsyms_to_send.join(","))+"; expires="+date.toGMTString()+"; path=/";
  document.cookie="my_secsyms="+toret["secsyms"]+"; expires="+date.toGMTString()+"; path=/";
  delete secsyms_to_send;
  delete date;
  return toret;
}
function metoxes_moy_after_get() {//alert("Dummy metoxes_moy_after_get");
}
function metoxes_moy_get(check_cookie){   
	
  var a = new PHP_Serializer();
  var b = a.serialize(this.cretera_tosend(check_cookie));	
  if(!this.getting && this.count>0){
    var what_to_do = new Object();
    what_to_do.callback = do_metoxes_moy_get;
    what_to_do.extra_data = this;
    x_get_metoxes_moy(b,true,what_to_do);
    this.getting = true;
  }
  else {
  	this.is_shown = false;
  	this.is_opened = false;
  	this.show();
  }
  delete a; delete b;
}
function do_metoxes_moy_get(that,aaa){
  aaa.getting = false;
  var b = that;
  if(b["status"]==1){
    aaa.load(b["data"]["set"]);
    aaa.last_modified = (aaa.last_modified<b["data"]["last_modified"])?b["data"]["last_modified"]:aaa.last_modified;
    aaa.last_transaction_id = (aaa.last_transaction_id<b["data"]["last_transaction_id"])?b["data"]["last_transaction_id"]:aaa.last_transaction_id;
    aaa.show();
  }
  else if(b["status"]==0){
  	aaa.last_modified = (aaa.last_modified<b["data"]["last_modified"])?b["data"]["last_modified"]:aaa.last_modified;
    aaa.show();
  }
  aaa.after_get();
  handle_status(b["status"],b["mesg"]); 
  delete b; delete that;
}

function metoxes_moy_show(){

  if(this.is_shown && this.count>0){
  	this.draw_changes();
  }
  else {
    if(innerHTML(this.inner,this.draw())){
      show(this.inner);
    }
    if(this.count>0){  
      if(innerHTML(this.inner2,this.draw_inside())){
        if(this.is_opened){
          show(this.inner2);
        }
        else {
          hide(this.inner2)		
        }
      }	
    }
    else {
      hide(this.inner2)	
    }
  }
} 
function metoxes_moy_draw(){
  var t = "";
  t += "<TABLE border='0' cellpadding='0' cellspacing='0' width='100%'>";
  t += "<TR>";
  t += "  <TD>";
  t += "  <TABLE border='0' cellpadding='0' cellspacing='0' width='100%' >";
  t += "  <TR>";
  t += "    <TD class='bold white bigger' style='padding: 2px'>Οι&nbsp;Μετοχές&nbsp;μου</TD>";
  t += "    <TD><img src='http://cnt4.express.gr/xrimatistirio/images//plus.gif' alt='Πρόσθεση μετοχής' id='"+make_name(this.name)+"_add_metoxi_inner' class='link' onclick='"+this.name+".show_prosthesi_metoxis(this)' onmouseover='"+this.name+".show_help_prosthesi_metoxis(this)' onmouseout='"+this.name+".hide_help_prosthesi_metoxis(this)' /></TD>";
  t += "    <TD><img src='http://cnt4.express.gr/xrimatistirio/images//minus.gif' alt='Αφαίρεση μετοχής' id='"+make_name(this.name)+"_rem_metoxi_inner' class='link' onclick='"+this.name+".show_afairesi_metoxis(this)' onmouseover='"+this.name+".show_help_afairesi_metoxis(this)' onmouseout='"+this.name+".hide_help_afairesi_metoxis(this)' /></TD>";
  t += "    <TD width='50%'><img src='http://cnt4.express.gr/xrimatistirio/images//down_arrow.gif' class='link' border='0' align='right' "+((this.count>0)?"onclick=\""+this.name+".is_opened=!"+this.name+".is_opened;(("+this.name+".is_opened)?show("+this.name+".inner2):hide("+this.name+".inner2))\"":"")+" /></TD>";//"+((this.count>0)?((this.is_opened)?"-":"v"):"")+"
  t += "  </TR>";
  t += "  </TABLE>";  
  t += "  </TD>";  
  t += "</TR>"; 
  t += "</TABLE>";
  return t;
}
function metoxes_moy_draw_inside(){
  var table = "";
  table += "<TABLE border='0' cellpadding='0' cellspacing='0' width='100%'>";
  table += "<TR>";
  table += "  <TD>";
  table += "  <TABLE border='0' cellpadding='0' cellspacing='0' width='100%'>";
  table += "  <TR>";
  table += "    <TD class='small border_grey_right_left_bottom'>&nbsp;</TD>";
  table += "    <TD class='small border_grey_right_bottom' style='padding-left: 4px;'>Κωδικός</TD>";
  table += "    <TD class='small right border_grey_right_bottom'>Τιμή</TD>";
  table += "    <TD class='small right border_grey_right_bottom'>Μετ.</TD>";
  table += "    <TD class='small right border_grey_right_bottom'>Μετ.%</TD>";
  //table += "    <TD class='small right border_grey_right_bottom' style='padding-right: 4px;'>Απόδ. έτους</TD>";
  table += "    <TD class='small right border_grey_right_bottom' style='padding-right: 4px;'>Απόδ. 2012</TD>";
  table += "  </TR>";  
  var i=0;var cl = '';var cl1 = '';var cl2 = '';var cl3 = '';
  for(var secsym in this.set){
    cl1 = "border_grey_right_left_bottom";
    cl2 = "border_grey_right_bottom";
    cl3 = "border_grey_right_bottom";
    cl = (this.set[secsym].price_diff>0.0)?"green":((this.set[secsym].price_diff<0.0)?"red":"blue");
    table += "  <TR>";
    //table += "    <TD class='small "+cl+" "+cl1+"' id='"+make_name(this.name)+"_"+make_name(secsym)+"_metavoli_inner' valign='middle' align='center'><img src='http://cnt4.express.gr/xrimatistirio/images//"+((this.set[secsym].metavoli==0.0)?"blue-square.gif":((this.set[secsym].metavoli>0.0)?"up.gif":"down.gif"))+"' alt='' width='10'></TD>";
    table += "    <TD class='small "+cl+" "+cl1+"' id='"+make_name(this.name)+"_"+make_name(secsym)+"_metavoli_inner' valign='middle' align='center'><img src='http://cnt4.express.gr/xrimatistirio/images//"+((this.set[secsym].price_diff==0.0)?"blue-square.gif":((this.set[secsym].price_diff>0.0)?"up.gif":"down.gif"))+"' alt='' width='10'></TD>";
    table += "    <TD class='small link bold "+cl+" "+cl2+"' id='"+make_name(this.name)+"_"+make_name(secsym)+"_name_inner' onclick=\"open('http://www2.express.gr/agores/agores/137oz_20071206137.php3?view=metoxi&secsym="+d(secsym)+"','_self','')\" style='padding-left: 4px;'>"+secsym+"</TD>";
    table += "    <TD class='small right "+cl+" "+cl2+"' id='"+make_name(this.name)+"_"+make_name(secsym)+"_price_inner'>"+number_format(this.set[secsym].price,4,",",".")+"</TD>";
    table += "    <TD class='small right "+cl+" "+cl2+"' id='"+make_name(this.name)+"_"+make_name(secsym)+"_diff_inner'>"+number_format(this.set[secsym].diff,4,",",".")+"</TD>";
    table += "    <TD class='small right "+cl+" "+cl2+"' id='"+make_name(this.name)+"_"+make_name(secsym)+"_price_diff_inner'>"+number_format(this.set[secsym].price_diff,2,",",".",true)+"%</TD>";
    table += "    <TD class='small right "+cl+" "+cl3+"' id='"+make_name(this.name)+"_"+make_name(secsym)+"_apodosi_etous_inner' style='padding-right: 4px;'>"+number_format(this.set[secsym].apodosi_etous,2,",",".",true)+"%</TD>";
    table += "  </TR>";  
    i++;
  }
  table += "  </TABLE>";  
  table += "  </TD>";  
  table += "</TR>";
  table += "</TABLE>";
  return table;	
}

function metoxes_moy_draw_changes(){  
  var secsym = "";var tmp_prefix_id = "";
  if(!this.is_ana_klado){
    for(var i in this.set){
  	  tmp_prefix_id = make_name(this.name)+"_"+make_name(i);
      get_element(tmp_prefix_id+"_name_inner").style.background = "";	
      get_element(tmp_prefix_id+"_price_inner").style.background = "";
      get_element(tmp_prefix_id+"_diff_inner").style.background = "";	
      get_element(tmp_prefix_id+"_price_diff_inner").style.background = "";	
    }
  }
  for(var i in this.set_just_changed){
  	secsym = this.set_just_changed[i];
  	if(typeof(this.set[secsym])=="object"){
  	  tmp_prefix_id = make_name(this.name)+"_"+make_name(secsym);
  	  color = (this.set[secsym].price_diff>0.0)?"green":((this.set[secsym].price_diff<0.0)?"red":"blue");
  //	  innerHTML(tmp_prefix_id+"_metavoli_inner","<img src='http://cnt4.express.gr/xrimatistirio/images//"+((this.set[secsym].metavoli==0.0)?"blue-square.gif":((this.set[secsym].metavoli>0.0)?"up.gif":"down.gif"))+"' alt='' width='10'>");
  	  innerHTML(tmp_prefix_id+"_metavoli_inner","<img src='http://cnt4.express.gr/xrimatistirio/images//"+((this.set[secsym].price_diff==0.0)?"blue-square.gif":((this.set[secsym].price_diff>0.0)?"up.gif":"down.gif"))+"' alt='' width='10'>");
      get_element(tmp_prefix_id+"_name_inner").style.color = color;
      
      get_element(tmp_prefix_id+"_price_inner").style.color = color;
      innerHTML(tmp_prefix_id+"_price_inner",number_format(this.set[secsym].price,4,",","."));
  
      get_element(tmp_prefix_id+"_diff_inner").style.color = color;
      innerHTML(tmp_prefix_id+"_diff_inner",number_format(this.set[secsym].diff,4,",",".",true));
            
      get_element(tmp_prefix_id+"_price_diff_inner").style.color = color;
      innerHTML(tmp_prefix_id+"_price_diff_inner",number_format(this.set[secsym].price_diff,2,",",".",true)+"%");   
       
      get_element(tmp_prefix_id+"_apodosi_etous_inner").style.color = color;
      innerHTML(tmp_prefix_id+"_apodosi_etous_inner",number_format(this.set[secsym].apodosi_etous,2,",",".",true)+"%");        
      
      get_element(tmp_prefix_id+"_name_inner").style.background = "yellow";
      get_element(tmp_prefix_id+"_price_inner").style.background = "yellow";
      get_element(tmp_prefix_id+"_diff_inner").style.background = "yellow";
      get_element(tmp_prefix_id+"_price_diff_inner").style.background = "yellow";
      get_element(tmp_prefix_id+"_apodosi_etous_inner").style.background = "yellow";
  	}
  }
} 

function metoxes_moy_show_prosthesi_metoxis(that){
  if(innerHTML(this.inner_popup2,this.draw_prosthesi_metoxis())){
    var that_id = (that)?that.id:"";
    var p = getElementPosition(that_id)
    show(this.inner_popup,(p.left-500), (p.top-210));  	
    //makeDraggable(get_element(this.inner_popup2));
  }
} 
function metoxes_moy_draw_prosthesi_metoxis(){
  var t = "";
  t += "<TABLE border='0' cellpadding='1' cellspacing='1' width='100%'>";
  t += "<TR>";
  t += "  <TD class='big bold'>Πρόσθεση Μετοχής</TD>";
  t += "</TR>"; 
  t += "<TR>";
  t += "  <TD>Εισάγετε μέχρι 15 σύμβολα</TD>";
  t += "</TR>";   
  t += "<TR>";
  t += "  <TD>";
  t += "  <TABLE border='0' cellpadding='1' cellspacing='1' width='100%'>";
  t += "  <TR>";
  var j=0;
  for(var i in this.set){
  	if(j%5==0 && j>0){
  	  t += "</tr><tr>";	
  	}
  	t += "<TD><INPUT type='text' name='"+make_name(this.name)+"_add_secsym_"+j+"_input' value='"+i+"' class='small border' size='9' maxlength='9' readonly></TD>"; 
  	j++;    
  }
  this.count = j;
  while (j<15 && j>=0){
  	if(j%5==0 && j>0){
  	  t += "</tr><tr>";	
  	}
    t += "<TD><INPUT type='text' name='"+make_name(this.name)+"_add_secsym_"+j+"_input' value='' class='small border' size='9' maxlength='9' readonly></TD>";	
    j++;
  }
  t += "  </TR>";
  t += "  </TABLE>";
  t += "  </TD>";
  t += "</TR>";  
  t += "<TR>";
  t += "  <TD class='right'><img src='http://cnt4.express.gr/xrimatistirio/images//go.gif' alt='go' id='"+make_name(this.name)+"_add_secsyms_go_ctl' onclick=\""+this.name+".is_opened=true;"+this.name+".is_shown=false;"+this.name+".get();hide("+this.name+".inner_popup);\" class='link'/></TD>";
  t += "</TR>";     
  t += "</TABLE>";
  return t;	
}
function metoxes_moy_hide_prosthesi_metoxis(e){
  var is_clicked_inside = false;
  var p = getElementPosition(this.inner_popup);
  var c = mouseCoords(e);
  is_clicked_inside = ((c.x > p.left) && (c.x < p.left+p.width) && (c.y > p.top) && (c.y < p.top+p.height))?true:false; 
  if(!is_clicked_inside){
    hide(this.inner_popup)	
  }	
}

function metoxes_moy_show_help_prosthesi_metoxis(that){
  if(innerHTML(this.inner_help2,this.draw_help_prosthesi_metoxis())){
    var that_id = (that)?that.id:"";
    var p = getElementPosition(that_id)
    show(this.inner_help,(p.left+p.width-90), (p.top-53));
    delete p;
  }
} 
function metoxes_moy_draw_help_prosthesi_metoxis(){
  var t = "";
  t += "<TABLE border='0' cellpadding='2' cellspacing='2' width='60'>";
  t += "<TR>";
  t += "  <TD class='white center'>Πρόσθεση Μετοχής</TD>";
  t += "</TR>";
  t += "</TABLE>";
  return t;	
}
function metoxes_moy_hide_help_prosthesi_metoxis(){
  hide(this.inner_help);	
}


function metoxes_moy_show_afairesi_metoxis(that){
  if(innerHTML(this.inner_popup4,this.draw_afairesi_metoxis())){
    var that_id = (that)?that.id:"";
    var p = getElementPosition(that_id)
    show(this.inner_popup3,(p.left-300), (p.top-100));  	
    makeDraggable(get_element(this.inner_popup3));
    delete p;
  }
} 
function metoxes_moy_draw_afairesi_metoxis(){
  var t = "";
  t += "<TABLE border='0' cellpadding='2' cellspacing='2' width='300' class='' >";
  t += "<TR>";
  t += "  <TD class='big bold'>Αφαίρεση Μετοχής</TD>";
  t += "</TR>"; 
  t += "<TR>";
  t += "  <TD>Πατήστε πάνω στο «x» δίπλα στο σύμβολο για το αφαιρέσετε απο το χαρτοφυλακίο σας</TD>";
  t += "</TR>";   
  t += "<TR>";
  t += "  <TD>";
  t += "  <TABLE border='0' cellpadding='2' cellspacing='2'>";
  t += "  <TR>";
  var j=0;
  for(var i in this.set){
  	if(j%5==0 && j>0){
  	  t += "</tr><tr>";	
  	}
  	t += "<TD class='small'>"+i+"</td><td><div style='width: 12px; height: 12px;' class='border link bold center bg_white' onclick=\""+this.name+".rem('"+i+"',"+j+");\">X</div></TD>"; 
  	j++;    
  }
  this.count = j;
  t += "  </TR>";
  t += "  </TABLE>";
  t += "  </TD>";
  t += "</TR>";  
  t += "<TR>";
  t += "  <TD class='right'><img src='http://cnt4.express.gr/xrimatistirio/images//go.gif' alt='close' id='"+make_name(this.name)+"_add_secsyms_go_ctl'  onclick=\""+this.name+".is_opened=true;"+this.name+".is_shown=false;"+this.name+".get();hide("+this.name+".inner_popup3);\" class='link' /></TD>";
  t += "</TR>";     
  t += "</TABLE>";
  return t;		
}
function metoxes_moy_hide_afairesi_metoxis(e){
  var is_clicked_inside = false;
  var p = getElementPosition(this.inner_popup3);
  var c = mouseCoords(e);
  is_clicked_inside = ((c.x > p.left) && (c.x < p.left+p.width) && (c.y > p.top) && (c.y < p.top+p.height))?true:false; 
  if(!is_clicked_inside){
    hide(this.inner_popup3)	
    if(this.count>0 && p.width>0){
      this.is_opened=true;
      this.is_shown=false;
      this.get();
    }
  }	
  delete p;delete c;delete is_clicked_inside;
}
function metoxes_moy_show_help_afairesi_metoxis(that){
  if(innerHTML(this.inner_help2,this.draw_help_afairesi_metoxis())){
    var that_id = (that)?that.id:"";
    var p = getElementPosition(that_id)
    show(this.inner_help,(p.left),(p.top-53));
    delete p;delete that_id;delete that;
  }
} 
function metoxes_moy_draw_help_afairesi_metoxis(){
  var t = "";
  t += "<TABLE border='0' cellpadding='2' cellspacing='2' width='60'>";
  t += "<TR>";
  t += "  <TD class='white center'>Αφαίρεση Μετοχής</TD>";
  t += "</TR>";
  t += "</TABLE>";
  return t;
}
function metoxes_moy_hide_help_afairesi_metoxis(){
  hide(this.inner_help);	
}
function metoxes_moy_rem(secsym, position){
  if(typeof(this.set[secsym])=="object"){
  	var tmp = new Array();
  	for(var i in this.set){
  	  if(i!=secsym){
  	    tmp[i] = this.set[i];
  	  }
  	}
  	this.set = tmp;
  	innerHTML(this.inner_popup4,this.draw_afairesi_metoxis());
  	delete tmp;
  }
}

function static_metoxes_moy_hide(e) {
  e = e || window.event;	
  for (var i in my_objects){
    if (typeof(my_objects[i])=="object"){
      if(my_objects[i].hide_prosthesi_metoxis) {
        my_objects[i].hide_prosthesi_metoxis(e);
      }
      if(my_objects[i].hide_afairesi_metoxis){
      	my_objects[i].hide_afairesi_metoxis(e);
      }            
    }
  }
}
window.popupWindowOldEventListener2 = document.onmouseup;
if (window.popupWindowOldEventListener2 != null) {
  document.onmouseup = function(e){
    window.popupWindowOldEventListener2(e); 
    static_metoxes_moy_hide(e);
  }
}
else {
  document.onmouseup = static_metoxes_moy_hide;
}
