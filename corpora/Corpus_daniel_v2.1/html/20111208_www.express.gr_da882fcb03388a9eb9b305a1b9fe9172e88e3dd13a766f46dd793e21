// methods
  this.init = metoxes_my_init;
  this.get_by_letter = metoxes_my_get_by_letter;
  this.get_by_secsym = metoxes_my_get_by_secsym;
  this.get_by_catego = metoxes_my_get_by_catego;
  this.fill_found_metoxes = metoxes_my_fill_found_metoxes;
  this.onselect = metoxes_my_onselect;
//
  this.init();
}
function metoxes_my_init(){
  this.set = new Array();
}
function metoxes_my_get_by_letter(letter,do_regexp){
  this.init();
  if (letter.length>0){
    var re = (do_regexp)?make_regexp(letter):new RegExp('^'+letter);
    for(var i in this.set_oles){
      if(re.exec(this.set_oles[i].compnam)){
        this.set[i]=this.set_oles[i];
      }
    }
  }
  this.fill_found_metoxes();
}
function metoxes_my_get_by_secsym(secsym){
  this.init();
  if (secsym.length>0){
    var re = make_regexp(secsym);
    for(var i in this.set_oles){
      if(re.exec(this.set_oles[i].secsym)){
        this.set[i]=this.set_oles[i];
      }
    }
  }
  this.fill_found_metoxes();
}
function metoxes_my_get_by_catego(comp_categ){
  this.init();
  if (comp_categ.length>0){
    for(var i in this.set_oles){
      if(comp_categ==this.set_oles[i].comp_categ){
        this.set[i]=this.set_oles[i];
      }
    }
  }
  else {
    this.set=this.set_oles;
  }
  this.fill_found_metoxes();
}
function metoxes_my_fill_found_metoxes(){
  if(typeof(this.forma)=='undefined'){
    return false;
  }
  var t = '';
  for( var i in this.set ){
    t +=  '<div onclick="search_metoxes_my.onselect(\''+this.set[i].secsym+'\');" class="link">'+this.set[i].compnam+' ('+this.set[i].secsym+')'+'</div>\n';   
  }
  get_element('found_metoxes_set_inner_my').innerHTML = t;
}
function metoxes_my_onselect(value){
  alert(value);
}
var search_metoxes_my = new Metoxes_my('search_metoxes_my');
search_metoxes_my.forma = document.search_metoxi_my_forma;
