<script type="text/javascript">
new tabmenu("#Messmenu > li","#home2_topic .normal","on","mouseover");
$(document).ready(function(){  
		//针对IE6.0里的PNG图片问题
        //$('body').pngFix( );
		
		//显示字体下拉菜单
	   $(".font_set span").bind("click", function(){
		   $(".font_m").slideDown("slow");
		});
		
		//隐藏字体下拉菜单
	   $(".font_m").bind("mouseleave", function(){
		   $(".font_m").slideUp("slow");
		});
		
		 //显示更多分享下拉菜单
	   $(".more_icon").bind("mouseover", function(){
		   $(".news_tool_more").show("slow");
		});
			
		//隐藏更多分享下拉菜单
	   $(".news_tool_more").bind("mouseleave", function(){
		   $(".news_tool_more").hide("slow");
		});
		
		 //焦点位置图片放大效果
		 /*
		$("a#focus_img").fancybox({
			'hideOnContentClick': true
		});
		
		//Activate FancyBox
			$("p#test2 a").fancybox({
				'zoomSpeedIn':	0, 
				'zoomSpeedOut':	0, 
				'overlayShow':	true
			});
	*/
	//设置字体大小
	var P_length = $("#news_con p").length;
	for(var i=0;i<P_length;i++)
	{
		var P_Content = $("#news_con p").get(i).innerHTML;
		var lastChar = right(P_Content,1);
		//var reg=/^[\u0391-\uFFE5]+$/;    
		if(!(/[^\u4e00-\u9fa5]/.test(lastChar)) && P_Content.length<20){
			P_Content = "<b>" + P_Content + "<b>";
			$("#news_con p").get(i).innerHTML = P_Content;
		}
	}
	
    });

function right(mainStr,lngLen) {
	 if (mainStr.length-lngLen>=0 && mainStr.length>=0 && mainStr.length-lngLen<=mainStr.length){
	 return mainStr.substring(mainStr.length-lngLen,mainStr.length)}
	 else{return null}
}


