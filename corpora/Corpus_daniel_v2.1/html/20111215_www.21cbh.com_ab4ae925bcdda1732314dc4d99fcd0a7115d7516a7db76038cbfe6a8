      <div id="newcomment">评论加载中...</div>
            <script type="text/javascript" src="http://static.21cbh.com/js/comment_setting.js"></script> 
      <script language="javascript">

	$(document).ready(function(){

		$("#newcomment").load("http://www.21cbh.com/comment/?c=content2&type=ajax&keyid=phpcms-content-title-388621&verify=4f9b9631b873c0b48bd298c99c13ec41&catid=314",function(){
			loginform();
			
			/*if(setting.ischecklogin == 0 && getcookie('auth') === null)
            {
                $('#comment').val("请您登陆后发表评论！");
				$('#comment').css("background","#eeeeee");
                $('#comment').attr("disabled","disabled");
                $('#sbutton').attr("disabled","disabled");
            }
            else 
            {
                $('#comment').val("欢迎您发表评论，本网保留不刊登无关和不雅评论的权利。");
            }*/
			$('#comment').val("欢迎您发表评论，本网保留不刊登无关和不雅评论的权利。");
		});
	});
	function reply_restoration()
	{
		if($('#comment').val() == '')
		{
			$('#comment').val("欢迎您发表评论，本网保留不刊登无关和不雅评论的权利。");
		}
	}
	function reply_clearfield()
	{
		if ($('#comment').val() == "欢迎您发表评论，本网保留不刊登无关和不雅评论的权利。") 
		{
			$('#comment').val("");   
		}
		document.getElementById('code_img').style.display = 'inline';document.getElementById('code_img').src='checkcode.php';
	}
	function checkForm()
	{
		if($('#comment').val() == '' || $('#comment').val() == "欢迎您发表评论，本网保留不刊登无关和不雅评论的权利。")
		{
			alert("内容不能为空");
			$('#comment').focus();
			return false;
		}
		if ($('#comment').val().length > 1000) 
		{
			alert("内容太长，最多 1000 个文字");
			return false;
		}

	}
	function ajaxload(commentid)
	{
		$('#comment_'+commentid).append("<div id='reply_layer'><div class='window'><h4><a href='javascript:close("+commentid+");' class='jqmClose' target='_self'><img src='http://img.21cbh.com/images/close.gif' height='16px' width='16px'/></a>用户评论</h4><div class='commenttext'><form target=\"_self\" method='post name='reply' action=''><input type='hidden' id='commentid' name='commentid' value='"+commentid+"' /><input type='hidden' value='phpcms-content-title-388621' name='keyid' id='keyid'/><input type='hidden' value='4f9b9631b873c0b48bd298c99c13ec41' name='verify' id='verify' /><input type='hidden' name='catid' id='catid' value='314'/><ul><li><textarea id='contenttext' name='contenttext' rows='8' cols='65' class='textarea_style'></textarea></li><li><input type='button' value='回复评论' name='dosubmit' class='button_style' onclick='jsonadd("+commentid+");' /></li></ul></form></div></div></div>");
		$('#rb_'+commentid).hide();
	}
	function jsonadd(commentid) 
	{	
		if (!$('#contenttext').val())
		{   
			alert("内容不能为空");
			$('#contenttext').focus();
			return false;
		}
		if ($('#contenttext').val().length > 500) {
			alert("内容太长，最多 1000 个文字");
			return false;
		}
		var contenttext = $('#contenttext').val();
		$.getJSON("http://comment.21cbh.com/?action=jsonadd", "&commentid="+commentid+"&catid=314&keyid=phpcms-content-title-388621&verify=4f9b9631b873c0b48bd298c99c13ec41&contenttext="+contenttext+"&callback=?", function(data){
			if(data.status==1)
			{
				alert("回复成功");
				location.reload();
			}
			else
			{
				alert("请重试");
				$("#reply_layer").html("");
			}
		}); 
	}
	function close(commentid) 
	{
		$("#reply_layer").remove();
		$('#rb_'+commentid).show();
	}
	function voteFor(id)
	{
		this.id = id;
		var url = "http://comment.21cbh.com/?action=vote";
		var data = '&id='+id + '&field=support';
		$.ajax({
			url: url,
			data: data,
			type: 'post',
			success :function (msg){
				$('#count_'+ id+'_1').text(msg);
			}
		});
	}
/**
* 反对
**/
function voteAgainst (id )
{
	this.id = id;
	var url  = "http://comment.21cbh.com/?action=vote";
	var data = '&id='+id + '&field=against';
	$.ajax({
		url: url,
		data: data,
		type: 'post',
		success :function (msg){
			$('#count_'+ id +'_0').text(msg);
		}
	});
}
</script> 

    </div>
