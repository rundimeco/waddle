			});
			$("#mbtn").bind('click', function(){
				$("#content").removeClass().addClass("LeftPhotoInfo_M");
				btn_init();
				$("#mbtn").attr({ src: "/i/content/wsize02_2.gif" });
			});
			$("#lbtn").bind('click', function(){
				$("#content").removeClass().addClass("LeftPhotoInfo_L");
				btn_init();
				$("#lbtn").attr({ src: "/i/content/wsize03_2.gif" });
			});
