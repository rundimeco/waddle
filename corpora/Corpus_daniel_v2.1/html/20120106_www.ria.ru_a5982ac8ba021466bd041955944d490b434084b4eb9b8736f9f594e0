<script type="text/javascript">
	var ar_essence = (function(metasName){
		var keys = '', i, j, metas = document.getElementsByTagName('meta');
		for(i in metasName){
			if (!metasName.hasOwnProperty(i)) continue;
			for (j = 0; j < metas.length; j++){
				if (metas[j].name.search(metasName[i]) != -1){
					keys += ';' + metas[j].content.replace(/\s*,\s*/g, ';');
					break;
				}
			}
		}
		return keys;
	}(['rian_trend', 'rian_theme']));
</script>
