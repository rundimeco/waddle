			return null;
		}

	},
	getIntro : function(){
		var intro = '',t='';
		var para = document.getElementById("artibody").getElementsByTagName('p');
		for(var i=0;i<para.length;i++){
			t = para[i].innerHTML.replace(/(\<.*?\>)|\&nbsp;|[\t\n\r ]/ig,'');
			if(t.length>30){
				intro = t.length>180?t.substr(0,180)+'...':t;
				break;
			};
		};
		return intro;
	}
}
