    <hr class="hidden">
  </div>
  <div class="article-listen-player"><img src="/files/1/audio_player.gif"></div>    <script>
	var google_adnum = 0;
        
        function google_ad_request_done(google_ads) {
            var s = '';
            var i;
            if (google_ads.length == 0) {
                return;
            }
            s += '<div class="promoTitle" style="text-align:left;"><a href="https://www.google.com/adsense/support/bin/request.py?contact=abg_afc">Ads by Google</a></div>';
            for (i = 0; i < google_ads.length; i++) {
                s += '<div class="manyAds">' +
						'<div class="addTitle" >' +
							'<a style="font-size:13px;" href="' + google_ads[i].url + ' "onmouseout="window.status=\'\';return true;" ' +
							'onmouseover="window.status=\'go to ' + google_ads[i].visible_url + '\';return true; " style="text-decoration:none;">' +
							google_ads[i].line1 +
							'</a>' +
						'</div>' +
						'<div class="addText">' +
							 google_ads[i].line2 + ' ' + google_ads[i].line3 +
						'</div>' +
						'<div class="addUrl">' +
							'<a href="' + google_ads[i].url + ' "onmouseout="window.status=\'\';return true;" ' +
							'onmouseover="window.status=\'go to ' + google_ads[i].visible_url + '\';return true; " style="text-decoration:none;">' +
							google_ads[i].visible_url +
							'</a>' +
						'</div>' +
					'</div>';
            }


            if (google_ads[0].bidtype == "CPC") {
                google_adnum = google_adnum + google_ads.length;
            }
			
            if (google_adnum <= 1) {
				
                var divtop = document.getElementById('google-ads-top-1pc');
                divtop.innerHTML = s;
				
            }

            if (google_adnum > 1) {
                var divbottom = document.getElementById('google-ads-bottom-3pcs');
                divbottom.innerHTML = s;
            }

            return;
        }
  
    </script><div id="google-ads-top-1pc" class="promo-banners gAds" style="border-width:0px;"></div><script language="JavaScript" type="text/javascript">            
      google_ad_client = 'pub-3451081775397713';
    
		google_ad_channel = '8122263051';
	
            google_ad_output = 'js';
