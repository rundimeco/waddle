<script type="text/javascript">
(function ($) {
    var przycisk = $('a.tooltipZamknij');
    przycisk.click(function(event) {
        event.preventDefault();
        $('.cookieTooltipKontener').hide();
        var data = new Date();
        data.setDate(data.getDate() + 365);
        data = data.toUTCString();
        document.cookie = 'polityka_prywatnosci_cookies=true; expires=' + data + '; path = /; domain=naszemiasto.pl';
    });
})(jQuery);
</script>
