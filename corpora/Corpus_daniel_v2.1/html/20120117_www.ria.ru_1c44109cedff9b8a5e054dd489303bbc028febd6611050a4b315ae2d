    function showBlogview() {			
        if ($("blogview").style.display != 'block') {
                $("blogview").style.display = 'block'; 
				$("resend").style.display = 'none';
				$("phototext").style.display = 'none';
				$("photoview").style.display = 'none';}
        else if ($("blogview").style.display = 'block') {
                $("blogview").style.display = '';};		
        return false;}
		
	function showPhototext() {			
        if ($("phototext").style.display != 'block') {
                $("phototext").style.display = 'block'; 
				$("resend").style.display = 'none';
				$("blogview").style.display = 'none';
				$("photoview").style.display = 'none';
				document.getElementById('withtext').className = 'active';
				document.getElementById('withouttext').className = 'inactive';
				}
        else if ($("phototext").style.display = 'block') {
                $("phototext").style.display = '';
                document.getElementById('withtext').className = 'inactive';
                };		
        return false;}
		
	function showPhoto() {			
        if ($("photoview").style.display != 'block') {
                $("photoview").style.display = 'block'; 
				$("resend").style.display = 'none';
				$("blogview").style.display = 'none';
				$("phototext").style.display = 'none';
				document.getElementById('withouttext').className = 'active';
				document.getElementById('withtext').className = 'inactive';
				}
        else if ($("photoview").style.display = 'block') {
                $("photoview").style.display = '';
                document.getElementById('withouttext').className = 'inactive';
                };		
        return false;}
        
        function showBlock(id) { 
        display = document.getElementById(id).style.display;  
        if(display=='block'){  
        document.getElementById(id).style.display='none';  
        }else{  
        document.getElementById(id).style.display='block';  
        }  
        }
