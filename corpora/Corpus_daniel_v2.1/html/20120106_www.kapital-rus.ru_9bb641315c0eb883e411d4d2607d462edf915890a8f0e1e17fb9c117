<script>
$$(function(){
	{//Все изоражения новости, имеющие класс "zoomme" уменьшатся и станут кликабельными
		var imgsize = 400//размер превьюшки по умолчанию
		var i=0//вспомогательная переменная для определения чередования расположения (слева, справа, ...)
		$$(".zoomme").each(function(){
			var im = $$(this)
			var url = im.attr("src")
			if(im.attr("imgsize")){imgsize = im.attr("imgsize")}
			im.attr('src', '/img/iResize.php?size='+imgsize+'&img='+url)
			im.css({"border":"none"})
			{//определяем стиль чтобы располагать фотки чередованием (слева, справа, слева, справа, ...)
				i++
				var styleStr = "float:right; margin-left:10px"
				if(i==2){
					styleStr = "float:left; margin-right:10px"
					i=0
				}
			}
			im.wrap('<a href="'+url+'" class="zoom" style="'+styleStr+'" />')
		})
		$$(".zoom").lightBox()
	}
})
</script>

