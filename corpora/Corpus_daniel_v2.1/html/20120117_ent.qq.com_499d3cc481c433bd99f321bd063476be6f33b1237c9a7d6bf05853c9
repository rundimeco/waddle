<script type="text/javascript">
//保持在底部
_MI.GoTop1 = {
	_body : null,
	delay : {},
	resize : 0,
	position : function(){
		this.build();
	},
	build : function(main,bar) {
		var _main = _$(main);
		if (!_main) {
			return;
		}
		var Self = this,
		leftFix = 5,
		left = _MUI.getX(_main) + _MUI.width(_main) - leftFix,
		foot,
		windowHeight = _MUI.windowHeight(),
		pageHeight = _MUI.pageHeight(),
		scrollY,
		resize,
		y;
		Self._body = _$(bar);
		if (_main) {
			foot = pageHeight - (_MUI.getY(_main) + _MUI.height(_main));
			position();
			Self._body.onfocus = function(){
				this.blur();
			}
			_MUI.EA(window,'scroll',position);
			_MUI.EA(window,'resize',position);

		}
		function position(e){
			resize = (e && _MUI.E(e).type == 'resize') || Self.resize;
			clearTimeout(Self.delay.position);
			if (_MUI.B.ie6) {
				_MUI.hide(Self._body);
			}
			Self.delay.position = setTimeout(set,_MUI.B.ie6 ? 400 : 0);
		}
		function set(){
			var pageHeight = _MUI.pageHeight();
			if (resize) {
				windowHeight = _MUI.windowHeight()
				left = _MUI.getX(_main) + _MUI.width(_main) - leftFix;
				Self.resize = 0;
			}
			scrollY = _MUI.scrollY();
			Self._body.style.cssText = 'left:' + left + 'px;';
				_MUI.show(Self._body);
			y = scrollY + windowHeight - pageHeight;
			if (y >= -foot) {
				if(!_MUI.B.ie)
					Self._body.style.cssText += ';position:absolute;top:' + (pageHeight - 45 - foot) + 'px';
				Self._body.style.cssText += ';position:absolute;top:' + (pageHeight - 30 - foot) + 'px';
			}
		}
	}
}
_MUI.ready(function(){
	_MI.GoTop1.build('Main-Article-QQ','mbToolbar_mod');
