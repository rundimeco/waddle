/**
微博小助手类
*/
function mbAssistantTool(){
	this.tips_mod = _$('tips_mod');
	this._body = _$('mbToolbar_mod');
	this._msgTxt = _$('msgTxt');
	this._firstMsg = _$$('.first_msg')[0];
	this._secondMsg = _$$('.second_msg')[0];
	this._wbTalkBox = _$$('.wb_talkbox')[0];
	this._msg_area = _$$('.msg_area')[0];
	this._talkBoxObj = new _MI.TalkBox("talkBox");
	if(sosoArticlKey!=""&&!(/&#[0-9]+;/.test(sosoArticlKey))) 
		this._msgTxt.value = "#"+sosoArticlKey+"#";
	this.initTalkBox();
	var _this = this;
	function openCommentBox(){
		_this.tips_mod.style.display = 'none';
		_this.showrelayArea();
		_MI.Bos('btnMbAssistant','','qqcom.dp.btnMbAssistant');
	}
	_$$(this._body, '.bg')[0].onclick = openCommentBox;
	_$$(this._body, '.bgSmall')[0].onclick = openCommentBox;
	_$$(this._body, '.mbNumZone')[0].onclick = openCommentBox;
	//1024分辨率控制
	var UerScreenD = window.screen.width;
	if(UerScreenD<=1024){
		_$$(_this._body, '.bgRight')[0].style.display = "none";
		_$$(_this._body, '.bgLeft')[0].style.display = "";
		_$$(_this._body, '.bg')[0].style.display = "none";
		_$$(_this._body, '.bgSmall')[0].style.display = "";
	}
	_$$(_this._body, '.bgRight')[0].onclick = function(){
		this.style.display = "none";
		_$$(_this._body, '.bgLeft')[0].style.display = "";
		_$$(_this._body, '.bg')[0].style.display = "none";
		_$$(_this._body, '.bgSmall')[0].style.display = "";
	}
	_$$(_this._body, '.bgLeft')[0].onclick = function(){
		this.style.display = "none";
		_$$(_this._body, '.bgRight')[0].style.display = "";
		_$$(_this._body, '.bg')[0].style.display = "";
		_$$(_this._body, '.bgSmall')[0].style.display = "none";
	}
	_$('addArticleLink').onclick = function(){
		_this.addLinkClick();
	}
	_$$(this._body, '.send_box')[0].onclick = function(){
		//显示控制
		this.style.display = "none";
		_this._wbTalkBox.style.display = "";
		_$$(_this._body, '.show_msg')[0].style.border="none";
		_$$(_this._body, '.show_msg')[0].style.margin="0";
		_$$(_this._body, '.comment_msg')[0].style.borderBottom = "none";
		_$$(_this._body, '.comment_msg .down')[0].style.margin = "0";
		_this._msg_area.style.height = "80px";
		_$$(_this._body, '.msg_loading')[0].style.height = "80px";
	}
	//评论控制
	this.comments = [];
	this.getCommentMsgs(0);
	_$$(_this._body, '.msg_updown')[0].onmouseover=function(){
		_this.lastScrollTop = -1;
		_this.timer = setInterval(function(){
			_this._msg_area.scrollTop += 1;
			var p_msg = _$$(_this._body, '.previous_msg')[0];
			var n_msg = _$$(_this._body, '.next_msg')[0];
			if(_this._msg_area.scrollTop == _this.lastScrollTop){
				clearInterval(_this.timer);
				p_msg.style.background = 'url(http://mat1.gtimg.com/news/imgcss_article_qq/mbtools_msg.gif) 0px -34px no-repeat';
			}
			else{
				if(n_msg.style.background.indexOf('mbtools_msg.gif') < 0)
					n_msg.style.background = 'url(http://mat1.gtimg.com/news/imgcss_article_qq/mbtools_msg.gif) 0px 0px no-repeat';
			}
			_this.lastScrollTop = _this._msg_area.scrollTop;
		}, 1);
	}
	_$$(_this._body, '.msg_updown')[0].onmouseout=function(){
		clearInterval(_this.timer);
	}
	_$$(_this._body, '.msg_updown')[1].onmouseover=function(){
		_this.lastScrollTop = -1;
		_this.timer = setInterval(function(){
			_this._msg_area.scrollTop -= 1;
			var p_msg = _$$(_this._body, '.previous_msg')[0];
			var n_msg = _$$(_this._body, '.next_msg')[0];
			if(_this._msg_area.scrollTop == _this.lastScrollTop){
				clearInterval(_this.timer);
				n_msg.style.background = 'url(http://mat1.gtimg.com/news/imgcss_article_qq/mbtool_updown.gif) 0px -16px no-repeat';
			}
			else{
				if(p_msg.style.background.indexOf('mbtool_updown.gif') < 0)
					p_msg.style.background = 'url(http://mat1.gtimg.com/news/imgcss_article_qq/mbtool_updown.gif) 0px 0px no-repeat';
			}
			_this.lastScrollTop = _this._msg_area.scrollTop;
		}, 1);
	}
	_$$(_this._body, '.msg_updown')[1].onmouseout=function(){
		clearInterval(_this.timer);
	}
}

mbAssistantTool.prototype.showMessage = function(page){
	for(var i=0; i<this.comments.length; ++i){
		var msg = _MUI.DC('div');
		msg.innerHTML = msg.innerHTML + '<a class="comment_account" href="http://t.qq.com/'+this.comments[i].userName +'" target="_blank">'+this.comments[i].userNick+'</a><span class="comment_content">：'+ this.comments[i].content+'</span><a class="time_zone" href="http://t.qq.com/p/t/'+ this.comments[i].id + '" target="_blank">(' + this.comments[i].time+')</a>';
		_MUI.addClass(msg, 'msg_content');
		_MUI.append(msg, this._msg_area);
		if(i != (this.comments.length-1)){
			var div_line = _MUI.DC('div');
			_MUI.addClass(div_line, 'div_line');
			_MUI.append(div_line, this._msg_area);
		}
	}
}

mbAssistantTool.prototype.getCommentMsgs = function(page){
	var _this = this;
	var url = 'http://public.open.t.qq.com/api/other/url_converge';
	var params = {
		format: 'getCommentMsgCallback',
		pageflag: '0',
		url: ARTICLE_INFO.article_url,
		pagetime: 'xxx',
		tweetid: 'xxx',
		type: '1',
		words: '5',
		falg: '1',
		reqnum: '10',
		detaillevel: '3'
	};
	var data = [];
	for(i in params)
		data.push(i + '=' + encodeURIComponent(params[i]));
	url += ('?'+data.join('&'));
	_MUI.crossAsynJson(url,'getCommentMsgCallback',function(data){
		if(!data)
			return;
		if(data.ret == 0){
			_$$(_this._body, '.msg_loading')[0].style.display = "none";
			var msgCount = 0;
			if(data.data.countinfo != null){
				for(var i=0; i<data.data.countinfo.length; ++i){
					if(data.data.countinfo[i].counttype==1){
						msgCount = data.data.countinfo[i].countnum;
						break;
					}
				}
			}
			if(msgCount > 99)
				msgCount = 99;
			//Message Count Setting
			if(msgCount > 0){
				_$$(_this._body, '.mbNum')[0].innerHTML = '<span>' + msgCount + '</span>';
				_$$(_this._body, '.mbNumZone')[0].style.display = "";
			}
			if(data.data.tweetinfo){
				for(var i=0; i<data.data.info.length; ++i){
					if(data.data.tweetinfo[i].type != 1)
						continue;
					var msgObj = {
						content: data.data.info[i].text, 
						userName: data.data.tweetinfo[i].name, 
						userNick: data.data.tweetinfo[i].nick, 
						time: _this.formatTime(data.data.tweetinfo[i].time),
						id: data.data.tweetinfo[i].tweetid
					};
					_this.comments.push(msgObj);
				}
			}
			if(msgCount == 0){
				_$$(_this._body, '.send_box')[0].style.display = "none";
				_this._wbTalkBox.style.display = "";
				_$("msgTxt").style.height = "175px";
				_$$(_this._body, '.text_box')[0].style.height = "185px";
				_$$(_this._body, '.comment_msg')[0].style.display = "none";
			}
			_this.showMessage();
		}
	}, 'utf-8')
}

/**
	time: 消息发表时间
	return: 刚刚， xx分钟前，xx小时前，xx天前
*/
mbAssistantTool.prototype.formatTime = function(time){
	var now = Math.floor(new Date().getTime()/1000);
	var diff = now - time;
	if(diff < 60)
		return '刚刚';
	else if(diff >= 60  && diff < 60*60)
		return Math.floor(diff/60) + '分钟前';
	else if(diff >= 60*60 && diff < 60*60*24)
		return Math.floor(diff/3600) + '小时前';
	else{
		var postTime = new Date();
		postTime.setTime(time*1000);
		var m = postTime.getMonth()+1;
		m = m > 9 ? m : ("0" + m);
		var d = postTime.getDate() > 9 ? postTime.getDate() : "0" + postTime.getDate();
		var h = postTime.getHours() > 9 ? postTime.getHours() : "0" + postTime.getHours();
		var min = postTime.getMinutes() > 9 ? postTime.getMinutes() : "0" + postTime.getMinutes();
		return  (m + "月" + d + "日 " + h + ":" + min);
	}
}

mbAssistantTool.prototype.showrelayArea = function(){
	var _this = this;
	var c = MI.Login.getUin();
	var e = MI.S("account_mbid_" + c);
	MI.Login.setCallback("broad_login", UI.bind(function(){
		document.getElementById("talkBox").style.display = "block";
		_this.focusTextBox();
	}));
	MI.AccountInfo.setCallback("broad_login", UI.bind(function(a){
		var nickname = a.mbnick ? a.mbnick : a.nick;
		document.getElementById("talkbox_title").innerHTML = '<a onclick="MI.Bos(\'btnPortalJump1\')" href="http://t.qq.com/'+a.mbid+'/?pref=qqcom.dp.mbassistant" target="_blank">您好，'+nickname+'</a>';
	}));
	if (!c || !e) {	
		MI.Login.showPopup("broad_login", this)
	}
	else{
		this.SetUserNick();
		_$("talkBox").style.display = "block";
	}
	this.focusTextBox();
}

mbAssistantTool.prototype.SetUserNick = function(){
	var a = MI.Login.getUin()
	var nickname = MI.S("account_mbnick_" + a)?MI.S("account_mbnick_" + a):MI.S("account_nick_" + a);
	var mbid = MI.S("account_mbid_" + a)?MI.S("account_mbid_" + a):MI.S("account_id_" + a);
	document.getElementById("talkbox_title").innerHTML = '<a onclick="MI.Bos(\'btnPortalJump1\')" href="http://t.qq.com/'+mbid+'/?pref=qqcom.dp.mbassistant" target="_blank">您好，'+nickname+'</a>';
}

mbAssistantTool.prototype.addEvent =function(obj, event, func){
	if(typeof obj.onclick != "function"){
		if(window.attachEvent)	{
			obj.attachEvent("on"+event,func);
		}
		else if(window.addEventListener) {
			obj.addEventListener(event,func,false);
		}
	}
	else{
		var funcOld = obj.onclick;
		obj.onclick = function(){
			func();
			funcOld();
		}
	}
}

mbAssistantTool.prototype.focusTextBox = function(){
	if(_MUI.B.ie){
		var range = this._msgTxt.createTextRange();
		range.collapse(true);
		range.moveStart('character', this._msgTxt.value.length);
		range.select();
	}else{
		this._msgTxt.focus();
	}
}

mbAssistantTool.prototype.initTalkBox = function(){
	var _this = this;
	if(this._msgTxt.value.indexOf(window.location)==-1)
		this._talkBoxObj.txtMax = 140-11;
	this._talkBoxObj.success = function(data){
		var afterSendObj = _$("mbAfterSend");
		var backgroundObj = _$("afterBack_mod");
		if(data.result == 0){
			afterSendObj.style.display = "block";
			backgroundObj.style.display = "block";
			setTimeout(function(){
				afterSendObj.style.display = "none";
				backgroundObj.style.display =  "none";},  3000);
			if(sosoArticlKey!=""&&!(/&#[0-9]+;/.test(sosoArticlKey))) 
				_this._msgTxt.value = "#"+sosoArticlKey+"#";
			_this.focusTextBox();
			_this.addLinkClick();
		}
	};
	this._talkBoxObj.getContentFunc = function(){
		if(_MUI.G('addArticleLink').checked && _this._msgTxt.value.indexOf(window.location)==-1)
			return _this._msgTxt.value + window.location;
		else
			return _this._msgTxt.value;
	};
}

mbAssistantTool.prototype.addLinkClick = function(){
	if(_MUI.G('addArticleLink').checked && document.getElementById("msgTxt").value.indexOf(window.location)==-1){
		this._talkBoxObj.txtMax = 140-11;
		this._talkBoxObj.countTxt();
	}else{
		this._talkBoxObj.txtMax = 140;
		this._talkBoxObj.countTxt();
	}
}

mbAssistantTool.prototype.addArticleLink = function(){
	_MI.Bos('btnMbAssistantB','','qqcom.dp.btnMbAssistantB');
	this._talkBoxObj.sourcePic = '';
	var imgs = _$$(document.getElementById("Cnt-Main-Article-QQ"), 'img');
	for(var i in imgs){
		var o = imgs[i], w = _MUI.width(o), h = _MUI.height(o);
		if(w>100 || h>100){
			this._talkBoxObj.sourcePic = o.src;
			break;
		}	
	}
}
	var mbAssistantObj = new mbAssistantTool();
	_MUI.ready(function(){
		mbAssistantObj.addEvent(document.getElementById("btnSend"),"click",function(){
			mbAssistantObj.addArticleLink();
		});
	});	
