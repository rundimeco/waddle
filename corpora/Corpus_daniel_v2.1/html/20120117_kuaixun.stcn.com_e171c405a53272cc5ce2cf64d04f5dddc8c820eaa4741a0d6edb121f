<script type="text/javascript" src="../../../common/js/stocksuggest.js" charset="gb2312"></script>
<script type="text/javascript" src="../../../common/js/suggestfinance.js" charset="gb2312"></script>
<script type="text/javascript" src="../../../common/js/fundsuggest.js" charset="gb2312"></script>
<link rel="stylesheet" href="../../../public/14652.files/style/style.css" >
