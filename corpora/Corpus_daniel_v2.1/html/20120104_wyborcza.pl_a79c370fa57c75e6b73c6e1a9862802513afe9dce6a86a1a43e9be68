function xxFromHref(href){
        var wynik=new String(href);
        var indx1=wynik.indexOf(",");
        var indx2=0;
        if (indx1 > 0) {//
                wynik=wynik.substring(indx1+1);
                indx1=wynik.indexOf(",");
                if (indx1 > 0){
                        wynik=wynik.substring(indx1+1);
                        indx1=wynik.indexOf(",");
                        if (indx1 > 1){
                                wynik=wynik.substring(0,indx1);
                        }
                }
        }
        indx2=wynik.lastIndexOf(".");
        if (indx2 > 0) wynik=wynik.substring(0,indx2);
        return wynik;
}
