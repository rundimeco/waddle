<script type="text/javascript">
    function showPlayer() {
        
        $('#mediaspace').css('visibility', 'visible');
        $('#mediaspace').css('width', '618px');
        $('#mediaspace').css('height', '22px');
        

        if (plyAudioPlayer.view) {
            plyAudioPlayer.sendEvent('PLAY');
        } else {
            setTimeout(startPlayer, 100);
        }
    }

    function startPlayer() {
        try {
            plyAudioPlayer.sendEvent('PLAY');
        }
        catch (ex) { }
    }
    
    
</script>

