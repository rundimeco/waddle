 <script type="text/javascript">

  var login_destination;
  $(function() {
    check_user();
    var match = document.location.hash.match(/^#?login\?goto=(.*)$/ );
    if(match) { 
        login_destination = "/" + match[1];
	$('#nouserF').click(); 
    }
  }); 

  function onlogin_handler(user) {
    var callback = $("#popup-placeholder").data('onlogin');
    if(callback) { 
	callback(user);
	$("#popup-placeholder").data('onlogin', null);
    }	
  }

  function _openForm(form) { 
	if(!window.loaded_forms) window.loaded_forms={};
        var wform = window.loaded_forms[form];
	var place = $("#popup-placeholder");
	if( wform) {

	    place.html(wform);
	    if(window['initialize_' + form]) window['initialize_' + form](place);
	} else { 
/*
	    wajax('getHTML/' + form, {},'GET', {dataType:'html'}, function(r) { 
		window.loaded_forms[form] =  r;
		place.html(r);
		if(window['initialize_' + form]) window['initialize_' + form](place); 
	    });
*/
            wajax('getCTPP/' + form, {},'GET', {dataType:'html'}, function(r) {
                window.loaded_forms[form] =  r;                                
                place.html(r);                                                 
                if(window['initialize_' + form]) window['initialize_' + form](place); 
            });
	}
	return false;
  }



</script>


<script type="text/javascript">
  function user_logout() {
    wajax('logout',{},'POST',{}, function(r) {
        
                $('#nouserF').show();
                $('#userF').hide();
                $('#user').html('');
        
    });
    return false;
  }
</script>

