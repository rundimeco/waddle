			<script type="text/javascript">
function showModalBox() {

	jQuery('#dialogBox').empty();

	jQuery('#glassBox').fadeTo(400, 0.2);
	jQuery('#notLoggedInBox').clone().appendTo('#dialogBox');
	jQuery('#dialogBox').find('.noAccessLoginBox').attr('id',
			'notLoggedInBoxModal').find('.noAccessLoginBox_closeIcon').show();
	jQuery('#dialogBox').find('.closeBoxString').show();
	jQuery('#dialogBox').show().addClass('noAccessLoginBoxDialog')
			.css(
					'left',
					(document.body.clientWidth - jQuery('#dialogBox')
							.outerWidth()) / 2);

	jQuery('.noAccessLoginBox_closeIcon').bind('click', function(event) {
		jQuery('#dialogBox').hide().empty();
		jQuery('#glassBox').fadeOut(400);
		jQuery(".storyContent").find('p').show();
		jQuery('#dialogBox').removeClass('noAccessLoginBoxDialog');
		setCookie('closeId', '784593_76262');
		location.href = 'http://www.rp.pl/artykul/60513,784593-Koszykarze-AZS-Politechniki-walcza-o-przetrwanie.html';
	});
	jQuery('.closeBoxString').bind('click', function(event) {
		jQuery('#dialogBox').hide().empty();
		jQuery('#glassBox').fadeOut(400);
		jQuery(".storyContent").find('p').show();
		jQuery('#dialogBox').removeClass('noAccessLoginBoxDialog');
		setCookie('closeId', '784593_76262');
		location.href = 'http://www.rp.pl/artykul/60513,784593-Koszykarze-AZS-Politechniki-walcza-o-przetrwanie.html';
	});
}

function hideAndShow() {
	jQuery("#story").find('p').hide();
	jQuery("#story").find('p.lead').show();
	jQuery("#story").find('p:eq(1)').show();

}

jQuery(document).ready(
	function() {
	jQuery('.articleLoginInput').live('focus', function(event) {
		if(jQuery(this).val() == jQuery(this).attr("data-default")){
			jQuery(this).val("");
		}
	}).live('blur', function(event) {
		if(jQuery.trim(jQuery(this).val()) == ""){
			jQuery(this).val(jQuery(this).attr("data-default"));
		}
	});

	jQuery('.articleLoginInputPass').live(
			'focus',
			function() {
				if (jQuery(this).attr('type') == 'text') {
					this.type = "password";
				}
				if(jQuery(this).val() == jQuery(this).attr("data-default")){
					jQuery(this).val("");
				}
			}).live('blur', function(){
				if(jQuery.trim(jQuery(this).val()) == "" || jQuery.trim(jQuery(this).val()) == jQuery(this).attr("data-default")){
					this.type = "text";
					this.value = jQuery(this).attr("data-default");
				}
			});
			
	jQuery('.noAccessLoginBox_smsInput').bind('focus', function(event) {
		if(jQuery(this).val() == jQuery(this).attr("data-default")){
			jQuery(this).val("");
		}
	});
});
</script>
