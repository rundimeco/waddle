	  ref="su"
	></fb:like>
<img style="display: none;" src="http://gazeta.hit.gemius.pl/redot.gif?id=nF6a2AyFf_IJ3woQ3DRcKcewPxLw1u7SUeW3YDM9WRb.P7/stparam=rginixnsvh" border="0"></img>
