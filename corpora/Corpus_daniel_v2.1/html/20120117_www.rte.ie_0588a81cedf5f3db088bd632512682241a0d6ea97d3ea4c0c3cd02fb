      <div class="rte_gr_8">
        <div id="news-article-container">
          <h1>New fertility clinic opens in Dublin</h1>
          <p class="date">Updated: 13:52, Tuesday, 17 January 2012</p>
          <div id="user-options-top">

	<a class="print-view" href="javascript:void(window.open(''+self.location+'?view=print'));" title="Print View"></a>

	<div class="share-facebook-recommend">	
		<fb:send></fb:send>
	</div>

	<div class="share-facebook-recommend-wide">	
		<fb:like layout="button_count" show_faces="false" width="80" action="recommend" font="arial"></fb:like>
	</div>
	
	<div class="share-google-plusone">	
		<g:plusone size="medium"></g:plusone>
	</div>	

	
	<a href="http://twitter.com/share" class="twitter-share-button" data-count="horizontal" data-via="rtenews">Tweet</a><script type="text/javascript" src="http://platform.twitter.com/widgets.js"></script>	


	<div class="share-linkedin">	
		<!--script type="text/javascript" src="http://platform.linkedin.com/in.js"></script><script type="in/share" data-counter="right"></script-->		
	</div>
	
	<div class="clear"></div>
		
