			WP.$('#navimap-coords area, .listaSt li a').each(function(){
			WP.$(this)
				.mouseenter(function(){
					var w = WP.$(this).attr('alt');
					WP.$(".navi-"+w).animate({opacity:1},{queue:false});
					WP.$(".navilink-"+w).css({textDecoration:'underline'});
				})
				.mouseleave(function(){
					var w = WP.$(this).attr('alt');
					WP.$(".navi-"+w).animate({opacity:0},{queue:false});
					WP.$(".navilink-"+w).css({textDecoration:'none'});
				})
			});
