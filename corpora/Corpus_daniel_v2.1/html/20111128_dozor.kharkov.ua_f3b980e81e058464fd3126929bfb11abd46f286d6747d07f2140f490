function PostRating(r)
{
    document.getElementById('star-rating').innerHTML = '<img src="http://dozor.kharkov.ua/images/spinner_mac.gif" border="0px" alt="">';
    $.post(
        'http://dozor.kharkov.ua/__ratings/',
        { clas: 'documents', id: '1101365', compid: GetClientID(), rating: r },
        function(data)
        {
            val = Math.round(((0+parseInt(data))/1)*20);
            document.getElementById('ratings').innerHTML =
            '<ul class="star-rating">'+
                '<li class="current-rating" style="width: '+val+'%;"></li>'+
            '</ul>'+
            '<p style="float: left; margin: 0; padding: 8px 5px 0 0; color: #666">'+
                '<b>Рейтинг:<b> '+val/20+' (оценок: 1)'+
            '</p>';
        }
    );
    return false;
}
</script>

