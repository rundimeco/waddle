                        <script type="text/javascript">
                            function AjaxCalendar(year, month)
                            {
                                if ( month == 0 ) { month = 12; year--; }
                                else if ( month == 13 ) { month = 1; year++; }
                                $.ajax({
                                    type: "GET",
                                    url: 'http://dozor.kharkov.ua/ajaxcalendar.html?calendar_year='+year+'&calendar_month='+month,
                                    dataType: "html",
                                    success: function(data) { document.getElementById('calBody').innerHTML = data;}
                                });
                                return false;
                            }
                        </script>
