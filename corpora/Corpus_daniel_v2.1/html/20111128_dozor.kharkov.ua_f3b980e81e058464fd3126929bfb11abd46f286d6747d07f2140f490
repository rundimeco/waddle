<script type="text/javascript">
    var ustamp = 1322492423*1000;
    var months = Array('Января','Февраля','Марта','Апреля','Мая','Июня','Июля','Августа','Сентября','Октября','Ноября','Декабря');
    var day = Array('Воскресенье','Понедельник','Вторник','Среда','Четверг','Пятница','Суббота');
    function Timer()
    {
        var mydate = new Date(ustamp);
        var date_str = mydate.getDate()+' '+months[mydate.getMonth()]+', '+day[mydate.getDay()]+', '+mydate.toLocaleTimeString();
        document.getElementById('date').innerHTML = date_str;
        ustamp += 1000;
    }
    function DateTimer()
    {
        timerId = setInterval('Timer()', 1000);
    }

    $('date').ready(function() { DateTimer(); });
</script>

