                           <script type="text/javascript">
                                        var cur_menu = '1';
                    var base_menu = '1';
                    var red_menu = 65;
                    var tid = '';

                    function SetMenuTimeout(id)
                    {
                        tid = id;
                        setTimeout( function(id) { return function(){ ShowMenu(id); } }(id), 500);
                        return false;
                    }

                    function ShowMenu(id)
                    {
                        if ( tid != id ) return false;
                        if ( cur_menu != '' ) document.getElementById('menu_'+cur_menu).childNodes[0].childNodes[0].className = base_menu == cur_menu ? 'ac' : ( red_menu == cur_menu ? 'red-ac' : '');
                        if ( cur_menu != '' && document.getElementById('sub'+cur_menu) != undefined ) document.getElementById('sub'+cur_menu).style.display = 'none';
                        if ( document.getElementById('sub'+id) != undefined ) document.getElementById('sub'+id).style.display = 'block';
                        cur_menu = id;
                        return false;
                    }

                    function HideMenu(id)
                    {
                        //document.getElementById('sub'+id).style.display = 'none';
                        document.getElementById('menu_'+cur_menu).childNodes[0].childNodes[0].className = 'menuac';
                        tid = '';
                        return false;
                    }

                </script>
