<script type="text/javascript">

function GetRandomString(l)
{
    var chars = "0123456789ABCDEFGHIJKLMNOPQRSTUVWXTZabcdefghiklmnopqrstuvwxyz";
    var rstr = '';
    for (var i=0; i < l; i++)
    {
        var rnum = Math.floor(Math.random() * chars.length);
        rstr += chars.substring(rnum, rnum+1);
    }
    return rstr;
}

function PostResult(result)
{
    document.getElementById('antibot').src = 'http://dozor.kharkov.ua/__antibot/?p=documents&sign='+GetRandomString(16);
    document.forms['postcomment'].antibot.value = '';
    if ( result == 'BOT' )
    {
        alert('Неверный код. Введите шестизначный код, указанный на изображении слева');
        document.forms['postcomment'].antibot.focus();
        return false;
    }
    if ( result != 'OK' )
    {
        alert('Невозможно добавить коментарий. Попробуйте позже.');
        return false;
    }
    alert('Ваш коментарий успешно добавлен');
    LoadComments();
    return false;
}

function PostComment()
{
    f = document.forms['postcomment'];
    if ( f.author.value == '' ) { f.author.focus(); return false }
    if ( f.body.value == '' ) { f.body.focus(); return false }
    $.post(
        'http://dozor.kharkov.ua/__comments/',
        { clas: f.clas.value, id: f.id.value, author: f.author.value, body: f.body.value, antibot: f.antibot.value },
        function(data){ PostResult(data); }
    );
    return false;
}

function LoadComments(num)
{
    $.ajax({
        type: "GET",
        url: 'http://dozor.kharkov.ua/__comments/?clas=documents&id=1101365&page='+num,
        dataType: "html",
        success: function(data) { document.getElementById('comments').innerHTML = data; document.getElementById('antibot').src = 'http://dozor.kharkov.ua/__antibot/?p=documents&sign='+GetRandomString(16); }
    });
}
function ToggleComments()
{
    id = document.getElementById('comments');
    id.style.display = id.style.display == 'none' ? 'block' : 'none';
    return false;
}

$(document).ready(function(){ LoadComments(0); });

</script>


