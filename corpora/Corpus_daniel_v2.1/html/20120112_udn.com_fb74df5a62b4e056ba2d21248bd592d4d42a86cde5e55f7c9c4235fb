<script>
function getByteLength(str){
  var iCount = 0;
  for( k=0; k<str.length; k++ ) {
    c = str.charCodeAt(k);
    if ( c>=32 && c<=126 ) iCount++;
    else iCount += 2;
  }
  return iCount;
}

function getSubString(str,maxwclength) {
  var iCount = 0;

  for( k=0; k<str.length; k++ ) {
    c = str.charCodeAt(k);
    if ( c>=32 && c<=126 ) iCount++;
    else iCount += 2;

    if (iCount > maxwclength) {
      str = str.substr(0,k) + "...";
      break;
    }
  }

  return str;
}
</script>

