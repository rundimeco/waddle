<script type="text/JavaScript">
//非法字符过滤( or---->或,not_----->非,and ----->与 )

// 过滤空格，实现java中的trim功能：
String.prototype.trim=function(){
	return this.replace(/(^\s*)|(\s*$)/g, "");
}
function judgeValue(SearchKey,isValid){       
	var reExpression=/(%|\*|\+|\=|\-|\/|\?|\.|\。|\&|\:|\：)+/g;		
	var x=reExpression.test(SearchKey);
	if(isValid && x) {
		alert("检索词中含有非法字符!");
		$('#key').val('');
	}
	return x;
}

function submitSearchContentIt() {
	var keyWords = $("#key").val();
	// 去掉前后的空格：
	keyWords = keyWords.trim();
	if(keyWords=="") {
		showDialog(99);
		return false;
	}
	if(judgeValue(keyWords)) {
		showDialog(997);
		$('#keyWords').val('');
		return false;
	}
	var searchwordTemp="";

	var keywordsValueArray = keyWords.split(" ");
	var sTemp="";
	var element = "";
	var j = 0;
	for(var i=0;i<keywordsValueArray.length;i++) {
		element = keywordsValueArray[i];
		if(element=="" || element=="not" || element=="or" || element=="and" || element=="in"){
			continue;
		}
		j++;
		if(j==1){
			sTemp = element;
			continue;
		}
		sTemp = sTemp + " and " + element;
	}
	searchwordTemp = " doctitle =("+sTemp+") or contents =("+sTemp+") ";

	var searchForm = document.getElementById("searchContentForm");
	document.getElementById("word").value = searchwordTemp;
	document.getElementById("id").value = "1121";
	searchForm.action = "http://find.jfdaily.com/search_utf.asp";
	searchForm.submit();
}
</script>
<form id="searchContentForm" action="http://find.jfdaily.com/search_utf.asp" target="_blank" method="get" onsubmit="submitSearchContentIt();return false;"><input type="hidden" name="validateRequest" value="ecf465e1ec68bb5a5895e380f9ebdff6">
<input type="hidden" name="searchword" id="word"/>
<input type="hidden" name="channelid" id="id"/>
