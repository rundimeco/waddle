else if( sType==("live")) {result = 'https://favorites.live.com/quickadd.aspx?mkt=zh-tw&url='+sURL+'&title='+sTitle+'&top=1&wa=wsignin1.0';}
else if( sType==("addthis")) {result = 'http://www.addthis.com/bookmark.php?pub=addthis&url='+sURL+'&title='+sTitle;}
