{
	 rekeywd = "<a href='/searchresult.asp?mod=ysm&q="+ escape(keywd[i]) +"' class='ysmkw' title='搜尋關於 ["+ keywd[i] + "]' target='ysm'>" + keywd[i] + "</a>";
	 tmp_cont = replaceSubstring(tmp_cont,keywd[i],rekeywd,10);
 }
document.getElementById("ysm").innerHTML = tmp_cont;
