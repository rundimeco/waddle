  document.onkeydown = function(e)
  {
	var e = window.event || e;

	if (e.ctrlKey && e.keyCode == 13)
	{
		var str = GetSelectedText();

		if (str != "")
		{
			var doch = $(window).height();
			var docw = $(window).width();
			var fromtop = doch/2;
			fromtop = parseInt(fromtop);
			var opechatkaH = parseInt($('#opechatka').height()/2);
			fromtop -= opechatkaH;

			var fromleft = docw/2;
			fromleft = parseInt(fromleft);
			var opechatkaW = parseInt($('#opechatka').width()/2);
			fromleft -= opechatkaW;

		    var scrlTop = $(window).scrollTop();
			fromtop += scrlTop;

		    $("#opechatka").css('top', fromtop+'px');
		    $("#opechatka").css('left', fromleft+'px');

			str = str.substr(0, 200);

			$("#opechatkaText").val(str);
			$("#selection").text(str);
			$("#opechatka").show();
		}

	}
   }

function GetSelectedText()
{
	var selectedtext = '';

	if (window.getSelection)
	{
		var range = window.getSelection ();                                        
		selectedtext = range.toString();
	}else if (document.selection.createRange)
	{
		var range = document.selection.createRange ();
		selectedtext = range.text;
	}

	return selectedtext;
}

function sendOpechatka()
{
	$('#opechatkaT').hide();

	$('#opechatkaMsgs').html('<div style="text-align:center;"><img src="/i/loading.gif" width="32" height="32" alt="loading" style="margin:40px auto 15px auto;"></div>');
	$('#opechatkaMsgs').show();

	var opechatkaText = $('#opechatkaText').val();
	var comments = $('#opechatkaComments').val();

	$.ajax(
	{
		type: "POST",
		url: "/opechatka.php",
		data: "sec_id=4&item_id=20111128061836438&opechatkaText="+opechatkaText+"&comments="+comments,
		success: function(msg)
		{
			if (msg) $('#opechatkaMsgs').html('<div style="text-align:center; font-size:12px; padding:20px;">Информация отослана. Спасибо.</div>');
			else $('#opechatkaMsgs').html('<div style="text-align:center; font-size:12px; padding:20px;">Возникла ошибка.</div>');
		}
	});

	$('#opechatkaComments').val('');
	return false;
}

function wordwrap( str, width, brk, cut )
{
	brk = brk || '\n';
	width = width || 75;
    cut = cut || false;
 
    if (!str) { return str; }
 
    var regex = '.{1,' +width+ '}(</p>|$)' + (cut ? '|.{' +width+ '}|.+$' : '|\\S+?(</p>|$)');

	return str.match( RegExp(regex, 'gi') );//.join( brk );
}

var symbolsNotWrapped = 6000;
var symbolsMiddleWrapped = 8500;
var symbolsLastPage = 500;
var symbolsperpage = 4000;
var pages = 0;
var curPage = 0;
var content = '';
var animate = false;

function rebuildContent()
{
	content = content.replace(/\n/g, "");
	content2 = content.replace(/<object .+<\/object>/gi, "");

	var conLen = content2.length;
//	var conLen = content.length;
//	console.log(conLen);

	var Opera = /opera/i.test(navigator.userAgent);
	if (Opera) conLen = 0;

	if (conLen <= symbolsNotWrapped)
	{
	}
	else if (conLen > symbolsNotWrapped && conLen < symbolsMiddleWrapped)
	{
		pages = 2;

		textParts = wordwrap(content, symbolsperpage, "%%", false);
//		textParts = contentWrapped.split("%%");

		if (curPage == 0) $('#content').html(textParts[curPage]);
		else 
		{
			var text = textParts[curPage];
			if (textParts[curPage+1] != undefined)
			{
				text += textParts[curPage+1];
			}
			$('#content').html(text);
		}
		
		if (animate) $('html,body').animate({scrollTop: $("#artc_title").offset().top}, 400);

		pagesHtml = '';
		for (i=1;i<=pages ;++i )
		{
			if (i == (curPage+1))
			{
				pagesHtml += '<strong>'+i+'</strong>';
			}else
			{
				pagesHtml += '<a href="#page'+i+'" onClick="changePage('+i+');">'+i+'</a>';
			}
		}
		$('#pagesHolder').show();
		$('#pages').html(pagesHtml);

		curPage = parseInt(curPage);
		if (curPage > 0) $('#pageofpage').html('Страница '+(curPage+1)+' из '+pages).css('margin-bottom', '10px');
		else $('#pageofpage').html('').css('margin-bottom', '0');

	}else
	{
		textParts = wordwrap(content, symbolsperpage, "%%", false); 		
//		textParts = contentWrapped.split("%%");
		textPartsLen = textParts.length;
		
		pages = textPartsLen;

		var lastPageText = '';
		if (textParts[pages-1].length < 500)
		{
			pages -= 1;
			if (curPage == pages-1)
			{
				lastPageText = textParts[pages];
			}
		}

		$('#content').html(textParts[curPage]+lastPageText);

		if (animate) $('html,body').animate({scrollTop: $("#artc_title").offset().top}, 400);

		pagesHtml = '';
		for (i=1;i<=pages ;++i )
		{
			if (i == (curPage+1))
			{
				pagesHtml += '<strong>'+i+'</strong>';
			}else
			{
				pagesHtml += '<a href="#page'+i+'" onClick="changePage('+i+');">'+i+'</a>';
			}
		}
		$('#pagesHolder').show();
		$('#pages').html(pagesHtml);

		curPage = parseInt(curPage);
		if (curPage > 0) $('#pageofpage').html('Страница '+(curPage+1)+' из '+pages).css('margin-bottom', '10px');
		else $('#pageofpage').html('').css('margin-bottom', '0');
	}
}

function changePage(pageN)
{
	curPage = pageN-1;
	animate = true;
	rebuildContent();
}

function addLink()
{
	var body_element = document.getElementsByTagName('body')[0];

	var pagelink = "<br><br> Подробности: <a href='"+location.href+"'>"+location.href+"</a><br>Любое использование материалов допускается только при наличии гиперссылки на 1news.az";

	var selection;
	if (Browser.Version() <= 9)
	{
		selection = document.selection.createRange();
		var copytext = selection.htmlText + pagelink;
	}else
	{
		selection = window.getSelection();
		var copytext = selection + pagelink;
	}

	var newdiv = document.createElement('div');
	var newdiv2 = document.createElement('div');

	newdiv.style.position='absolute';
	newdiv.style.left='-99999px';
	newdiv.style.backgroundColor='white';
	newdiv.style.color='black';
	newdiv.style.textAlign='left';
	newdiv.style.fontSize='12px';

	newdiv2.style.position='absolute';
	newdiv2.style.left='-99999px';

	document.body.insertBefore(newdiv2, document.body.firstChild);

	newdiv.innerHTML= copytext;

	newdiv2.appendChild(document.createElement("br"));
	newdiv2.appendChild(newdiv);

	if (Browser.Version() <= 9)
	{
		pagelink=document.body.createTextRange();
		pagelink.moveToElementText(newdiv);
		pagelink.select();
	}else
	{
		selection.selectAllChildren(newdiv);
	}

//	setTimeout(function() {body_element.removeChild(newdiv);},0);
}

var Browser = {
  Version: function() {
    var version = 999; // we assume a sane browser
    if (navigator.appVersion.indexOf("MSIE") != -1)
      // bah, IE again, lets downgrade version number
      version = parseFloat(navigator.appVersion.split("MSIE")[1]);
    return version;
  }
}

