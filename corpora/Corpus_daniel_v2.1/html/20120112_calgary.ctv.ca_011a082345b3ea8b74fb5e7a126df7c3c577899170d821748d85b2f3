<script>
var node = document.getElementById("mainTableLeft");
var els = node.getElementsByTagName('div');
var elsLen = els.length;
var topImage;
var foundImage = false;
var imageString = "http://images.ctv.ca/archives/CTVNews/img2/20060901/110_ctv_Toronto_060901.jpg"
