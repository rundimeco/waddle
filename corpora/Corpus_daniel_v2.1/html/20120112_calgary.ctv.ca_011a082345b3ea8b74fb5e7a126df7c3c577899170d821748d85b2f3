    if (els[i].className == "MostPopularBlockTD")
    {
        var ul = els[i].getElementsByTagName('a');
        if (ul.length >0 )
        {
        //var nodeli = ul[0].getElementsByTagName('a')[0];
        //videoString = "none";
        //videohref = nodeli.getAttribute('href').replace(/&/g, "@amp@");
        var newS = ul[0].getAttribute('href');
        videohref = newS.replace(/&/g, "@amp@");
        }
    }
}

</script>


<script>
document.write("<img src=\"http://www.ctv.ca/servlet/HTMLTemplate?tf=/ctv/mar/etracker/etracker.html&cf=ctv/mar/ctv.cfg&uniqueID=CGY_pmma_deaths_120112&typeStat=TO&displayText=Five drug deaths linked to PMMA||Calgary police are warning the public about a lethal form of the street drug Ecstasy being distributed which is laced with a chemical five times more toxic than the drug itself.||" + imageString + "||" + videoString + "||" + videohref + "&longURL=" + window.location + "&io=2316496\" height=\"1\" width=\"1\" />");
</script>






