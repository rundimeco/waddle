<script language="javascript">
function findit()
{
	var searchTerm = document.getElementById("searchText");
	if (searchTerm.value != null && searchTerm.value != "e.g. plumbers")
	{
		var townName = "carlisle";
		var pcode = "CA1";
		var matchedid = 34;
		if ((window.location.href.indexOf("newsandstar") != -1) || (window.location.href.indexOf("Cumberland") != -1))
		{
			townName = "carlisle";
			pcode = "CA1";
			matchedid = 34;
		}
		if (window.location.href.indexOf("timesandstar") != -1)
		{
			townName = "workington";
			pcode = "CA14";
			matchedid = 141;	
		}
		if (window.location.href.indexOf("nwemail") != -1)
		{
			townName = "barrow";
			pcode = "LA14";
			matchedid = 14;	
		}
		if (window.location.href.indexOf("whitehaven") != -1)
		{
			townName = "whitehaven";
			pcode = "CA28";
			matchedid = 138;	
		}


		newloc = "?type=" + encodeURI(searchTerm.value) + "&name=&town=" + townName + "&location=" + pcode + "&dist=30&sort=0&siteID=0&matchedTown=" + matchedid;
		window.location = "http://www.clickin2findit.co.uk/search.aspx"+newloc;
		return true;
	}
}

function handleKeyPress(e){
var key=e.keyCode || e.which;
if (key==13){
findit();
}
}

function searchBoxClicked()
{
	var searchTerm = document.getElementById("searchText");
	searchTerm.style.color="black";
	if (searchTerm.value == "e.g. plumbers")
		searchTerm.value = "";
}
</script>
