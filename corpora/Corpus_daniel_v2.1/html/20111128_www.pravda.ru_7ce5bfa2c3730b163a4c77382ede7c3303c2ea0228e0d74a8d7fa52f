    function getAuthPopup() {
        if (!Cookie.get('popupAuth')) {
            Cookie.set('popupAuth', 1, 10, '/', '.pravda.ru');
            facebox.reveal('<div id="auth_container"></div>');
            widgets.auth({
                container: "auth_container",
                siteId: 1
            })
        }
    }

    addLoadEvent(
        function() {
            setTimeout('getAuthPopup()', 10000);
        }
    );
</script>
<script type="text/javascript" src="http://www.google.com/friendconnect/script/friendconnect.js"></script>
</body>
</html>

