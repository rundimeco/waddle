        var totalString = "ctvnews : " + chec(s1) + chec(s2) + chec(s3) + pnn.toLowerCase();
        Tracking.SetPageName(totalString.toLowerCase());
        Tracking.SetSubSections(cap(s1), cap(s2), cap(s3));
        Tracking.TrackSearch(document.getElementById("searchText").innerHTML, 0);
        Tracking.Track();    
    }
	else if (locBar.indexOf("/search") != -1)
    {
        Tracking.SetSiteSection( "ctvhome" );
        var s1 = "searchctv_ca";
        var s2 = "hub";
        var s3 = null;
        var pnn = "searchctv_ca";//document.getElementById("searchText").innerHTML;
        var totalString = "ctvhome : " + chec(s1) + chec(s2) + chec(s3) + pnn.toLowerCase();
        Tracking.SetPageName(totalString.toLowerCase());
        Tracking.SetSubSections(cap(s1), cap(s2), cap(s3));
        Tracking.TrackSearch(document.getElementById("searchText").innerHTML, 0);
        Tracking.Track();    
    }
    else if (locBar.indexOf("www.tsnn.ca/ctvnews") != -1 || locBar.indexOf("www.tsn.ca/ctvnews") != -1)
    {

    }    
    else
    {

    }
}
	
	
	
	
</script>
