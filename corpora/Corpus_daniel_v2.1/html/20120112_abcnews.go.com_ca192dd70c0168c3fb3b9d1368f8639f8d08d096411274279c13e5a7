	<script type="text/javascript">

	carouselManager2 = createCarouselManager('video-carousel-content2');
	carouselManager2.activeClass = "jumpCircleItemOn";
	carouselManager2.inactiveClass = "jumpCircleItemOff";
	carouselList2 = carouselManager2.playlist;
	

