	carouselPane1.addMouseOut(function(){carouselPane1.disengage()});
	carouselPane1.addEngage(function(){carouselList1.pause()});
	carouselPane1.addDisengage(function(){carouselList1.resume()});

	var carouselQuickAnim = new Animation();

	var carouselLeftAnim1 = new Animation();

	//old moving left
	var carLeftOldSlide = new Sequence();
	carLeftOldSlide.start = 0;
	carLeftOldSlide.end = -962;
	carLeftOldSlide.duration = 1000;
	carLeftOldSlide.min=0.5;
	carLeftOldSlide.ease = Easing['easeBothStrong'];
	carLeftOldSlide.callback = function(){
		carouselPane1.oldContent.style.left = "-962px";
	};
	carLeftOldSlide.handler = function(val){
		val = Math.round(val);
		carouselPane1.oldContent.style.left = val+"px";
		return val;
	};
	carouselLeftAnim1.addSequence(0,carLeftOldSlide);

	//new moving left
	var carLeftSlide = new Sequence();
carLeftSlide.start = 962;
	carLeftSlide.end = 0;
	carLeftSlide.duration = 1000;
	carLeftSlide.min=0.5;
	carLeftSlide.ease = Easing['easeBothStrong'];
	carLeftSlide.init = function(){
		if(carouselPane1.content){
			carouselPane1.content.style.left="962px";
		}	
		if(carouselPane1.content && carouselPane1.content.parentNode != carouselPane1.viewport){
			carouselPane1.viewport.appendChild(carouselPane1.content);
	};
};
	carLeftSlide.handler = function(val){
		val = Math.round(val);
		carouselPane1.content.style.left = val+"px";
		return val;
	};
	carLeftSlide.callback = function(){
		carouselPane1.content.style.left = "0px";
	}
	carouselLeftAnim1.addSequence(0,carLeftSlide);

	var carouselRightAnim1 = new Animation();

	//old moving right
	var carRightOldSlide = new Sequence();
	carRightOldSlide.start = 0;
carRightOldSlide.end = 962;
	carRightOldSlide.duration = 1000;
	carRightOldSlide.min=0.5;
