    <script>
      function clear_comment_info_msg() {
        var comment_body_element = document.getElementById('comment_body');
        var comment_body = comment_body_element.value;
        comment_body_element.value = comment_body.replace(/^Maximum\sof\s1750\scharacters\s\(about\s300\swords\)/, '');
      }
      function check_comment_length() {
        var comment_body_element = document.getElementById('comment_body');
        var comment_body = comment_body_element.value;
        if (comment_body.length > 1750) {
          alert('Your comment must not exceed 1750 characters.');
          comment_body_element.value = comment_body.substr(0, 1750);
        }
      }
      function validate_comment() {
          var chkd = document.getElementById('tnc_acceptance').checked;
          if (!chkd) {
              alert('You must accept the terms and conditions to post a comment.'); return false;
          }
          if (!document.getElementById('poster_name').value) {
              alert('You must enter a name to post a comment.'); return false;
          }
          if (!Account.logged_in) {
            if (!document.getElementById('poster_email').value.match(/^['A-Z0-9._%+-]+@[A-Z0-9.-]+\.[A-Z]{2,}$/i)) {
                alert('You have not entered a valid email address. Please enter a valid address to post a comment.'); return false;
            }
          }
          clear_comment_info_msg();
          if (!document.getElementById('comment_body').value.match(/[\w]+/)) {
              alert('You must enter a comment to post.'); return false;
          }
          return true;
      }
    </script>

