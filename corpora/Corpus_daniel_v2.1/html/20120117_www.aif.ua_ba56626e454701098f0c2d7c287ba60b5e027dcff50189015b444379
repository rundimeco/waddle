	user_name: {required: true, minlength: 3},
	user_email: {email: true},
	comments_inp: {required: true, minlength: 5, maxlength: 2500}
};

var afValidationMessages = {
	user_name: {
		required: "Укажите Ваш псевдоним или логин",
		minlength: "Минимальная длина поля должна быть не менее 3 символа"
