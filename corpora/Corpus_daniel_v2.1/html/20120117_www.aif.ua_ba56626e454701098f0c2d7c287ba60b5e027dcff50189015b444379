	}
};

function afSuccess(json)
{
	if( json.result == 'ok')
	{
		$('textarea[name=comments_inp]').each(function(){
			$(this).get(0).value = '';
		});

		$('#comments').html(json.content);

       $('#c_content_2').load('http://www.aif.ua/aforism/?url='+document.URL+'&'+Math.random());
	} else {
		SuccessFailedAF(json);
	}

	setSubmitAF();
}
//-->

function getRulesAF()
{
showRule = true;
var rules = new Array(
	"В комментариях на AIF.UA запрещены нецензурные выражения. Будьте умны и терпимы к чужому мнению. Нам всем очень не хватает позитива!",
	"Напоминаем, что на сайте AIF.UA выработан спокойный уважительный тон общения. Комментарии, содержащие более 1000 знаков, будут обрезаны - все желающие смогут их прочитать полностью, нажав ссылку ''продолжение комментария''.",
	"Язык комментариев на AIF.UA – русский или украинский. Если у Вас нет кириллической раскладки, пользуйтесь транслитом",
	"В комментариях на сайте AIF.UA не принято оскорблять оппонентов. Комментарии с бранью удаляются. Если Вам неприятен человек, чье мнение Вы не разделяете, будьте добры, обращайтесь к нему вежливо, например: «Уважаемый»!",
	"На сайте AIF.UA удаляются оскорбительные по отношению к разным национальностям выражения. Комментарии, их содержащие, как правило, не редактируются, а удаляются полностью.",
	"На сайте AIF.UA у всех одинаковые возможности высказать свое мнение. Будьте добры, не пишите в комментариях фразы ПРОПИСНЫМИ БУКВАМИ. КОММЕНТАРИИ, НАПИСАННЫЕ НА СAPS LOCK, БУДУТ УДАЛЕНЫ"
);

var res = rules[Math.floor(Math.random() * rules.length)];
return res + '...';
}

</script>
<script type="text/javascript">
function SuccessGrantedAF(json) {
	if (json.content != null) {
		if ($.cookie('auth_referer') !== null) { // see user recupes for example
			var ref = $.cookie('auth_referer').split('|')
			window.location.replace(ref[0]);
		} else {
			$(afId).html(json.content);
		}
	} else {
		$(afId).html(json.result);
	}
}

$(document).ready(function()
{
	setValidatorAF();
});
</script>
