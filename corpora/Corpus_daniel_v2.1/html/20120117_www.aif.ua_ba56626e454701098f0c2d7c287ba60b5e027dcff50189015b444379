<script>

function vote( id )
  {


$.cookie('voted',$.cookie('voted')+','+id+',',{
  expires: 7,
  path: "/"
});




//alert("vote: " + id);


	var has_answer = false;
    var answer='';


if (document.voteForm.radiocount.value==0) { 


myRadio = eval('document.voteForm.radio'+id);	

	for(var i = 0; i < myRadio.length; i++) {
		if(myRadio[i].checked) {
			answer = myRadio[i].value;
			has_answer = true;
		}
	}
	
} else {


 	for(var i = 1; i <= document.voteForm.radiocount.value; i++) {

 	myRadio = eval('document.voteForm.radio'+i);

  		if(myRadio.checked>0) {

if (answer!='')  {answer=answer+','};

//alert(myRadio.value);
			
answer = answer + myRadio.value;
			has_answer = true;
		}
	}
}

	if (! has_answer ) return false;
	//return false;
	var url = '/vote/vote/vote_id/' + id + '/answer/' + answer ;
	
	jQuery('div#vote000').load(url);

//alert(url);

	

	return false;
  }


var uservoted = ''+$.cookie('voted');
var activevotes=new Array();
var randomvotes=new Array();
activevotes[0]=127;activevotes[1]=126;activevotes[2]=125;activevotes[3]=124;activevotes[4]=120;var active_total=5;var v_id=0;

var randcount = 0;

for (aa in activevotes) {
vv_id=activevotes[aa];
var myRegExp = ','+activevotes[aa]+',';
var matchPos1 = uservoted.search(myRegExp);

if(matchPos1 == -1) {
     randcount++;
     randomvotes[randcount]=activevotes[aa];	}

};


var lastvote = 1*$.cookie('lastvote');
if (lastvote > 0) {} else {lastvote=0};

if (randcount>0) {

if (lastvote >= randcount) {lastvote=0};
lastvote++;
$.cookie('lastvote',lastvote);

v_id=randomvotes[lastvote];

} else {

if (lastvote >= active_total) {lastvote=0};
lastvote++;
$.cookie('lastvote',lastvote);

v_id=activevotes[lastvote-1];

};

if (v_id==0) {v_id=vv_id};


</script>

