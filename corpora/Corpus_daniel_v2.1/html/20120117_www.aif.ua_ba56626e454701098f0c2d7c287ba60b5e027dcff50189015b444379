<script type="text/javascript">
function limitChars(textid, limit, infodiv)
{
	var text = $('#'+textid).val();
	var textlength = text.length;
	if(textlength > limit)
	{
		$('#' + infodiv).html('Достигнуто ограничение в '+limit+' символов!');
		$('#'+textid).val(text.substr(0,limit));
		return false;
	}
	else
	{
		$('#' + infodiv).html('Осталось символов: '+ (limit - textlength) + '.');
		return true;
	}
}

$(document).ready(function() {

 	$('#comments_inp').keyup(function(){
 		limitChars('comments_inp', 2500, 'charlimitinfo');
 	})

	var login = $.cookie('login');
	var hash = $.cookie('hash');

	if (login == null)
	{
		// BC with old auth!
		if ( hash != null ) {
			$.post('/ajax/setlogin', function(data) {$('#user_name').val(data.result); $('#user_name').attr('readonly', true); $('#login_msg').html('Зарегистрированный пользователь:');});
			$('#form_email').hide();
			$('#auth_msg').hide();
			var heightAF = 385;
		} else {
			setCookie('comments', 'f7c02944a6cc36583f3892e1518ca020', {expires: 7, path: '/', domain: 'www.aif.ua', secure: false});
			$('#user_msg').hide();
			$('#user_name').val('');

			var heightAF = 460;
		}

	} else {
		$('#form_email').hide();
		$('#auth_msg').hide();
		$('#user_name').val(login);
		$('#user_name').attr('readonly', true);
		$('#login_msg').html('Зарегистрированный пользователь:');
		var heightAF = 385;
	}

	$('#comments_inp').val(getRulesAF());
	$('#comments_inp').css('color', '#999999');

	$('#tabs-block').height(heightAF);
	$('#tabs-block').show();


	$('#comments_inp').focus(function()
	{
		//alert(showRule);
		if (showRule) {
			$('#comments_inp').css('color', '#000000');
			$('#comments_inp').val('');
			showRule = false;
		}
	});

});




//--< AjaxForm setup - see forms.js
