	}
	SoForm.submit();
	return true;
}
            //点击复制
            var EndPage = function(){
            }
            if (NTES("#h1title")) {
				var getTitle = NTES("#h1title").innerHTML.replace(/["']+/g,'');
			}
				else {
				// alert ("获取标题失败");
			}
                EndPage.clearField = function(){
                if (NTES("#inputWord").value == EndPage.tip) {
                NTES("#inputWord").value = "";
                }
            }
            EndPage.mail = function(){
                try {
                    var RightMailAgent = new ActiveXObject("FlashMailShell.FlashMailIEExt");
                } 
                catch (e) {
                    window.open("http://fm.163.com/commend.html?tab=5?yjzf100201", "")
                }
                var str = NTES("#endText").innerHTML;
                str = str.replace(/<\/?(?:embed|object|param)[^>]*>/ig, "");
				str += "<br /><br />";
				str += '“你也想和好友分享精彩新闻吗?请\<a href=\"http:\/\/dl.163.com\/NxMail\/Update\/installer\/flashmail-2.0beta.exe\"\>下载闪电邮\<\/a\>,并阅读\<a href=\"http:\/\/fm.163.com\/introduce.html?yjzf100201\"\>闪电邮介绍\<\/a\>”';
				
				if (RightMailAgent != null) {
                    RightMailAgent.addSubject("您的好友推荐文章《"+getTitle+"》");
                    RightMailAgent.addContent(str);
                }
            }
            EndPage.copyUrl = function(){
                var cbc = "";
                cbc += getTitle +'\r\n' + document.URL;
                window.clipboardData.setData("Text", cbc);
                NTES("#IECopyUrl").innerHTML = "<span class=\"cDRed\">已复制</span>";
            }
           
		   //初始化 
            if (NTES.browser.msie) {
                NTES("#IEHtml").innerHTML = "\<span class=\"right cDGray\"\>\<span class=\"copyLink\" id=\"IECopyUrl\" title=\"点击复制标题和链接\" style=\"background: url(http:\/\/img1.cache.netease.com\/cnews\/css09\/bg_n1.gif\) no-repeat scroll left -362px transparent; padding: 2px 0pt 2px 23px;\"\>\<a style=\"cursor:pointer; text-decoration:none;\"\>复制网址\<\/a\>\<\/span\> \<a href=\"mailto:\" class=\"mail\" id=\"IEMailSend\" title=\"你也想和好友分享精彩新闻吗?请下载闪电邮(点击直接可下载)\" style=\"background: url(http:\/\/img1.cache.netease.com\/cnews\/css09\/bg_n1.gif) no-repeat scroll -154px -363px transparent; padding: 2px 0pt 2px 23px;\"\>闪电邮转发\<\/a\>\<\/span\>";
                NTES("#IEMailSend").addEvent("click", function(e){
                    EndPage.mail();
                    e.preventDefault();
                });
                NTES("#IECopyUrl").addEvent("click", function(e){
                    EndPage.copyUrl();
                    e.preventDefault();
                });
            }
            //]]>
