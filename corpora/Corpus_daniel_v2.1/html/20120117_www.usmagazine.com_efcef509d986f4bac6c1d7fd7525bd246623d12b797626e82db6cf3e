(function(){
  function loadChartbeat() {
    window._sf_endpt=(new Date()).getTime();
   var e = document.createElement('script');
    e.setAttribute('language', 'javascript');
    e.setAttribute('type', 'text/javascript');
    e.setAttribute('src',
       (("https:" == document.location.protocol) ? "https://a248.e.akamai.net/chartbeat.download.akamai.com/102508/" : "http://static.chartbeat.com/") +
       "js/chartbeat.js");
    document.body.appendChild(e);
  }
  var oldonload = window.onload;
  window.onload = (typeof window.onload != 'function') ?
     loadChartbeat : function() { oldonload(); loadChartbeat(); };
	 var plend = new Date();
	 var plload = plend.getTime() - plstart.getTime();
	 if(plload<1000)
		 lc = "Very Fast < 1s";
	 else if (plload<2000)
		 lc = "Fast      < 2s";
	 else if (plload<3000)
		 lc = "Medium    < 3s";
	 else if (plload<5000)
		 lc = "Sluggish  < 5s";
	 else if (plload<7500)
		 lc = "Creepy    < 7.5s";
	 else if (plload<10000)
		 lc = "Slow      < 10s";
	 else
		 lc="Very Slow > 10s";
	 var fn = document.location.pathname;
	 fn += document.location.search ? document.location.search : '';
	 try {
		 _gaq.push(['loadTracker._setAccount', 'UA-21507378-1']);
		 _gaq.push(['loadTracker._trackEvent','Page Load (ms)',lc + ' Loading Pages',fn,plload]);
		 _gaq.push(['loadTracker._trackPageview']);
	 } catch(err){}
})();
 
</script>


</body>
</html>	

