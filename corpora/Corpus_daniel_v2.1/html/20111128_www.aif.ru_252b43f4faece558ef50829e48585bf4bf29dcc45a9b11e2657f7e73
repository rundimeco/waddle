<script type="text/javascript">
$(document).ready(function() {
	var login = $.cookie('login');

	if (login == null)
	{
		$('.hello').html('Здравствуйте, Гость<br /><a href="/auth/login">Вход</a> | <a href="/auth/registration">Регистрация</a>');
	} else {
		$('.hello').html('Здравствуйте, ' + login + '!<br /><a href="/auth/logout">Выйти</a>');
	}
});
</script></div>

