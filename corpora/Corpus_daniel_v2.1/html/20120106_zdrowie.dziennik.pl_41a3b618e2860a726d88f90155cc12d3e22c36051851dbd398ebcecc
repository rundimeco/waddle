var currentTab = 0; 
function openTab(clickedTab) {
var thisTab = $(".boxRounded .tabs600 a").index(clickedTab);
$(".boxRounded .tabs600 li a").removeClass("active");
$(".boxRounded .tabs600 li div").css('display', 'none');
$(".boxRounded .tabs600 li a").parent().css('background-color', 'white');
$(".boxRounded .tabs600 li a:eq("+thisTab+")").addClass("active");
$(".boxRounded .tabs600 li a:eq("+thisTab+")").parent().css('background-color', '#e6f4fa');
$(".boxRounded .tabs600 li a:eq("+thisTab+")").parent().children("div").css('display', 'block');
$(".boxRounded .tabbed-content").hide();
$(".boxRounded .tabbed-content:eq("+thisTab+")").show();
currentTab = thisTab;
}
//$(document).ready(function() {
// $(".tabs600 li:eq(0) a").css("border-left", "none");
$(".boxRounded .tabs600 li a").click(function() {
openTab($(this)); return false;
});
