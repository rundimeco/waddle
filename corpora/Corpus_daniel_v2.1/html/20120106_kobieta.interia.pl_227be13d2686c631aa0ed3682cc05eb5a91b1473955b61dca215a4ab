<div class="box articleSingle print" id="articleSingle1">
    <div class="boxBegin">
    </div>
    <div class="boxHeader">
            			            <h1>To ci wyjdzie na zdrowie</h1>                                   
		
                    <div class="fbLikeButton"><div id="fb-root"></div>
                    <script>(function(d, s, id) {
                      var js, fjs = d.getElementsByTagName(s)[0];
                      if (d.getElementById(id)) return;
