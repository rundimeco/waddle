                      fjs.parentNode.insertBefore(js, fjs);
                    }(document, 'script', 'facebook-jssdk'));</script><fb:like href="http://kobieta.interia.pl/zdrowie/news-to-ci-wyjdzie-na-zdrowie,nId,421651" send="false" width="450" show_faces="false" action="recommend"></fb:like></div>                    
            
        
