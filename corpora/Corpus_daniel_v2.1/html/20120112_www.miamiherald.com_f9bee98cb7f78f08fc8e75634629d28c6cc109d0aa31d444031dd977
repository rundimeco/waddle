<script  type="text/javascript">
/* This function loads the more stories page via AJAX if SSI fails */
function listLoad(elem,url) {
   err = "[an error occurred while processing this directive]";
   if($("#"+elem).html().indexOf(err) > 0) {
       $("#"+elem).load(url);
   }
}
</script>



