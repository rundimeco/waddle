    var t_array = decodeURIComponent(query[1].replace(/\+/g, " ")).split(' ');
    for (var i in t_array)
    {
        t_array[i] = t_array[i].replace(/[^a-zA-Z0-9_+|-]/g, "");
        t_array[i] = t_array[i].replace(/\+/g, "{plus}");
        t_array[i] = escape(t_array[i]);
        if (t_array[i].length < 1) continue;
        kw_str += 'kw=' + t_array[i] + ';';
    }
}
