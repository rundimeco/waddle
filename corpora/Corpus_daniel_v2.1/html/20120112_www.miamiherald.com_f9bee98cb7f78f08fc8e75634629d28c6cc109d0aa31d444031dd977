<script type="text/javascript" language="Javascript">
    var string1 = "";
    var string2 ="";
    var last = "";
    var finalc = "";
    var string1 = "miamiherald.news";

    var mystring = "News/Florida/Florida Wires";

    // this is now known as the "top level hack"
    // if you have a top level directory, that is NOT a top level DART section list it below
    // Miami uses only Opinion(513) as top level directory
    var tlh = new Array("513");

    var startpoint;
    var addpoint;
    for (var i=0;i<tlh.length;i++) {
        if ("775" == tlh[i]) {
            startpoint = 0;
            addpoint = 2
        } else {
            startpoint = 1;
            addpoint = 1
        }
    }
    

    var array = mystring.split("/");
    string1 += "/" + array[array.length-1] + ";";
    if (array.length != 1) {
        for (var i = startpoint;i<array.length;i++) {
	    string1 += "c" + (i + addpoint) + "=" + array[i] + ";";
	}
	finalc = array.length+1;
    } else {
        finalc = array.length+1;
    }

    last = array[array.length-1];
    string1 += "c" + finalc + "=" + last + "_homepage;";
    if ("story-detail" == "section-front") {
	string1 += "template=channel;";
    } else {
	string1 += "template=article;";
    } 
    string1 = string1 + '!category=' + last;

    // GENERIC:
    // replace " & " with underscore as in Crime & Courts
    string1 = string1.replace(/ & /g,"_");
    // replace whitespace with underscore
    string1 = string1.replace(/\s+/g,"_");
    // replace & with dash
    string1 = string1.replace(/&/g,"-");
    // remove single quote
    string1 = string1.replace(/'/g,"");
    // remove double quote
    string1 = string1.replace(/"/g,"");
    
    // SITE SPECIFIC REPLACEMENTS
    // replace front_page with homepage
    string1 = string1.replace(/front_page/g,"homepage");
    string1 = string1.replace(/front/g,"homepage");

    var src_string = string1.toLowerCase();
    
</script>

 
 

