    ctxt_ad_url = window.location.href.split('?')[0].split('#')[0];     //context URL
    ctxt_ad_url_cat = 'news';     //fallback category name
    ctxt_ad_id = 'news';     //context ID name
    ctxt_ad_keywords = '';     //keywords (URL safe values only)
    ctxt_ad_oe = 'latin1';     //output character encoding

//Type Tag
    ctxt_ad_type = 'news';     //tag for reports--can be ctxt_ad_section
//Click Wrapper
    ctxt_ad_cw = '';     //click wrapper URL
//Display Parameters
    //ctxt_ad_bc = '000000';     //cell border color
    //ctxt_ad_cc = 'ffffff';     //cell background color
