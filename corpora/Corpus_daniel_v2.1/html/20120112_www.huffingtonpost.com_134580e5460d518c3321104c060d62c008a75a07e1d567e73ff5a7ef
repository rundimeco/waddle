<script type="text/javascript">
	var group = 'A';
	var trk = HPTrack.Module('galleries-' + group, 'Photo Galleries - ' + group);
	trk.impression();

	jQuery('#hp_module_photos_all_pages a').click(function(e) {		
		var label = jQuery(this).text() || jQuery(this).next().text();
		trk.trackLink(e, jQuery.trim(label));
	});

	function create_url_for_slideshow_posts(page) {
		var url = '';
		if($('slideshow_ids_'+page))
			return url = '/ajax/common/photo_galleries.php?vertical=&page='+page+'&entries_ids='+$('slideshow_ids_'+page).innerHTML;
		else
			return false;
	}
	 var module_photos_pagination = new CommonPaginator({'moduleName': 'module_photos', 'maxPage': 1, 'is_circle': true, 'is_ajax_update': true, 'create_page_ajax_url':create_url_for_slideshow_posts, 'update_images': true});
</script>
