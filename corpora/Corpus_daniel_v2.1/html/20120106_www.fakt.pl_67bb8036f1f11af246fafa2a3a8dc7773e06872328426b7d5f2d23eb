					function MOnewsl(url){
          	$('<iframe frameborder="0" class="DLGnewsl" id="DLGnewsl" src="'+url+'"></iframe>').dialog({
          		title: 'Opis programu',
              width:640,
              closeOnEscape: true,
              height:480,
              modal:true,
              resizable:false,
              draggable:false,
							open: function (){
								$('iframe.DLGnewsl').height( $(this).height() );
								$('iframe.DLGnewsl').width( $(this).width()+320 );
							}
  					});
        	}


