<script type="text/javascript"><!--

var OMNI_DEBUG_LEVEL = 33; 



function GetURLVars()
{
	
	var args = new Object();
	a_pairs = document.location.search.split(/[?&]/);
	for (i = 0; i < a_pairs.length; i ++) {
		
		a_bits = a_pairs[i].split('=');
		if ((a_bits.length == 2) && (a_bits[0].length)) {
			if (args[a_bits[0]] == undefined) {
				
				args[a_bits[0]] = unescape(a_bits[1]);
			} else {
				
				args[a_bits[0]] = args[a_bits[0]] + ',' + unescape(a_bits[1]);
			}
		}
	}

	return args;
}

function GetCampaign()
{
	var urlvars = GetURLVars();
	var campaignSrc;
	if (typeof(urlvars.campaign) != 'undefined') {
		campaignSrc = urlvars.campaign;
	} else {
		campaignSrc ="";
	}
	return campaignSrc;
}


var s_account;
s_account = s_account || "tmrmgnthreeaggprod,tmrmirrorprod";
