					function insertUnGUI(){
						if (WP.$('.hot').length){
							WP.$('.hot').last().after(unregisteredUsersStartView);
							createMinClosePanel();
							startupMinimize();
						} else {
							setTimeout(insertUnGUI, 1000);
						}	
					};
