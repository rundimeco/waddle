                <script type="text/javascript" language="javascript">
  function updateChars(dom_id, remaining_chars_dom_id, max_length) {

    var message_text = $(dom_id).value;
    var remaining_characters = max_length - message_text.length;
    if (remaining_characters < 0) {
      message_text = message_text.substring(0, max_length);
      $(dom_id).value = message_text;
      remaining_characters = max_length - message_text.length;
    }

    $(remaining_chars_dom_id).innerHTML = remaining_characters;
  }

  function cancelAddComment(element) {
    var parentElement = $(element).up('.comment_add');
    parentElement.childElements()[0].remove();
    parentElement.hide();
  }

  function removeComment(element) {
    $(element).up('.comment').hide();
  }
</script>

