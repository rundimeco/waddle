            <script type="text/javascript">
            //<![CDATA[
                function ToggleInnerLabel (input,label,on) {
                var labelExists = (input.value == label ? 1 : 0);
                if (on && !labelExists && !input.value) {
                input.value = label;
                input.className = 'textInputNote';
                }
                if (!on && labelExists) {
                input.value = '';
                input.className = 'textInput';
                }
                return false;
                }
            //]]>
            </script>
            <input name="Ticker" class="textInputNote" type="text" value="Enter Symbol" onfocus="ToggleInnerLabel(this,'Enter Symbol');" onblur="ToggleInnerLabel(this,'Enter Symbol',1);" onclick="this.select();"/>
            <input class="submitBtn" type="submit" name="submit" value="GO" alt="GO" />
