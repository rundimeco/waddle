{
	for (var j=0; j<scriptArray.length; j++)
	{
		if (scriptArray[j].style.display == 'none')
		{
			isAdBlocked=1;
			break;
		}
	}
}

if(isAdBlocked)
{
	s.prop50="ad blocking";
}




  
