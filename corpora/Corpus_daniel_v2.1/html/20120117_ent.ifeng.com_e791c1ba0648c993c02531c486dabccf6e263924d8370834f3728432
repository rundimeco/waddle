    V.removeListener($("rmd_f"), 'click', function(){rmd_form9();});
    $("rmd_f").value = "已推荐";
    $("rmd_f").style.cursor="auto";
    $("rmd_f").title="";
    $("rmd_d_left").style.display="";
    $("rmd_d_center").style.display="";
    $("rmd_d_right").style.display="";
  }
}
