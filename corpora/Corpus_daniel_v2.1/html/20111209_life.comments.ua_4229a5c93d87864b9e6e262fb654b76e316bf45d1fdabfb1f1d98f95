<script>
var cur_act = 0;
var max_act = 6;
function nextAct() {
	var next_act = cur_act==max_act ? 0 : cur_act+1;
	$("#actual_"+cur_act).css("z-index","998");
	$("#actual_"+next_act).css("z-index","999");
	$("#actual_"+next_act).slideDown('slow',function() {
		$("#actual_"+cur_act).hide(0,function(){
			cur_act = next_act;
		});
	});
}
$(document).ready(function(){ 
	var topActualId = setInterval(function() {
		nextAct();
	}, 15000);
	$('.photo_box').hover( 
		function() { clearInterval(topActualId); } 
	);  
});
</script>
