var	ajaxImgObj = 'http://media.wwltv.com/designimages/belo-ajax-loader.gif';
$(document).ready(function() {
	$("#regModal").dialog({ 
		position: 'top',
    	autoOpen: false,
    	modal: true,
    	width:"640px"
    });

