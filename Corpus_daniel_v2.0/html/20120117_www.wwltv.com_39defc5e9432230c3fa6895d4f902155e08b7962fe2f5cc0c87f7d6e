function belologout() {
	bimreg.logout();
};  
function regAuthEvents() {
	beloregCount();
	};
bimreg.listener.addListener("regAuth",regAuthEvents);
bimreg.listener.addListener("regAuthNoUser",belologoutFromNav);  
function userloginEvents(event){
	belologinFromNav();
			userNameErrorCheck();
		createIMGCookie();
	initRedirect();
		};
bimreg.listener.addListener("userLogin",userloginEvents);
function beloregCount() {
		if(!BELOJS.reg.optout.get()) {
		if(bimreg.loggedin) {
			if(bimreg.getuserkey("BELO_additional_registration") == "") {
				if(BELOJS.reg.pagecount.get() > 3) {
					BELOJS.reg.pagecount.reset();
					window.location = "/register/?form=int&docReferrer=http://www.wwltv.com/home/Community-pays-tribute-to-Angel-Baby-Trip-Roth-who-died-Saturday-137389683.html";
				}
			}
		} else {
			if(BELOJS.reg.pagecount.get() > 5) {
				BELOJS.reg.pagecount.reset();
				window.location = "/register/?form=signup&docReferrer=http://www.wwltv.com/home/Community-pays-tribute-to-Angel-Baby-Trip-Roth-who-died-Saturday-137389683.html";
			}
		}
	}
};  
function initRedirect() {
			}
	
function createIMGCookie() {
	var zipcode = bimreg.getuserkey("zip_code");
	if(typeof(zipcode) == "undefined") zipcode = "";
	var firstname = bimreg.getuserkey("first_name");
	if(typeof(firstname) == "undefined") firstname = "";
	genus.setcookie("IMG","|"+firstname+"|"+zipcode+"||");	
}
        
function userNameErrorCheck() {
	if ( bimreg.getuserkey("email").replace(/\W/g,"") === bimreg.getuserkey("username").replace(/\W/g,"") ) {
		var failedName = bimreg.getuserkey("username");  
		var forcedUpdateName = failedName.replace(/@.*/,"").replace(/\W/g,"");
		var currentTime = new Date();
		var seconds = currentTime.getTime();
		var secString = seconds+'';
		var digits = secString.length + forcedUpdateName.length - 16;
		if (digits > 0) {
			seconds = secString.substring(digits,secString.length);
		}
		username = forcedUpdateName + seconds;
		//update user
		bimreg.updateprofile({username:username});
		
		if ( document.URL.indexOf('form=login') > -1 ) {
			window.location.replace("/register/?form=user");
		} else {
			window.location = "/register/?form=user";
		}
	}
}
        
function checkLoginReferrer() {
	if(referrer) {
		genus.setcookie("BIMREG_signup_referrer","");
		window.location = referrer;
	} else {
		try {
			if (document.referrer) {
				window.location = document.referrer+window.location.hash; 
							} else if (window.location.hash !== "") { 
								window.history.go(-1);
			} else {
				window.location = "/";
			}
		} catch(e) {
			window.location = "/";
		}  
	} 	
}
    
function beloYouNewsPostPage() {
	var username = bimreg.getuserkey("username");
	$("#submission").show();
			if(bimreg.loggedin && document.URL.indexOf('form=post') > -1){
			document.getElementById('username').appendChild(document.createTextNode(username));
			document.forms['ynform']['element.author'].value = username;
			$("p#author").text(username);
			document.forms['ynform']['element.ae'].value = bimreg.getuserkey("private.email");
			$("div#authorcopy div.text-note p a").attr("href", "/register/?profile="+username);
		}
	}
function belologinFromNav() {
	var username = bimreg.getuserkey("username");
	var toolbarObject = $("#toolbar-status");
	$(toolbarObject).removeClass("loggingiIn").removeClass("hide");
	$(toolbarObject).find("ul").show();
	$(toolbarObject).find("#status-loggingin").hide();
	$(".status-out").hide();
	$("li#login-item.top-level-item").addClass("hide");
	$(toolbarObject).show();
	$("li.status-loggedin").show();
	$('span.username').text(bimreg.getuserkey("username"));
	$(toolbarObject).find('li#status-welcome').html('Welcome, <a href=\"/register?profile='+username+'"\'>'+username+'<\/a>');
	if(typeof bimreg.getuseravatar() != "undefined"){
		$("div#avatar-image").html("<img src='"+bimreg.getuseravatar()+"' width='40' height='34'/>");
	};
	$(toolbarObject).find('li#status-profile').html('<a href=\"/register?profile='+username+'"\'>My Profile<\/a>');
	var surveyObject = $('#content-two table .cmP_survey') 
	$(surveyObject).find('.cmP_question, .cmP_response, .cmP_responses, #survey-submit, .cmP_input, .cmP_label, #survey-signup-newsletters').removeClass("hide");
	$("#survey-reg-prompt").addClass("hide");
	$("form[name=loginformnav] input[name=login-nav-password]").val("");
}
    
function belologoutFromNav() {
	$(".status-out").show();
	$("li#login-item.top-level-item, li#login-item.top-level-item #login-button").removeClass("hide");
	$("#toolbar-status").hide();
	$("li.status-loggedin").hide();
	$("#status-loggingin").hide();
	}

$("#loginWFacebook, #modalFaceReg").click(function(){
	bimreg.ssoPop("facebook_reg");
	BELOJS.reg.resetModal();
	$("#regOptions").hide();
	BELOJS.reg.startSSO();
	return false;	
});
$("#loginWTwitter, #modalTwitterReg").click(function(){
	bimreg.ssoPop("twitter_reg");
	BELOJS.reg.resetModal();
	$("#regOptions").hide();
	BELOJS.reg.startSSO();
	return false;	
});

$("#regModal #showRegModal").click(BELOJS.reg.showRegModal);


$("#newsletterSignup").submit(function(e) {
	e.preventDefault();
	$("#newsletterSignup .loadingHolder").append('<span id="loading-user-newsletter"><img src="'+ajaxImgObj+'" id="loadingGraphic" />Registering User...</span>');
	$("#newsletterUpdate").hide();
	BELOJS.reg.createmodaluser();
	return false;
});

$("#overlayLogin").submit(function(e){
	e.preventDefault();
   	$("#overlayLogin .login-submit").hide();
       $("#overlayLogin input").attr("readonly", true);
       $(".loginError").after('<div id="loading-user"><img src="'+ajaxImgObj+'" id="loadingGraphic" /><p class="updating">Logging in...</p></div>');
       $(".error-message").empty();
       var username = $("#overlayUsername").val();
       var password = $("#overlayPassword").val();
       if($("#rememberMe:checked").val()) {
           genus.setcookie("BELO_rememberme",username,30);
       }
       bimreg.login(username,password);
});

$("#normReg").validate({
	rules:{
		reg_birthdateobject:{oldenough : true},
		reg_zip:{postalcode : true}
		},
	submitHandler: function(form) {
		$("div.error").hide();
		BELOJS.reg.showNewslettersReg();
	},
   invalidHandler: function(form, validator) {
      var errors = validator.numberOfInvalids();
      if (errors) {
        var message = errors == 1
          ? 'You missed 1 field. It has been highlighted'
          : 'You missed ' + errors + ' fields. They have been highlighted';
        $("#normReg div.error span").html(message);
        $("#normReg div.error").show();
      } else {
        $("#normReg div.error").hide();
      }
    }
});
    	

function onloadlogout(status) {
	$("li#login-item.top-level-item").removeClass("hide");
	$(".signin").removeClass("menu-open");
	BELOJS.reg.pagecount.reset();
	belologoutFromNav();
}
bimreg.listener.addListener("userLogout",onloadlogout);

var carouselRan = false;
function modalShow() {
	$("#regModal").removeClass("hide").dialog();
	BELOJS.reg.showLoginModal();
	bimreg.listener.addListener("userLogin",BELOJS.reg.loginModalClose);
	if(!carouselRan){
		setTimeout("loadCarousel()",300);
		carouselRan = true;
	}
	return false;
}
    
$(function() {
    if(document.URL.indexOf('form=int') > -1 || document.URL.indexOf('form=signup') > -1) {
        $(function() {
            window.onunload = function() { genus.setcookie("BELO_OptOut","true",30); }
        })
    }
    
    $("#status-signout a").click(function(e) {
        belologout(belologoutFromNav);
        return false;
    });

    $("#login-button a.signin").click(function(e) {
    	modalShow();
    	return false;
	});
});

        		$("#comments-message a").click(modalShow);		
			bimreg.listener.addListener("userLogin",showcommentTools);
			function commentLogOut(){  	
			  	hidecommentTools()
			}
			bimreg.listener.addListener("userLogout",commentLogOut);
            
            if ($("span#charlimit").size() > 0) {
                window.setInterval(function() {
                    var charlimitEl = $("span#charlimit");
                    var chartxtEl = $("span#charlimittxt");
                    var maxLen = 1000;
                    if (maxLen < 1) {
                        maxLen = 1000;
                    }
                    var commentLen = $("form textarea").val().length;
                    if (commentLen < maxLen) {
                        $(charlimitEl).text(maxLen-commentLen).css("color", "#000");
                        $(chartxtEl).css("color", "#000");
                    } else if (commentLen === maxLen) {
                        $(charlimitEl).text("0").css("color", "red");
                        $(chartxtEl).css("color", "red");
                    } else {
                        $("form textarea").val($("form textarea").val().substring(0, maxLen));
                        $(charlimitEl).val("0").css("color", "red");
                        $(chartxtEl).css("color", "red");
                    }
                }, 1000);
            }

			    
			var commentsDisplay = {
				api: {
					hideComments: function() {
						jQuery("#comments-reorder .comment").addClass('hide');
						jQuery("#comment-pagination").addClass("hide");
						jQuery("#comment-view-label").addClass("hide");
					},
					showComments: function() {
						jQuery("#comment-pagination").removeClass("hide");
						jQuery("#comment-view-label").removeClass("hide");
					},
					showFifteen: function() {
						jQuery("#comments-reorder .comment").slice(0, 5).removeClass('hide');
					},
					reverseOrder: function() {
						jQuery("#comments-reorder .comment").addClass('hide');
						jQuery("#comments-reorder .comment").each(function() {
							$(this).prependTo( $(this).parent() );
						});
						jQuery("#comments-reorder .comment").slice(0, 5).removeClass('hide');
					},
					setCookie: function(commentOrder) {
							setcookie("beloCommentOrder", commentOrder, 30);
					},
					readCookie: function() {
							var queryResult = readcookie("beloCommentOrder");
							return queryResult;
					},
					commentOrderCheck: function(previousOrder) {
						var selectedOrder = jQuery('#commentOrder :selected').val();
						this.setCookie(selectedOrder);
						
						if (selectedOrder != previousOrder && selectedOrder != "None") {
							if(selectedOrder == "oldestToNewest" && previousOrder == "None" || selectedOrder == "oldestToNewest" && previousOrder == "newestToOldest" || selectedOrder == "newestToOldest" && previousOrder == "oldestToNewest"  ) {
								jQuery("#comments-reorder").slideUp();
								this.showComments();
								this.reverseOrder();
								jQuery("#comments-reorder").slideDown();
							} else if (selectedOrder == "newestToOldest" && previousOrder == "None") {
								this.showComments();
								jQuery("#comments-reorder").slideUp();
								this.showFifteen();
								jQuery("#comments-reorder").slideDown();
							}
						}else if (selectedOrder == "None") {
							if(previousOrder == "oldestToNewest") {
									this.reverseOrder();
								}
							 jQuery("#comments-reorder").slideUp();
							 	this.hideComments();
						}; 
					}	
				},
				init: function() {
						this.api.hideComments("#comments .comment");
						commentCookieCheck = this.api.readCookie();	
						jQuery("#comments-reorder").slideUp();		
						if (!commentCookieCheck || commentCookieCheck == "" || commentCookieCheck == "undefined") {
							$('#commentOrder').val("newestToOldest");
							this.api.setCookie("newestToOldest");
							this.api.showComments();
							this.api.showFifteen();
							jQuery("#comments-reorder").slideDown();
							commentCookieCheck = this.api.readCookie();	
						} else if (commentCookieCheck == "oldestToNewest") {
							$('#commentOrder').val("oldestToNewest");
							 this.api.showComments();
							 this.api.reverseOrder();
							 jQuery("#comments-reorder").slideDown();
						}else if (commentCookieCheck == "newestToOldest") {
							$('#commentOrder').val("newestToOldest");
							 this.api.showFifteen();
							 this.api.showComments();
							 jQuery("#comments-reorder").slideDown();
						}else if (commentCookieCheck == "None") {
							$('#commentOrder').val("None");
							 this.api.hideComments();	 
						};
						jQuery("#commentOrder option select").val(commentCookieCheck).attr('selected', 'selected');
						return this.api;
					}
			}
			
			//initiate Comment reordering tools
           	var commentRun = commentsDisplay.init();
			$('#commentOrder').bind('change',function() { 
				previousOrder = commentRun.readCookie("beloCommentOrder");
				commentRun.commentOrderCheck(previousOrder);	
			}); 
			
										//Function that configures the modal window options
		$('.comment div.commentFlag').click(function(e) {
			var abusiveComment = $(e.target).prev().text();
			var commentorName = $(e.target).siblings('p.comment-details');
			var commentorName = $(commentorName).children('.comment-author');
			var commentorName = $(commentorName).text();
			var abusiveCommenttext = $(e.target).siblings('p.comment-body').get(0);
			var abusiveCommenttext = $(abusiveCommenttext).text();
			var abusiveCommenttext = '\"' + abusiveCommenttext + '\"';
			
			$('#comment-flag-reason').focus();
			$('#other-field-container').slideUp().addClass('hide');	
			$('#flag-submit').attr("disabled","disabled");
			$('#comment-flag-proc').removeClass('hide');
			$('#comment-flag-succ').addClass('hide');
			$('#comment-flag-reason').val("Select One");
			$('#otherfield').val('');
			$('.comment-lightbox #flaggedCommentId').val(abusiveComment);
			$('.comment-lightbox #comment-text').html(abusiveCommenttext);
			$('.comment-lightbox #commentorName').val(commentorName);
		});
		
	//enable submission only when a reason is picked	
		$('#comment-flag-reason').change( function() {
			$('#other-field-container').slideUp();
			var reason = $('#comment-flag-reason').val();
			if (reason == 'default') {
				$('#other-field-container').slideUp();
				$('#flag-submit').attr("disabled","disabled");				
			} else if (reason == 'Other') {
				$('#other-field-container').removeClass('hide').slideDown();
				$('#otherfield').focus();
				$('#flag-submit').attr("disabled","disabled");
			} else if (reason != 'default' || reason !='Other') {
				$('#comment-flag-reason').focus();
				$('#other-field-container').slideUp();
				$('#flag-submit').removeAttr("disabled")
				
			}
		});
		
	//Only let other submit if more than 10 characters
		$('#otherfield').keyup(function() {
			if($('#otherfield').val().length >= 10) {
				$('#flag-submit').removeAttr("disabled")
			} else {
				$('#flag-submit').attr("disabled","disabled");
			}
		});
		
	//Show the light box content in modal window
	var commentIDFill = function() {
		$('.comment-lightbox').show();
		}
		
