function lastFolder() {
	var folderPath = window.location.pathname;
	var folderPathArray = folderPath.split("/");
	var lastfolder = folderPathArray[folderPathArray.length - 1];
	return lastfolder;
};	

function rollUpUrl() {
	var om_location_path = belo_tracking_sitepage;
	om_location_path = om_location_path.replace("http://","");
	var om_location_path_array = om_location_path.split("/");
	var om_location_end = om_location_path_array.length - 1;
    var s_hier1Roll = om_location_path_array[0];
    for(i=1;i<om_location_path_array.length; i++){
        if(i==1){
            s_hier1Roll += "/" + om_location_path_array[i];
        }else{
            s_hier1Roll += "," + om_location_path_array[i];
        }
    }	
    return s_hier1Roll;
};
