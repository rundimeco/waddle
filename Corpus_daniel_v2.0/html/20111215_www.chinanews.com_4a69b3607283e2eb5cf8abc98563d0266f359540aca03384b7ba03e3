<script language=JavaScript> 
　　//更改字体大小 
　　var status0=''; 
　　var curfontsize=15; 
　　var curlineheight=24; 
　　function fontZoomA(){ 
　　 if(curfontsize>8){ 
　　 document.getElementById('fontzoom').style.fontSize=(--curfontsize)+'px'; 
　　 document.getElementById('fontzoom').style.lineHeight=(--curlineheight)+'px'; 
　　 } 
　　} 
　　function fontZoomB(){ 
　　 if(curfontsize<64){ 
　　 document.getElementById('fontzoom').style.fontSize=(++curfontsize)+'px'; 
　　 document.getElementById('fontzoom').style.lineHeight=(++curlineheight)+'px'; 
　　 } 
　　} 
</script>
