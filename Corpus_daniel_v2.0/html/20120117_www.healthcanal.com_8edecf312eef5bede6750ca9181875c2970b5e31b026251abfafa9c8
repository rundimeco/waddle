                    function onVote (index) {
                        voteParam = {};
                        voteParam.action = 'article';
                        voteParam.cmd = 'vote';
                        voteParam.ARTICLE_id = 25509;
                        voteParam.ARTICLE_vote = index;
                        voteParam.template_output = 'box/article_vote';
                                                    
