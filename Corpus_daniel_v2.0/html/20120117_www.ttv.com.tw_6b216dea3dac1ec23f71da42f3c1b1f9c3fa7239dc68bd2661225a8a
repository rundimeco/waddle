function fblike() {
	$.ajax({
		type: "GET",
	    url: "/news/fblikecount.asp",
		data: {type:"news", i:"10101174354001I", newsid:"{78B34FE9-5EBB-456B-AFB0-16492139758A}"},
	    cache: false,
		error: function(xhr) {
//			alert('FB Like Request Error!!');
		},
		success: function(response) {
//			alert('FB Like Request Success!!');
		}
	});
	
//    var ttvnewspage = document.getElementById("ttvnewspage");
//    ttvnewspage.style.display = "block";
}

function fbcommentcount() {
