      var size = 1;
      $('select_size').addClassName('size_'+size);
      new Control.Slider('slider_control','slider_handle',{
        onSlide:function(v){updateSlider(v)},
        onChange:function(v){updateSlider(v)},
        range:$R(1,5),
        sliderValue:size
      });
      function updateSlider(v){
        document.cookie='textSize='+Math.round(v);
        $('select_size').removeClassName('size_'+size);
        size = Math.round(v);
        $('select_size').addClassName('size_'+size);
      }
      
