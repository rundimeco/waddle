<script>
function changeMainNews()
{
	if (current_main_news > main_news_cnt) current_main_news = 1;
	if (current_main_news < 1) current_main_news = main_news_cnt;

	lnk = eval('MN_arr' + current_main_news + '[\'lnk\']');
	d.getElementById('main_news_lnk1').href = lnk;

	d.getElementById('main_news_img').style.backgroundImage = 'url(\''+eval('MN_arr' + current_main_news + '[\'img\']')+'\')';
	d.getElementById('main_news_title').innerHTML = eval('MN_arr' + current_main_news + '[\'title\']');
}

function MN_loop()
{
	current_main_news ++;
	changeMainNews()
	ME_timeout = setTimeout('MN_loop()', ME_tm);
}

function MN_rew()
{
	current_main_news --;
	changeMainNews();
	ME_ini();
}

function MN_forw()
{
	current_main_news ++;
	changeMainNews();
	ME_ini();
}

function ME_ini()
{
	if (ME_timeout) clearTimeout(ME_timeout);
	ME_timeout = setTimeout('MN_loop()', ME_tm);
}

ME_ini();

</script>


	</td>
</tr>
</table>

