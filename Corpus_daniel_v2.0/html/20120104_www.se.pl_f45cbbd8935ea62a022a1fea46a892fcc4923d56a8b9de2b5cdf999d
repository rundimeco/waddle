	var article_toogle_hide = false;

	function hide_article_toggle(){
		article_toogle_hide = true;
		$("#article_toogler").animate({"right": "-347px"}, "fast");
	}

    jQuery(function($){
    	$(window).scroll(function(){
    		var article_toggle = $("#article_toogler");
    		$(window).scrollTop();
			start_toogle = $(document).height() / 4.5;
			if(!start_toogle)
				start_toogle = 500;

			if($(window).scrollTop() > start_toogle){
    			if(article_toggle.css('right') == '-347px' && !article_toogle_hide){
    				article_toggle.show();
    				article_toggle.animate({"right": "0px"}, "fast");
    			}
			}
			else{
				if(article_toggle.css('right') == '0px'){
					article_toggle.animate({"right": "-347px"}, "fast");
				}
				article_toogle_hide = false;
			}
    	});
    });
</script><div class="toggler" id="article_toogler"><div class="togclose"><a onclick="hide_article_toggle()" href="javascript:void(0);"><img alt="Zamknij" src="/media/images/toggle_close.png"></a></div><div class="togcontent"><div class="left"><a href="/"><img alt="Nie daj się porwać i sam sobie gotuj" src="http://cdn9.se.smcloud.net/t/photos/thumbnails/213558/nie_daj_sie_porwac_i_90x72_crop_rozmiar-niestandardowy.jpg"></a></div><div class="right"><a href="/" title="PORADY dla Donalda Tuska: Nie daj się PORWAĆ i sam sobie GOTUJ">PORADY dla Donalda Tuska: Nie daj się PORWAĆ i sam sobie...</a></div></div></div><script type="text/javascript">
    jQuery(function($){
    	$(window).scroll(function(){
    		var floater=$("#socializer_id");
    		var topMargin=$('.ad').height();
    		if (topMargin == 0)
    			topMargin = 30;
    		$(window).scrollTop();
    		if($(window).scrollTop()>=topMargin)
    		{
    			floater.css({'top':($(window).scrollTop()-topMargin)+'px'});
    		}
    		else
    		{
    			floater.css({'top':'0px'});
    		}
    	});

    	
    });

    function set_page_startup(){
    	var isIe = !!window.ActiveXObject;
    	if(isIe){
    		document.body.style.behavior='url(#default#homepage)';
    		document.body.setHomePage('http://www.se.pl');
    	}
    	else{
			$('#box_startup_info').fadeIn('fast');
    	}
    }
</script><div id="box_startup_info" class="cien"><div class="popuphowto"><div class="row zamknij"><a onclick="$('#box_startup_info').fadeOut('fast');">zamknij</a></div><div class="row howto"><p class="title">Aby ustawić se.pl jako stronę startową:</p><p class="header">Mozilla Firefox</p><p>Na pasku adresu obok <strong>http://www.se.pl</strong> jest czerwona kropka z napisem "se.pl". Złap ją i przeciągnij na ikonkę domu przy prawym górnym rogu przeglądarki. Naciśnij OK. Gotowe!</p><p class="header">Google Chrome</p><p>Wejdź w Ustawienia -> Po uruchomieniu. Zaznacz opcję Otwórz konkretną stronę lub zestaw stron (druga od góry) i wybierz <strong>http://www.se.pl</strong> jako docelową stronę. Gotowe! </p></div></div></div><div class="social_rel"><div class="socializer" id="socializer_id"><div class="head"><a onClick="this.style.behavior='url(#default#homepage)';this.setHomePage('http://se.pl');"><img alt="socializer" src="/media/images/socializer-top.png"></a></div><div class="cont count_long"><div class="starter"><a href="javascript:set_page_startup();"><img alt="ustaw stronę jako startową" src="/media/images/set_as_homepage.png"></a></div><div class="fb_tab"><div class="fb-like" data-href="http://www.se.pl/wydarzenia/kraj/jak-odzyskac-pieniadze-z-nfz-za-nadplate-za-leki-wzor-pozwu-do-sadu_220768.html" data-send="false" data-layout="box_count" data-width="450" data-show-faces="false"></div></div><div class="gplus_tab"><g:plusone size="tall"></g:plusone></div><div class="twitter_tab"><a href="https://twitter.com/share?count=vertical" class="twitter-share-button" data-lang="pl">Tweetnij</a><script>!function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0];if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src="//platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs);}}(document,"script","twitter-wjs");</script></div><div class="on_mobile"><a target="_blank" href="http://itunes.apple.com/sn/app/superexpress/id407765026?mt=8"><img alt="" src="/media/