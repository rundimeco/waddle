		            <param name="fmtjDocURI" value="/news/health-16510507"/>
                
            <param name="companionId" value="bbccom_companion_16510424" />
        
                    <param name="config_settings_showShareButton" value="true" />
            <param name="config_settings_showUpdatedInFooter" value="true" />
        </object>
        <!-- embedding script -->
        
        <script type="text/javascript">
