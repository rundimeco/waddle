	<h1 class="story-header">PIP breast implants: Harley Medical Group will not replace implants</h1>

     
         
 
        		
		        
        <!--  Embedding the video player -->
<!--  This is the embedded player component -->


	
 

<div class="videoInStoryC">
    <div id="emp-16510424-45396" class="emp">
        		
        <noscript>
            <div class="warning">
                                <img class="holding" src="http://news.bbcimg.co.uk/media/images/57820000/jpg/_57820351_jex_1286179_de27-1.jpg" alt="Mel Braham, group chairman of the Harley Medical Group" />
                                <p><strong>Please turn on JavaScript.</strong> Media requires JavaScript to play.</p>
            </div>
        </noscript>
        <object width="0" height="0">
            <param name="id" value="embeddedPlayer_16510424" />
                    <param name="width" value="320" />
            <param name="height" value="180" />    
                    <param name="size" value="Small" />
					<param name="holdingImage" value="http://news.bbcimg.co.uk/media/images/57820000/jpg/_57820351_jex_1286179_de27-1.jpg" />
							<param name="externalIdentifier" value="p00n4ss6" />
		            <param name="playlist" value="http://playlists.bbc.co.uk/news/health-16510424A/playlist.sxml" />
            <param name="config_settings_autoPlay" value="false" />
            <param name="config_settings_showPopoutButton" value="false" />
            <param name="config_plugin_fmtjLiveStats_pageType" value="eav2" />
