<script type="text/javascript"><!--
 function search_action(){
        var url = document.getElementById("selecturl").value;

        if(url == "blogs"){
            document.searchForm.action = "http://www.nydailynews.com/blogs/nydn";
            document.searchForm.q.name = "search";
            document.searchForm.submit();
        }
        else {
            document.searchForm.action ="/search-results/search-results-7.113";
            document.searchForm.submit();
        }
 }
