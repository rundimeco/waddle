						document.write(google_ads[i].line2);
						document.write('<div class="clear">&nbsp;</div>');
						document.write(google_ads[i].line3);
						document.write('<div class="clear">&nbsp;</div>');
						document.write('<a target="_blank" href="' + google_ads[i].url + '"' + 'onmouseout="window.status=\'\'"' + 'onmouseover="window.status=\'go to ' + google_ads[i].visible_url + '\';return true;">' + google_ads[i].visible_url + '</a>');
						document.write('</div>');
						document.write('</div>');
					}
					document.write('</div>');
					document.write('</div>');
					document.write('</div>');
				}
			// -->
			</script>
			<script language="javascript" src="http://pagead2.googlesyndication.com/pagead/show_ads.js"></script>

<!-- /life/googlestoryad.inc -->
