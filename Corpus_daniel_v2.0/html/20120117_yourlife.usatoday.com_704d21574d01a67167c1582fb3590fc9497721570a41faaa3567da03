var partnerID=1660;
function getClickExpire() {
if (self.clickExpire) return clickExpire; 
return "";
}
function appendAndEscape(origUrl, appendix){
if(appendix == "") return escape(origUrl);
var returnUrl ="";
var i = origUrl.indexOf(appendix);
var j = origUrl.indexOf("?");
if(i == -1){
if(j == -1){
returnUrl = origUrl+"?"+appendix;
}else{
returnUrl = origUrl+"&"+appendix;
}
}else{
returnUrl = origUrl;
}
return escape(returnUrl);
}
function getClickURL() {
if (self.clickURL) return clickURL;
return document.location.href;
}
function getClickTitle() {
if (self.clickTitle) return clickTitle;
return document.title;
}
function PT () {
FDCPUrl('http://www.printthis.clickability.com/pt/printThis?clickMap=printThis'+getLocation(),true); 
}
function getLocation () {
var theUrl = escape(getClickURL());
var theTitle = escape(getClickTitle());
theUrl = appendAndEscape(getClickURL(),"")
var theLoc = "&fb=Y&url="+theUrl+"&title="+theTitle+"&random="+Math.random()+"&partnerID="+partnerID+"&expire="+escape(getClickExpire());
return theLoc;
}
