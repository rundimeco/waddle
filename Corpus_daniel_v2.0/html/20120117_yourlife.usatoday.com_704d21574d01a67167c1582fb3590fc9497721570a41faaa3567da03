function megaHoverOver(){
jQuery(this).find(".sub, .sub-sponsorship").stop().fadeTo('fast', 1).show();
}
function megaHoverOut(){ 
jQuery(this).find(".sub, .sub-sponsorship").stop().fadeTo('fast', 0, function() {
jQuery(this).hide(); 
});
}
