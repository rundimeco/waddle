		    /* ajax.Request */  
		function _VIHIVFHYFUajaxRequest(url,data) 
		    {
		    //alert('url: ' + url + 'data: ' + data);   
			$.ajax({
				type: 'POST',
				url: "http://"+location.host+"/"+url,
				data: data,
				complete: _VIHIVFHYFUgetResponse
			});
		     $('#VIHIVFHYFU_submit').style.display= "none";
		      $('#VIHIVFHYFU_loading').style.display= "block";
		   }  
		   /* ajax.Response */  
		   function _VIHIVFHYFUgetResponse(oReq) 
		   {  

			//alert(oReq.responseText);
		   	$("#_VIHIVFHYFU_comments").html( $("#_VIHIVFHYFU_comments").html() + oReq.responseText);  
		   	 $('#VIHIVFHYFU_username').val('');
		   	 $('#VIHIVFHYFU_useremail').val('');
		   	 $('#VIHIVFHYFU_number').val('');
		   	 $('#VIHIVFHYFU_msg').val('').removeData();
		   	 $('#VIHIVFHYFU_loading').style.display= "none";
		   	 $('#VIHIVFHYFU_submit').style.display= "block";
		   }  
		  </script>  
			


