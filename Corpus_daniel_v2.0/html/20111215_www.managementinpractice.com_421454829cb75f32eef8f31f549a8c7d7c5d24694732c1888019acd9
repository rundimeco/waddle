		var nContainerWidth = oContainerDiv.offsetWidth;

		var oViewPortDiv    = document.getElementById("crumbviewport");
		var nViewPortWidth  = oViewPortDiv.offsetWidth;
		
		while (nContainerWidth > nViewPortWidth)
		{
			var sHTML    = oContainerDiv.innerHTML;
			var nIndex   = sHTML.indexOf("|");
			
			if (nIndex > -1)
			{
				oContainerDiv.innerHTML = sHTML.substring(nIndex+1);
				nContainerWidth = oContainerDiv.offsetWidth;
			}
			else
			{
				break;
			}
		}
