else if( sType==("funp")) {result = 'http://funp.com/pages/submit/add.php?&tags[]=%E6%96%B0%E8%81%9E&tags[]=%E7%94%9F%E6%B4%BB&tags[]=%E9%86%AB%E5%AD%B8&tags[]=%E7%BE%8E%E5%AE%B9&tags[]=%E5%81%A5%E5%BA%B7&tags[]=uho&via=tools&&s='+sTitle+'&url='+sURL+'&r='+escape(via)+'&t='+sDesc;}
