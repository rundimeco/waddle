function searchKeyWord() {
    var txt = '';
     if (window.getSelection) {
        txt = window.getSelection();
    } else if (document.getSelection) {
        txt = document.getSelection();
    } else if (document.selection) {
        txt = document.selection.createRange().text;
    }
	if (txt == "")
		window.open('http://data.udn.com//2010/images/news2data/history.html','history','height=580,width=500,top=0,left=0,toolbar=no,menubar=no,scrollbars=no, resizable=no,location=no, status=no');
	else {
		if(confirm("您要查詢的關鍵字是(" + txt+")"))
			openWin3("http://data.udn.com/data/Search?Keywords="+txt);
	}
}
