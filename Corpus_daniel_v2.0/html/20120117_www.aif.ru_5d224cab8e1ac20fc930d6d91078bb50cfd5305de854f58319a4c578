<script>
    
 function setAifCookie(name, value, params)
{
// не переписываем имеющуюся куку!
if (jQuery.cookie(name) != null) {
//alert(jQuery.cookie(name));
return false;
}

if (params == null){
jQuery.cookie(name, value, {expires: 7, path: '/', secure: false});
    } else {
jQuery.cookie(name, value, params);
}
return true;
}   
    
$comments_inp = false;
$(document).ready(function()
{
    
   
	var login = $.cookie('login');
	var hash = $.cookie('hash');

	if (login == null)
	{
		// BC with old auth!
                
		if ( hash != null ) {
			
                        $.post('/ajax/setlogin', function(data) {$('#user_name').val(data.result); $('#user_name').attr('readonly', true); $('#login_msg').html('Зарегистрированный пользователь:');});
			
                        $('#user_email').hide();
                   
			$('#auth-register').hide();
                        
		} else { // u/a
                   
			setAifCookie('comments', '4efc15e5f7de7fbc785f8df14601d669', {expires: 7, path: '/', domain: 'www.aif.ru', secure: false});
			
                        $('#user_name').val('');
		}

	} else {

$('#user_label').text('');
		$('#user_email').hide();
		$('.auth-register').html('<a href="/auth/logout">выйти</a>');
		$('#user_name').val(login);
		$('#user_name').attr('readonly', true);
		//$('#login_msg').html('Зарегистрированный пользователь:');
	}
        
          
	$('#ajaxform').submit(function(e){
		e.preventDefault();
		var action = $(this).attr('action'), data = $(this).serialize();
		$.post(action, data, function(r){
			if('ok'==r.result) {
                            
                            				
                                $('#article-comments').html(r.content);
				$('#comments_form').load('http://www.aif.ru/aforism/?'+Math.random());
                            
                                                         
			} else {
				alert(r.result);
			}
		}, 'json');
               
	});


  $("a.reply").click(function()
	{
		if ($comments_inp) {
			var cmt = $('#comments_inp').val() + "\n"; // wipe rules
		} else {
			var cmt = '';
			$comments_inp = true;
			$('#comments_inp').css('color', '#000000');
		}

		var comment_id = $(this).attr('href').replace(/[^0-9]+/, '');
		var author = $('div.username', '#meta'+ comment_id).text();

		var comment = $('blockquote', '#comment'+ comment_id).text();

		//var ocomment = $('#comment'+ comment_id);//alert(ocomment);
		//var selected = comment.text().substring(range.start, range.end); alert(selected);

		//$('#comments_inp').val(cmt + author + " писал(а):\n" + comment + "\n---\n");
		$('#comments_inp').val(cmt + author + " писал(а):\n<i>" + comment + "</i>\n---\n");
		$('#comments_inp').focus();
        return false;
  });
});
</script>
