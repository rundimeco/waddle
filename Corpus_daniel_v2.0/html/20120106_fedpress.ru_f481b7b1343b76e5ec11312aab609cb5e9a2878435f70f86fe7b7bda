            else {

                //document.getElementById("td_" + arr[i]).InnerHTML = '<a href="#" onclick="list_news('+arr[i]+'); return false;" id="item_'+arr[i]+'">'+document.getElementById("item_" + arr[i]).InnerHTML+'</a>';

                document.getElementById("td_" + arr19[i]).className = 'trazd2';

                document.getElementById("colum_" + arr19[i]).style.display = 'none';

            }

        }

    }

</script>

<table class="trazd">

<tr>


