<script>
$(document).ready(function(){
	$(".spec_extra_button").click(function() {
		$(".spec_projects_submenu").show();
		return false;
	});
	$(".spec_projects_submenu").click(function() {
		$(".spec_projects_submenu").hide();
	});
	$(document).click(function() {
		$(".spec_projects_submenu").hide();
	});
	$(".spec_projects_submenu").click(function(e) {
		e.stopPropagation();
	});
	$('.scrollable').scrollable({  
	    size: 3,
	    speed:400,
		easing: 'swing'
	}).circular();
});
</script>
