var a_curHref = curHref.split("/");
var a_host = a_curHref[3];
if(a_curHref[3]=='news') $("#m_news").addClass("active");
if(cursm==null&&(a_curHref[3]=='news'||a_curHref[3]=='articles')) {
cursm = sm_a[querySt("tid")];
}
if(cursm==null) {
for(var ii=0;ii<sm_a.length;ii++) { 
if(a_host==sm_a[ii]) cursm = sm_a[ii]; 
}
}
