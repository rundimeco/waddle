<script type="text/javascript">
    function getLikeForm() {
        if (!Cookie.get('likeUs')) {
            new Ajax.Request('http://www.pravda.ru/?area=popupLikeUs', {
                method		: 'get',
                onComplete	: function(transport) {
                    facebox.reveal(transport.responseText);
                    Cookie.set('likeUs', 1, 3, '/', '.www.pravda.ru');
                }
            });
        }
    }

    addLoadEvent(
        function() {
            setTimeout('getLikeForm()', 10000);
        }
    );
</script>
