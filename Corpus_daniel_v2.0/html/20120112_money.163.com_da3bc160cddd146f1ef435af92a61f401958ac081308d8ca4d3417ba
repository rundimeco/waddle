        (function NTES_Nav(){
            function Y_G(id){
                return document.getElementById(id);
            }
            function clickselect(e){
                e = e || window.event;
                e.cancelBubble = true;
                var name = this.innerHTML;
                var txt = document.createTextNode(name);
                selected.removeChild(selected.firstChild);
                selected.appendChild(txt);
                select_main.style.display = "none";
                var title = this.getAttribute("title");
                if (title == "新闻") {
                  Y_G("timesort").value="10";
                  Y_G("searchsite").value="163.com";
                } else {
                  Y_G("timesort").value="0";
                  Y_G("searchsite").value="";
                }
            }
            function a_mouseover () {
                this.className = "active";
            }
            function a_mouseout () {
                this.className = "";
            }
            Y_G("divSelectKey").innerHTML = '<div id="select_area_yd" class="select_box"><span id="selected_yd" class="search_site">新闻</span><div id="select_main_yd" class="select_list"><a title="新闻">新闻</a><a title="网页">网页</a></div></div>';
