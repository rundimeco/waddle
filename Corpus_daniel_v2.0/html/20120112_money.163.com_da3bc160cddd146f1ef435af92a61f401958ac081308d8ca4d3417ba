<script type="text/javascript">
var ydwords = new Array(
"订票电话加价"
, "最牛工资单"
, "淘宝商城改名"
, "辽宁钻石矿"
, "春晚主持阵容"
, "山东橡皮鸡蛋"
, "泄密门落网"
, "金龙鱼晒坏说"
, "网上售票垄断"
, "中国巴菲特离婚"
);
var ydwi=0;
var ydwlengthLimit = 13;
function ydwShow() {
  var html = "";
  var len = 0;
  var i = ydwi;
  while (true) {
    if (i >= ydwords.length) {
      i=0;
    }
    var kw = ydwords[i];
    if (kw.length > ydwlengthLimit) {
      i++;
      if (i == ydwi) {
        break;
      }
      continue;
    }
    len += kw.length;
    if (len > ydwlengthLimit) {
      ydwi = i;
      break;
    }
    if (window.encodeURIComponent) {
      var eq = encodeURIComponent(kw);
    } else {
      var eq = kw;
    }
    html += "<a target='_blank' href='http://www.youdao.com/search?q=" + eq + "&keyfrom=163.page.hotword'>" + kw + "</a>&nbsp;";
    len++;
    i++;
    if (i == ydwi) {
      break;
    }
  }
  document.getElementById("key").innerHTML = html;
  var t = setTimeout("ydwShow()", 5000);
}
//var t = setTimeout("ydwShow()", 1);
</script>
