function renderCaptcha312510() {
     var randNum = Math.ceil(Math.random()*10000000);
     var captchaImg = jQuery("#captcha_image-312510");
     if (captchaImg.length > 0) {
        captchaImg.attr("src","/captcha?" + randNum); 
     } else {
        var captcha = jQuery('#captcha_container-312510');
        captcha.html('<img src="/captcha?' +randNum +'" class="captcha_img" id="captcha_image-312510" />');
        jQuery("#captcha_image-312510").click(renderCaptcha312510);
    }
}

jQuery().ready(function() { 
    
    //jQuery("#captcha_container-312510").click(renderCaptcha312510);
    
    jQuery("#comment_this_article-312510").click(function() {
        firstTimeCaptcha312510();
        jQuery("#comments_container-312510").slideToggle("fast");
    });
    
});

  jQuery().ready(function() {
    /* "Error management" */  
    var validator = jQuery("#commentBlogPostForm-312510").validate(validation);
  });
  
</script>
</div>
  
  <div id="commentsField-312510" style="display: none;"></div>
  <script type="text/javascript">
    function showComments312510() {
        if (jQuery("#commentsField-312510 .commentList").length == 0) {
          polopoly.comments.get('/news/top-stories/new-summa-rehab-hospital-to-open-soon-in-akron-1.255410/7.312510', 0, "#commentsField-312510",
               function() { jQuery("#commentsField-312510").slideDown("fast"); });
      }
    }
    
    
    jQuery("#show_comments-312510").click(showComments312510);
    jQuery("#comment_this_article-312510").click(showComments312510);
  </script>
  </div>
</div>

      
  
                <br />
        </div> --> <!--end main coulm-->
         
                <div>
                 <img src='/logger/p.gif?a=1.255410&amp;d=/2.224/2.236/2.262&amp;type=ARTICLE' alt='' />                </div><!--end request preview-->
        
  </div><!--End center-story-page-->
  
      
  <!--End Main Story-->                
                

    

