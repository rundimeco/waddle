<script>
 function subnavurl(str)
	  {
	//alert(str);
		  if(str != "")
		  {
			var re = /\$|,|@|#|~|`|\/|\+|\=|\[|\-|\_|\]|\[|\}|\{|\;|\:|\'|\"|\<|\>|\||\\|\!|\$|\./g;
			str = str.replace(re,"");
			str = str.replace("?","ibncrickhabar");
			//alert($("#"+str+"-sssj").length)
			
		
		if($("#"+str+"-sssj").length)
		{
			//alert("A");
			var pid = $("#"+str+"-sssj").parent()[0].id;
			document.getElementById(str+"-sssj").style.background = '';
			var tpid = pid.split("-sss");
			var  superid = tpid[0];
			var ssfid = $('ul#'+pid+' li:first').attr('id'); 
