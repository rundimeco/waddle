</div><script type="text/javascript" xmlns:QueryString="urn:QueryString">
            
                $(document).ready(function() {
                    
                    $('.media_gallery').each(function() {
                        var galleryContainer = $(this);
                        var videosContainer = $('.videoContent', galleryContainer);
                        var albumsContainer = $('.photo_content', galleryContainer);
                        
                        var videosTab = $('.media_gallery_video_tab', galleryContainer);
                        var sepTab = videosTab.parent().next();
                        var albumsTab = $('.media_gallery_photo_tab', galleryContainer);
                        
                        var videoItems = $('.videoL', videosContainer);                        
                        var albumItems = $('.thumb', albumsContainer);
                        
                        if(videoItems.length == 0 || albumItems.length == 0) {
                            if(videoItems.length == 0 && albumItems.length == 0) {
                                galleryContainer.hide();
                            }
                            else {
                                sepTab.hide();
                                if(videoItems.length == 0) {
                                    albumsTab.click();
                                    videosTab.hide();
                                }
                                else {
                                    videosTab.click();
                                    albumsTab.hide();
                                }
                            }
                        }
                    });
                });
            
        </script>

