            <script>

                function showSponsoredFeaturex31(){
                    if (window.sponJSON) {
                        if(typeof(sponJSON) === 'object') {
                          adx31 = sponJSON;
                          var node = jQ('.json-features.x31');
                          convertJSONtoAd(
                              node.find('img:first-child'),
