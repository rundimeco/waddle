      width: 70, height: 23
    };
    var faceBookLikeScrollableOptions = {
      src: href="http://www.facebook.com/plugins/like.php?locale=pl_PL&amp;href="+encodeURIComponent(location.href)+"&amp;layout=box_count&amp;show_faces=false&amp;width=70&amp;action=like&amp;colorscheme=light&amp;height=70",
      attr: { scrolling: "no", frameborder: "0", style: "border:none; overflow:hidden; width:70px; height: 90px;", allowTransparency: "true" },
      width: 70, height: 90
    };
    $(document).ready(function(){
        $('#articleRecommend .fb').iframe(faceBookLikeOptions);
        $('#articleRecommendScroll .fb').iframe(faceBookLikeScrollableOptions);
    });
    $('#articleRecommend .tp').remove();
  }
  else {
    $('#articleRecommend .fb').html('<fb:like layout="button_count" show_faces="false" action="like" colorscheme="light" width="70" height="20"></fb:like>');
    $('#articleRecommend .tp').html('<fb:send></fb:send>');
    $('#articleRecommendScroll .fb').html('<fb:like send="true" layout="box_count" width="70" height="90" show_faces="false"></fb:like>');
    window.fbAsyncInit = function() {
      FB.init({ appId: '', channelURL: 'http://www.wspolczesna.pl/js/channel.html', status: true, cookie: true, oauth: true, xfbml: true });
    };
    (function(d){
      var js, id = 'facebook-jssdk'; if (d.getElementById(id)) {return;}
      js = d.createElement('script'); js.id = id; js.async = true;
      js.src = "//connect.facebook.net/pl_PL/all.js";
      d.getElementsByTagName('head')[0].appendChild(js);
    }(document));
  }
}
