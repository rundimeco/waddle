<script type="text/javascript">
  (function(tr){
    var licznik,
    nazwa,
    lista = ['aktualnosci', 'polska', 'popularne', 'komentowane'],
    zakladki = ['pierwsza', 'druga', 'trzecia', 'czwarta'],
    i,
    ln;
    GR('$$', 'zakladka-link', $('rankingi-lista'), '', function(e) {
      nazwa = e.getElementsByTagName("a").item(0).innerHTML;
      tr.push({elements: [e], href: '/ranking/zakladka:' + nazwa.trim()});
    });
    
    ln = zakladki.length;
    for(i = 0; i < ln; i++) {
      licznik = 1;
      GR('$$', 'artykul-link-zakladka-' + lista[i], $('najpopularniejsze-box-linki'), '', function(e) {
        tr.push({elements: [e], href: '/ranking/' + lista[i] + '/link:' + licznik++});
      });
    }
    ln = zakladki.length;
    for(i = 0; i < ln; i++) {
      if($('zakladka-ranking_' + zakladki[i])) {
        GR('$$', 'wiecej', $('zakladka-ranking_' + zakladki[i]), '', function(e) {
          var zakladka = '' + lista[i];
          tr.push({elements: [e.getElementsByTagName("a").item(0)], href: '/ranking/zakladka:' + zakladka + '/wiecej'});
        });
      }
    }
  })(oG_OSA.tracking);
</script>
