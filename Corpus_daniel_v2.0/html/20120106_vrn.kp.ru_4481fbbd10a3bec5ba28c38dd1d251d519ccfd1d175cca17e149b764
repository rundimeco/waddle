	(function(){
		function isF(c,t){
			var is=!!(document.cookie.indexOf(c)==-1),e;
			if (is){
				e=new Date();e.setTime(e.getTime()+t);
				document.cookie=c+'=1; expires='+e.toGMTString()+'; path=/;';
			}
			return is
		}
