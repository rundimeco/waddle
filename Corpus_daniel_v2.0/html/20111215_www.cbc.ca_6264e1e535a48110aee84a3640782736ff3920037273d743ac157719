	<h3>Related</h3>
	
	<!-- MEDIA BUNDLE -->
		





	<!-- END MEDIA BUNDLE -->
	
		<!-- EXTERNAL LINKS -->
		<div id="extlinks">
					<h4>External Links</h4>
					<dl>
					
							<dt><a target="_new" href="http://www.thelancet.com/journals/lanonc/article/PIIS1470-2045(11)70296-0/fulltext">DOCUMENT: Human papillomavirus testing </a></dt>
					
					</dl>
				<p><em>(Note:CBC does not endorse and is not responsible for the content of external links.)</em></p>
