    <script type="text/javascript">
        function hidePlaceholder() {
            var inp = $('searchString');
            if (inp.value == 'Поиск')
                inp.value = '';            
        }
        function showPlaceholder() {
            var inp = $('searchString');
            if (inp.value == '')
                inp.value = 'Поиск';            
        }
        function exampleSearch() {
            $('searchString').value = $('exampleSearchLink').innerText;
            window.open ($('exampleSearchLink').href, $('exampleSearchLink').title);
            window.focus();
            return false;
        }
    </script>
