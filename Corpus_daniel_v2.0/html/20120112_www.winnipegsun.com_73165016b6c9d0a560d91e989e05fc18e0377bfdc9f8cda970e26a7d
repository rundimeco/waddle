			<script>
				
				$(function() { 
					
					var news_pin = [{"title":"Misericordia","latitude":"49.879527","longitude":"-97.160747","zindex":"0","information":""}];
					var useZoom = 0;
					if (false){
						useZoom = 0;
					}
					
					function setMarkers(map, locations) {
						// Add markers to the map
				
						// Marker sizes are expressed as a Size of X,Y
						// where the origin of the image (0,0) is located
						// in the top left of the image.
				
						// Origins, anchor positions and coordinates of the marker
						// increase in the X direction to the right and in
						// the Y direction down.
						var image = new google.maps.MarkerImage('/assets/img/interface/sun_mst_map_pointer.png',
								// This marker is 20 pixels wide by 32 pixels tall.
								new google.maps.Size(25, 32),
								// The origin for this image is 0,0.
								new google.maps.Point(0,0),
								// The anchor for this image is the base of the flagpole at 0,32.
								new google.maps.Point(20, 30));
						var shape = {
								coord: [1, 1, 1, 20, 18, 20, 18 , 1],
								type: 'poly'
						};
						
						for (var i = 0, n = locations.length; i < n; i++) {
							googlemap_pin(locations[i]);
						}
									  
						function googlemap_pin(news_pin){
							var myLatLng = new google.maps.LatLng(news_pin.latitude, news_pin.longitude);
							var marker = new google.maps.Marker({
								position: myLatLng,
								map: map,
								icon: image,
								shape: shape,
								title: news_pin.title
							});
										
							google.maps.event.addListener(marker, 'click', function(event) {
								var info = new google.maps.InfoWindow();
								info.setContent(news_pin['information']);
								info.setPosition(myLatLng);
								info.open(map);
							});
						}
									  
					}
					
					var div = document.getElementById("map_canvas");
					if (div) {
						var bounds = new google.maps.LatLngBounds();
						for (var i = 0, n = news_pin.length; i < n; i++) {
							var pin = news_pin[i]
							var latlng = new google.maps.LatLng(pin.latitude, pin.longitude)
							bounds.extend(latlng);
						}
						
						var map = new google.maps.Map(div, { mapTypeId: google.maps.MapTypeId.ROADMAP });
						
						
						if (useZoom == 0) {
						  map.fitBounds(bounds);
						} else {
						  map.setCenter(bounds.getCenter());
						  map.setZoom(useZoom);
						}
		
		
						setMarkers(map, news_pin);
					}
					
					var img = $('#map_generated');
					if (img) {
						var html = '<a href="#map_canvas" class="mapBox"><img class="photo" src="http://maps.google.com/maps/api/staticmap?size=210x180&maptype=roadmap&markers=icon:http://img35.imageshack.us/img35/4084/sunmstmappointer.png';
						for (var i = 0, n = news_pin.length; i < n; i++) {
							var x = news_pin[i];
							html += '|' + x['latitude'] + ',' + x['longitude'];
						}
						html += '&sensor=false';
						if (useZoom > 0) {
							html += '&zoom=' + useZoom;
						}
						html +=  '" alt=""/></a>';
						img.html(html);
					}
					
					
					// Google maps
					$(".mapBox").live('click',function(){
						$.colorbox({
							inline:true,
							innerWidth:725,
							href:'#map_canvas',
							innerHeight:500,
							onComplete:function(){
								google.maps.event.trigger(map, "resize");
								if (useZoom == 0) {
									  map.fitBounds(bounds);
								} else {
								  map.setCenter(bounds.getCenter());
								  map.setZoom(useZoom);
								}
							}
						});
					});
					
				});
				
			</script>
		


					
							
			
			
			
			













		
		
	
	
	
	



