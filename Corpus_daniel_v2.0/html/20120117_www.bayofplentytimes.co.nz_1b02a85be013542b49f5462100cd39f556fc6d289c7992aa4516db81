    // highlight the menu item
    
        $('#mainNav > ul > li > a.mt_news:first').addClass('current');
        $('#mainNav > ul > li > ul.mt_news:first').addClass('current');
    
    
    /*
    We need to drop in an adSpace if the Content (not including html elements) is GT 1500 characters
    */
    $(document).ready(function() {
        if ($(".storyContent .entryAdspace").length > 0) {
            var lengthCheck = 1250;
            var storyContent = $(".storyContent");
            if (storyContent.children("p").text().length > lengthCheck) {
                count = 0;
                // In ported data the story content uses double br tags instead of paragraph tags, the following is to deal with that
                if (storyContent.children("p").length == 1) {
                    var text = storyContent.children("p").html();
                    var adInsert = false;
                    var running = 0;
                    var pos = 0;
                    var textManip = text;
                    // loop through story content finding br tags and find a good safe position to insert the adspace div
                    while (adInsert == false) {
                        pos = textManip.indexOf("<br>");
                        // if a <br isn't found... ABORT!
                        if (pos == -1) {
                            adInsert = true;
                        } else {    
                            if (running+pos >= 1250) {
                                adInsert = true;
                            } else {
                                running += pos;
                                textManip = textManip.substr(pos, textManip.length);
                                textManip = textManip.replace("<br>", '');
                                // add 4 characters to compensate for the stripped out <br> tag
                                running += 4;
                            }
                        }
                    }
                    if (running > 0) {
                        var pre = text.substring(0, running);
                        var post = text.substring(running, text.length);
                        // get the html of the adspace including itself
                        var ad = $(".storyContent .entryAdspace").clone().wrap('<div></div>').parent().html();
                        $(".storyContent .entryAdspace").remove();
                        $(".storyContent").children("p").html(pre+ad+post);
                    }
                    renderContentAd();
                } else {
                    $.each(storyContent.children("p"), function() {
                        count += $(this).text().length;
                        if (lengthCheck < count) {
                            $(this).after($(".storyContent .entryAdspace"));
                            renderContentAd();
                            return false;
                        }
                    });
                }
            }
        }
    });
