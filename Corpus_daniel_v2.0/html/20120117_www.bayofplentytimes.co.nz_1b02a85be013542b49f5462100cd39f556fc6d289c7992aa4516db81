$("body").bind("APNCore.Advertising.complete", function () {
    var $salesCarousel, $salesCarouselItems, $bindItems, bindItemsArray, interval, assignedInterval;

    //Cache
    $salesCarouselContainer = $("#salesCarouselContainer");
    $salesCarousel = $("#salesCarousel");
    $salesCarouselNav = $("#salesCarouselNav");
    
    //Ad cleanup for bserver items
    $salesCarousel.find(".salesCarouselItem:hidden").each(function() {
        //remove the a link based off 'id' naming
        var itemNum = $(this).attr('id').split('salesCarouselAd',2);
        var itemName = "#salesCarouselNav ."+itemNum[1];
        $(itemName).remove();
        $(this).remove(); //remove the hidden div
    });
    
    //Prepare a list of selectors to feed to the plugin after cleanup has occurred above
    $bindItems = $salesCarouselNav.children("a");

    if ($salesCarousel.find(".salesCarouselItem").length > 0) {
        if ($salesCarousel.find(".salesCarouselItem").length > 1) {
            
            //Show, and highlight the first item - which should have been after cleanup.
            $salesCarouselNav.show().children(":first").addClass("current");
            
            //While hovered, the adverts don't continue.
            interval = 5000;
            assignedInterval = setInterval("$('#nextSalesCarouselItem').trigger('click');", interval);
            $('#salesCarouselContainer').hover(function() {
              assignedInterval = clearInterval(assignedInterval);
            }, function() {
              assignedInterval = setInterval("$('#nextSalesCarouselItem').trigger('click');", interval);
            });
            
            $salesCarousel.jCarouselLite({
                //auto: 5000,
                btnNext: "#nextSalesCarouselItem",
                btnPrev: "#prevSalesCarouselItem",
                visible: 1,
                scroll: 1,
                btnGo: $bindItems,
                afterEnd: function(a) {
                    var salesCarouselItem =  a.attr('id').split('salesCarouselAd',2);
                    var theItem = "#salesCarouselNav ."+salesCarouselItem[1];
                    //console.log(theItem);
                    $(theItem).addClass("current").siblings().removeClass("current");
                }
            });
            
            
            
        }
    } else {
        $salesCarouselContainer.hide();
    }
});
