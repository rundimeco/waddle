    /*
     * Initialise FB
     */

    window.fbAsyncInit = function() {

        if ( $.browser.msie && $.browser.version <= 9.0 ) {
            // Taking the channel out for IE 7/8/9 users
            FB.init({appId: '227245930623113', status: true, cookie: true, xfbml: true, oauth: true});
        } else {
            FB.init({appId: '227245930623113', status: true, cookie: true, xfbml: true, oauth: true, channelUrl: 'http://www.bayofplentytimes.co.nz/fb_channel.html'});
        }

        // This code solves a FB bug
        FB.UIServer.setLoadedNode = function (a, b) {
            FB.UIServer._loadedNodes[a.id] = b;
        };

        FB.Event.subscribe('edge.create', function(response) {
            var key = "like:";
            if (typeof Activity !== "undefined") {
                if (response.indexOf('://') > -1) {
                    key = key + "/" + response.split("/").slice(3).join("/");
                } else {
                    key = key + response;
                }
                Activity.record(key);
            }
        });
    };

    // Load the SDK Asynchronously
    (function(d){
        var js, id = 'facebook-jssdk'; if (d.getElementById(id)) {return;}
        js = d.createElement('script'); js.id = id; js.async = true;
        js.src = "//connect.facebook.net/en_GB/all.js";
        d.getElementsByTagName('head')[0].appendChild(js);
    }(document));

    function facebookConnect(next) {
        next = next ? next : '/';
        FB.getLoginStatus(function(response) {
            if (response.authResponse) {
                // user is logged in and granted some permissions.
                window.location = '/facebook/login/?next=' + next;
            } else {
                // user cancelled login or did not grant authorization, try login
                FB.login(function(response) {
                    if (response.authResponse) {
                        window.location = '/facebook/login/?next=' + next;
                    }
                }, {scope: 'email'});
            }
            return false;
        });
        return false;
    }

