<script language='JavaScript'>
var docUrl = "http://ent.ifeng.com/idolnews/special/gdgdizidajizhe/news1/detail_2012_01/12/11935105_0.shtml";
var docName = "郭德纲受访向伤者道歉 称艺人生存现状很悲催";
