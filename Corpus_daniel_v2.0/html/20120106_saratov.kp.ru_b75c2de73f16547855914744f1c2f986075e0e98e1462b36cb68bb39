var addate = new Date();
(function(placeholderId, bannerPosition, banWidth, banHeight, link){
    var dl = escape(document.location),
        placeholder = document.getElementById(placeholderId);
    link += '&dl=' + dl + '&phid=' + placeholderId + '&pap1=' + bannerPosition;
    function addEvent(e,t,f){
        if (e.addEventListener) e.addEventListener(t, f, false);
        else if (e.attachEvent) e.attachEvent('on'+t, f);
    }
    function removeEvent(e,t,f){
        if (e.removeEventListener) e.removeEventListener(t, f, false);
        else if (e.detachEvent) e.detachEvent('on'+t, f);
    }
    function loadScriptAdWolf(obj,src){
        var createElementScript = document.createElement("script");
        createElementScript.type = 'text/javascript';
        createElementScript.async = true;
        createElementScript.src = src;
        afterPlaceholder = obj.nextSibling;
        afterPlaceholder.parentNode.insertBefore(createElementScript, afterPlaceholder);
    }
    function getPosition(obj){
        var t = 0, l = 0;
        if (obj) {
            while (obj.offsetParent) {
                t += obj.offsetTop;
                l += obj.offsetLeft;
                obj = obj.offsetParent;
            }
        }
        return {"left":l, "top":t};
    }
    function getWindowGeometry() {
        var cMode = document.compatMode,
            ch = self.innerHeight || cMode == "CSS1Compat" && document.documentElement.clientHeight || document.body.clientHeight,
            st = self.pageYOffset || cMode == "CSS1Compat" && document.documentElement.scrollTop || document.body.scrollTop,
            cw = self.innerWidth || cMode == "CSS1Compat" && document.documentElement.clientWidth || document.body.clientWidth,
            sl = self.pageXOffset || cMode == "CSS1Compat" && document.documentElement.scrollLeft || document.body.scrollLeft;
        return {"clientHeight":ch, "clientWidth":cw, "scrollTop":st, "scrollLeft":sl};
    }
    function getBannerVisibility(){
        var position = getPosition(placeholder),
            windowGeometry = getWindowGeometry();

        if ( (position.top < windowGeometry.clientHeight && position.left < windowGeometry.clientWidth) || ( (windowGeometry.scrollTop - position.top < banHeight && windowGeometry.clientHeight + windowGeometry.scrollTop > position.top) && (windowGeometry.scrollLeft - position.left < banWidth && windowGeometry.clientWidth + windowGeometry.scrollLeft > position.left) ) ) { 
            removeEvent(window, 'scroll', getBannerVisibility);
            loadScriptAdWolf(placeholder,link);
        }
    }
    if (placeholder) addEvent(window, 'scroll', getBannerVisibility);
})('placeholderIdAdwolf40x200', 'center_left', 40, 200, 'http://a.adwolf.ru/prepareCode?p1=cec&p2=cd&pct=a&pfc=a&pfb=a&plp=a&pli=a&pop=a&pr=' + pr +'&pt=b&pd=' + addate.getDate() + '&pw=' + addate.getDay() + '&pv=' + addate.getHours() + '&prr=' + afReferrer + '&puid1=&puid2=&puid3=&puid5=&puid6=');
</script> 
<!-- Asynchronous code AdWolf end -->
