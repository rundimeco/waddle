<div id="uslotitem8627"><!--  START facebook app  -->
<div id="fb-root"></div>
<script type="text/javascript" src="http://www.google.com/jsapi"></script>
<script language="JavaScript">
//	google.load("prototype", "1.6.1.0");
//	google.load("scriptaculous", "1.8.2");
	google.setOnLoadCallback(OnLoad);

	function OnLoad(){
			//consolelog('Google JavaScript API are loaded.');
			var f_login=0;
 
			window.fbAsyncInit = function() {
				FB.init({appId: '191944177588942', cookie: true, logging: true, status: true, xfbml: true});
 
				FB.Event.subscribe('auth.login', function(response) {
					//consolelog('User is Logged In');
				});
 
				FB.Event.subscribe('auth.logout', function(response) {
					//consolelog('Session was terminated!');
				});
 
				FB.Event.subscribe('edge.create', function(response) {
					//consolelog("User liked <a target='_blank' href='" + response + "'>this page.</a>");
				});

				FB.getLoginStatus(function(response) {
					if (response.status === 'connected') {
						// the user is logged in and has authenticated your app, and response.authResponse supplies
						// the user's ID, a valid access token, a signed request, and the time the access token 
						// and signed request each expire
						var uid = response.authResponse.userID;
						var accessToken = response.authResponse.accessToken;
						$('fb_loader').show();
						$('fb_nologin').hide();
						f_login=1;

						// проверяем состояние публикации
						var f_switch = parseInt(readCookie('fbSwitch'));
						if(f_switch != -1){ // если не запрещено, то ставим по умолчанию разрешение
							createCookie('fbSwitch', 1, 90);
							$('fb_switch').removeClassName('off');
							$('fb_switch').addClassName('on');
						}

						// получаем данные пользователя
						FB.api('/me?fields=first_name,last_name,link,username,picture', function(response) {
							////consolelog('Get user info. '+response.picture+' '+response.first_name+' '+response.last_name+' ');
							fbUserInfo(response); // выведем данные пользователя
							fbUserActivity();
						});
						

					} else if (response.status === 'not_authorized') {
						// the user is logged in to Facebook, but has not authenticated your app
						$('fb_loader').hide();
						$('fb_login').hide();
						$('fb_nologin').show();
						fbPopupShow();
					} else {
						// the user isn't logged in to Facebook.
						$('fb_loader').hide();
						$('fb_login').hide();
						$('fb_nologin').show();
						fbPopupShow();
					}
					//consolelog('Status: '+response.status);

				}, true);
			};

			(function() {
				var e = document.createElement('script');
				e.async = true;
				e.src = document.location.protocol + '//connect.facebook.net/ru_RU/all.js';
				document.getElementById('fb-root').appendChild(e);
			}());
	}
	
	function fbPopupShow() {
		$('fb_loader').hide();
		$('fb_login').hide();
		$('fb_nologin').show();

		var f_popup = parseInt(readCookie('fbPopup'));
		if(f_popup != 1){ // если еще не показали поп-ап
			//$('fb_popup').show(); // поп-ап для рекламмы приложения
			var domain = document.domain.split(".").splice(-2).join('.');
			createCookie('fbPopup', 1, 1,'.'+domain); // запоминаем в куке 
		}
	}
 
	function fbUserInfo(response){

		var img = new Element('img', {src: response.picture.data.url, title: response.first_name+" "+response.last_name});

		$('fb_avatar').href=response.link;
		$('fb_avatar').innerHTML='';
		$('fb_avatar').appendChild(img);

		$('fb_username').innerHTML = response.first_name+" "+response.last_name;
		$('fb_username').href=response.link;
		
		$('fb_profile_url').href=response.link;
		
		var f_switch = parseInt(readCookie('fbSwitch'));
		if(f_switch == 1){
			$('fb_switch').removeClassName('off');
			$('fb_switch').addClassName('on');
		}else{
			$('fb_switch').removeClassName('on');
			$('fb_switch').addClassName('off');
		}

		$('fb_loader').hide();
		$('fb_login').show(); // покажем инфо пользователя
	}

	function deleteRequest(requestId) {
		FB.api( "/" + requestId, "delete",
		function(response) {
			//consolelog(dump(response));
			$('act_li_'+requestId).fade({duration: 0.3});
		});
	}

	function fbUserActivity(){
		postAct();

		// удаляем старые действия
		while ($('fb_activity_list').childNodes.length > 0){
			$('fb_activity_list').removeChild($('fb_activity_list').lastChild);
		}

		// получаем последние действия
		FB.api('/me/news.reads?date_format=U', function(response) {
			for( var i = 0; i < (( response.data < 6 ) ? data.length : 6 ); i++  ){
				if(response.data[i] != undefined){
					//consolelog(" QQ: " +response.data[i]['data']['article']['title']+" "+response.data[i]['id']+" "+response.data[i]['start_time']);
				
					var li = new Element('li', {id: 'act_li_'+response.data[i]['id']});
					var a_t =  new Element('a', {href: response.data[i]['data']['article']['url'], target: '_blank', class: 'news'}).update(response.data[i]['data']['article']['title']);
					li.appendChild(a_t);
					var d_date = new Element('div', {class: 'date'}).update(GetTime(response.data[i]['start_time']));
					li.appendChild(d_date);
					var a_delete =  new Element('a', {href: '#', title: 'Удалить запись', class: 'close', onClick: 'deleteRequest(' + response.data[i]['id']+'); return false;'});
					li.appendChild(a_delete);
				
					$('fb_activity_list').appendChild(li);
				}
			}
		});		
	}

 	function fbLogout(){
		FB.logout(function(response) {
			$('fb_login').hide();
			$('fb_loader').show();
			document.location.href = document.location.href;
		});
	}
 
	function fbLogin(){
		FB.login(function(response) {
			if (response.authResponse) {
			
				//consolelog('Welcome!  Fetching your information.... ');
				$('fb_nologin').hide();
				$('fb_loader').show();
				//$('fb_popup').hide(); // скрываем поп-ап

				// проверяем состояние публикации
				var f_switch = parseInt(readCookie('fbSwitch'));
				if(f_switch != -1){ // если не запрещено, то ставим по умолчанию разрешение
					createCookie('fbSwitch', 1, 90);
					$('fb_switch').removeClassName('off');
					$('fb_switch').addClassName('on');
				}

				FB.api('/me?fields=first_name,last_name,link,username,picture', function(response) {
					//consolelog('Good to see you, ' + response.name + '.');
					fbUserInfo(response);
					fbUserActivity();
				});

			} else {
				//consolelog('User cancelled login or did not fully authorize.');
			}
		}, {scope:'publish_actions'});
	}

//function createCookie( name, value, days ) {
//function readCookie( name ) {
//function eraseCookie( name ) {
	function fbSwitch(){
		var f_switch = parseInt(readCookie('fbSwitch'));
		if(f_switch == 1){
			createCookie('fbSwitch', -1, 90);
			$('fb_switch').removeClassName('on');
			$('fb_switch').addClassName('off');
		}else{
			createCookie('fbSwitch', 1, 90);
			$('fb_switch').removeClassName('off');
			$('fb_switch').addClassName('on');
			postAct(); // если включили, то сразу публикуем активность
		}
	}

	function fbRemoveApp(){
		FB.api("me/permissions", "DELETE", function(response){
			eraseCookie('fbSwitch');
			document.location.href = document.location.href;
		});
	}
	
	function postAct(){
		if($$("meta[property='og:type']")[0] == undefined) return;

		var f_switch = parseInt(readCookie('fbSwitch'));
		//consolelog("Switch: " +f_switch);
		var url='';
		
		if($$("meta[property='og:type']")[0].getAttribute('content') == 'article' && f_switch != -1){
			$('fb_switch').addClassName('switch-on');
			url=$$("meta[property='og:url']")[0].getAttribute('content');
		}else{
			return;
		}

		FB.api(
			'/me/news.reads?article=' + url,
			'post',
			function(response) {
				if (!response || response.error) {
					//consolelog('Error post act');
				} else {
					//consolelog('Read successful! Action ID: ' + response.id);
					//var ll = new Element('li').update('adasdasdasdasd'); // добавим в ленту активности
					//$('fb_activity_list').insertBefore(ll, $('fb_activity_list').firstChild);	

					
					fbUserActivity(); // обновляем активность, при успешной публикации
				}
		});
	}

	Event.observe(document.body, 'click', function(event) {
		// First element from event source with 'container' among its CSS classes
		if (!$(Event.element(event)).up('#fb_login') && !$(Event.element(event)).up('#fb_corner') && !$(Event.element(event)).up('#fb_settings') ){
			fbHidePopup();
		}else{
			return true; // alert("No..."); //
		}
	});	

	function GetTime (utime){
		var current_timestamp = Math.round((new Date()).getTime() / 1000);
		var time_ago = current_timestamp - utime,
		days = Math.floor( time_ago/86400 ),
		hours = Math.floor( ( time_ago - days * 86400)/3600 ),
		min = Math.floor( ( time_ago - days * 86400 - hours * 3600 )/60 ),
		sec = time_ago - days * 86400 - hours * 3600 - min * 60,
		time_ago_text = (( days > 0 ) ? days + " " + WordMorf( days, "день", "дня", "дней" ) :
			(( hours > 0 ) ? 
				hours + " " + WordMorf( hours, "час", "часа", "часов" ) : 
					(( min > 0 ) ? 
						min + " " + WordMorf( min, "минута", "минуты", "минут" ) : 
							(( sec > 0 ) ? sec + " " + WordMorf( sec, "секунда", "секунды", "секунд" ) : "0" ) 
					)
			)
		) + " назад";
										
		return time_ago_text;
	}
	
	
	function WordMorf(n, one, two, many){
		//WordMorf( n, 'кальмар', 'кальмара', 'кальмаров' ); 
		return n % 10 == 1 && n % 100 != 11 ? one : n % 10 >= 2 && n % 10 <= 4 && (n % 100 < 10 || n % 100 >= 20) ? two : many;
	}

	function fbHidePopup(){
		if($('fb_activity').style.display != "none") $('fb_activity').fade({ duration: 0.2 });
		if($('fb_settings').style.display != "none") $('fb_settings').fade({ duration: 0.2 });
		if($('fb_corner').style.display != "none") $('fb_corner').fade({ duration: 0.2 });
	}

	
	
		function dump(arr,level) {
	var dumped_text = "";
	if(!level) level = 0;
	
	//The padding given at the beginning of the line.
	var level_padding = "";
	for(var j=0;j<level+1;j++) level_padding += "	";
	
	if(typeof(arr) == 'object') { //Array/Hashes/Objects 
		for(var item in arr) {
			var value = arr[item];
			
			if(typeof(value) == 'object') { //If it is an array,
				dumped_text += level_padding + "'" + item + "' ...\n";
				dumped_text += dump(value,level+1);
			} else {
				dumped_text += level_padding + "'" + item + "' => \"" + value + "\"\n";
			}
		}
	} else { //Stings/Chars/Numbers etc.
		dumped_text = "===>"+arr+"<===("+typeof(arr)+")";
	}
	return dumped_text;
}	


</script>

