    function initZeroClipboard() {
		clip = new ZeroClipboard.Client();
        clip.setHandCursor( true );
		clip.glue( 'ccopy' );
		clip.addEventListener('complete', oncomplete);
    }
	function oncomplete(client) {
		$('ccopy').innerHTML = "Код скопирован";
