<script language="javascript">
$(document).ready(function(){
	$("#ysm a").click(function () {
		if ($(this).attr("class").indexOf("ysmkw") == -1){
			if ($(this).attr("href").indexOf("url.asp") == -1){
			$(this).attr("href", function() {
			return "/url.asp?url=" + escape(this.href);	 });
			};
		};
	});
});
