	YUI(_CBC_LOADER).use('cbc_core', 'node', 'gigya', function(Y, result){
		if (result.success){
			var conf = {
				APIKey:CBC.SETTINGS.getVal('gigyaApiKey')
			};
				
			var ua = new gigya.services.socialize.UserAction();
			ua.setUserMessage("Share this CBC content");
			ua.setLinkBack(document.location.href);
			
			ua.setTitle("Ecstasy laced with meth blamed in 5 Calgary deaths");
			
			var cid = '';
			
				cid = "Canada";
			
			var params1 ={
				userAction:ua,
				shareButtons:'facebook, twitter, google-plusone, share, email',
				containerID: "sharecount",
				cid:cid,
				layout:'vertical'
			};
			var params2 ={
				userAction:ua,
				shareButtons:'facebook, twitter, google-plusone, share, email',
				containerID: "sharetools",
				showCounts: "none",
				cid:cid,
				layout:'horizontal'
			};
			var params3 ={
				userAction:ua,
				shareButtons:'facebook, twitter, google-plusone, share, email',
				containerID: "sharetoolstwo",
				showCounts: "none",
				cid:'',
				layout:'horizontal'
			};
			if(Y.one("#sharecount"))
				gigya.services.socialize.showShareBarUI(conf,params1);
			if (Y.one("#sharetools"))
				gigya.services.socialize.showShareBarUI(conf,params2);
			if (Y.one("#sharetoolstwo"))
				gigya.services.socialize.showShareBarUI(conf,params3);
		}
	});
</script>

	
