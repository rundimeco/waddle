                                                gogoFacebook();                              
                                }
                }
}
function gogoFacebook(){
var facebook_span = document.getElementById('facebookComments');
facebook_span.style.display = "block";  
theelement = document.getElementById('facebookComments');
facebooknode = document.createElement('fb:comments');
var links = document.getElementsByTagName('link');
for (var x=0,y=links.length; x<y; x++) {
  if (links[x].rel.toLowerCase() == "canonical") {
    canonical = links[x];
    //facebooknode.setAttribute('href', canonical.href);
    facebooknode.setAttribute('xid', canonical.href);
   }
}
facebooknode.setAttribute('width', '625');
facebooknode.setAttribute('migrated', '1');
theelement.appendChild(facebooknode);
}
</script>

  
  

