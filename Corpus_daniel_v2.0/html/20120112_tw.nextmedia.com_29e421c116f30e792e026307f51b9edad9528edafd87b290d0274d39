	<script type="text/javascript">
		var submenu_is_on = false;
		$(document).ready(function(){
			$("#site_nav li.bigger >a.drop_btn").toggle(function(){
				$("nav ul li .submenu:visible").hide();
				$(this).parent().children("ul").show();
				$(this).parent().children("div").show();
				submenu_is_on = true;
			},
			function(){
				$("nav ul li .submenu:visible").hide();
				submenu_is_on = false;	
			})
			$("body").mouseup(function(){
				if(submenu_is_on){
					$("nav ul li .submenu:visible").hide();
					submenu_is_on = false;
				}
			});	
		});
		
	</script>
