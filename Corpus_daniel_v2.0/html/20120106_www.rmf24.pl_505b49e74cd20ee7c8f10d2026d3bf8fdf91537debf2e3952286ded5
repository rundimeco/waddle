jQuery(document).ready(function(){
submitMenyTreadsPrevent = function(form){

var form=jQuery(form);
var ret = true;

form.find('li.text').removeClass('invalid').find('.invalidInfo').text('');
form.find('li.title').removeClass('invalid').find('.invalidInfo').text('');
form.find('li.author').removeClass('invalid').find('.invalidInfo').text('');

if(!form.find("#threadText").val()) {
form.find('li.text').addClass('invalid').find('.invalidInfo').text('Wpisz treść');
ret=false;
} else if(form.find("#threadText").val().length > 3000) {
form.find('li.text').addClass('invalid').find('.invalidInfo').text('Treść przekracza dozwoloną długość');
ret=false;
}
if(form.find("#threadTitle").is('input') && !form.find("#threadTitle").val()) {
form.find('li.title').addClass('invalid').find('.invalidInfo').text('Wpisz tytuł');
ret=false;
} else if( form.find("#threadTitle").is('input') && form.find("#threadTitle").val() > 150 ) {
form.find('li.title').addClass('invalid').find('.invalidInfo').text('Tytuł przekracza dozwoloną długość');
ret=false;
}
if(!form.find("#threadAuthor").val()){
form.find('li.author').addClass('invalid').find('.invalidInfo').text('Wpisz swój podpis');
ret=false;
} else if(form.find("#threadAuthor").val().length > 255){
form.find('li.author').addClass('invalid').find('.invalidInfo').text('Podpis przekracza dozwoloną długość');
ret=false;
}

return ret;
}

jQuery('#createThreadForm form').submit(function(){
return submitMenyTreadsPrevent(this);
});

jQuery(document).ready(function(){
if(Inpl.User.isLoggedIn()){
try{
var forumUserAvatar = Inpl.User.getAvatar({photoClass: 'C300'});

jQuery('.threadData input#threadAuthor').replaceWith('<span class="user">'+Inpl.User.getNick()+'</span><input type="hidden" name="threadAuthor" id="threadAuthor" value="'+Inpl.User.getNick()+'" />');

if(forumUserAvatar && true){
jQuery('fieldset.threadData .author label').after('<img src="'+forumUserAvatar+'" alt="'+Inpl.User.getNick()+'" />');
}

jQuery(this).find('.author .addInfo').hide();
}catch(e){}
}
});
});    //]]>
</script>
