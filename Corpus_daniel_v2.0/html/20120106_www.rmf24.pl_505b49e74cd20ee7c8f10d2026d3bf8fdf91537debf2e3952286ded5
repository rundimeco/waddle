jQuery(document).ready(function(){
(function(){
var boxJQ  = jQuery('#relatedContentList1');
var listJQ = boxJQ.find('.list');
var paginationJQ = boxJQ.find('.pagination');
var queueJQ = jQuery('<div/>');

paginationJQ.find('span').click(function(){
var packId = jQuery(this).attr('class');
var parent = jQuery(this).parent('li').get(0);

packId = packId.substr(1);
packId = parseInt(packId,10);

if(packId && !jQuery(parent).hasClass('current')) {

queueJQ.queue(function(){
listJQ.find('li:visible')
.each(function(){
queueJQ.queue(function(){});
})
.animate(
{opacity: "hide"},
{
duration: 300,
complete: function() {
queueJQ.dequeue();
}
}
);
queueJQ.dequeue();
});

queueJQ.queue(function(){
listJQ.find('.p'+packId)
.each(function(){
queueJQ.queue(function(){});
})
.animate(
{opacity: "show"},
{
duration: 300,
complete: function() {
queueJQ.dequeue();
}
}
);

queueJQ.dequeue();
});

paginationJQ.find('li').removeClass('current');
jQuery(parent).addClass('current');
}
return false;
});
})();
});    //]]>
</script>
