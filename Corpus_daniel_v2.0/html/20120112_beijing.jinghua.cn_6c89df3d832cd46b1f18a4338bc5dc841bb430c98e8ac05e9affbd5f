<script type="text/javascript">
//onload=init;
var index=0;
var imgpaths;
var imgtitles;
function init()
{
imgtitles=new Array();
imgpaths=new Array();
index=-1;
nextp();
}
function nextp()
{
var imgtag=document.getElementById("pic");
var imgtitle=document.getElementById("title");
try{
index=(++index)%imgpaths.length;
imgtag.src=imgpaths[index];
imgtitle.innerHTML=imgtitles[index];
}catch(e){}
//showIndex();
}
function prevp()
{
var imgtag=document.getElementById("pic");
var imgtitle=document.getElementById("title");
if(index==0)
index=imgpaths.length;
--index;
imgtag.src=imgpaths[index];
imgtitle.innerHTML=imgtitles[index];
//showIndex();
}
function showIndex()
{
var imgindex=document.getElementById("index");
imgindex.innerHTML=index+1;
}
init();
</script>
