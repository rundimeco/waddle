           <script type='text/javascript'>
function toggleItem(elid)
 {
  var el = document.getElementById(elid);
  if (el == null) return;
  if (el.style.display == '') el.style.display = 'none';
  else el.style.display = '';
 }
function preview()
 {
  var uname = document.getElementById("uname").value;
  var ucomment = document.getElementById("ucomment").value;
  $.post("post/preview", {un: uname, uc: ucomment}, function(data){$('#preview').html(data); });
  document.getElementById('preview').style.display='block';
 }
function send()
 {
  var did = document.getElementById("docid").value;
  var uname = document.getElementById("uname").value;
  var uemail = document.getElementById("uemail").value;
  var ucomment = document.getElementById("ucomment").value;
  var ucaptcha = document.getElementById("captcha").value;
  var uagree = (document.getElementById("agree").checked)?1:0;
  $.post("post/save", {um: uemail, un: uname, uc: ucomment, captcha: ucaptcha, agree: uagree, docid: did}, function(data){$('#comment_form').html(data); });
 }
function set_page()
 {
  var page = parseInt(document.getElementById("page").value, 10);
  var base;
  if (document.baseURI === undefined)
     {
      var baseTags = document.getElementsByTagName ("base");
      if (baseTags.length > 0) base = baseTags[0].href;
      else base = window.location.href;
     }
  else base = document.baseURI;
  if(isNaN(page) || page<1) return false;
  else window.location=base+'medicine_and_health/2011/11/27/327965/'+page+'#comments';
 }
