    <h1 class="title">Słodkie orzeźwienie</h1>	 
    	 

    <!-- begin content --><div class="node">
          <em class="info">Autor: Joanna Janczy. Data publikacji Śro, 2011-05-18 15:22</em>
    <div class="content">
<div style="float: right;">
<script type="text/javascript"><!--
google_ad_client = "pub-4991647332706873";
google_ad_width = 200;
google_ad_height = 200;
google_ad_format = "200x200_as";
google_ad_type = "text_image";
google_ad_channel = "";
google_color_border = "C4B28A";
